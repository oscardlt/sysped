      *** OPPDATERE SVXH-/HEAD-REGISTER
     C     STPRT1        TAG
     C  N32              MOVE      'Q'           THST
      * VED THWS<>TVINR OG TH0035=2 --> SENDE TOLLDEK. I TESTPERIODE
     C   32THWS          IFNE      'TVINR'
     C     TH0035        ANDEQ     '2'
     C                   MOVE      'Q'           THST
     C                   ELSE
     C     EDIM2K        SETLL     EDIM2
     C     EDIM2K1       READPE    EDIM2                                  33
      *
     C                   DOW       *IN33 = *OFF
     C                   SELECT
     C                   WHEN      M1N07 = '045'
     C                   MOVE      'Z'           THST
     C                   LEAVE
     C                   WHEN      M1N07 = '028'
     C                   MOVE      'U'           THST
     C                   LEAVE
     C                   WHEN      M1N07 = '029'
     C                   MOVE      'P'           THST
     C                   LEAVE
     C                   ENDSL
     C     EDIM2K1       READPE    EDIM2                                  33
     C                   ENDDO
N05  C   33              EVAL      THST='X'
      *
     C                   END
     C                   MOVE      *BLANKS       THWS
     C     PRTTYP        IFNE      'M'
     C     PRTTYP        ANDNE     'E'
     C                   exsr      arkivsr
N04  C                   MOVE      THST          XTHST             1
N04  C     SVHK          CHAIN     SVXH                               33
N04  C                   MOVE      XTHST         THST
N04  C                   MOVE      *BLANKS       THWS
     C                   EXCEPT    UPDSVH
