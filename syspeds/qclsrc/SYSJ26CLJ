             PGM        PARM(&OPDF &OPDT &PRT18 &PT18)
             DCL        VAR(&OPDF) TYPE(*CHAR) LEN(7)
             DCL        VAR(&OPDT) TYPE(*CHAR) LEN(7)
             DCL        VAR(&PRT18) TYPE(*CHAR) LEN(10)
             DCL        VAR(&PT18) TYPE(*CHAR) LEN(10)
             DCL        VAR(&JOBK) TYPE(*CHAR) LEN(1)
       /* */
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             OVRPRTF    FILE(SYSJ26P) OUTQ(&PRT18) PAGESIZE(72 80) +
                          CPI(10) OVRFLW(72) FORMTYPE(&PT18)
       /* */
             CALL       PGM(SYSJ26) PARM(&OPDF &OPDT)
       /* */
             ENDPGM
