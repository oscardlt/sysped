             PGM        PARM(&FRALIB &TILLIB &TOMME)
             DCL        VAR(&FRALIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TILLIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TOMME)  TYPE(*CHAR) LEN(1)

             CLRPFM     FILE(&TILLIB/LOGUSRF)
             OVRDBF     FILE(LOGUFNY) TOFILE(&TILLIB/LOGUSRF)
             OVRDBF     FILE(LOGUFGM) TOFILE(&FRALIB/LOGUSRF)
             CALL       PGM(V5FKR41)
             IF         COND(&TOMME = 'J') THEN(CLRPFM +
                          FILE(&FRALIB/LOGUSRF))

             ENDPGM
