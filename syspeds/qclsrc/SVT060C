             PGM        PARM(&USR)
             DCL        VAR(&USR) TYPE(*CHAR) LEN(10)
             DCL        VAR(&LIBF) TYPE(*CHAR) LEN(10)
             DCL        VAR(&LIBFTDS) TYPE(*CHAR) LEN(128)
             DCL        VAR(&LIBFAUT) TYPE(*CHAR) LEN(128)
             DCL        VAR(&LIBFUSR) TYPE(*CHAR) LEN(128)
             DCL        VAR(&LIBFUSRP) TYPE(*CHAR) LEN(128)
             MONMSG     MSGID(CFP0000)
             RTVDTAARA  DTAARA(EDIDTA (171 10)) RTNVAR(&LIBF)

             CRTDIR     DIR(&LIBF) DTAAUT(*RWX) OBJAUT(*ALL)
             MONMSG     MSGID(CPFA0A0)

             CHGVAR     VAR(&LIBFTDS) VALUE(&LIBF *TCAT '/' *TCAT 'TDS')
             CRTDIR     DIR(&LIBFTDS) DTAAUT(*RWX) OBJAUT(*ALL)
             MONMSG     MSGID(CPFA0A0)

             CHGVAR     VAR(&LIBFAUT) VALUE(&LIBFTDS *TCAT '/' *TCAT 'AUT')
             CRTDIR     DIR(&LIBFAUT) DTAAUT(*RWX) OBJAUT(*ALL)
             MONMSG     MSGID(CPFA0A0)

             CHGVAR     VAR(&LIBFUSR) VALUE(&LIBFAUT *TCAT '/' *TCAT 'USR')
             CRTDIR     DIR(&LIBFUSR) DTAAUT(*RWX) OBJAUT(*ALL)
             MONMSG     MSGID(CPFA0A0)

             CHGVAR     VAR(&LIBFUSRP) VALUE(&LIBFUSR *TCAT '/' *TCAT &USR)
             CRTDIR     DIR(&LIBFUSRP) DTAAUT(*RWX) OBJAUT(*ALL)
             MONMSG     MSGID(CPFA0A0)

ENDPGM
