     C*
     C                   ENDSR
      **************************************************************************
     c     srfali        begsr
      **************************************************************************
     C                   move      'N'           slettf            1
n70  c                   move      'N'           slett             1
     C                   SETOFF                                       34
     C     HEADFK        SETLL     FAKT
      *
      * start leseløkke
      *
     C     *IN34         DOWEQ     '0'
     C     HEADFK        READE     FAKT                                   34
      *
      * IKKE FAKTURERT og linje 0 blir tatt hånd om annet sted
      *
     C  N34FAFAKT        ifeq      *ZEROS
     c     faopko        andle     'C'
n72  c     fali          andne     0
      *
      * KUN S OG K LINJER
      *
N60  C     FASK          IFEQ      'S'
N60  C     FASK          OREQ      'K'
N59  C                   MOVE      FACURI        FACURG            3
N70  C                   move      fakunr        fakung            8 0
      *
      * RETTER S/K LINJER m.h.t. PART KUNDENUMMER OG VALUTAKODE
      *
N68   *------------------------------------------
n68   * WSKNKF = 0 FIKS WSKNSF TIL RIKTIG SIDE
n68   * FAKTURERING KUN MOT 1 SIDE
N68   *------------------------------------------
N68  C     WSKNKF        IFEQ      0
N68  C                   MOVE      WSKDFF        FASK
N68  C                   MOVE      WSKNSF        FAKUNR
n68  C     WSVALS        IFEQ      FICURR
n68  C                   MOVE      *BLANKS       FACURI
N68  C                   MOVE      FICURR        FAVAL
n68  C                   ELSE
n68  C                   MOVE      WSVALS        FACURI
N68  C                   MOVE      WSVALS        FAVAL
N60  C                   END
N68  C                   ELSE
N60   *------------------------------------------
N68   * WSKNKF <> 0 FIKS WSKNSF TIL RIKTIG SIDE
n68   * FAKTURERING MOT TO PARTER
N68   *------------------------------------------
N68   *
N68  C     WSKDFF        IFEQ      FASK
N68  C     WSVALS        IFEQ      FICURR
N68  C                   MOVE      WSKNSF        FAKUNR
n68  C                   MOVE      *BLANKS       FACURI
n68  C                   MOVE      FICURR        FAVAL
N68  C                   ELSE
N68  C                   MOVE      WSVALS        FACURI
N68  C                   MOVE      WSVALS        FAVAL
N68  C                   END
n68  C                   ELSE
n68  C                   MOVE      WSKNKF        FAKUNR
N68  C     WSVALK        IFEQ      FICURR
n68  C                   MOVE      *BLANKS       FACURI
n68  C                   MOVE      FICURR        FAVAL
N68  C                   ELSE
N60  C                   MOVE      WSVALK        FACURI
N68  C                   MOVE      WSVALK        FAVAL
n68  C                   END
N68  C     WSVALK        IFEQ      FICURR
N68  C                   MOVE      *BLANKS       FACURI
N68  C                   MOVE      FICURR        FAVAL
N68  C                   ELSE
n68  C                   MOVE      WSVALK        FACURI
n68  C                   MOVE      WSVALK        FAVAL
N68  C                   END
n68  C                   END
n68  C                   END
n68  C*                  END
N68  C*
N68  C* TESTER VALUTAKODE/BELØP VAREVERDI
N68   *
N68   * valutakode endret
n68   *
n72  c     facuri        ifne      facurg
N68  C     FACURI        IFEQ      *blanks
n68  C                   Z-add     FABELN        FABELV
n68  C                   else
N68  C     valke4        chain     valul01                            49
N68  C     *IN49         IFEQ      '0'
n68  C     FABELN        MULT      OMRFAK        XWORK            15 4
n68  C     XWORK         DIV(H)    VALKU1        FABELV
N68  C                   end
N68  C                   end
N72  C                   end
n70  c     facuri        ifne      facurg
n70  c     fakunr        orne      fakung
n70  c                   move      'J'           slett             1
s74  c*                  end
n68  C                   UPDATE    FAKTR
N74  c                   end
N68  C                   end
n68  C                   end
N68  C                   END
n68   *
n68   * SLUTT PÅ LESELØKKEN
N68   *
      *
      * sletter eventuell ferdigmeldte samlefaktura
      * hvis valta eller kunde er edret
N70  C     TRKDFF        ifne      WSKDFF
n70  c     WSKNKF        orne      wsknkfg
n70  c     wsknsf        orne      wsknsfg
N70  c     slett         oreq      'J'
     c                   setoff                                       34
     c     HEADFK        setll     saml1
     c     *in(34)       doweq     '0'
     C     HEADFK        reade     saml1                                  34
     c     *in(34)       ifeq      '0'
     c                   delete    samlr
     c                   move      'J'           slettf
     c                   end
     c                   end
      *
      * sletter eventuell ferdigmeldt til faktura
      *
     c                   setoff                                       34
     c     HEADFK        setll     ferdl01
     c     *in(34)       doweq     '0'
     C     HEADFK        reade     ferdl01                                34
     c     *in(34)       ifeq      '0'
     c                   delete    ferdr
     c                   move      'J'           slettf
     c                   end
     c                   end
     c     slettf        ifeq      'J'
     c                   exfmt     SLETFERD
     c                   end
n70  c                   end
     c                   endsr
      **********************************************************
     C     endvalt       begsr
      **********************************************************
      *
      * HENTER INFO OM FRAKTTILLEGG PÅ NY
     C                   MOVE      'H'           UUR
     C                   MOVE      *ZEROS        UVALP
     C                   CALL      'SYFA24B'
     C                   PARM                    UUR               1
     C                   PARM      WSAVD         UAVDOP            4 0
     C                   PARM      WSKNSF        UKNOP             8 0
     C                   PARM                    WSVALT
     C                   PARM                    UVALP             3 1
     C                   MOVE      UVALP         WSVALP
     C                   move      wsknsf        prevalt           8 0
     C                   ENDSR
     C***********************************************
     C     EFRABR        BEGSR
     C***********************************************
     C*
      * Gir local data area navnet LDADS internt i programmet.
      * Dette for å få oppdatert LDA etter å ha hentet routing
     C     *DTAARA       DEFINE    *LDA          LDADS
     C                   MOVEL     *BLANKS       DSTINF
     C                   OUT       LDADS
     C                   MOVE      'N'           XANRUT            1
     C                   MOVE      'N'           XFLYTM            1
     C                   Z-ADD     0             UFLAGG
     C                   Z-add     1             uulope            3 0
     C     WSPK8         IFEQ      'J'
     C                   CALL      'SYEF10B'
     C                   PARM                    FIFIRM
     C                   PARM                    WSAVD
     C                   PARM                    WSOPD
     C                   PARM                    uulope
     C                   PARM                    XSTLF            15
     C                   PARM                    XKTLF            15
     C                   PARM                    XANRUT            1
     C                   PARM                    XFLYTM            1
     C                   PARM                    HØ
     C                   PARM                    LE
     C                   PARM                    BR
     C                   PARM                    AN
     C                   PARM                    WSMSG
     C     UFLAGG        PARM      UFLAGG        UFLAGG
     C*
     C                   ELSE
     C                   CALL      'SYEF10C'
     C                   PARM                    FIFIRM
     C                   PARM                    WSAVD
     C                   PARM                    WSOPD
     C                   PARM                    uulope
     C                   PARM                    XSTLF            15
     C                   PARM                    XKTLF            15
     C                   PARM                    XANRUT            1
     C                   PARM                    XFLYTM            1
     C                   PARM                    HØ
     C                   PARM                    LE
     C                   PARM                    BR
     C                   PARM                    AN
     C                   PARM                    WSMSG
     C     UFLAGG        PARM      UFLAGG        UFLAGG
     C                   END
     C*
     C     UFLAGG        COMP      3                                      03
     C     UFLAGG        COMP      5                                      05
     C     UFLAGG        COMP      1                                      12
     C*
     C                   ENDSR
     C*
N29   ***********************************************
N29  C     PRTSKILL      BEGSR
N29   ***********************************************
N29  C
N29  C                   MOVE      '  '          SCATYP            2
N29  C                   MOVEL     WSAVD         SCASTR11         11
N29  C                   MOVE      WSOPD         SCASTR11         11
N29  C                   MOVEL     SCASTR11      SCASTR           20
N29  C                   CALL      'ARCA000CL'
N29  C                   PARM                    SCATYP
N29  C                   PARM                    SCASTR
N29  C                   ENDSR
N38   ***********************************************
N38  C     ABUD          BEGSR
N38   ***********************************************
N48  C     PRIKK         KLIST
N48  C                   KFLD                    KUNDALF           8
N48  C                   KFLD                    XAVTN             1
N48  C                   KFLD                    FAVK
N48  C                   MOVE      '0'           XAVTN             1
N38  C     hepro         ifne      0
N38  C     HEPRO         CHAIN     TURER14                            34
N38  C  N34              MOVE      TUKNT2        TRANUM
N38  C  N34TRAKEY        CHAIN     TRAN                               34
N38  C  N34              Z-ADD     VMTRLE        FALEVN
N38  C  N34LEVE1K        CHAIN     LEVEL01                            34
N48  C                   MOVE      KUNDNR        KUNDALF
N38  c     *IN34         ifeq      '0'
N48  C     PRIKK         CHAIN     PRIK                               34
N38  C                   MOVE      KUNDNR        KUNDGM            8 0
N38  c                   move      heavd         uavda             4
N38  c                   move      heopd         uopda             7
N38  c                   move      kundnr        uknrA             8
N38  C                   MOVE      *BLANKS       UVAL              3
S48  C*                  MOVE      FAVK          UGEB              3
N48  C                   MOVE      'FRA'         UGEB              3
N38  C                   MOVE      'FRA'         UGEBTY            3
N38  C                   MOVE      *ZEROS        UUBELØ           13 2
N38  C                   MOVE      KUNDNR        UUKN              8 0
N38  C                   MOVE      FASK          USKA              1
N38  C                   CALL      'SYGE14A'
N38  C                   PARM                    UAVDA
N38  C                   PARM                    UOPDA
N38  C                   PARM                    UKNRA
N38  C                   parm                    uska
N38  C                   PARM                    UGEB
N38  C                   PARM                    UGEBTY
N38  C                   PARM                    UVAL
N38  C                   PARM                    UUBELØ
N38  C                   MOVE      KUNDGM        KUNDNR            8 0
N38  C     UUBELØ        IFNE      0
N48  C     PKBELØ        IFNE      0
N48  c  N34              mult      pkbeLØ        uubelØ
N48  c  N34              div       100           uubelØ
N48  C                   END
N38  C                   Z-add     UUBELØ        FABELU
N38  C                   MOVE      UVAL          FACU33
N38  C                   MOVE      'A'           FAKDUK
N38  C                   EXSR      KOSTAU
N38  C                   end
N38  C                   end
N38  C                   END
N38  C                   ENDSR
N38   ***********************************************
N38  C     KOSTAU        BEGSR
N38   ***********************************************
N38   *  Automatisk budsjettering
N38   *  BRUKER FAJOUR - TIL Å LAGRE X-REF TIL KOSBU
N38   *  FAJOUR ER "LEDIG" HELT TIL OVERFØRING TIL REGNSKAP.
N38   *
N38  C                   Z-ADD     FAJOUR        KTNR
N38   * TILDELER REFNUMMER
N38   *
N38  C     KTNR          CHAIN(N)  KOSBU                              34
N49  C  N34BUST          IFNE      'F'
N38  C   34              MOVE      *ZEROS        FAJOUR
N38  C     FAJOUR        IFEQ      *ZEROS
N38  C     'Å'           CHAIN     KOSTT                              34
N38  C   34              MOVE      'Å'           KTTYP
N38  C   34              Z-ADD     1000000       KTNR
N38  C   34              WRITE     KOSTTR
N38  C  N34              ADD       1             KTNR
N38  C  N34              UPDATE    KOSTTR
N38  C                   END
N38   *
N38  C     KTNR          CHAIN     KOSBU                              34
N38  C   34              MOVE      ' '           BUST
N38  C   34              MOVE      *ZEROS        BUBNR2
N38  C* IKKE OPPDATER HVIS FØRT/PLUKKET TIL KOSTN.BILAG..
N38  C     BUST          IFEQ      ' '
N38  C     BUBNR2        ANDEQ     *ZEROS
N38  C                   MOVE      KTNR          BUBNR
N38  C                   MOVEL     HEDTÅ         BUPCC
N38  C                   MOVE      HEDTÅ         BUPÅR
N38  C                   MOVE      HEDTM         BUPMN
N38  C                   MOVE      *ZEROS        BUTNR
N38  C                   MOVE      FALEVN        BULNR
N38  C                   MOVE      FACU33        BUVAL
N38  C     BUVAL         IFEQ      '   '
N38  C                   MOVE      FICURR        BUVAL
N38  C                   END
N38  C                   Z-ADD     FABELU        BUBL
N38  C                   MOVE      FAVK          BUVK
N38  C                   MOVE      HESG          BUSG
N38   * Sikre riktig BUTAVD (kan være ulik HEAVD hvis samlast)
N38  C                   MOVE      *ZEROS        TUAVD
N38  C                   MOVE      *ZEROS        TUPRO
N38  C     HEPRO         IFNE      *ZEROS
N38  C     HEPRO         CHAIN     TURER14                            31
N38  C                   MOVE      TUAVD         BUTAVD
N38  C                   MOVE      TUPRO         BUTUNR
N38  C                   END
N38  C                   MOVE      HEAVD         BUOAVD
N38  C                   MOVE      HEOPD         BUOPD
N38  C* + VISSE FELTER "INITIERES" KUN VED NY (BETING  34 LAGT PÅ)
N38  C   33              MOVE      'E'           BUBLST
N38  C   33              MOVE      'E'           BUTYPE
N38  C   33              MOVE      *BLANKS       BUTXT
N38  C   33              MOVE      *BLANKS       BUBILK                         *BILKODE
N38  C   33              MOVE      *BLANKS       BUBILN                         *BILNUMMER
N38  C   33              MOVE      *ZEROS        BUFEDT                         *FERJ/TURDTO
N38  C   33              MOVE      'O'           BUFER                          *O = OPPDRAG
N38  C   33              MOVE      *ZEROS        BUBNR2                         *FØRT BNR
N38  C   33              MOVE      *ZEROS        BUBNR3                         *"SLAVE" AV.
N38  C   33              MOVE      *ZEROS        BULNR3                         SLAVE-LØPENR
N38  C   33              MOVE      *ZEROS        BUBLFD                         *FORDELT BEL
N38  C                   END
N38  C   34              WRITE     KOSBUR
N38  C  N34              UPDATE    KOSBUR
N38  C                   MOVE      KTNR          FAJOUR
N49  C                   END
N38  C                   ENDSR
N58  C***********************************************
N58  C     DOKUFVSR      BEGSR
N58  C***********************************************
N58   * HENTER VERDIER FRA MOROPPDRAG
N58  C     DOKUFVKG      CHAIN     DOKUFV                             31
N58  C     *IN31         IFEQ      '0'
N58  C                   MOVE      HEAVD         FVAVD
N58  C                   MOVE      HEOPD         FVOPD
N58   * SER OM DENNE LINJE ER PÅ DUPP
N58  C     DOKUFVKN      CHAIN     DOKUFV                             31
N58  C   31              WRITE     DOKUFVR
N58  C                   END
N58  C                   ENDSR
