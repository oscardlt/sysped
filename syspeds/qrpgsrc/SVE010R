     C                   CLEAR                   EDIMR
     C                   EVAL      M0004=SVTF_0004
     C                   EVAL      M0010=SVTF_0010
S03  C*                  EVAL      M0035=SVTF_0035
N03  C                   EVAL      M0035=SVEH_0035
     C                   MOVEL     SVEH_SYAV     W0062            11
     C                   MOVE      SVEH_SYOP     W0062
     C                   EVAL      M0062=W0062
     C                   EVAL      M0065='CUSDEC'
     C                   EVAL      M1001=SVEH_MTYP
     C                   EVAL      M1004=SVEH_TUID
     C                   EVAL      M1225=SVEH_MTYP
     C                   EVAL      M1N07='SVE'
     C                   EVAL      MMN=CMN
     C                   EVAL      MSR='S'
     C                   MOVEL     WDT           MDT
     C                   MOVEL     WTM           MTM
     C                   EVAL      MAVD=SVEH_SYAV
     C                   EVAL      MTDN=SVEH_SYOP

     C                   WRITE     EDIMR
