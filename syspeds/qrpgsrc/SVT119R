     C     XML3          BEGSR

     C                   MOVE      *BLANKS       W11               3
     C                   MOVE      *BLANKS       W12               2
     C                   MOVE      *BLANKS       W13               1
     C                   MOVE      *BLANKS       W14               4
     C                   MOVE      *BLANKS       W15              10
     C                   MOVE      *BLANKS       W16              10

     C     B01           SCAN      RDATA         X                        20
     C                   IF        *IN20
     C                   ADD       15            X
     C     '"'           SCAN      RDATA :X      Y                        21
     C  N21              EVAL      Y=999
     C                   IF        *IN20
     C     Y             SUB       X             Z
     C                   EVAL      W11=%SUBST(RDATA :X:Z)
     C                   ENDIF
     C                   ENDIF

     C     B02           SCAN      RDATA         X                 3 0    20
     C                   IF        *IN20
     C                   ADD       21            X
     C     '"'           SCAN      RDATA :X      Y                 3 0    21
     C                   IF        *IN20
     C     Y             SUB       X             Z                 3 0
     C                   EVAL      W12=%SUBST(RDATA :X:Z)
     C                   ENDIF
     C                   ENDIF

     C     B03           SCAN      RDATA         X                        20
     C                   IF        *IN20
     C                   ADD       15            X
     C     '"'           SCAN      RDATA :X      Y                        21
     C                   IF        *IN20
     C     Y             SUB       X             Z
     C                   EVAL      W13=%SUBST(RDATA :X:Z)
     C                   ENDIF
     C                   ENDIF

     C     B04           SCAN      RDATA         X                        20
     C                   IF        *IN20
     C                   ADD       26            X
     C     '"'           SCAN      RDATA :X      Y                        21
     C                   IF        *IN20
     C     Y             SUB       X             Z
     C                   EVAL      W14=%SUBST(RDATA :X:Z)
     C                   ENDIF
     C                   ENDIF

     C     B05           SCAN      RDATA         X                 3 0    20
     C                   IF        *IN20
     C                   ADD       9             X
     C     '"'           SCAN      RDATA :X      Y                 3 0    21
     C                   IF        *IN20
     C     Y             SUB       X             Z                 3 0
     C                   EVAL      W15=%SUBST(RDATA :X:Z)
     C                   ENDIF
     C                   ENDIF

     C     B06           SCAN      RDATA         X                        20
     C                   IF        *IN20
     C                   ADD       11            X
     C     '"'           SCAN      RDATA :X      Y                        21
     C                   IF        *IN20
     C     Y             SUB       X             Z
     C                   EVAL      W16=%SUBST(RDATA :X:Z)
     C                   ENDIF
     C                   ENDIF

     C                   IF        W15=*BLANKS
     C                   EVAL      W15='9999-12-31'
     C                   ENDIF
     C
     C                   EVAL      WDT10=W15
     C                   EVAL      WDT08Y=WDT10Y
     C                   EVAL      WDT08M=WDT10M
     C                   EVAL      WDT08D=WDT10D

     C     WDT08         IFLT      WDT
     C                   EVAL      W11=*BLANKS
     C                   EVAL      W12=*BLANKS
     C                   EVAL      W13=*BLANKS
     C                   EVAL      W14=*BLANKS
     C                   EVAL      W15=*BLANKS
     C                   EVAL      W16=*BLANKS
     C                   ENDIF

     C                   SELECT
     C                   WHEN      SVTX19_11A=*BLANKS
     C                   EVAL      SVTX19_11A=W11
     C                   EVAL      SVTX19_12A=W12
     C                   EVAL      SVTX19_13A=W13
     C                   EVAL      SVTX19_14A=W14
     C                   EVAL      SVTX19_15A=W15
     C                   EVAL      SVTX19_16A=W16
