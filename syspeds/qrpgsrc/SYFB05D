     C*
     C                   EXSR      INIT
     C*
     C     START         TAG
     C                   MOVE      '1'           *IN93
     C                   WRITE     SUBCTL
     C                   MOVEA     '00'          *IN(93)
     C                   Z-ADD     *ZEROS        ZRECNO
     C                   MOVE      W2TRAN        YTRAN             8 0
      *_____________
      * Fyll Subfile
      *"""""""""""""
     C     KEYW2         SETLL     TRAPRAV
     C     START1        TAG
     C                   READ(N)   TRAPRAV                                94
     C   94              MOVE      *BLANKS       TGUSR
     C   94              MOVE      *BLANKS       WSNR
      *
     C     START2        TAG
      *
     C  N94              DO
     C                   DO        7
     C                   ADD       1             ZRECNO
s01  c*                  move      *blanks       VMNAVN
n01  c                   move      *blanks       LNAVN
s01  c*    keytrf        chain     tran                               34
n01  c     keytrf        chain     level01                            34
     C                   WRITE     SUBFIL
     C                   READ(N)   TRAPRAV                                94
     C  N94              MOVE      TRTRAN        YTRAN
     C  N94              END
     C                   END
     C*
     C                   Z-ADD     ZRECNO        YRECNO            5 0
     C     START3        TAG
     C                   MOVEA     '11'          *IN(91)
     C     ZRECNO        IFEQ      0
     C                   EXFMT     NYWRK
     C                   GOTO      ST41
     C                   ELSE
     C                   WRITE     BUNN
     C                   END
     C                   SETOFF                                       98
     C                   EXFMT     SUBCTL
     C*
     C*** AVSLUTT
     C     ST41          TAG
     C   03              GOTO      SLUTT
     C*
     C*** FRISK OPP
     C   05              GOTO      START
     C*
     C*** LAGE NY LANDKODE
     C   06              GOTO      START5
     C*** SØK FRA LANDKODE
     C   08              GOTO      START6
     C*
     C*** BLA VIDERE
     C   25              DO
     C                   SETOFF                                       25
     C                   Z-ADD     YRECNO        ZRECNO
     C  N94KEYRO         SETLL     TRAPRAV
     C                   GOTO      START2
     C                   END
     C*
     C     ZRECNO        IFEQ      0
     C                   EXFMT     NYWRK
     C                   GOTO      ST41
     C                   ELSE
     C                   EXSR      HENT
     C     *IN27         IFEQ      *ON
     C     WSNR          ANDEQ     '1'
     C                   GOTO      SLUTT
     C                   ENDIF
     C   98              GOTO      START3
     C                   END
     C*
     C*** SLETT LANDKODE
     C     WSNR          IFEQ      '4'
     C                   MOVE      ' '           WSNR
     C                   EXFMT     VAREL3
     C  N12              DO
     C     KEYPF         CHAIN     TRAPRAV                            33
     C  N33              DELETE    TRAPRAVR
     C                   EXSR      MOVB1
     C                   MOVEL     '*DLT'        TGUSR
     C                   END
     C                   UPDATE    SUBFIL
     C                   GOTO      START3
     C                   END
     C*
     C*** LAGE/ENDRE LANDKODE
     C     START5        TAG
     C                   EXFMT     VARELI
     C                   MOVEA     '000'         *IN(68)
     C                   SETOFF                                       30
      *søk transportør
     C     *in04         IFEQ      '1'
     C                   CALL      'SYTR04A'
     C                   PARM                    WSTRAN
     C                   GOTO      START5
     C                   END
     C   03              GOTO      SLUTT
     C   12              DO
     C   40              UPDATE    SUBFIL
     C                   SETOFF                                       40
     C                   EXSR      MOVB2
     C                   GOTO      START3
     C                   END
     C                   EXSR      TESTB1
     C   30              GOTO      START5
     C                   GOTO      START3
     C*
     C*** SØK FRA transportør
     C     START6        TAG
     C                   EXFMT     VAREL2
     C   03              GOTO      SLUTT
     C                   GOTO      START
     C*
     C     SLUTT         TAG
     C                   SETON                                        LR
     C                   RETURN
     C*
     C***********************************************
     C     TESTB1        BEGSR
     C***********************************************
     C  N40              DO
      *transportør
s01  C*    KEYTRW        CHAIN     TRAN                               71
n01  C     KEYTRW        CHAIN     LEVEL01                            71
s01  C*  71              MOVE      'SPT0133'     MSGNR
N01  C   71              MOVE      'SPL0029'     MSGNR
     C   71              SETON                                        30
     C   71              GOTO      SLTB
     C                   END
     C*
     C*
     C*** OPPDATERE PÅ REGISTER OG SUBFIL
     C     KEYWS         CHAIN     TRAPRAV                            33
     C                   EXSR      MOVWLK
     C  N33              UPDATE    TRAPRAVR
     C   33              WRITE     TRAPRAVR
     C   40              UPDATE    SUBFIL
     C  N40              ADD       1             YRECNO
     C  N40              Z-ADD     YRECNO        ZRECNO
     C  N40              WRITE     SUBFIL
     C                   WRITE     SUBCTL
     C                   SETOFF                                       40
     C                   EXSR      MOVB2
     C*
     C     SLTB          ENDSR
     C*
     C***********************************************
     C     HENT          BEGSR
     C***********************************************
     C                   MOVEA     '00'          *IN(91)
     C     *IN98         DOWEQ     '0'
     C                   READC     SUBFIL                                 98
     C  N98WSNR          IFEQ      '2'
     C     KLKNV4        COMP      '*DLT'                                 98
     C   98              GOTO      SLHENT
     C                   EXSR      MOVLKW
     C                   MOVE      ' '           WSNR
     C                   SETON                                        40
     C                   GOTO      SLHENT
     C                   END
     C  N98WSNR          IFEQ      '4'
     C     KLKNV4        COMP      '*DLT'                                 98
     C                   GOTO      SLHENT
     C                   END
     C                   END
     C*
     C     SLHENT        ENDSR
     C*
     C***********************************************
