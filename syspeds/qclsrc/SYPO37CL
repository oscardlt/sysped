/* KINGSRØD SKUFF 2    */
/*N01 * hopp ut ved f3 / avbrudd   */
             PGM
             DCL        VAR(&CPI) TYPE(*CHAR) LEN(2) VALUE('15')
             DCL        VAR(&HOLD) TYPE(*CHAR) LEN(4)
             DCL        VAR(&SAVE) TYPE(*CHAR) LEN(4)
             DCL        VAR(&KOPI) TYPE(*DEC) LEN(2 0)
             DCL        VAR(&STR) TYPE(*CHAR) LEN(5)
             DCL        VAR(&LAS) TYPE(*CHAR) LEN(10)
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1)
             DCL        VAR(&AVDA) TYPE(*CHAR) LEN(4)
             DCL        VAR(&AVD) TYPE(*DEC) LEN(4 0)
             DCL        VAR(&UTNR) TYPE(*DEC) LEN(3 0) VALUE(69)
             DCL        VAR(&DEV) TYPE(*CHAR) LEN(10)
             DCL        VAR(&USER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&FORMS) TYPE(*CHAR) LEN(10)
             DCL        VAR(&HEAD) TYPE(*CHAR) LEN(1)
             DCL        VAR(&KOPFM) TYPE(*CHAR) LEN(5)
             RTVJOBA    USER(&USER)

             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CALL       PGM(SYFA00) PARM(*IN01)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT))

             RTVDTAARA  DTAARA(*LDA (15 4)) RTNVAR(&AVDA)
             CHGVAR     VAR(&AVD) VALUE(&AVDA)
             CALL       PGM(SYGE01) PARM(&AVD &UTNR &DEV &FORMS +
                          &HEAD &KOPFM)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT))
        /* */
             CALL       PGM(SYGE18) PARM(&UTNR &DEV &LAS &STR &FORMS +
                          &HOLD &SAVE &KOPI &IN01)
/*N01*/      IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT))
        /* */
             OVRPRTF    FILE(SYPO37P) PAGESIZE(70 120) CPI(15) +
                          OVRFLW(54) DRAWER(2) OUTQ(&DEV) +
                          FORMTYPE(&FORMS) COPIES(&KOPI) +
                          HOLD(&HOLD) SAVE(&SAVE)
             CALL       PGM(SYPO37R)
/*N01*/      SNDPGMMSG  MSG('Utskrift av fraktavtaler er sendt til +
                          printeren.') MSGTYPE(*INFO)

 SLUTT:      ENDPGM
