     C     YRECNO        IFGT      0
     C     1             CHAIN     SUBFIL                             33
     C     NVEH          IFEQ      '*D'
     C     TRLMNK        SETLL     TRILMN
     C     TRLMNK        READE     TRILMN                                 33
     C     *IN33         DOWEQ     '0'
     C                   DELETE    TRILMNR
     C     TRLMNK        READE     TRILMN                                 33
     C  N33              ENDDO
     C                   ELSE
     C                   MOVE      NVEH          UNEH
     C                   MOVE      NVNT          UNNT
     C                   MOVE      NVNTEH        UNNTEH
     C                   EVAL      UNMN = NVMN
     C                   MOVE      NVMNSK        UNMNSK
S02  C*                  MOVE      TVEH          UTEH
S02  C*                  MOVE      TVNT          UTNT
S02  C*                  MOVE      TVMN          UTMN
S02  C*                  MOVE      TVMNSK        UTMNSK
     C                   ADD       1             XANTL
     C                   END
     C     2             DO        YRECNO        ZRECNO
     C     ZRECNO        CHAIN     SUBFIL                             33
     C     NVEH          IFEQ      '*D'
     C     TRLMNK        SETLL     TRILMN
     C     TRLMNK        READE     TRILMN                                 33
     C     *IN33         DOWEQ     '0'
     C                   DELETE    TRILMNR
     C     TRLMNK        READE     TRILMN                                 33
     C  N33              ENDDO
     C                   ELSE
     C     XANTL         IFEQ      0
S02  C*                  MOVE      TVEH          UTEH
S02  C*                  MOVE      TVNT          UTNT
S02  C*                  MOVE      TVMN          UTMN
S02  C*                  MOVE      TVMNSK        UTMNSK
S02  C*                  MOVE      NVEH          UNEH
S02  C*                  MOVE      NVNT          UNNT
S02  C*                  MOVE      NVNTEH        UNNTEH
S02  C*                  EVAL      UNMN = NVMN
S02  C*                  MOVE      NVMNSK        UNMNSK
     C                   ADD       1             XANTL
     C                   ELSE
     C                   ADD       1             XANTL
     C                   WRITE     TRILKLR
     C                   MOVE      TVMN          XTMN             42
     C                   MOVE      TVMNSK        XTMNSK            2
     C                   MOVEL     NVMN          XNMN             35
     C                   MOVE      NVMNSK        XNMNSK            2
     C     TRLMNK        CHAIN     TRILMN                             33
     C                   MOVE      XTMN          TVMN
     C                   MOVE      XTMNSK        TVMNSK
     C                   EVAL      NVMN = XNMN
     C                   MOVE      XNMNSK        NVMNSK
     C     *IN33         IFEQ      '1'
     C                   WRITE     TRILMNR
     C                   ELSE
     C                   UPDATE    TRILMNR
     C                   END
     C                   END
     C                   END
     C                   ENDDO
     C                   END
