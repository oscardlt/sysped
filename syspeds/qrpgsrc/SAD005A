     C*
     C                   EXSR      INIT
     C*
     C     START         TAG
N01  C                   IF        UTDN < 0
N04  C                   EVAL      UTDN = UTDN * -1
N04  C     SADHK         CHAIN     SADH                               99
N04  C                   MOVE      SITLL         X_SITLL          10
N04  C                   EVAL      UTDN = UTDN * -1
N01  C     SADHK         CHAIN     SADH                               99
S04  C*    SIST          COMP      'P'                                    39
N04  C     SITLL         COMP      X_SITLL                            3939
N03  C                   IF        SIMF <> 'DEB' AND SIMF <> 'DRE'
N03  C                             AND SIMF <> 'DSO'
N04  C                             AND SITLL = X_SITLL
N03  C                   EVAL      SITLL = *BLANKS
N03  C                   EVAL      SITLE = *BLANKS
N03  C                   EVAL      SIDTG = *BLANKS
N03  C                   END
N01  C                   EXFMT     BILDEOM
N01  C                   ELSE
     C                   EXFMT     BILDE
N01  C                   END
     C                   SETOFF                                       99
     C*
     C   01              Z-ADD     1             UFLAGG
     C   03              Z-ADD     3             UFLAGG
     C   12              Z-ADD     9             UFLAGG
     C   01
     COR 03
     COR 12              GOTO      SLUTT
     C   04              DO
     C                   EXSR      VISR28
     C                   GOTO      START
     C                   END
     C   14              DO
     C                   EXSR      SRM
     C   03              GOTO      SLUTT
     C                   GOTO      START
     C                   END
     C*
     C                   EXSR      UTDEL2
     C  N01
     CANN03              GOTO      START
     C*
     C     SLUTT         TAG
     C                   SETON                                        LR
     C                   RETURN
     C*
     C***********************************************
     C     VISR28        BEGSR
     C***********************************************
     C     SIFID         IFNE      99999999
     C                   SETON                                        99
     C                   ELSE
     C                   CALL      'VISRUB28'
     C                   PARM                    UAVD
     C                   PARM                    UTDN
     C                   PARM                    UFLAGG
     C                   END
     C*
     C                   ENDSR
     C*
     C***********************************************
     C     UTDEL2        BEGSR
     C***********************************************
     C     SADHK         CHAIN     SADW                               33
     C     *IN33         IFEQ      '1'
     C                   CALL      'SAD005B'
     C                   PARM                    UAVD
     C                   PARM                    UTDN
     C                   PARM                    UFLAGG
     C                   ELSE
     C                   MOVE      '9'           UFLAGG
     C                   CALL      'SAD061B'
     C                   PARM                    UFIRM             2
     C                   PARM                    UAVD
     C                   PARM                    UTDN
     C                   PARM                    UUR               1
     C                   PARM                    UFAKNO            9 0
     C                   PARM                    UFT              17
     C                   PARM                    UWS              10
     C                   PARM                    UTVIN             1
     C                   PARM                    UIN28             1
     C                   PARM                    UIN29             1
     C                   PARM                    UIN36             1
     C                   PARM      SIDT          USIDT             8 0
     C                   PARM                    UFLAGG
     C                   END
     C     UFLAGG        COMP      1                                      01
     C     UFLAGG        COMP      3                                      03
     C     UFLAGG        COMP      9                                      12
     C*
     C                   ENDSR
     C*
     C***********************************************
     C     INIT          BEGSR
     C***********************************************
     C     SADHK         KLIST
     C                   KFLD                    UAVD
     C                   KFLD                    UTDN
     C     FRIKEY        KLIST
     C                   KFLD                    UAVD
     C                   KFLD                    UTDN
     C                   KFLD                    WSKODE
     C     *LIKE         DEFINE    FSKODE        WSKODE
     C                   MOVEL     '*CR'         WSKODE
     C*
     C     *ENTRY        PLIST
     C                   PARM                    UAVD              4 0
     C                   PARM                    UTDN              7 0
     C                   PARM                    UFLAGG            1 0
     C                   Z-ADD     0             UFLAGG
     C                   MOVE      *ZEROS        SIDT
     C                   MOVE      *ZEROS        WSDT
     C*
     C     SADHK         CHAIN     SADH                               99
N01  C                   IF        UTDN < 0
N01  C     SADHK         CHAIN     SADHOM                             99
N01  C                   END
     C     FRIKEY        CHAIN     FRISOK                             59
     C  N59              EVAL      EXTREF=FSSOK
     C   59              EVAL      EXTREF=*BLANKS
     C* RETURNCODE 0 MEANS NOT FOUND
     C   99              SETON                                        LR
     C   99              RETURN
     C     SIDP          COMP      64                                     51
     C  N51SIDP          COMP      65                                     51
     C  N51SIDP          COMP      49                                     51
     C     SIRABX        IFEQ      *BLANKS
     C                   MOVE      *ZEROS        WSRAB
     C                   ELSE
     C                   MOVE      SIRAB         WSRAB
     C                   END
     C                   MOVE      SIFIDD        WSFIDD
     C                   MOVE      SIFIDM        WSFIDM
     C                   MOVE      SIFIDÅ        WSFIDÅ
     C                   MOVE      SIDTD         WSDTD
     C                   MOVE      SIDTM         WSDTM
     C                   MOVE      SIDTÅ         WSDTÅ
     C*
     C                   ENDSR
     C*
