/* 01 JOV UTUPE->UPROG + USN substring fra &MBR (som i ediOCC)*/
/* 01     INGEN MERKING - BLE SÅ ROTETE                       */
             PGM        PARM(&LIB &FIL &MBR)
             DCL        VAR(&FILNVN) TYPE(*CHAR) LEN(256)
             DCL        VAR(&LIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&FIL) TYPE(*CHAR) LEN(10)
             DCL        VAR(&MBR) TYPE(*CHAR) LEN(10)
             DCL        VAR(&SN) TYPE(*CHAR) LEN(9)
             DCL        VAR(&PROG) TYPE(*CHAR) LEN(10)

             CHGVAR     VAR(&SN) VALUE(%SUBSTRING(&MBR 2 9))

/* Hent IFS filnavn via &MBR */
             CALL       PGM(XMLRCVR1) PARM(&MBR &FILNVN)

/* Legg ut XML-tager i databasefil med Nøkler som representerer nivå */
             ADDLIBLE   LIB(LIBHTTP) POSITION(*LAST)
             MONMSG     MSGID(CPF0000)
             CALL       PGM(XMLRCVR) PARM(&FILNVN &SN)

/* Finn ut HVA/Fra HVEM - loggfør i EDIS sett / kall behandlingsprog*/
             CALL       PGM(XMLRCVR2) PARM(&SN &LIB &FIL &MBR &PROG)
             CALL       PGM(&PROG) PARM(&SN)

             ENDPGM
