             PGM PARM(&QTYPE &VALUE &TCP)
/*____________________*/
/* Definier variabler */
/* MIDLERTIDIG HEADF I SYENDPTF */
/*""""""""""""""""""""*/
             DCL        VAR(&LIB2) TYPE(*CHAR) LEN(10) +
                          VALUE('SYSPEDF   ')
             DCL        VAR(&QTYPE) TYPE(*CHAR) LEN(3)
             DCL        VAR(&VALUE) TYPE(*CHAR) LEN(15)
             DCL        VAR(&TCP) TYPE(*CHAR) LEN(1)
             OVRDBF     FILE(KODFRI) TOFILE(&LIB2/KODFRI)
             OVRDBF     FILE(TRACKL02) TOFILE(&LIB2/TRACKL02)
             OVRDBF     FILE(DOKUF) TOFILE(&LIB2/DOKUF)
             OVRDBF     FILE(TRAN) TOFILE(&LIB2/TRAN)
             OVRDBF     FILE(FREETXT2) TOFILE(&LIB2/FREETXT2)
             OVRDBF     FILE(DOKEF) TOFILE(&LIB2/DOKEF)
             OVRDBF     FILE(FIRM) TOFILE(&LIB2/FIRM)
             OVRDBF     FILE(HEAD12) TOFILE(&LIB2/HEAD12)
             OVRDBF     FILE(HEAD17) TOFILE(&LIB2/HEAD17)
             OVRDBF     FILE(HEADF) TOFILE(&LIB2/HEADF)
             OVRDBF     FILE(HEAD3) TOFILE(&LIB2/HEAD3)
             OVRDBF     FILE(FRISOL01) TOFILE(&LIB2/FRISOL01)
             OVRDBF     FILE(FRISOK) TOFILE(&LIB2/FRISOK)
             OVRDBF     FILE(KODTA) TOFILE(&LIB2/KODTA)
             OVRDBF     FILE(KODTFS) TOFILE(&LIB2/KODTFS)
             CALL       PGM(EDI20R3) PARM(&QTYPE &VALUE &TCP)
             ENDPGM
