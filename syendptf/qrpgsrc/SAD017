      *__________________________________________________________
     C* SR for å behandle BGM-segmentet                   SRBGMH
      *""""""""""""""""""""""""""""""""""""""""""""""""""""""""""
     C     SRBGMH        BEGSR
      * LES 3 '+'
     C                   DO        3
     C                   MOVE      *BLANKS       TEGN
     C                   DOW       TEGN <> LOGPLU
     C                   EXSR      GETCH
     C   LR              GOTO      ENDBGMH
     C                   ENDDO
     C                   ENDDO
      * LES MELDINGSTYPE
     C                   MOVE      *BLANKS       FE
     C     1             DO        3             NR
     C                   EXSR      GETCH
     C   LR              GOTO      ENDBGMH
     C                   IF        ((TEGN = LOGPLU) OR (TEGN = LOGAPP))
     C                   LEAVE
     C                   END
     C                   MOVE      TEGN          FE(NR)
     C                   END
     C                   MOVEL     FELT          XAN03             3
      *
     C                   SELECT
     C                   WHEN      XAN03 = '9'
     C                   MOVE      'DFU'         SE1225
     C                   MOVEL     'FULLSTENDIG 'SE1225T
     C                   WHEN      XAN03 = 'ZZ'
     C                   MOVE      'DMA'         SE1225
     C                   MOVEL     'MANUELL     'SE1225T
     C                   WHEN      XAN03 = '14'
     C                   MOVE      'DFO'         SE1225
     C                   MOVEL     'FORELØPIG   'SE1225T
     C                   WHEN      XAN03 = '2'
     C                   MOVE      'DEN'         SE1225
     C                   MOVEL     'ENDELIG     'SE1225T
     C                   WHEN      XAN03 = '4'
     C                   MOVE      'DKO'         SE1225
     C                   MOVEL     'KORRIGERING 'SE1225T
S01  C*                  WHEN      XAN03 = '18'
S01  C*                  MOVE      'DOM'         SE1225
S01  C*                  MOVEL     'OMREGNING   'SE1225T
N01  C                   WHEN      XAN03 = '43'
N01  C                   MOVE      'DSO'         SE1225
N01  C                   MOVEL     'STA. OPPRET.'SE1225T
N01  C                   WHEN      XAN03 = '18'
N01  C                   MOVE      'DEB'         SE1225
N01  C                   MOVEL     'ETTERBEREGN.'SE1225T
N01  C                   WHEN      XAN03 = '19'
N01  C                   MOVE      'DRE'         SE1225
N01  C                   MOVEL     'REFUSJON    'SE1225T
     C                   ENDSL
N01   *
N01   * OMBEREGNING
N01  C                   IF        XAN03 = '43' OR XAN03 = '18' OR XAN03 = '19'
N01   * LES FORBI TO +
N01  C     1             DO        2
N01  C                   DOW       TEGN <> LOGPLU
N01  C                   EXSR      GETCH
N01  C   LR              GOTO      ENDBGMH
N01  C                   ENDDO
N01  C                   ENDDO
N01   * LES FORBI :
N01  C                   DOW       TEGN <> LOGKOL
N01  C                   EXSR      GETCH
N01  C   LR              GOTO      ENDBGMH
N01  C                   ENDDO
N01   *
N01  C                   MOVE      *BLANKS       FE
N01  C     1             DO        20            NR
N01  C                   EXSR      GETCH
N01  C   LR              GOTO      ENDBGMH
N01  C                   IF        TEGN = LOGAPP
N01  C                   LEAVE
N01  C                   END
N01  C                   MOVE      TEGN          FE(NR)
N01  C                   ENDDO
N01  C                   MOVEL     FELT          SETLL
N01  C                   EVAL      *IN39 = *ON
N01  C                   END
      *
     C                   EXSR      GETSEG
      *
     C     ENDBGMH       ENDSR
      *
      *__________________________________________________________
     C* SR for å behandle CST-segmentet                   SRCSTH
      *""""""""""""""""""""""""""""""""""""""""""""""""""""""""""
     C     SRCSTH        BEGSR
      * LES FORBI '+'
     C                   MOVE      *BLANKS       TEGN
     C                   DOW       TEGN <> LOGPLU
     C                   EXSR      GETCH
     C   LR              GOTO      ENDCSTH
     C                   ENDDO
      * LES DEKLARASJONSTYPE EU/EX
     C                   MOVE      *BLANKS       FE
     C     1             DO        2             NR
     C                   EXSR      GETCH
     C   LR              GOTO      ENDCSTH
     C                   IF        TEGN = LOGKOL
     C                   LEAVE
     C                   END
     C                   MOVE      TEGN          FE(NR)
     C                   END
     C                   MOVEL     FELT          SEDTY
      * LES FORBI '+'
     C                   DOW       TEGN <> LOGPLU
     C                   EXSR      GETCH
     C   LR              GOTO      ENDCSTH
     C                   ENDDO
      * LES TRANSAKSJONSTYPE
     C                   MOVE      *BLANKS       FE
     C     1             DO        2             NR
     C                   EXSR      GETCH
     C   LR              GOTO      ENDCSTH
     C                   IF        TEGN = LOGKOL
     C                   LEAVE
     C                   END
     C                   MOVE      TEGN          FE(NR)
     C                   END
S03  C*                  MOVEL     FELT          SETST
N03  C                   IF        %SUBST(FELT:2:1) = ' '
N03  C                   MOVEL     FELT          XAN01             1
N03  C                   MOVE      XAN01         SETST
N03  C                   ELSE
N03  C                   MOVEL     FELT          XAN02             2
N03  C                   MOVE      XAN02         SETST
N03  C                   END
N01   *
N01   * OMBEREGNING
N01  C                   IF        *IN39
N01   * LES FORBI +
N01  C                   DOW       TEGN <> LOGPLU
N01  C                   EXSR      GETCH
N01  C   LR              GOTO      ENDCSTH
N01  C                   ENDDO
N01   * TYPE
N01  C                   MOVE      *BLANKS       FE
N01  C     1             DO        4             NR
N01  C                   EXSR      GETCH
N01  C   LR              GOTO      ENDCSTH
N01  C                   IF        TEGN = LOGAPP
N01  C                   LEAVE
N01  C                   END
N01  C                   MOVE      TEGN          FE(NR)
N01  C                   ENDDO
N01  C                   MOVEL     FELT          SETYPE
N01  C                   END
      *
     C                   EXSR      GETSEG
      *
     C     ENDCSTH       ENDSR
