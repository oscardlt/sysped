     C     KODFS2        KLIST
     C                   KFLD                    YUNI
     C                   KFLD                    WSFNR
     C     KODFCK        KLIST
     C                   KFLD                    XUNI
     C                   KFLD                    WSFNR
     C                   KFLD                    WSFRA
     C     GMKEY         KLIST
     C                   KFLD                    XUNI
     C                   KFLD                    WSFNR
     C                   KFLD                    KFCFRA
     C     KODFC2        KLIST
     C                   KFLD                    XUNI
     C                   KFLD                    KFCFNR
     C                   KFLD                    KFCFRA
     C*
     c                   move      '  '          WSUNIK
     c                   exfmt     BILDE01
     c     *in03         ifeq      '1'
     c                   seton                                        lr
     c                   return
     c                   end
     C     wsunik        ifeq      *blanks
     c                   move      'FC'          WSUNIK
     c                   end
     C                   MOVE      wsunik        XUNI              2
     C*                  MOVE      'FC'          XUNI              2
     C                   MOVE      'FS'          YUNI              2
