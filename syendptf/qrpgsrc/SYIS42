     C     *ENTRY        PLIST
     C                   PARM                    UKNA              8
     C                   PARM                    UTRF              8
     C                   PARM                    UKNAN1            8
     C                   PARM                    UKNAN2            8
     C                   PARM                    UKNAN3            8
     C                   PARM                    UFPFO             5
     C                   PARM                    UFDAT             6
     C                   PARM                    UTDAT             6
     C                   PARM                    USAKN             7
     C                   PARM                    UFLG              1
      *
     C                   Z-ADD     0             ANTSID            2 0
      *
     C                   READ      FIRMR                                  20
      *
     C     ISOFL17K      KLIST
     C                   KFLD                    WSKNA             8 0
     C                   KFLD                    YDATOF
     C     ISOFL18K      KLIST
     C                   KFLD                    UFPFO
     C                   KFLD                    YDATOF
     C     ISOFL19K      KLIST
     C                   KFLD                    WSTRF             8 0
     C                   KFLD                    YDATOF
     C     ISOKDAK1      KLIST
     C                   KFLD                    ISFPFO
     C                   KFLD                    ISFAVA
     C     ISOKDAK2      KLIST
     C                   KFLD                    X_ISFPFO          5
     C                   KFLD                    ISFAVA
      *
     C     KUNKEY        KLIST
     C                   KFLD                    FIFIRM
     C                   KFLD                    ISKNA
      *
     C     HEDKEY        KLIST
     C                   KFLD                    ISFAVD
     C                   KFLD                    ISFOPD
      *
     C     TRNKEY        KLIST
     C                   KFLD                    FIFIRM
     C                   KFLD                    ISFTRF
      *
     C                   MOVE      UFDAT         XDATO
     C                   MOVE      XDTDD         YDFDD
     C                   MOVE      XDTMM         YDFMM
     C     XDT≈≈         IFGT      50
     C     XDT≈≈         ADD       1900          YDF≈≈
     C                   ELSE
     C     XDT≈≈         ADD       2000          YDF≈≈
     C                   END
     C                   IF        UTDAT = *ZEROS
     C                   EVAL      YDATOT = 99999999
     C                   ELSE
     C                   MOVE      UTDAT         XDATO
     C                   MOVE      XDTDD         YDTDD
     C                   MOVE      XDTMM         YDTMM
     C     XDT≈≈         IFGT      50
     C     XDT≈≈         ADD       1900          YDT≈≈
     C                   ELSE
     C     XDT≈≈         ADD       2000          YDT≈≈
     C                   END
     C                   END
      *
     C                   MOVE      UKNA          WSKNA
     C                   MOVE      UTRF          WSTRF
     C                   MOVE      UKNAN1        WSKNAN1           8 0
     C                   MOVE      UKNAN2        WSKNAN2           8 0
     C                   MOVE      UKNAN3        WSKNAN3           8 0
     C                   MOVE      USAKN         WSSAKN            7 0
      *
     C                   SELECT
     C     UFLG          WHENEQ    '5'
     C     WSSAKN        SETLL     ISOFL8
     C                   READ      ISOFL8                                 33
     C     UFLG          WHENEQ    '1'
     C                   IF        WSKNA = 99999999
     C                   EVAL      WSKNA = 0
     C     YDATOF        SETLL     ISOFL16
     C                   READ      ISOFL16                                33
     C  N33YDATOT        COMP      ISFRDA                               33
     C                   ELSE
     C     ISOFL17K      SETLL     ISOFL17
     C     WSKNA         READE     ISOFL17                                33
     C  N33YDATOT        COMP      ISFRDA                               33
     C                   END
     C     UFLG          WHENEQ    '2'
     C                   IF        WSTRF = 99999999
     C                   EVAL      WSTRF = 0
     C     YDATOF        SETLL     ISOFL16
     C                   READ      ISOFL16                                33
     C  N33YDATOT        COMP      ISFRDA                               33
     C                   ELSE
     C     ISOFL19K      SETLL     ISOFL19
     C     WSTRF         READE     ISOFL19                                33
     C  N33YDATOT        COMP      ISFRDA                               33
     C                   END
     C     UFLG          WHENEQ    '3'
     C     YDATOF        SETLL     ISOFL16
     C                   READ      ISOFL16                                33
     C  N33YDATOT        COMP      ISFRDA                               33
     C     UFLG          WHENEQ    '4'
     C     ISOFL18K      SETLL     ISOFL18
     C     UFPFO         READE     ISOFL18                                33
     C  N33YDATOT        COMP      ISFRDA                               33
     C                   OTHER
     C     YDATOF        SETLL     ISOFL16
     C                   READ      ISOFL16                                33
     C  N33YDATOT        COMP      ISFRDA                               33
     C                   ENDSL
      *
     C     START         TAG
      *
     C     *IN33         DOWEQ     '0'
      *
     C                   IF        WSKNA <> 0 AND WSKNA <> ISKNA
     C                             OR WSTRF <> 0 AND WSTRF <> ISFTRF
     C                             OR WSKNAN1 <> 0 AND WSKNAN1 <> ISKNAN1
     C                             OR WSKNAN2 <> 0 AND WSKNAN2 <> ISKNAN2
     C                             OR WSKNAN3 <> 0 AND WSKNAN3 <> ISKNAN3
     C                             OR UFPFO <> *BLANKS AND UFPFO <> ISFPFO
     C                             OR YDATOF > ISFRDA
     C                             OR YDATOT < ISFRDA
      * INGEN
     C                   ELSE
     C                   EXSR      CLRSCR
     C                   EXSR      FORMAT
     C                   WRITE     RPT1
     C                   ADD       1             ANTSID
     C                   END
      *
     C                   SELECT
     C     UFLG          WHENEQ    '5'
     C                   READ      ISOFL8                                 33
     C     UFLG          WHENEQ    '1'
     C                   IF        WSKNA = 99999999
     C                   READ      ISOFL16                                33
     C  N33YDATOT        COMP      ISFRDA                               33
     C                   ELSE
     C     WSKNA         READE     ISOFL17                                33
     C  N33YDATOT        COMP      ISFRDA                               33
     C                   END
     C     UFLG          WHENEQ    '2'
     C                   IF        WSTRF = 99999999
     C                   READ      ISOFL16                                33
     C  N33YDATOT        COMP      ISFRDA                               33
     C                   ELSE
     C     WSTRF         READE     ISOFL19                                33
     C  N33YDATOT        COMP      ISFRDA                               33
     C                   END
     C     UFLG          WHENEQ    '3'
     C                   READ      ISOFL16                                33
     C  N33YDATOT        COMP      ISFRDA                               33
     C     UFLG          WHENEQ    '4'
     C     UFPFO         READE     ISOFL18                                33
     C  N33YDATOT        COMP      ISFRDA                               33
     C                   OTHER
     C                   READ      ISOFL16                                33
     C  N33YDATOT        COMP      ISFRDA                               33
     C                   ENDSL
      *
     C  N33              ENDDO
      *
     C                   SETON                                        LR
     C                   RETURN
      *
      ***************************************************************
     C     FORMAT        BEGSR
      ***************************************************************
      *
     C     ISFPFO        IFEQ      *BLANKS
     C                   MOVEL     'EGEN'        PRTPFT
     C                   ELSE
     C     ISFPFO        CHAIN     ISKDTR                             11
     C                   MOVEL     TTEXT         PRTPFT
     C                   END
     C     ISOKDAK1      CHAIN     ISKDAR                             11
     C   11ISOKDAK2      CHAIN     ISKDAR                             11
     C                   MOVEL     ATEXT         PRTAVT
     C     ISFANS        CHAIN     ISKDPR                             11
     C                   MOVEL     PTEXT         PRTANT
      *
     C     ISFOPG        IFNE      0
     C     KUNKEY        CHAIN     KUNDR                              12
     C                   Z-ADD     ISFOPG        PRTOPN
     C                   MOVEL     KNAVN         PRTOPT
     C                   END
      *
     C     TRNKEY        CHAIN     TRANR                              13
     C                   Z-ADD     ISFTRF        PRTTRN
     C                   MOVEL     VMNAVN        PRTTRF
     C     KUTRAK        KLIST
     C                   KFLD                    VMFIRM
     C                   KFLD                    VMTRKU
     C     KUTRAK        CHAIN     CUNDL01                            14
     C                   MOVE      ADR1          PRTTA1
     C     POSTNR        IFNE      *ZEROS
     C                   MOVEL     POSTNR        PRTTA3           30
     C                   MOVE      ADR3          PRTTA3
     C                   MOVEL     PRTTA3        PRTTA2
     C                   ELSE
     C                   MOVEL     SYPOGE        PRTTA2
     C                   MOVE      ADR3          PRTTA2
     C                   END
      *
     C     HEDKEY        CHAIN     HEADFR                             14
     C                   MOVEL     HENAS         PRTAS1
     C                   MOVEL     HEADS3        PRTAS2
     C                   MOVEL     HENAK         PRTMO1
     C                   MOVEL     HEADK3        PRTMO2
     C                   MOVEL     HEVS1         PRTGDS
     C                   Z-ADD     HENT          PRTGKL
     C                   Z-ADD     HEVKT         PRTGKG
     C                   Z-ADD     HELM          PRTGLM
     C                   Z-ADD     HEM3          PRTGM3
