     A          R FAKTILR
     A            FTKUND         8S 0       COLHDG('KUNDE,(0=FIRMA)')
     A            FTNIV          1          COLHDG('NIVÅ, O=OPD,L=LIN')
     A            FTTYPE        10          COLHDG('TILLEGSTYPE')
     A            FTKODE         1          COLHDG('KODE')
     A            FTMAX          5S 0       COLHDG('MAX CM')
     A            FTLVK          3          COLHDG('LINJENIV. VAREKODE')
     A            FTLFK         10          COLHDG('LINJENIV. ADR FAREKL')
     A            FTLPRO         3S 0       COLHDG('LINJENIV. %-SATS')
     A          K FTKUND
     A          K FTNIV
     A          K FTTYPE
