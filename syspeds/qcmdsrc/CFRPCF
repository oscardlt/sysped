             CMD        PROMPT('CPYFRMSTMF - forenklet')
             PARM       KWD(STMF) TYPE(*CHAR) LEN(50) RTNVAL(*NO) +
                          RSTD(*NO) FULL(*NO) PROMPT('Fra PC-fil:')
             PARM       KWD(FIL) TYPE(*CHAR) LEN(10) RTNVAL(*NO) +
                          RSTD(*NO) FULL(*NO) PROMPT('Til AS/400-fil:')
             PARM       KWD(LIB) TYPE(*CHAR) LEN(10) RTNVAL(*NO) +
                          RSTD(*NO) FULL(*NO) PROMPT('Bibliotek:')
             PARM       KWD(RCDLEN) TYPE(*DEC) LEN(5) RTNVAL(*NO) +
                          RSTD(*NO) FULL(*NO) PROMPT('Postlengde:')
             PARM       KWD(ERS) TYPE(*CHAR) LEN(1) RTNVAL(*NO) +
                          FULL(*NO) PROMPT('Erstatt hvis finnes +
                          (J=Ja):')
