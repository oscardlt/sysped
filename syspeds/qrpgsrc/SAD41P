     H DECEDIT('0,') DATEDIT(*DMY.)
     FKODTFM    IF   E           K DISK
     FSADTRH    UF   E           K DISK
     FSADTRH07  IF   E           K DISK
     F                                     RENAME(SADTRHR:SADTRHR7)
     FSADTRV02  UF   E           K DISK
     FSTANDT    IF   E           K DISK
     FSADTRTB   UF   E           K DISK
     FHEADF     UF   E           K DISK
     FFIRM      IF   E           K DISK
     FWFILETR   UF A E           K DISK    USROPN
     FTARI      IF   E           K DISK
     FKODTP     IF   E           K DISK
     FTOLLDEK   O    F   85        PRINTER USROPN
     F                                     FORMLEN(72)
     F                                     FORMOFL(72)
     FTOLLDEK2  O    F   85        PRINTER USROPN
     F                                     FORMLEN(72)
     F                                     FORMOFL(72)
     FDATALIST  O    F   85        PRINTER USROPN
     D MN              S             17    DIM(20)
     D VS              S             25    DIM(20)
     D F1              S             75    DIM(9)
     D F2              S             75    DIM(9)
     D UCMD            DS          1024
     D                 DS
     D XCMD11          C                   CONST('OVRPRTF -
     D                                     FILE(TOLLDEK) -
     D                                     OUTQ(')
     D XCMD12          C                   CONST('OVRPRTF -
     D                                     FILE(TOLLDEK2) -
     D                                     OUTQ(')
     D XCMD13          C                   CONST('OVRPRTF -
     D                                     FILE(DATALIST) -
     D                                     OUTQ(')
     D XCMD21          C                   CONST('PAGESIZE(72 85) -
     D                                     CPI(10) -
     D                                     OVRFLW(72) -
     D                                     USRDTA(')
     D XCMD22          C                   CONST('PAGESIZE(48 85) -
     D                                     CPI(10) -
     D                                     OVRFLW(48) -
     D                                     USRDTA(')
     D XCMD23          C                   CONST('PAGESIZE(72 85) -
     D                                     CPI(10) -
     D                                     OVRFLW(72) -
     D                                     USRDTA(')
     D XCMD31          C                   CONST(') COPIES(')
     D XCMD01          C                   CONST('CLRPFM -
     D                                     FILE(WFILETR) -
     D                                     MBR(SADPRT)')
     D XCMD02          C                   CONST('OVRDBF -
     D                                     FILE(WFILETR) -
     D                                     TOFILE(WFILETR) -
     D                                     MBR(SADPRT)')
     D                 DS
     D  SECDTÅ                 1      4  0
