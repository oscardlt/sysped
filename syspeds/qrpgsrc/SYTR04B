     FFIRM      IF   E           K DISK
     FTRAN      IF   E           K DISK
     FTRANL01   IF   E           K DISK
     F                                     RENAME(TRANR:TRANR2)
     FTURER16   IF   E           K DISK
     FSYTR04FB  CF   E             WORKSTN
     F                                     SFILE(SUBFIL:ZRECNO)
     F                                     INFDS(FBAREA)
     D FBAREA          DS
     D  WSCLOC               370    371B 0
     D  WSSUBN               378    379B 0
     D                 DS
     D  VMNAVN                 1     30
     D  WSNAVN                 1     20
SV5  D*           DS
SV5  D*                                       1   60TURDT
SV5  D*                                       1   20TUDTÅ
SV5  D*                                       3   40TUDTM
SV5  D*                                       5   60TUDTD
     D                 DS
     D  TMDT                   1      6  0
     D  TMDTD                  1      2  0
     D  TMDTM                  3      4  0
     D  TMDTÅ                  5      6  0
     D                 DS
EV5  D  TOMDT                  1      8  0
EV5  D  TOMDTÅ                 1      4  0
EV5  D  TOMDTM                 5      6  0
EV5  D  TOMDTD                 7      8  0
     C*
     C                   EXSR      INIT
     C*
     C     START         TAG
     C                   MOVE      '1'           *IN93
     C                   WRITE     SUBCTL
     C                   MOVEA     '00'          *IN(93)
     C                   Z-ADD     *ZEROS        ZRECNO
      *_____________
      * Fyll Subfile
      *"""""""""""""
     C  N27TRANK         SETLL     TRAN
     C   27TRAN1K        SETLL     TRANL01
     C     START1        TAG
     C  N27FIFIRM        READE     TRAN                                   94
     C   27FIFIRM        READE     TRANL01                                94
     C     *IN94         IFEQ      '0'
     C                   EXSR      SELECT
     C     SEL           IFEQ      'N'
     C                   GOTO      START1
     C                   END
     C                   END
      *
     C     START2        TAG
      *
     C  N94              DO
     C                   Z-ADD     1             ZNR               2 0
     C     ZNR           DOWLE     10
     C                   ADD       1             ZNR
     C                   ADD       1             ZRECNO
     C     VMSTOP        COMP      'J'                                    55     *RED
     C     VMSTOP        COMP      'N'                                    56     *WHT
     C                   MOVE      VMTRLE        WSKLNR
     C                   WRITE     SUBFIL
     C     NEXT          TAG
     C  N27FIFIRM        READE     TRAN                                   94
     C   27FIFIRM        READE     TRANL01                                94
     C     *IN94         IFEQ      '0'
     C                   EXSR      SELECT
     C     SEL           IFEQ      'N'
     C                   GOTO      NEXT
     C                   END
     C                   END
     C  N94              END
     C                   END
     C*
     C                   Z-ADD     ZRECNO        YRECNO            5 0
     C     *IN29         IFEQ      '0'
     C                   Z-ADD     4             LINNBR
     C                   Z-ADD     4             POSNBR
     C                   MOVE      '1'           *IN29
     C                   ELSE
     C                   Z-ADD     5             LINNBR
     C                   Z-ADD     2             POSNBR
     C                   END
     C     START3        TAG
     C                   MOVEA     '11'          *IN(91)
     C     ZRECNO        IFEQ      0
     C                   Z-ADD     4             POSNBR
     C                   EXFMT     NYWRK
     C                   ELSE
     C                   WRITE     BUNN
     C                   EXFMT     SUBCTL
     C                   END
     C*
     C                   SETOFF                                       9830
     C*
     C*** AVSLUTT
     C   03
     COR 12              GOTO      SLUTT
     C*
     C*** FRISK OPP
     C   05              GOTO      START
     C*
     C*** LAGE NY
     C   06              DO
     C                   MOVE      'N'           USØK              1
     C                   CALL      'SYTR04'
     C                   PARM                    USØK              1
     C                   PARM                    W2KOD
     C                   GOTO      START
     C                   END
     C*
     C*** BLA VIDERE
     C   25              DO
     C                   SETOFF                                       25
     C                   Z-ADD     YRECNO        ZRECNO
     C                   GOTO      START2
     C                   END
      *
     C     W2NR          IFEQ      '1'
     C     W2KOD         ANDNE     0
     C                   MOVE      W2KOD         UKOD
     C                   GOTO      SLUTT
     C                   END
     C     W2KOD         IFNE      0
     C                   MOVE      '0'           *IN27
     C                   GOTO      START
     C                   END
     C     W2SØK         IFNE      *BLANKS
     C                   MOVE      '1'           *IN27
     C                   GOTO      START
     C                   END
     C*
     C     ZRECNO        IFEQ      0
     C                   GOTO      START3
     C                   ELSE
     C                   EXSR      HENT
     C   98              GOTO      START3
     C                   GOTO      SLUTT
     C                   END
      *
     C     SLUTT         TAG
     C                   SETON                                        LR
     C                   RETURN
     C*
     C***********************************************
     C     SELECT        BEGSR
     C***********************************************
     C                   MOVE      'N'           SEL               1
      *
      * IKKE KODE 9 = EKSKLUDER
     C     VMINCR        IFNE      9
     C                   GOTO      ENDSEL
     C                   END
      *
      * ALLE...
     C     UF4KD         IFEQ      'A'
     C                   MOVE      'J'           SEL
     C                   GOTO      ENDSEL
     C                   END
      *
      *
      * ÅPNE UFERDIGE/UKONTOLLERDE (KODE BLANK) ... ELLER B = BEGGE
     C     UF4KD         IFEQ      ' '
     C     UF4KD         OREQ      'B'
     C                   MOVE      ' '           ZAVRE
      * FINNES MINST EN TUR FOR TRANSPORTøREN MED AVR.STAT BLANK??
     C     TURK16        SETLL     TURER16
     C     TULES1        TAG
     C     TURK16        READE     TURER16                                34
     C     *IN34         IFEQ      *ON
     C                   GOTO      UTTU1
     C                   ENDIF                                                  <*IN34
SV5  C*          TURDT     IFGT TOMDT
NV5  C     TUDT          IFGT      TOMDT
     C                   GOTO      UTTU1
     C                   ENDIF                                                  <TURDT
     C     UAVD          IFNE      *ZEROS
     C     UAVD          ANDNE     TUAVD
     C                   GOTO      TULES1
     C                   ENDIF                                                  <UAVD
      * LEST TUR SKAL MED....
     C                   MOVE      'J'           SEL
     C                   GOTO      ENDSEL
     C                   END
     C     UTTU1         TAG
      *
      *
      *
      *  FERDIGE/KONTOLLERTE (KODE K) ... ELLER B = BEGGE
     C     UF4KD         IFEQ      'K'
     C     UF4KD         OREQ      'B'
     C                   MOVE      'K'           ZAVRE
      * FINNES MINST EN TUR FOR TRANSPORTøREN MED AVR.STAT BLANK??
     C     TURK16        SETLL     TURER16
     C     TULES2        TAG
     C     TURK16        READE     TURER16                                34
     C     *IN34         IFEQ      *ON
     C                   GOTO      ENDSEL
     C                   ENDIF                                                  <*IN34
SV5  C*          TURDT     IFGT TOMDT
NV5  C     TUDT          IFGT      TOMDT
     C                   GOTO      ENDSEL
     C                   ENDIF                                                  <TURDT
     C     UAVD          IFNE      *ZEROS
     C     UAVD          ANDNE     TUAVD
     C                   GOTO      TULES2
     C                   ENDIF                                                  <UAVD
      * LEST TUR SKAL MED....
     C                   MOVE      'J'           SEL
     C                   GOTO      ENDSEL
     C                   END
      *
     C     ENDSEL        ENDSR
     C***********************************************
     C     HENT          BEGSR
     C***********************************************
     C                   MOVEA     '00'          *IN(91)
     C     *IN98         DOWEQ     '0'
     C                   READC     SUBFIL                                 98
     C  N98WSNR          IFEQ      '1'
     C                   MOVE      VMTRAN        UKOD
     C                   GOTO      SLHENT
     C                   END
     C                   END
      *
     C                   MOVE      'DIV0012'     MSGNR
     C                   SETON                                            30
      *
     C     SLHENT        ENDSR
