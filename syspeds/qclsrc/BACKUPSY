             PGM
             DCL        VAR(&STRTID) TYPE(*CHAR) LEN(6)
             DCL        VAR(&EXPDATE) TYPE(*CHAR) LEN(6)
             DCL        VAR(&REFDATE) TYPE(*CHAR) LEN(6)
             DCL        VAR(&REFTIME) TYPE(*CHAR) LEN(6)
             DCL        VAR(&XLOOP) TYPE(*LGL) LEN(1)
             DCL        VAR(&WS) TYPE(*CHAR) LEN(10)
             DCL        VAR(&IN03) TYPE(*LGL) LEN(1)
             DCL        VAR(&MSG1) TYPE(*CHAR) LEN(7) VALUE(BAC0001)
             DCL        VAR(&MSG2) TYPE(*CHAR)
             DCL        VAR(&MSG3) TYPE(*CHAR)
             DCL        VAR(&MSGTXT) TYPE(*CHAR)
             DCLF       FILE(SYSPED/START) RCDFMT(START)
             MONMSG CPF0000
             RTVMSG     MSGID(&MSG1) MSGF(SYENDPTF/SYMSGF) MSG(&MSGTXT)
             RTVJOBA    JOB(&WS)
             RTVDTAARA  DTAARA(SYSPED/BACK (1 6)) RTNVAR(&STRTID)
             SNDRCVF    DEV(&WS) RCDFMT(START)
             IF         COND(&IN03 = '1') THEN(GOTO CMDLBL(EXIT))
             CHGDTAARA  DTAARA(SYSPED/BACK (1 6)) VALUE(&STRTID)
             SBMJOB     CMD(CALL PGM(SYSPED/NATTBACKSY) PARM(&STRTID +
                          &EXPDATE &XLOOP &WS &REFDATE &REFTIME)) +
                          JOB(BACKUP) JOBD(QCTL) JOBQ(QSYS/QCTL) +
                          JOBPTY(9) LOG(4 0) LOGCLPGM(*YES) +
                          INQMSGRPY(*SYSRPYL) HOLD(*NO) DATE(*SYSVAL)
             SNDPGMMSG  MSG(&MSGTXT *CAT QCTL *CAT QSYS)
                          GOTO END
 EXIT:       SNDPGMMSG  MSG('F3 ble trykket, backup ble ikke +
                          igangsatt.')
END:         ENDPGM
