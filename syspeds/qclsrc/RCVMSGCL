             PGM
             DCL        VAR(&MSGID) TYPE(*CHAR) LEN(7)
/*___________________*/
/* Definier varabler */
/*"""""""""""""""""""*/
             MOVE       OBJ('/dds2.txt') TOOBJ('/dds3.txt')
             MONMSG     MSGID(CPF0000) EXEC(DO)
             RCVMSG     MSGQ(*PGMQ) MSGTYPE(*INQ) MSGID(&MSGID)
             SNDPGMMSG  MSG('test' *BCAT &MSGID *BCAT 'END') +
                          TOUSR(SVEIN)
                          ENDDO
/*           IF         COND(&MSGID *EQ 'CPDBC19') THEN(GOTO FEIL)  */
             ENDPGM
