    /* UTSKRIFT AV ORDRE FRA TIL DATO */
             PGM
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1) /* Hvis &in01 +
                          på, betyr det avslutt program fra +
                          avdelingsvalg. */
             DCL        VAR(&FIL) TYPE(*CHAR) LEN(10) +
                          VALUE('SYTR43FI') /* FIL */
             DCL        VAR(&MBR) TYPE(*CHAR) LEN(10) +
                          VALUE('SYTR43FI') /* FIL */
             DCL        VAR(&EPOST) TYPE(*CHAR) LEN(1) /* EPOST */
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10) /* Printernavn */
             DCL        VAR(&PT) TYPE(*CHAR) LEN(10) /* Papirtype */
             DCL        VAR(&PT) TYPE(*CHAR) LEN(10) /* Papirtype */
             DCL        VAR(&HE) TYPE(*CHAR) LEN(1) /* HEADING */
             DCL        VAR(&TYPE) TYPE(*CHAR) LEN(1) /* TRANSPORTØRTYPE */
             DCL        VAR(&AVD) TYPE(*DEC) LEN(4 0) VALUE(0)
             DCL        VAR(&BILN) TYPE(*CHAR) LEN(10) /* BILNUMMER */
             DCL        VAR(&DATO) TYPE(*DEC) LEN(6 0) VALUE(0)
             DCL        VAR(&DATO2) TYPE(*DEC) LEN(6 0) VALUE(0)
             DCL        VAR(&ANT) TYPE(*DEC) LEN(3 0) VALUE(0)
             DCL        VAR(&ANTA) TYPE(*CHAR) LEN(3)
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CALL       PGM(SYFA00) PARM(&IN01)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT)) /* +
                          Hvis &IN01 på, avslutter program. */
             CALL       PGM(SYTR43A) PARM(&IN01 &BILN &PRT +
                          &PT &HE &DATO &DATO2 &TYPE &EPOST)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT)) /* +
                          Hvis &IN01 på, avslutter program. */
    /* */
    /* FIX KONVOLUTT            */
    /* */
             OVRPRTF    FILE(SYTR43PF) PAGESIZE(70 132) CPI(20) +
                          OVRFLW(70) OUTQ(&PRT) FORMTYPE(&PT)
    /* */
             CLRPFM     FILE(SYTR43FI)
    /* */
             CALL       PGM(SYTR43) PARM(&BILN &DATO &DATO2 &TYPE +
                          &HE &ANT &EPOST)
    /* */
             CALL       PGM(SYGE55CL) PARM(&FIL &MBR)
    /* */
    /* */
 SLUTT:      ENDPGM
