/* TEST BANE                      */
             PGM        PARM(&BANE)
             DCL        VAR(&FINS1) TYPE(*CHAR) LEN(1)
             DCL        VAR(&BANE) TYPE(*CHAR) LEN(20)
             DCL        VAR(&QUSER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QNBR) TYPE(*CHAR) LEN(6)
             DCL        VAR(&QJOB) TYPE(*CHAR) LEN(10)

             RTVJOBA    JOB(&QJOB) USER(&QUSER) NBR(&QNBR)
             CHGVAR     VAR(&FINS1) VALUE('N')
             OVRPRTF    FILE(QSYSPRT) OUTQ(OUTMAIL)
             DSPLNK     OBJ(&BANE) OUTPUT(*PRINT) OBJTYPE(*DIR) +
                          DETAIL(*NAME) DSPOPT(*USER)
             MONMSG     MSGID(CPFA0A9 CPA0701) EXEC(MD DIR(&BANE) +
                          DTAAUT(*RWX) OBJAUT(*ALL))
             CHGVAR     VAR(&FINS1) VALUE('J')
             DLTSPLF    FILE(QSYSPRT) JOB(&QNBR/&QUSER/&QJOB) +
                          SPLNBR(*LAST)
 VIDERE:     ENDPGM
