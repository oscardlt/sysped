/*   FAKTURAKOPI TIL BATCH   */
             PGM        PARM(&AVD &FANR)
             DCL        VAR(&AVD) TYPE(*CHAR) LEN(4)
             DCL        VAR(&FANR) TYPE(*CHAR) LEN(7)
             SBMJOB     CMD(CALL PGM(SYFA22CLD) PARM(&AVD &FANR)) +
                          JOB(FAKTKOPI) JOBD(*USRPRF) +
                          JOBQ(SYJOBQPRT)
             ENDPGM
