     C     '00000001'    SETLL     PRIK2
     C                   READ      PRIK2                                  33
     C     *IN33         DOWEQ     '0'
*OBS C     PKGEBY        IFEQ      'FRA'
     C                   EXSR      ADDGEB
     C                   END
     C                   READ      PRIK2                                  33
     C                   END
