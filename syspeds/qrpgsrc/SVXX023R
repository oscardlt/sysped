     FSVXHE     IF   E           K DISK
     D  XVXH_BYTE      S              7S 0
      */////////////
      * MAIN LOGIC /
      */////////////
     C                   EXSR      SRINIT
     C                   EXSR      SRB
     C                   MOVE      *ON           *INLR
      *////////////////////////////////////////////////////////////
      * SR for init                                        SRINIT /
      *////////////////////////////////////////////////////////////
     C     SRINIT        BEGSR
     C     *LIKE         DEFINE    XVXH_BYTE     UVXH_BYTE
     C     *LIKE         DEFINE    SVXHE_SYA     UVXHE_SYA
     C     *LIKE         DEFINE    SVXHE_SYO     UVXHE_SYO
      * INIT AV ARBEIDSFELT
     C                   EVAL      UVXH_BYTE=0
      *________________________
      * Input/output parameters
      *""""""""""""""""""""""""
     C     *ENTRY        PLIST
     C                   PARM                    UVXHE_SYA
     C                   PARM                    UVXHE_SYO
     C                   PARM                    UVXH_BYTE
      *__________________
      * Definiere Nøkkler
      *""""""""""""""""""
     C     KEYA          KLIST
     C                   KFLD                    UVXHE_SYA
     C                   KFLD                    UVXHE_SYO
     C     KEYA          SETLL     SVXHE
     C     KEYA          READE     SVXHE                                  51
     C     ENDINI        ENDSR
      *////////////////////////////////////////////////////////////
      * SR Les og tell                                        SRB /
      *////////////////////////////////////////////////////////////
     C     SRB           BEGSR
     C     *IN51         DOWEQ     *OFF
     C                   ADD       80            UVXH_BYTE
     C     KEYA          READE     SVXHE                                  51
     C                   ENDDO
     C                   SUB       80            UVXH_BYTE
     C     ' '           CHECKR(E) SVXHE_DTA     XVXH_BYTE
     C                   ADD       XVXH_BYTE     UVXH_BYTE
     C                   ENDSR
