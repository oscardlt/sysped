             PGM
             DCL        VAR(&UNIK) TYPE(*CHAR) LEN(14)
             DCL        VAR(&QTIME)  TYPE(*CHAR) LEN(6)
             DCL        VAR(&QYEAR)  TYPE(*CHAR) LEN(2)
             DCL        VAR(&QMONTH) TYPE(*CHAR) LEN(2)
             DCL        VAR(&QDAY)   TYPE(*CHAR) LEN(2)
             DCL        VAR(&IFS) TYPE(*CHAR) LEN(70)
             DCL        VAR(&BANE) TYPE(*CHAR) LEN(26) +
                          VALUE('/SYSPEDFX/NAVITRO/EDISE1K/')
             DCL        VAR(&FILE) TYPE(*CHAR) LEN(10)
             DCL        VAR(&MBR) TYPE(*CHAR) LEN(10)
             DCL        VAR(&FROM) TYPE(*CHAR) LEN(35) VALUE('NO005801')
             DCL        VAR(&TO) TYPE(*CHAR) LEN(35) VALUE('NAVITRO')
             DCL        VAR(&QNBR) TYPE(*CHAR) LEN(6)
             DCL        VAR(&STMF) TYPE(*CHAR) LEN(18)
             RTVJOBA    NBR(&QNBR)

             RTVSYSVAL  SYSVAL(QYEAR)  RTNVAR(&QYEAR)
             RTVSYSVAL  SYSVAL(QMONTH) RTNVAR(&QMONTH)
             RTVSYSVAL  SYSVAL(QDAY)   RTNVAR(&QDAY)
             RTVSYSVAL  SYSVAL(QTIME)  RTNVAR(&QTIME)

/********************************************************************/
/*                                                                  */
/*  LEGGER UT DATA FOR LEVERANDØRER NAVLEVS                         */
/*                                                                  */
/********************************************************************/
             RUNQRY     QRY(NAVLEVSND)
/*                                                                  */
             CHGVAR     VAR(&UNIK) VALUE('LE' *CAT &QYEAR *CAT +
                          &QMONTH *CAT &QDAY *CAT &QTIME)
/*                                                                  */
/*  LEGGER UT FILE I EN TEMPFIL                                     */
/*                                                                  */
             CHGVAR     VAR(&STMF) VALUE('NAVLEVS.CSV')
             CHGVAR     VAR(&IFS) VALUE(&BANE *CAT &STMF)
             CHGVAR     VAR(&FILE) VALUE('NAVLEVS')
             CHGVAR     VAR(&MBR) VALUE('NAVLEVS')
             CALL       PGM(SYGE55CLF) PARM(&FILE &MBR &IFS)
/*                                                                  */
/*  KALLER PROGRAM SOM SENDER TIL                                  */
/*                                                                  */
             CALL       PGM(SYGE62) PARM(&FROM &TO &IFS &UNIK)

             ENDPGM
