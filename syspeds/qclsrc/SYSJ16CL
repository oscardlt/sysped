/* *********************************************************************   */
/*       * RETTINGER I DETTE PROGRAM:                                      */
/* PTFnr:*Sek Sig Vers  Beskrivelse..........................              */
/* """"""*"""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""    */
/* 10XXX * 01 YBC PTF10 PARAMETER FOR AVBRYT                               */
/* ********************************************************************    */
/*S01        PGM     PARM(&OPDF &OPDT &PRT31 &PT31 &JOBK &FAX &CVP &UTSKRIFT) */
/*N01*/      PGM        PARM(&OPDF &OPDT &PRT31 &PT31 &JOBK &FAX +
                          &CVP &UTSKRIFT &FLAG)
/*N01*/      DCL        VAR(&FLAG) TYPE(*CHAR) LEN(1)
             DCL        VAR(&OPDF) TYPE(*DEC) LEN(7 0)
             DCL        VAR(&OPDT) TYPE(*DEC) LEN(7 0)
             DCL        VAR(&OPDF2) TYPE(*CHAR) LEN(7)
             DCL        VAR(&OPDT2) TYPE(*CHAR) LEN(7)
             DCL        VAR(&PRT31) TYPE(*CHAR) LEN(10)
             DCL        VAR(&PT31) TYPE(*CHAR) LEN(10)
             DCL        VAR(&JOBK) TYPE(*CHAR) LEN(1)
             DCL        VAR(&FAX) TYPE(*CHAR) LEN(1)
             DCL        VAR(&CVP) TYPE(*CHAR) LEN(1)
             DCL        VAR(&UTSKRIFT) TYPE(*CHAR) LEN(1)
       /* */
             CHGVAR     VAR(&OPDF2) VALUE(&OPDF)
             CHGVAR     VAR(&OPDT2) VALUE(&OPDT)
             IF         COND((&JOBK *EQ 'J') *AND (&FAX *NE 'J')) +
                          THEN(DO)
/*E01*/      SBMJOB     CMD(CALL PGM(SYSJ16CLJ) PARM(&OPDF2 &OPDT2 +
                          &PRT31 &PT31 &JOBK &FAX &CVP &UTSKRIFT +
                          &FLAG)) JOB(ADVISER) JOBD(*USRPRF) +
                          JOBQ(*LIBL/SYJOBQNMAX)
                        ENDDO
             ELSE       CMD(DO)
/*E01*/      CALL       PGM(SYSJ16CLJ) PARM(&OPDF2 &OPDT2 &PRT31 +
                          &PT31 &JOBK &FAX &CVP &UTSKRIFT &FLAG)
                        ENDDO
       /* */
             ENDPGM
