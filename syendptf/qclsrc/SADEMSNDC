             PGM
/* VED FLERFIRMA ER DETTE HALVFABRIKATA - EN INSTANSCE PR FIRMA/DELSYSTEM  */
/*     firmaavvik på linje 2 i kommandoen:                                 */
/*                        /apache/expressmanif-client_XX                   */

             DCLF       FILE(UUIDF) RCDFMT(*ALL)
             IF         COND(&IPCMD = ' ') THEN(DO)
/* SETT LIBL (Kun for å finne rett syspe jobbKø i submit, når far wrkjobscde) */
             CALL       PGM(SYSPED/INLIBL)

             SBMJOB     CMD(QSH CMD('java -jar +
                          /apache/expressmanif-client+
                          /upload-engine-expressmanif-client.jar +
                          "http://10.11.47.61:8080/syjservicestn-expft+
                          /prodUpload/expressmanifest"')) +
                          JOB(ESPSNDEMTC) JOBQ(*LIBL/SYJOBQNMAX) +
                          USER(SY400USR)
             ENDDO
             ELSE       CMD(DO)
             SBMJOB     CMD(QSH CMD(&IPCMD)) JOB(ESPSNDEMTC) +
                          JOBQ(SYJOBQNMAX) USER(&IPUSER)
             ENDDO
             ENDPGM
