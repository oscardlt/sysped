     C                   MOVE      VAANR         XANR
     C                   WRITE     SUBFIL
     C                   END
     C     1             DO        100           XN                3 0
     C     LC(XN)        IFEQ      *BLANKS
     C     LC(XN)        OREQ      VALLOK
     C                   LEAVE
     C                   END
     C                   ENDDO
     C     LC(XN)        IFEQ      *BLANKS
     C                   MOVE      VALLOK        LC(XN)
     C                   ADD       1             UPOS
     C                   END
     C                   ADD       VASALD        USALDO
     C                   END
     C                   END
     C                   END
     C     LAGJ8K        READE     LLAGJS08                               94
     C  N94              ENDDO
