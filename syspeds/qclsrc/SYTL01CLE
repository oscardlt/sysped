             PGM        PARM(&ARB &GODSNR &MOTT &TRANSIT)
             DCL        VAR(&ARB) TYPE(*CHAR) LEN(1)
             DCL        VAR(&GODSNR) TYPE(*CHAR) LEN(15)
             DCL        VAR(&MOTT) TYPE(*CHAR) LEN(15)
             DCL        VAR(&TRANSIT) TYPE(*CHAR) LEN(18)
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1)

             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CALL       PGM(SYFA00) PARM(&IN01)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(UT))
             CALL       PGM(SYTL01) PARM(&ARB &GODSNR &MOTT &TRANSIT)

UT:          ENDPGM
