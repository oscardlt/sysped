     C     W2NR          IFEQ      '1'
     C     W2KUNR        ANDNE     0
     C     KUND1K        CHAIN     CUNDL01                            33
     C   33              GOTO      START
     C                   MOVE      W2KUNR        UKUNR
     C                   MOVE      KNAVN         USØKKN
     C                   GOTO      SLUTT
     C                   END
     C     W2ALFA        IFNE      *BLANKS
     C                   EXSR      SETMAX
     C                   MOVEA     '000'         *IN(82)
     C     W2POST        COMP      *BLANKS                            818180
     C   81W2PO24        COMP      *BLANKS                            828281
     C   82W2PO34        COMP      *BLANKS                            838382
     C   83W2PO44        COMP      *BLANKS                            848483
     C                   GOTO      START
     C                   END
     C*
     C     ZRECNO        IFEQ      0
     C                   GOTO      START3
     C                   ELSE
     C     WSCLOC        DIV       256           LINNBR
     C                   MVR                     POSNBR
     C                   EXSR      HENT
     C                   GOTO      START3
     C                   END
     C*
     C     SLUTT         TAG
     C                   SETON                                        LR
     C                   RETURN
     C*
     C***********************************************
     C     HENT          BEGSR
     C***********************************************
     C                   Z-ADD     1             LINNBR
     C                   MOVEA     '00'          *IN(91)
     C                   MOVE      '0'           *IN99
     C     *IN98         DOWEQ     '0'
     C                   READC     SUBFIL                                 98
     C  N98WSNR          IFEQ      '1'
     C                   Z-ADD     KUNDNR        UKUNR
     C                   MOVE      KNAVN         USØKKN
     C                   SETON                                        LR
     C                   RETURN
     C                   END
     C  N98WSNR          IFEQ      '5'
     C     ZRECNO        DIV       5             LINNBR
     C                   MVR                     XLIN              2 0
     C     XLIN          IFEQ      0
     C                   Z-ADD     5             LINNBR
     C                   ELSE
     C                   Z-ADD     XLIN          LINNBR
     C                   END
     C                   MULT      2             LINNBR
     C                   MOVE      '1'           *IN99
     C                   EXFMT     BILDE01
     C                   MOVE      ' '           WSNR
     C                   UPDATE    SUBFIL
     C                   END
     C  N98WSNR          IFEQ      '8'
     C     ZRECNO        DIV       5             LINNBR
     C                   MVR                     XLIN              2 0
     C     XLIN          IFEQ      0
     C                   Z-ADD     5             LINNBR
     C                   ELSE
     C                   Z-ADD     XLIN          LINNBR
     C                   END
     C                   MULT      2             LINNBR
     C                   MOVE      '1'           *IN99
     C                   EXSR      CUNDCA
     C                   CALL      'VCUNDCR'
     C                   PARM                    FIRMA
     C                   PARM                    KUNDNR
     C                   PARM                    RRN              10 0
     C                   PARM                    UEMFA             1
     C                   PARM                    UCONTA           30
     C     RRN           IFNE      *ZEROS
     C                   MOVEL     'KP'          UPAR12           12
     C                   MOVE      RRN           UPAR12
     C                   MOVEL     UPAR12        UPAR13           13
     C                   MOVE      UEMFA         UPAR13
     C                   MOVEL     UPAR13        USØKKN
     C                   Z-ADD     KUNDNR        UKUNR
     C                   SETON                                        LR
     C                   RETURN
     C                   END
     C                   END
     C                   END
