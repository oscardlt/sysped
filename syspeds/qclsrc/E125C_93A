             PGM
             DCL        VAR(&WS) TYPE(*CHAR) LEN(10)

             ALCOBJ     OBJ((E125F2 *FILE *SHRUPD))
             MONMSG     MSGID(CPF4128) EXEC(DO)
             SNDPGMMSG  MSG('DU KAN IKKE BEHANDLE FAKTURA NÅ PGA. AT +
                          EN ANNEN HOLDER PÅ Å OVERFØRE FAKTURA TIL +
                          REGNSKAP.')
             GOTO       CMDLBL(SLUTT)
             ENDDO
             DLCOBJ     OBJ((E125F2 *FILE *SHRUPD))

             OVRMSGF    MSGF(QUSERMSG) TOMSGF(EDIMSSGF)
             RTVJOBA    JOB(&WS)
             CLRPFM     FILE(E125F) MBR(&WS)
             MONMSG     MSGID(CPF3141) EXEC(ADDPFM FILE(E125F) +
                          MBR(&WS))
             OVRDBF     FILE(E125F) TOFILE(E125F) MBR(&WS)
             OVRPRTF    FILE(E125P) PAGESIZE(72 120) CPI(15) OVRFLW(66)
             OVRPRTF    FILE(E125P2) PAGESIZE(72 80) CPI(10) OVRFLW(66)

             OVRDBF     FILE(VAREL01) TOFILE(IBMDAT/VAREL01)
             CALL       PGM(E125R_93A)

 SLUTT:      ENDPGM
