N09   ***********************************************
N09  C     SRTSTDTA      BEGSR
N09   ***********************************************
N09  C     *LIKE         DEFINE    VAMN          X_VAMN
N09  C                   MOVE      'N'           XSLUTT            1
N09  C     LAGJ7K        SETLL     LLAGJL07
N09  C     LAGJ7K        READE(N)  LLAGJL07                               33
N09  C                   DOW       *IN33 = *OFF
N09  C     LVAREK        CHAIN(N)  LLVARE01                           33
N09  C                   MOVE      *ZEROS        X_VAMN
N09  C                   IF        LVSN = 'I'
N09  C     STTSTDTA1     TAG
N09  C                   Z-ADD     0             XANTSER           3 0
N09  C     VAMN          SETLL     LLSERL03
N09  C     VAMN          READE(N)  LLSERL03                               33
N09  C                   DOW       *IN33 = *OFF
N09  C                   ADD       1             XANTSER
N09  C     VAMN          READE(N)  LLSERL03                               33
N09  C                   ENDDO
N09  C                   IF        XANTSER < VAOANT
N09  C                   IF        VAMN = X_VAMN
N09  C                   MOVE      'J'           XSLUTT
N09  C                   GOTO      SLTSTDTA
N09  C                   END
N09  C                   CALL      'LL018R'
N09  C                   PARM                    VAGRUP
N09  C                   PARM                    VAKJED
N09  C                   PARM                    VAANR
N09  C                   PARM                    VALLOK
N09  C                   PARM                    VAMN
N09  C                   PARM                    VAOANT
N09  C                   MOVE      VAMN          X_VAMN
N09  C                   GOTO      STTSTDTA1
N09  C                   END
N09  C                   END
N09  C     LAGJ7K        READE(N)  LLAGJL07                               33
N09  C                   ENDDO
N09   *
N09  C     SLTSTDTA      TAG
N09  C                   ENDSR
