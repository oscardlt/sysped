        +'¨fecurr¨ : ¨'+%trim(fecurr)+'¨, '
        +'¨fepri1¨ : ¨'+%trim(%editc(fepri1:'M'))+'¨, '
        +'¨fepri2¨ : ¨'+%trim(%editc(fepri2:'M'))+'¨, '
        +'¨fepriN¨ : ¨'+%trim(%editc(fepriN:'M'))+'¨, '
        +'¨fetref¨ : ¨'+%trim(%editc(fetref:'Z'))+'¨, '
        +'¨ferund¨ : ¨'+%trim(%editc(ferund:'Z'))+'¨, '
        +'¨fedat2¨: ¨'+%trim(%editc(fedat2:'X'))+'¨, '
        +'¨fetime¨: ¨'+%trim(%editc(fetime:'X'))+'¨, '
        +'¨ferund¨ : ¨'+%trim(%editc(fekunr:'Z'))+'¨, '
        +'¨fefakt¨ : ¨'+%trim(%editc(fefakt:'Z'))+'¨, '
        +'¨fejour¨ : ¨'+%trim(%editc(fejour:'Z'))+'¨, '
        +'¨feopko¨ : ¨'+%trim(feopko)+'¨, '
        +'¨fecc¨ : ¨'+%trim(%editc(feCC:'Z'))+'¨, '
        +'¨fear¨ : ¨'+%trim(%editc(fe≈R:'Z'))+'¨, '
        +'¨femnd¨ : ¨'+%trim(%editc(feMND:'Z'))+'¨, '
        +'¨fedato¨ : ¨'+%trim(%editc(fedato:'Z'))+'¨, '
        +'¨fedaff¨ : ¨'+%trim(%editc(fedaff:'Z'))+'¨, '
        +'¨febiln¨ : ¨'+%trim(febiln)+'¨}';
      /end-free
