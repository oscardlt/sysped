             PGM        PARM(&FILNAM &FILNAM1 &FILNAM2 &FILNAM3 +
                          &FILNAM4 &MAPPE &MAPPE2)
             DCL        VAR(&FILNAM) TYPE(*CHAR) LEN(200) +
                          VALUE('Filnavn med time-stemple i +
                          backup-mappe')
             DCL        VAR(&FILNAM1) TYPE(*CHAR) LEN(200) +
                          VALUE('Filnavn med time-stemple i +
                          temp-mappe')
             DCL        VAR(&FILNAM2) TYPE(*CHAR) LEN(200) +
                          VALUE('Filnavn uten time-stemple i +
                          temp-mappe')
             DCL        VAR(&FILNAM3) TYPE(*CHAR) LEN(200) +
                          VALUE('Filnavn uten time-stemple uten +
                          mappenavn')
             DCL        VAR(&FILNAM4) TYPE(*CHAR) LEN(200) +
                          VALUE('Filnavn uten time-stemple i +
                          sendingsmappe')
             DCL        VAR(&MAPPE) TYPE(*CHAR) LEN(100) +
                          VALUE('Temp-mappe')
             DCL        VAR(&MAPPE2) TYPE(*CHAR) LEN(100) +
                          VALUE(SENDINGSMAPPE)

             CHGAUT     OBJ(&FILNAM) USER(*PUBLIC) DTAAUT(*RWX) +
                          OBJAUT(*ALL)
             MONMSG     MSGID(CPF0000)
             /* FJERN TIDLIGERE SENDINGSFIL HVIS DET FINNES */
             RMVLNK     OBJLNK(&FILNAM2)
             MONMSG     MSGID(CPF0000)
             /* KOPIERE FIL TIL TEMP-MAPPE */
             CPY        OBJ(&FILNAM) TODIR(&MAPPE)
             /* RENAME FIL FOR Å FJERNE TIME-STEMPLE */
             RNM        OBJ(&FILNAM1) NEWOBJ(&FILNAM3)
             CHGAUT     OBJ(&FILNAM2) USER(*PUBLIC) DTAAUT(*RWX) +
                          OBJAUT(*ALL)
             /* FLYTT FIL TIL SENDINGS MAPPE */
             MOV        OBJ(&FILNAM2) TODIR(&MAPPE2)
             MONMSG     MSGID(CPF0000) EXEC(DO)
             /* SLETT TIDLIGERE VERSJON AV KURERMANIFEST */
             RMVLNK     OBJLNK(&FILNAM4)
             /* FLYTT FIL TIL SENDINGS MAPPE */
             MOV        OBJ(&FILNAM2) TODIR(&MAPPE2)
             ENDDO

             ENDPGM
