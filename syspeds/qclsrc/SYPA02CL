             PGM
             DCLF       FILE(*LIBL/PALFIR) RCDFMT(*ALL)
    /* HENT PRINTER(OUTQ) */
             RCVF       DEV(*FILE) RCDFMT(*FILE)
    /* */
             OVRPRTF    FILE(SYPA06P) PAGESIZE(72 80) +
                          CPI(10) OVRFLW(64) OUTQ(&PFPRIN) FORMTYPE(A4)
             OVRPRTF    FILE(SYPA07P) PAGESIZE(72 80) +
                        CPI(10) OVRFLW(64) OUTQ(&PFPRIN) FORMTYPE(A4)

             OVRDBF     FILE(PAPAMOT) TOFILE(PAPA)
             OVRDBF     FILE(HEDUMMY) TOFILE(HEDUMMY) MBR(URA)

             CALL       PGM(SYPA02)
    /* */
             ENDPGM
