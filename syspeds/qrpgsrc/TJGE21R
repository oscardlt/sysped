      /copy SYSPEDS/qrpgsrcpy,hspecs
      /copy SYSPEDS/qrpgsrcpy,hspecsbnd
      *********************************************************************
     FBRIDF     IF   E           K DISK    USROPN
     FDOKUFV    IF   E           K DISK    USROPN
     FDOKUFVF   IF   E           K DISK    USROPN
     FDOKUFM    IF   E           K DISK    USROPN
      *--------------------------------------------------------------------
      * Variables common to all CGIs
      *--------------------------------------------------------------------
      /copy SYSPEDS/qrpgsrcpy,prototypeb
      /copy SYSPEDS/qrpgsrcpy,usec
      /copy SYSPEDS/qrpgsrcpy,variables3
      *
      * Varible for
     D WSUSER          S             10
     D AVD             S              4
     D AVDN            S              4S 0
     D OPD             S              7
     D OPDN            S              7S 0
     D FBN             S              3
     D FBNN            S              3S 0
     D JSONstring      S          32000
     D Error           S            150
     D AntLest         S              9  0
      *get input-string
      /copy syspeds/qrpgsrcpy,prolog3

      * Parse input
     C                   exsr      Parse
      * Open files etc
     C                   EXSR      INIT
      *
     c                   callp     gethtmlifs(
     C                             '/syspeddev/html/JSON.html':
     C                             '/CGITAG/')
     C                   callp     wrtsection('top')
      *
      * ............................................................................................
      * skriv JSON-Object start..(alltid '{' + x ant param. m/komma mellom (IKKE komma etter siste))
      * ............................................................................................
      *
      /free
        JSONstring='{'
        +'¬user¬ : ¬'+%trim(WSUSER)+'¬, '
        +'¬avd¬ : ¬'+%trim(%editc(AVDN:'Z'))+'¬, '
        +'¬opd¬ : ¬'+%trim(%editc(OPDN:'Z'))+'¬, '
        +'¬fbn¬ : ¬'+%trim(%editc(FBNN:'Z'))+'¬, '
        +'¬errMsg¬ : ¬'+%trim(Error)+'¬, ';
      /end-free
      * JSONfix escaper " i tekst,  for deretter å bytte ¬->"
     c                   call      'JSONFIX'
     c                   parm                    JSONstring
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')
      *
      * ............................................................................................
      * Skriv JSON-LISTE Start (en liste er EN parameter(fra [ til   )
      * ............................................................................................
     c                   eval      JSONstring ='"awblinelist" : ['
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')
      *
      * Liste
      *
     c     Error         ifeq      *blanks
     c     OPDN          andne     *zeros
     C     DOKUFVK       SETLL     DOKUFV
     C     DOKUFVK       READE     DOKUFV                                 55
     C     *IN55         DOWEQ     '0'
     C     DOKUFMK       CHAIN     DOKUFM                             56
     C                   IF        *IN56
     C                   EVAL      FMMRK1=*BLANKS
     C                   ENDIF
     C     DOKUFVFK      CHAIN     DOKUFVF                            56
     C                   IF        *IN56
     C                   EVAL      FFUNNR=*BLANKS
     C                   EVAL      FFEMBG=*BLANKS
n01  C                   EVAL      FFINDX=*BLANKS
     C                   EVAL      FFKLAS=*BLANKS
     C                   EVAL      FFSEDD=*BLANKS
     C                   EVAL      FFTRES=*BLANKS
     C                   EVAL      FFFAKT=*ZEROS
     C                   EVAL      FFANTK=*ZEROS
     C                   EVAL      FFANTE=*ZEROS
     C                   EVAL      FFENH=*BLANKS
     C                   EVAL      FFPOEN=*ZEROS
     C                   ENDIF
     C                   Add       1             AntLest
      * ............................................................................................
      * Skriv en JSON-Array-linje pr menypunkt - komma før hvert nytt element
      * ............................................................................................
      /free
       if AntLest=1;
          JSONstring=*blanks;
          else;
          JSONstring=', ';
          endif;
       JSONstring=%trim(JSONstring)+'{'
          +'¬fvavd¬ : ¬'+%trim(%editc(fvavd:'Z'))+'¬, '
          +'¬fvopd¬ : ¬'+%trim(%editc(fvopd:'Z'))+'¬, '
          +'¬fvfbnr¬ : ¬'+%trim(%editc(fvfbnr:'Z'))+'¬, '
          +'¬fvlinr¬ : ¬'+%trim(%editc(fvlinr:'Z'))+'¬, '
          +'¬fmmrk1¬ : ¬'+%trim(fmmrk1)+'¬, '
          +'¬fvant¬ : ¬'+%trim(%editc(fvant:'Z'))+'¬, '
          +'¬fvpakn¬ : ¬'+%trim(fvpakn)+'¬, '
          +'¬fvvt¬ : ¬'+%trim(fvvt)+'¬, '
          +'¬fvvkt¬ : ¬'+%trim(%editc(fvvkt:'Z'))+'¬, '
          +'¬fvvol¬ : ¬'+%trim(%editc(fvvol:'4'))+'¬, '
          +'¬fvlm¬ : ¬'+%trim(%editc(fvlm:'4'))+'¬, '
          +'¬fvlm2¬ : ¬'+%trim(%editc(fvlm2:'4'))+'¬, '
          +'¬fvlen¬ : ¬'+%trim(%editc(fvlen:'Z'))+'¬, '
          +'¬fvbrd¬ : ¬'+%trim(%editc(fvbrd:'Z'))+'¬, '
          +'¬fvhoy¬ : ¬'+%trim(%editc(fvhoy:'Z'))+'¬, '
          +'¬ffunnr¬ : ¬'+%trim(ffunnr)+'¬, '
          +'¬ffembg¬ : ¬'+%trim(ffembg)+'¬, '
N01       +'¬ffindx¬ : ¬'+%trim(ffindx)+'¬, '
          +'¬ffklas¬ : ¬'+ %trim(ffklas)+'¬, '
          +'¬ffsedd¬ : ¬'+%trim(ffsedd)+'¬, '
          +'¬fftres¬ : ¬'+%trim(fftres)+'¬, '
          +'¬fffakt¬ : ¬'+%trim(%editc(fffakt:'Z'))+'¬, '
          +'¬ffantk¬ : ¬'+%trim(%editc(ffantk:'Z'))+'¬, '
          +'¬ffante¬ : ¬'+%trim(%editc(ffante:'Z'))+'¬, '
          +'¬ffenh¬  : ¬'+%trim(ffenh)+'¬, '
          +'¬ffpoen¬ : ¬'+%trim(%editc(ffpoen:'Z'))+'¬}';
      /end-free
     C                   call      'JSONFIX'
     C                   parm                    JSONstring
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')


     C     READNEXT      TAG
     C     DOKUFVK       READE     DOKUFV                                 55
     C                   ENDDO
     c                   ENDIF
      * ............................................................................................
      * Skriv JSON-Liste/Array  Avslutning
      * ............................................................................................
     c                   eval      JSONstring =']'
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')
      * ............................................................................................
      * Skriv JSON-string Avsluttning
      * ............................................................................................
     c                   eval      JSONstring ='}'
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')

      * Quit
     C                   exsr      Exit


      *=====================================================================
      * Close output html and quit
      *=====================================================================
     C     Exit          begsr
      * Lukk fil
     C                   CLOSE     BRIDF
     C  N50              CLOSE     DOKUFV
     C  N50              CLOSE     DOKUFVF
     C  N50              CLOSE     DOKUFM

      * Do not delete the call to wrtsection with section name *fini.  It is needed
      * to ensure that all output html that has been buffered gets output.
     C                   callp     wrtsection('*fini')
      * Quit
     C                   MOVE      *ON           *INLR
     C                   return
     C                   endsr
      *
      *********************************************************
     C     Parse         Begsr
      *********************************************************
      * Get input
     C                   EVAL      WSUSER = zhbgetvar('USER')
     C                   EVAL      AVD    = zhbgetvar('AVD')
     C                   EVAL      AVDN   = c2n2(AVD)
     C                   EVAL      OPD    = zhbgetvar('OPD')
     C                   EVAL      OPDN = c2n2(OPD)
     C                   EVAL      FBN    = zhbgetvar('FBN')
     C                   EVAL      FBNN = c2n2(FBN)
     c                   endsr
      *
      *********************************************************
     C     INIT          Begsr
      *********************************************************
     C                   OPEN      BRIDF
      * Test innlogging
     C     WSUSER        CHAIN     BRIDF                              50
     C     BILIBL        ifeq      *blanks
     C                   callb     'INCGI'
     C                   parm                    BISTDL
     C                   else
     C                   call      BILIBL
     C                   end
      *
     C     DOKUFVK       KLIST
     C                   KFLD                    WFVRI
     C                   KFLD                    AVDN
     C                   KFLD                    OPDN
     C                   KFLD                    FBNN
     C     DOKUFMK       KLIST
     C                   KFLD                    FVRI
     C                   KFLD                    FVAVD
     C                   KFLD                    FVOPD
     C                   KFLD                    FVFBNR
     C                   KFLD                    FVLINR
     C     DOKUFVFK      KLIST
     C                   KFLD                    FVAVD
     C                   KFLD                    FVOPD
     C                   KFLD                    FVFBNR
     C                   KFLD                    FVLINR
     C     *LIKE         DEFINE    FVRI          WFVRI
     C                   EVAL      WFVRI='F'
     C                   Z-ADD     1             X                 3 0
      *
     C   50              eval      Error = 'Invalid userID'
     C  N50              OPEN      DOKUFV
     C  N50              OPEN      DOKUFVF
     C  N50              OPEN      DOKUFM

     c                   endsr
