     C     WKEY          CHAIN(N)  SVTX04L1                           53
     C                   MOVE      '1'           *IN41
     C                   MOVE      '0'           *IN42
     C                   MOVEA     '0000'        *IN(30)
     C                   MOVE      SVTX04_01     ÅSVTX04_01
     C                   MOVE      SVTX04_02     ÅSVTX04_02
     C                   MOVE      SVTX04_03     ÅSVTX04_03
     C                   MOVE      SVTX04_04     ÅSVTX04_04

     C                   MOVEL     SVTX04_02     XSVTX04_02
     C                   IF        SVTX04_01='E'
     C                   MOVE      '00'          XSVTX04_02
     C                   ENDIF
     C                   EVAL      WSVTX10_31=*BLANKS
     C     XKEY          CHAIN(N)  SVTX10F
     C                   IF        %FOUND
     C                   MOVEL     SVTX10_31     ÅSVTX10_31
