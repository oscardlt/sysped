             CMD        PROMPT('Kompr. SAD-Eksport i historikk')
             PARM       KWD(AVD) TYPE(*DEC) LEN(4) DFT(0000) +
                          RANGE(0000 9998) CHOICE('0000=alle, AVD') +
                          PROMPT('Avdeling')
             PARM       KWD(DT) TYPE(*DEC) LEN(6) DFT(000000) +
                          RANGE(000000 311299) CHOICE('000000, DMÅ') +
                          PROMPT('Til og med Dato')
