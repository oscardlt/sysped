     C***********************************************
     C     SETKEY        BEGSR
     C***********************************************
     C   41              DO
     C                   END
     C   42              DO
     C                   END
     C   43              DO
     C                   IF        WSATDD = 999999
     C                   EVAL      XSATDD = 99999999
     C                   ELSE
     C                   MOVE      WSATDDD       XSATDDD
     C                   MOVE      WSATDDM       XSATDDM
     C     WSATDDA       ADD       2000          XSATDDA
     C                   END
     C                   END
     C   44              DO
     C                   IF        WSDTR = 999999
     C                   EVAL      XSDTR = 99999999
     C                   ELSE
     C                   MOVE      WSDTRD        XSDTRD
     C                   MOVE      WSDTRM        XSDTRM
     C     WSDTRA        ADD       2000          XSDTRA
     C                   END
     C                   END
     C   45              DO
     C                   END
      *
     C                   EXSR      MINTUR
      *
     C                   ENDSR
      *
     C***********************************************
     C     MINTUR        BEGSR
     C***********************************************
      * SET KEY NÅR SØK KUN I EN !!!!  AVDELING
     C   41              IF        WSAVD = 0
     C     WSPRO         SETLL     SADKML02
     C                   ELSE
     C     SADKML01K2    SETLL     SADKML01
     C                   END
     C   42WSUUID        SETLL     SADKML03
     C   43XSATDD        SETLL     SADKML04
     C   44XSDTR         SETLL     SADKML05
     C*  45SADKML0XK     SETLL     SADKML0X
      *
     C                   ENDSR
      *
     C***********************************************
     C     LESREC        BEGSR
     C***********************************************
     C   41              IF        WSAVD = 0
     C                   READ      SADKML02                               94
     C                   ELSE
     C     WSAVD         READE     SADKML01                               94
     C                   END
     C   42              READ      SADKML03                               94
     C   43              IF        XSATDD = 99999999
     C                   READP     SADKML04                               94
     C                   ELSE
     C                   READ      SADKML04                               94
     C                   END
     C   44              IF        XSDTR = 99999999
     C                   READP     SADKML05                               94
     C                   ELSE
     C                   READ      SADKML05                               94
     C                   END
     C*  45              READ      SADKML0X                               94
      *
     C                   ENDSR
