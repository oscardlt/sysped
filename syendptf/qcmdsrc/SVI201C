             CMD        PROMPT('SKAPA INTRASTAT FIL')
             PARM       KWD(VATNO) TYPE(*CHAR) LEN(17) DFT('*ALL') +
                          PROMPT('VAT-NUMMER')
             PARM       KWD(DATOF) TYPE(*CHAR) LEN(8) DFT('20110101') +
                          RANGE(20080101 20991231) +
                          CHOICE('電電MMDD') PROMPT('F.O.M. DATO')
             PARM       KWD(DATOT) TYPE(*CHAR) LEN(8) DFT('20113031') +
                          RANGE(20080101 20991231) +
                          CHOICE('電電MMDD') PROMPT('T.O.M. DATO')
