             PGM
             DCL        VAR(&FLD003) TYPE(*CHAR) LEN(10)
             DCL        VAR(&FLD004) TYPE(*CHAR) LEN(4)
             DCL        VAR(&IN3) TYPE(*LGL) LEN(1) VALUE('0')
             DCLF       FILE(IFAXV)
             MONMSG CPF0000
             RTVDTAARA  DTAARA(IFAX (1 10)) RTNVAR(&FLD003)
             RTVDTAARA  DTAARA(IFAX (11 4)) RTNVAR(&FLD004)
             SNDRCVF    RCDFMT(IFAXV1)
             IF         COND((&IN3) = '1') THEN(GOTO CMDLBL(EXIT))
             CHGDTAARA  DTAARA(IFAX (1 10)) VALUE(&FLD003)
             CHGDTAARA  DTAARA(IFAX (11 4)) VALUE(&FLD004)
             SNDPGMMSG  MSG('Verdiene ble endret, og blir gjeldende +
                          efter neste ipl.')
 EXIT:       ENDPGM
