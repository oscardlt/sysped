      /copy SYSPEDS/qrpgsrcpy,hspecs
      /copy SYSPEDS/qrpgsrcpy,hspecsbnd
      *********************************************************************
     FBRIDF     IF   E           K DISK    USROPN
     FTARI      IF   E           K DISK    USROPN
     FKODTS2    IF   E           K DISK    USROPN
      *--------------------------------------------------------------------
      * Variables common to all CGIs
      *--------------------------------------------------------------------
      /copy SYSPEDS/qrpgsrcpy,prototypeb
      /copy SYSPEDS/qrpgsrcpy,usec
      /copy SYSPEDS/qrpgsrcpy,variables3
      *
      * Varible for
     D WSUSER          S             10
     D SVVNT           S              8
     D SVVNTN          S              8  0
     D SVLK            S              2
     D SVPRE           S              1
     D SVPVA           S              1
     D SVAS            S              7  2
     D JSONstring      s          32000
     D Error           S            150
     D AntLest         S              9  0

      *get input-string
      /copy syspeds/qrpgsrcpy,prolog3

      * Parse input
     C                   exsr      Parse
      * Open files etc
     C                   EXSR      INIT
      *
     c                   callp     gethtmlifs(
     C                             '/syspeddev/html/JSON.html':
     C                             '/CGITAG/')
     C                   callp     wrtsection('top')
      *
      * ............................................................................................
      * skriv JSON-Object start..(alltid '{' + x ant param. m/komma mellom (IKKE komma etter siste))
      * ............................................................................................
      *
      /free
        JSONstring='{'
        +'¬user¬ : ¬'+%trim(WSUSER)+'¬, '
        +'¬svvnt¬ : ¬'+%trim(SVVNT)+'¬, '
        +'¬svlk¬ : ¬'+%trim(SVLK)+'¬, '
        +'¬svpre¬ : ¬'+%trim(SVPRE)+'¬, '
        +'¬errMsg¬ : ¬'+%trim(Error)+'¬, ';
      /end-free
      * JSONfix escaper " i tekst,  for deretter å bytte ¬->"
     c                   call      'JSONFIX'
     c                   parm                    JSONstring
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')
      *
      * ............................................................................................
      * Skriv JSON-LISTE Start (en liste er EN parameter(fra [ til   )
      * ............................................................................................
     C                   eval      JSONstring ='"getPVA" : ['
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')
      *
      * Hæmta detta ena ærendet.
      *
     c     Error         ifeq      *blanks
     C                   EXSR      TATEST
      * ............................................................................................
      * Skriv en JSON-Array-linje pr menypunkt - komma før hvert nytt element
      * ............................................................................................
      /free
          JSONstring=*blanks;
       JSONstring=%trim(JSONstring)+'{'
          +'¬svas¬ : ¬'+%trim(%editc(SVAS:'4'))+'¬, '
          +'¬svpva¬ : ¬'+%trim(SVPVA)+'¬}';
      /end-free
     C                   call      'JSONFIX'
     C                   parm                    JSONstring
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')
      *
     c                   endif
     c                   eval      JSONstring =']'
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')
      * ............................................................................................
      * Skriv JSON-string Avsluttning
      * ............................................................................................
     c                   eval      JSONstring ='}'
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')
      *
      * Quit
     C                   exsr      Exit


      *=====================================================================
      * Close output html and quit
      *=====================================================================
     C     Exit          begsr
      * Lukk fil
     C                   CLOSE     BRIDF
     C  N50              CLOSE     TARI
     C  N50              CLOSE     KODTS2

      * Do not delete the call to wrtsection with section name *fini.  It is needed
      * to ensure that all output html that has been buffered gets output.
     C                   callp     wrtsection('*fini')
      * Quit
     C                   MOVE      *ON           *INLR
     C                   return
     C                   endsr
      *
      *********************************************************
     C     Parse         Begsr
      *********************************************************
      * Get input
     C                   EVAL      WSUSER  = zhbgetvar('USER')
     C                   EVAL      SVVNT   = zhbgetvar('SVVNT')
     C                   EVAL      SVVNTN  = c2n2(SVVNT)
     C                   EVAL      SVLK    = zhbgetvar('SVLK')
     C                   EVAL      SVPRE   = zhbgetvar('SVPRE')
     c                   endsr
      *********************************************************
     C     INIT          Begsr
      *********************************************************
     C                   OPEN      BRIDF
      * Test innlogging
     C     WSUSER        CHAIN     BRIDF                              50
     C     BILIBL        ifeq      *blanks
     C                   callb     'INCGI'
     C                   parm                    BISTDL
     C                   else
     C                   call      BILIBL
     C                   end

     C     KODS2K        KLIST
     C                   KFLD                    XS2UNI
     C                   KFLD                    SVLK
     C                   MOVE      'S2 '         XS2UNI            3
     C     TARIK         KLIST
     C                   KFLD                    SVVNTN
     C                   KFLD                    X2TAR
     C                   MOVE      *BLANKS       X2TAR             3
     C     TARIKM        KLIST
     C                   KFLD                    SVVNTN
     C                   KFLD                    X2TAR
     C                   KFLD                    XADATO
     C                   MOVE      99999999      XADATO            8 0
     C                   CALL      'SYGE15C'
     C                   PARM                    WDT               8
     C                   PARM                    WTM               6
     C                   MOVE      WDT           USIDT             8 0

     C   50              eval      Error = 'Invalid userID'
     C  N50              OPEN      TARI
     C  N50              OPEN      KODTS2

     c                   endsr
     C***********************************************
     C     TATEST        BEGSR
     C***********************************************
     C     KODS2K        CHAIN     KODTS2

     C     TARIKM        SETLL     TARI
     C     TARIK         READPE    TARI                                   87

     C                   MOVE      TAORDK        SVPVA
     C                   MOVE      TAORDB        SVAS
     C     SVPRE         IFEQ      'A'
     C                   MOVE      TAEØSB        SVAS
     C                   IF        TAEØSK <> ' '
     C                   EVAL      SVPVA = TAEØSK
     C                   END
     C                   ELSE
     C     SVPRE         IFEQ      'B'
     C     SVPRE         OREQ      'F'
     C                   MOVE      TAEFB         SVAS
     C                   IF        TAEFK <> ' '
     C                   EVAL      SVPVA = TAEFK
     C                   END
     C                   ELSE
     C     SVPRE         IFEQ      'C'
     C     SVPRE         OREQ      'E'
     C                   MOVE      TAEFTB        SVAS
     C                   IF        TAEFTK <> ' '
     C                   EVAL      SVPVA = TAEFTK
     C                   END
     C                   ELSE
     C     SVPRE         IFNE      'J'
     C     SVPRE         ANDNE     'N'
     C                   EXSR      TATES2
     C                   END
     C                   END
     C                   END
     C                   END

     C     SVAS          IFEQ      0
     C                   MOVE      'F'           SVPVA
     C                   END

     C                   ENDSR
     C***********************************************
     C     TATES2        BEGSR
     C***********************************************
