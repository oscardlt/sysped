     C                   OPEN      SYFI10FM
     C                   EXSR      INIT
     C                   GOTO      START1
     C*
     C     START         TAG
     C  N30              DO
     C                   MOVE      HEOPD         WSOPDG
     C                   MOVE      *ZEROS        WSOPD
     C                   MOVE      *ZEROS        HEAVD
     C                   MOVE      *ZEROS        HEOPD
     C                   SETOFF                                       279998
     C                   END
     C     STB1A         TAG
     C                   EXFMT     BILDE01
     C     UDUMMY        IFEQ      'J'
     C   12              GOTO      SLUTT
     C                   MOVE      *ZEROS        WSOPD
     C                   EXSR      TESTB1
     C                   MOVE      *ZEROS        WSDT
     C                   SETOFF                                       30
     C                   GOTO      ST21
     C                   END
     C                   SETOFF                                       30
     C                   MOVEA     '0000'        *IN(96)
     C                   MOVE      *BLANKS       WSMSG
     C*
     C   07              DO
     C                   EXSR      GETOPD
     C     UAVDE         IFEQ      0
     C     UOPDE         OREQ      0
     C                   GOTO      STB1A
     C                   END
     C                   END
     C   12              GOTO      SLUTT
     C     START1        TAG
     C                   EXSR      TESTB1
      *
      * Det kalles på TDV01R i CALTDV som lager oppdraget og legger
      * oppdragsnummeret i skjermbildefeltet WSOPD og fortsetter på
      * normal måte
     C     *IN20         IFEQ      '1'
     C     WSOPD         ANDEQ     *ZEROS
     C     *IN30         CABEQ     '1'           START
     C                   Z-ADD     *ZEROS        WSOPD
     C                   EXSR      CALTDV
     C                   MOVE      *OFF          *IN20
     C     WSOPD         CABEQ     *ZEROS        START
     C                   EXSR      TESTB1
     C                   ENDIF                                                  <*IN20
      *
     C*
     C   30UTYPE         IFEQ      'CR2'
     C                   GOTO      START
     C                   ELSE
     C                   GOTO      SLUTT
     C                   END
     C     MSGNR         IFNE      'YBC0036'
     C                   MOVE      'DIV0000'     MSGNR
     C                   END
     C*
     C*** UTYPE = 'CHF/CHO' CALL  FROM SYFF10 (CHANGE INVOICE)
     C  N05UTYPE         IFEQ      'CHF'
     C                   EXSR      TESTB2
     C                   GOTO      START3                                       TIL FAKTURA
     C                   END
     C*
     C   05              DO
     C                   EXSR      TESTB2
     C                   GOTO      START3                                       TIL FAKTURA
     C                   END
     C*
     C                   MOVEA     '000'         *IN(75)
     C                   MOVEA     '0'           *IN(56)
     C     START2        TAG
     C   16              DO
     C                   EXSR      MOVB1
     C                   MOVE      HEOPD         WSOPDG
     C                   MOVE      *ZEROS        HEOPD
     C                   SETOFF                                       5763
     C                   Z-ADD     2             LINNBR
     C                   Z-ADD     49            POSNBR
     C                   MOVE      'NY'          XNYGML            2
     C                   END
     C  N30
     CANN16              Z-ADD     2             LINNBR
     C  N30
     CANN16              Z-ADD     8             POSNBR
     C*
     C     ST21          TAG
     C*
     C                   EXSR      TSTOPL
     C*
     C                   EXSR      KDTFMT
     C                   EXFMT     BILDE02
     C                   EXSR      KDTFMT
     C     UDUMMY        IFEQ      'J'
     C                   EXSR      OPDHEA
     C                   GOTO      SLUTT
     C                   END
     C                   SETOFF                                       30
     C                   MOVEA     '00000000'    *IN(90)
     C                   MOVEA     '00000000'    *IN(82)
     C                   MOVEA     '0000'        *IN(78)
     C                   MOVEA     '00000000'    *IN(63)
     C                   MOVEA     '0'           *IN(71)
     C                   MOVEA     '00'          *IN(73)
     C                   MOVEA     '0000'        *IN(59)
N06  C                   MOVEA     '0'           *IN(54)
