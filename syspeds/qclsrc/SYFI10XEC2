             PGM        PARM(&UNIK &EMNE &AVNA &AVAD &INTNA1 &INTAD1 +
                          &INTNA2 &INTAD2 &INTNA3 &INTAD3 &INTNA4 +
                          &INTAD4 &INTNA5 &INTAD5)
 /* Forsøk på å send alle i en operasjon gis opp foreløpig  */
 /*          PGM        PARM(&UNIK &EMNE &AVNA &AVAD &TOADDR)  */
             DCL        VAR(&UNIK)   TYPE(*CHAR) LEN(7)
             DCL        VAR(&EMNE)   TYPE(*CHAR) LEN(70)
             DCL        VAR(&AVNA)   TYPE(*CHAR) LEN(50)
             DCL        VAR(&AVAD)   TYPE(*CHAR) LEN(50)
             DCL        VAR(&INTNA1) TYPE(*CHAR) LEN(50)
             DCL        VAR(&INTAD1) TYPE(*CHAR) LEN(50)
             DCL        VAR(&INTNA2) TYPE(*CHAR) LEN(50)
             DCL        VAR(&INTAD2) TYPE(*CHAR) LEN(50)
             DCL        VAR(&INTNA3) TYPE(*CHAR) LEN(50)
             DCL        VAR(&INTAD3) TYPE(*CHAR) LEN(50)
             DCL        VAR(&INTNA4) TYPE(*CHAR) LEN(50)
             DCL        VAR(&INTAD4) TYPE(*CHAR) LEN(50)
             DCL        VAR(&INTNA5) TYPE(*CHAR) LEN(50)
             DCL        VAR(&INTAD5) TYPE(*CHAR) LEN(50)
 /* Forsøk på å send alle i en operasjon gis opp foreløpig  */
 /*          DCL        VAR(&TOADDR) TYPE(*CHAR) LEN(700)   */

             ADDLIBLE   LIB(MMAIL)
             MONMSG     MSGID(CPF0000)

 /* Forsøk på å send alle i en operasjon/parameter gis opp foreløpig  */
 /*          EMLMSG     SUBJECT(&EMNE) FROMNAME(&AVNA) +
                          FROMADDR(&AVAD) TO(&TOADDR) +
                          TXTF(*LIBL/ARKSRC) TXTMBR(&UNIK) EDTMBR(*NO)  */
 /* hvis alle 5 utfylt */
             IF         COND(&INTAD5 *NE ' ') THEN(DO)
             EMLMSG     SUBJECT(&EMNE) FROMNAME(&AVNA) +
                          FROMADDR(&AVAD) TO(&INTAD1/&INTNA1/*TO  +
                          &INTAD2/&INTNA2/*CC +
                          &INTAD3/&INTNA3/*CC +
                          &INTAD4/&INTNA4/*CC +
                          &INTAD5/&INTNA5/*CC) +
                          TXTF(*LIBL/ARKSRC) TXTMBR(&UNIK) EDTMBR(*NO)
             GOTO       CMDLBL(EXIT)
             ENDDO
 /* hvis      4 utfylt */
             IF         COND(&INTAD4 *NE ' ') THEN(DO)
             EMLMSG     SUBJECT(&EMNE) FROMNAME(&AVNA) +
                          FROMADDR(&AVAD) TO(&INTAD1/&INTNA1/*TO  +
                          &INTAD2/&INTNA2/*CC +
                          &INTAD3/&INTNA3/*CC +
                          &INTAD4/&INTNA4/*CC) +
                          TXTF(*LIBL/ARKSRC) TXTMBR(&UNIK) EDTMBR(*NO)
             GOTO       CMDLBL(EXIT)
             ENDDO
 /* hvis      3 utfylt */
             IF         COND(&INTAD3 *NE ' ') THEN(DO)
             EMLMSG     SUBJECT(&EMNE) FROMNAME(&AVNA) +
                          FROMADDR(&AVAD) TO(&INTAD1/&INTNA1/*TO  +
                          &INTAD2/&INTNA2/*CC +
                          &INTAD5/&INTNA3/*CC) +
                          TXTF(*LIBL/ARKSRC) TXTMBR(&UNIK) EDTMBR(*NO)
             GOTO       CMDLBL(EXIT)
             ENDDO
 /* hvis      2 utfylt */
             IF         COND(&INTAD2 *NE ' ') THEN(DO)
             EMLMSG     SUBJECT(&EMNE) FROMNAME(&AVNA) +
                          FROMADDR(&AVAD) TO(&INTAD1/&INTNA1/*TO  +
                          &INTAD2/&INTNA2/*CC) +
                          TXTF(*LIBL/ARKSRC) TXTMBR(&UNIK) EDTMBR(*NO)
             GOTO       CMDLBL(EXIT)
             ENDDO
 /* ALLTID minst 1  */
             EMLMSG     SUBJECT(&EMNE) FROMNAME(&AVNA) +
                          FROMADDR(&AVAD) TO(&INTAD1/&INTNA1/*TO) +
                          TXTF(*LIBL/ARKSRC) TXTMBR(&UNIK) EDTMBR(*NO)

 EXIT:       RMVLIBLE   LIB(MMAIL)

             CLRPFM     FILE(ARKSRC) MBR(&UNIK)

             ENDPGM
