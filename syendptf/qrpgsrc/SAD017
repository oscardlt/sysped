     C* SR for å behandle UNH-segmentet                    SRUNH
      *""""""""""""""""""""""""""""""""""""""""""""""""""""""""""
     C     SRUNH         BEGSR
     C                   CLEAR                   SAEHEDIR
      * LES AVDELING OG OPPDRAGSNR
     C                   MOVE      *BLANKS       FE
     C     1             DO        15            NR
     C                   EXSR      GETCH
     C   LR              GOTO      ENDUNH
     C                   IF        TEGN = LOGPLU
     C                   LEAVE
     C                   END
     C                   MOVE      TEGN          FE(NR)
     C                   END
     C     UNH010        TAG
     C                   MOVEL     FELT          XAN14
      *
     C                   MOVE      XAN1411       SEAVD
     C                   IF        XAN1421 = *BLANKS
     C                   MOVE      XAN1412       SETDN
     C                   MOVE      XAN1413       SESG
     C                   ELSE
     C                   MOVE      XAN1422       SETDN
     C                   MOVE      XAN1423       SESG
     C                   END
      *
     C                   IF        ((XAVD <> SEAVD) OR (XTDN <> SETDN))
     C                   GOTO      ENDUNH
     C                   END
      *
     C                   EXSR      GETSEG
     C     SEAVD         CHAIN     STANDE                             33
      *
      *
     C                   EXSR      SRBGMH
     C                   EXSR      SRCSTH
     C                   IF        SEGNAM = 'RFF'
     C                   EXSR      SRRFFH
     C                   END
     C                   IF        SEGNAM = 'DTM'
     C                   EXSR      SRDTMH
     C                   END
     C                   EXSR      SRLOCH
      *
     C                   IF        SEGNAM = 'TDT'
     C                   EXSR      SRTDTH
     C                   EXSR      SRGISH
     C                   END
      *
     C                   EXSR      SRNADH
     C                   EXSR      SRCTAH
      *
     C                   IF        SEGNAM = 'TOD'
     C                   EXSR      SRTODH
N01  C                   IF        *IN39
N01  C                   EXSR      SRFTXH
N01  C                   END
     C                   EXSR      SRMOAH
     C                   EXSR      SRCUXH
     C                   END
      *
     C                   EXSR      SRUNSH
     C                   IF        SEGNAM = 'DMS'
     C                   EXSR      SRDMSH
     C                   END
      *
     C                   DOW       SEGNAM = 'CST'
     C                   CLEAR                   SAEVEDIR
     C                   EVAL      SVAVD = SEAVD
     C                   EVAL      SVTDN = SETDN
     C                   EXSR      SRCSTV
     C                   IF        SEGNAM = 'PAC'
     C                   EXSR      SRPACV
     C                   EXSR      SRPCIV
     C                   END
     C                   EXSR      SRLOCV
     C                   IF        SEGNAM = 'MOA'
     C                   EXSR      SRMOAV
     C                   END
     C                   EXSR      SRMEAV
     C                   IF        SEGNAM = 'TAX'
     C                   EXSR      SRTAXV
     C                   END
N02  C                   IF        SEGNAM = 'DCR'
     C                   EXSR      SRDCRV
N02  C                   END
     C                   EXSR      SRGDSV
     C                   EXSR      SRFTXV
     C                   WRITE     SAEVEDIR
     C                   ENDDO
      *
NXX  C                   EXSR      SRUNSV
     C                   IF        SEGNAM = 'TAX'
     C                   EXSR      SRTAXT
     C                   END
      *
SXX  C*                  EXSR      SRUNSV
      *
     C                   EXSR      SRCNTT
      *
     C                   IF        SERG <> *BLANKS
     C                   EVAL      SYRG   = SERG
     C     CUNDLFOK      CHAIN     CUNDLFO                            33
     C                   IF        *IN33 = *OFF
     C                   EVAL      SEKNK  = KUNDNR
     C                   EVAL      SENAK  = KNAVN
     C                   EVAL      SEADK1 = ADR1
     C                   EVAL      SEADK2 = ADR2
     C                   IF        POSTNR = 0
     C                   MOVEL     ADR3          SEADK3
     C                   ELSE
     C                   MOVEL     POSTNR        SEADK3
     C                   MOVE      ADR3          SEADK3
     C                   END
     C                   END
     C                   END
     C                   WRITE     SAEHEDIR
      *
     C     ENDUNH        ENDSR
