     C     KODTAK        KLIST
     C                   KFLD                    WSAUNI
     C                   KFLD                    WSAAVD
     C     STANEK        KLIST
     C                   KFLD                    WSAAVD
     C     KODKEY        KLIST
     C                   KFLD                    WOPUNI
     C                   KFLD                    WOPAVD
     C                   KFLD                    WOPLNR
     C     *LIKE         DEFINE    KOPAVD        WOPAVD
     C     *LIKE         DEFINE    KOPUNI        WOPUNI
     C     *LIKE         DEFINE    KOPLNR        WOPLNR
     C                   Z-ADD     0             WOPAVD
     C                   MOVE      'P'           WOPUNI
     C                   Z-ADD     0             WOPLNR
     C*
     C     *ENTRY        PLIST
     C                   PARM                    UIN01             1
     C                   PARM                    UHOVP            10
     C                   PARM                    UTILP            10
     C                   PARM                    ULSTP            10
     C                   PARM                    UUTLP            10
     C                   PARM                    UMRKP            10
     C                   PARM                    UDLP             10
     C*
     C                   MOVE      'A'           WSAUNI            1
     C     FIFIRM        SETLL     FIRM
     C                   READ      FIRM                                   99
     C*
     C                   MOVE      UAVD          WSAAVD
     C     KODTAK        CHAIN     KODTA                              99
     C  N99STANEK        CHAIN     STANDE                             99
     C  N99              GOTO      START2
     C                   SETOFF                                       99
     C                   MOVE      *ZEROS        WSAAVD
     C*
     C     START         TAG
     C                   EXFMT     BILDE
     C                   SETOFF                                       9998
     C*
     C     KODTAK        CHAIN     KODTA                              99
     C   99              GOTO      START
     C     STANEK        CHAIN     STANDE                             98
     C   98              GOTO      START
     C*
     C     START2        TAG
     C                   MOVE      WSAAVD        UAVD
     C                   MOVE      KOANVN        UAVDNV
     C                   MOVE      SEDL          UDL
     C                   MOVE      SETOLK        UTOLK
     C                   MOVE      '0'           UIN01
