     C                   ELSE
     C     WSSDFD        IFLT      3999
     C                   MOVE      WSSDFD        TMPDAT
     C                   EXSR      FIXDAT
     C                   MOVE      TMPDAT        WSSDFD
     C                   END
     C                   MOVE      WSDTD1        UDT1              2 0
     C                   MOVE      WSDTM1        UDT2              2 0
     C                   MOVE      WSDTÅ1        UDT3              2 0
     C                   CALL      'DATOFMT'
     C                   PARM                    UDT1
     C                   PARM                    UDT2
     C                   PARM                    UDT3
     C                   MOVE      UDT1          UDT1A             2 0
     C                   MOVE      UDT2          UDT2A             2 0
     C     UDT3          IFGT      50
     C     UDT3          ADD       1900          UDT3A
     C                   ELSE
     C     UDT3          ADD       2000          UDT3A
     C                   END
     C                   MOVE      UDT1          WSDTD1
     C                   MOVE      UDT2          WSDTM1
     C                   MOVE      UDT3          WSDTÅ1
     C                   END
     C*
     C*
     C* DATO 2
     C*
     C     WSSDTD        IFEQ      0
     C                   MOVE      *ZEROS        UDATOB
     C                   ELSE
     C     WSSDTD        IFLT      3999
     C                   MOVE      WSSDTD        TMPDAT
     C                   EXSR      FIXDAT
     C                   MOVE      TMPDAT        WSSDTD
     C                   END
     C                   MOVE      WSDTD2        UDT1              2 0
     C                   MOVE      WSDTM2        UDT2              2 0
     C                   MOVE      WSDTÅ2        UDT3              2 0
     C                   CALL      'DATOFMT'
     C                   PARM                    UDT1
     C                   PARM                    UDT2
     C                   PARM                    UDT3
     C                   MOVE      UDT1          WSDTD2
     C                   MOVE      UDT2          WSDTM2
     C                   MOVE      UDT3          WSDTÅ2
     C                   MOVE      UDT1          UDT1B             2 0
     C                   MOVE      UDT2          UDT2B             2 0
     C     UDT3          IFGT      50
     C     UDT3          ADD       1900          UDT3B
     C                   ELSE
     C     UDT3          ADD       2000          UDT3B
     C                   END
     C                   END
     C*
     C*
     C* DATO 3
     C*
     C     WSSFFD        IFEQ      0
     C                   MOVE      *ZEROS        UDATOC
     C                   ELSE
     C     WSSFFD        IFLT      3999
     C                   MOVE      WSSFFD        TMPDAT
     C                   EXSR      FIXDAT
     C                   MOVE      TMPDAT        WSSFFD
     C                   END
     C                   MOVE      WSDTD3        UDT1              2 0
     C                   MOVE      WSDTM3        UDT2              2 0
     C                   MOVE      WSDTÅ3        UDT3              2 0
     C                   CALL      'DATOFMT'
     C                   PARM                    UDT1
     C                   PARM                    UDT2
     C                   PARM                    UDT3
     C                   MOVE      UDT1          WSDTD3
     C                   MOVE      UDT2          WSDTM3
     C                   MOVE      UDT3          WSDTÅ3
