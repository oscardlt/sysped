     H DFTACTGRP(*NO)
     FSADH      IF   E           K DISK
     FSAEH      IF   E           K DISK
     FTRIH      IF   E           K DISK
     FTRUH      IF   E           K DISK
     FHEADF     IF   E           K DISK

     D XAVD            S              4  0
     D XOPD            S              7  0
      *
      *  Prototype for syreo01r3
     d syreo01r3       pr
     d uavd_                               like(uavd)
     d uopd_                               like(uopd)
     d utyp_                               like(utyp)
     d uok_                                like(uok)
      *  *ENTRY Interface for Main Procedure
     d syreo01r3       pi
     d uavd                           4
     d uopd                           7
     d utyp                           5
     d uok                            1
      /free

         exsr init;

         if utyp <> 'SAD-I';
           chain (xavd: xopd) sadh;
           if %found;
             uok = 'N';
           endif;
         endif;
         if utyp <> 'SAD-E';
           chain (xavd: xopd) saeh;
           if %found;
             uok = 'N';
           endif;
         endif;
         if utyp <> 'TET-I';
           chain (xavd: xopd) trih;
           if %found;
             uok = 'N';
           endif;
         endif;
         if utyp <> 'TET-E';
           chain (xavd: xopd) truh;
           if %found;
             uok = 'N';
           endif;
         endif;
         if utyp <> 'OPD';
           chain (xavd: xopd) headf;
           if %found;
             uok = 'N';
           endif;
         endif;

       *inlr = *on;
       return;

      //***********************************************
       begsr init;
      //***********************************************
       xavd = %float(uavd);
       xopd = %float(uopd);
       uok  = 'J';

       endsr;

      /end-free
