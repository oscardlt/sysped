             PGM        PARM(&LIB &PRENAVN)
             DCL        VAR(&LIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&PRENAVN) TYPE(*CHAR) LEN(3)
             DCL        VAR(&PGMNAVN) TYPE(*CHAR) LEN(10)

             CHKOBJ     OBJ(&LIB) OBJTYPE(*LIB)
             MONMSG     MSGID(CPF9801) EXEC(DO)
             SNDPGMMSG  MSG('BIBLIOTEK FINNES IKKE.')
             GOTO       CMDLBL(SLUTT)
             ENDDO

             CHGVAR     VAR(&PGMNAVN) VALUE(&PRENAVN *CAT 'SYEDIAU')
             ADDAJE     SBSD(SYEDI/SYEDISBS) JOB(&PGMNAVN) +
                          JOBD(&LIB/SYEDIAUTJD)

