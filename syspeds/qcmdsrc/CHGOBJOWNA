             CMD        PROMPT('Change Owner for All OBJ')
             PARM       KWD(LIB) TYPE(*CHAR) LEN(10) RSTD(*NO) +
                          DFT(*LIBL) CHOICE('Name, *LIBL') +
                          PROMPT('Library')
             PARM       KWD(OLDOWN) TYPE(*CHAR) LEN(10) RSTD(*NO) +
                          DFT(*USER) CHOICE('UserID, *USER, *ALL') +
                          PROMPT('Old Owner')
             PARM       KWD(NEWOWN) TYPE(*CHAR) LEN(10) RSTD(*NO) +
                          MIN(1) CHOICE('UserID') PROMPT('New Owner')
