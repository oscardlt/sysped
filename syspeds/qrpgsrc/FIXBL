     C     BLUR          COMP      'G'                                    27
     C                   EXSR      KDTFMT
     C     KUND1K        CHAIN     CUNDL01                            33
     C   33              MOVE      *BLANKS       KNAVN
     C*
     C     START2        TAG
     C                   EXFMT     BILDE02
     C*
     C  N05
     CANN12              GOTO      START2
     C   12              EXCEPT    DUMBL
     C   12              GOTO      START
     C*
     C     START3        TAG
     C                   TIME                    XTIME
     C                   MOVE      XAAR          SSDTÅ
     C                   MOVE      XMND          SSDTM
     C                   MOVE      XDAG          SSDTD
     C                   MOVE      XTID          SSTM
     C     STSK          CHAIN     STS                                33
     C  N33              GOTO      START3
     C                   MOVE      WSAVD         SSAVD
     C                   MOVE      WSOPD         SSOPD
     C                   MOVE      WSUSER        SSUSR
     C                   MOVE      WSSG          SSSG
     C   27              MOVE      'M'           SSRTYP
     C  N27              MOVE      'L'           SSRTYP
     C                   MOVE      BLST          SSSTG
     C                   MOVE      WSST          SSSTN
     C                   WRITE     STSR
     C*
     C                   MOVE      WSST          BLST
     C                   EXCEPT    ENDRE
     C                   MOVE      *ZEROS        WSOPD
     C                   MOVE      *BLANKS       WSST
     C                   GOTO      START
