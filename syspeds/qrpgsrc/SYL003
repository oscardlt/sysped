     C                   MOVE      '2'           FMELDNR
     C                   GOTO      SLTBDT
     C                   ENDIF
      *
      * TEST kunderef.1
     C     WSPAK         IFEQ      *BLANKS
     C                   IF        XSPRÅK = 'EN'
     C                   MOVEL     FEILEN04      FEILMELDING
     C                   ELSE
     C                   MOVEL     FEIL04        FEILMELDING
     C                   END
     C                   MOVE      '4'           FMELDNR
     C                   GOTO      SLTBDT
     C                   END
      *
      * TEST TRANSPORTØR
     C     BWSTN         IFNE      0
     C     TRANK         CHAIN     TRAN                               90
     C     *IN90         IFEQ      '1'
     C                   IF        XSPRÅK = 'EN'
     C                   MOVEL     FEILEN03      FEILMELDING
     C                   ELSE
     C                   MOVEL     FEIL03        FEILMELDING
     C                   END
     C                   MOVE      '3'           FMELDNR
     C                   GOTO      SLTBDT
     C                   END
     C                   END
      *
     C     SLTBDT        ENDSR
      *
      *=====================================================================
      *  Oppdater heading
      *=====================================================================
     C     OPDHEADING    begsr
     C     BWSMN         IFEQ      0
     C     1             CHAIN     LLTELL                             33
     C                   ADD       1             LLREC1
     C                   UPDATE    LLTELLR
     C                   MOVE      LLREC1        LHMN
     C                   MOVE      LLREC1        BWSMN
     C                   ELSE
     C                   MOVE      BWSMN         LHMN
     C                   ENDIF

     C     LH00K         CHAIN     LLHEAD                             60
     C     *IN60         IFEQ      '0'
     C     FEILMELDING   IFEQ      *BLANKS
     C                   EXSR      MOVEWSLH
     C                   ENDIF
     C                   UPDATE    LLHEADR
     C                   ELSE
     C                   EXSR      MOVEBLH
     C     FEILMELDING   IFEQ      *BLANKS
     C                   EXSR      MOVEWSLH
     C                   MOVE      'W'           LHSTA
     C                   WRITE     LLHEADR
     C                   ENDIF
     C                   ENDIF
      *
     C                   endsr
      *=====================================================================
      *  OPPDATER FELTER I FIL
      *=====================================================================
     C     MOVEWSLH      begsr
     C                   MOVE      WSBNR         LHBNR
     C                   MOVE      WSBNA         LHBNA
     C                   MOVE      WSBA1         LHBA1
     C                   MOVE      WSBA2         LHBA2
     C                   MOVE      BWSBPN        LHBPN
     C                   MOVE      WSBA3         LHBA3
     C                   IF        WSBLK = *BLANKS
     C                   MOVE      FILAND        LHBLK
     C                   ELSE
     C                   MOVE      WSBLK         LHBLK
     C                   END
     C                   MOVE      WSBPU         LHBPU
     C                   MOVE      BWSTDT        LHTDT
     C                   MOVE      BWSTN         LHTN
     C                   MOVE      WSLSD         LHLSD
     C                   MOVE      WSPAK         LHPAK
     C                   MOVE      WSORDR        LHORDR
     C                   MOVE      WSFLN         LHFLN
      *
     C                   IF        WSLSD <> *BLANKS
     C     LLVMLSK       CHAIN     LLVMLS                             33
     C                   IF        *IN33
     C                   EVAL      DSKN = BIKUNR
     C                   EVAL      DSVMNR = WSBNR
     C                   EVAL      DSLSKD = WSLSD
     C                   WRITE     LLVMLSR
     C                   ELSE
     C                   EVAL      DSLSKD = WSLSD
     C                   UPDATE    LLVMLSR
     C                   END
     C                   END
      *
     C                   endsr
