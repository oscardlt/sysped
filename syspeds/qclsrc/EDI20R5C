             PGM PARM(&VALUE &POD1 &POD2 &POD3 &TCP &PWD &PODNR)
/*____________________*/
/* Definier variabler */
/* MIDLERTIDIG HEADF I SYENDPTF */
/*""""""""""""""""""""*/
             DCL        VAR(&LIB2) TYPE(*CHAR) LEN(10) +
                          VALUE('SYSPEDF   ')
             DCL        VAR(&VALUE) TYPE(*CHAR) LEN(8)
             DCL        VAR(&POD1) TYPE(*CHAR) LEN(10)
             DCL        VAR(&POD2) TYPE(*CHAR) LEN(10)
             DCL        VAR(&POD3) TYPE(*CHAR) LEN(4)
             DCL        VAR(&TCP) TYPE(*CHAR) LEN(1)
             DCL        VAR(&PWD) TYPE(*CHAR) LEN(6)
             DCL        VAR(&PODNR) TYPE(*CHAR) LEN(2)
             OVRDBF     FILE(HEADF) TOFILE(&LIB2/HEADF)
             CALL       PGM(EDI20R5) PARM(&VALUE &POD1 &POD2 &POD3 +
                           &TCP &PWD &PODNR)
             ENDPGM
