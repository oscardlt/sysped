             PGM        PARM(&KNR &REF1 &REF2 &REF3 &BRUKER)
             DCL        VAR(&KNR) TYPE(*CHAR) LEN(8)
             DCL        VAR(&REF1) TYPE(*CHAR) LEN(17)
             DCL        VAR(&REF2) TYPE(*CHAR) LEN(17)
             DCL        VAR(&REF3) TYPE(*CHAR) LEN(17)
             DCL        VAR(&BRUKER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&MELDING) TYPE(*CHAR) LEN(250)

             CHGVAR     VAR(&MELDING) VALUE('Plukkliste er kommet +
                          fra kundenr=' *BCAT &KNR *BCAT ', +
                          Kunderef.1=' *BCAT &REF1 *BCAT ', +
                          Kunderef.2=' *BCAT &REF2 *BCAT ', +
                          Kunderef.3=' *BCAT &REF3 *BCAT '.')
             SNDMSG     MSG(&MELDING) TOUSR(&BRUKER)

             ENDPGM
