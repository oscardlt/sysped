     h dftactgrp(*no)
     fxmlrcvf   if   e           k disk
     ‚* Prototype for syxml44f
     d syxml44         pr
     d usn_                                like(usn)
     ‚* Prototype for 'SYXML44DKE'
     d syxml44dke      pr                  extpgm('SYXML44DKE')
     d usn_                                like(usn)
     ‚* Prototype for 'SYXML44DKI'
     d syxml44dki      pr                  extpgm('SYXML44DKI')
     d usn_                                like(usn)
     ‚* Prototype for 'SYXML44NOE'
     d syxml44noe      pr                  extpgm('SYXML44NOE')
     d usn_                                like(usn)
     ‚* Prototype for 'SYXML44NOI'
     d syxml44noi      pr                  extpgm('SYXML44NOI')
     d usn_                                like(usn)
     ‚* Prototype for 'SYXML44SEE'
     d syxml44see      pr                  extpgm('SYXML44SEE')
     d usn_                                like(usn)
     ‚* Prototype for 'SYXML44SEI'
     d syxml44sei      pr                  extpgm('SYXML44SEI')
     d usn_                                like(usn)
     ‚* *ENTRY Interface for Main Procedure
     d syxml44         pi
     d usn                            9
     ‚*----------------------------------------------------------------------
     ‚* Stand Alone Fields - TOP
     ‚*----------------------------------------------------------------------
     d uxrsn           s              9P 0
     d xie             s              3
     d xland           s              2
     ‚*----------------------------------------------------------------------
     ‚* Stand Alone Fields - BOTTOM
     ‚*----------------------------------------------------------------------
     c                   move      usn           uxrsn

      /free
       setll uxrsn xmlrcvf;
       reade uxrsn xmlrcvf;
       dow not %eof;
         select;
           // Landkode mottager ?
         when xrnv   = 'ReceiverID';
           xland = xrverd;
           // Import eller eksport ?
         when xrnv   = 'Code';
           xie = xrverd;
           leave;
         endsl;
         reade uxrsn xmlrcvf;
       enddo;

       // Kall på riktigt program NO/DK/SE og I eller E
       select;

       when xland  = 'DK';
         if xie = 'EXC';
           syxml44dke ( usn );
         endif;
         if xie = 'IMC';
           syxml44dki ( usn );
         endif;

       when xland  = 'NO';
         if xie = 'EXC';
           syxml44noe ( usn );
         endif;
         if xie = 'IMC';
           syxml44noi ( usn );
         endif;

       when xland  = 'SE';
         if xie = 'EXC';
           syxml44see ( usn );
         endif;
         if xie = 'IMC';
           syxml44sei ( usn );
         endif;
       endsl;

       *inlr = *on;
      /end-free
