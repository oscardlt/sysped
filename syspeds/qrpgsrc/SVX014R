     C                   ADD       1             XTOTL
     C                   EVAL      YKODE = TVEH
     C     SVK01K        CHAIN     SVXKODL01                          33
     C  N33              IF        TVNTEH = 0
     C                   MOVEL     TKTXTN        XTKTXTE
     C                   ELSE
     C                   EVAL      XTKTXTE = 'PIECES'
     C                   END
     C   33              MOVEL     *BLANKS       XTKTXTE
     C                   IF        *IN33 = *OFF
     C                   MOVE      *ZEROS        TVNT
     C                   MOVE      *BLANKS       TVMN
     C                   MOVE      *BLANKS       TVCNR
     C                   MOVE      *BLANKS       TVVT
     C                   SELECT
     C                   WHEN      XTKTXTE = 'PIECES'
     C                   ADD       1             XTOTL
     C                   WHEN      TKTXTE <> '.'
     C                   ADD       1             XTOTL
     C                   MOVEL     TKTXTE        XTKTXTE
     C                   ENDSL
     C                   END
      *
     C     SVVK          SETLL     SVXVMN
     C     SVVK          SETLL     SVXVCN
     C     SVVK          SETLL     SVXVVT
     C                   MOVEA     '000'         *IN(34)
     C     STSJ31        TAG
     C  N34SVVK          READE     SVXVMN                                 34
     C  N35SVVK          READE     SVXVCN                                 35
     C  N36SVVK          READE     SVXVVT                                 36
