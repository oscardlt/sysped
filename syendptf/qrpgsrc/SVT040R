     C                   MOVEL     SVTX04_02     XSVTX04_02
     C                   IF        SVTX04_01='E'
     C                   MOVE      '00'          XSVTX04_02
     C                   ENDIF
     C     XKEY          CHAIN     SVTX10F
     C                   IF        %FOUND
     C                   MOVEL     ÅSVTX10_31    SVTX10_31
     C                   UPDATE    SVTX10R
     C                   ELSE
     C                   MOVEL     XSVTX04_02    SVTX10_01
     C                   MOVEL     ÅSVTX10_31    SVTX10_31
     C                   WRITE     SVTX10R
     C                   ENDIF

N05  C                   CALL      'SVT041R'
N05  C                   PARM                    SVTX04_01
N05  C                   PARM                    SVTX04_02
N05  C                   PARM                    SVTX10_31

     C                   ENDIF
