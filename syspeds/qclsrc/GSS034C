/* *********************************************************************   */
/*       * RETTINGER I DETTE PROGRAM:                                      */
/* PTFnr:*Sek Sig Vers  Beskrivelse..........................              */
/* """"""*"""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""    */
/* 10027 * 01 YBC PTF10 ORDRETYPE                                          */
/* ********************************************************************    */
/*S01        PGM        PARM(&CNO2 &UTFAX &TNO &TYPE &CUR &MOD &COMM +  */
/*S01                     &KNC)                                         */
/*N01*/      PGM        PARM(&CNO2 &UTFAX &TNO &TYPE &ORDTYP &CUR +
                          &MOD &COMM &KNC)
             DCL        VAR(&CNO2) TYPE(*CHAR) LEN(8)
             DCL        VAR(&UTFAX) TYPE(*CHAR) LEN(1)
             DCL        VAR(&TNO) TYPE(*CHAR) LEN(11)
             DCL        VAR(&TYPE) TYPE(*CHAR) LEN(3)
/*N01*/      DCL        VAR(&ORDTYP) TYPE(*CHAR) LEN(1)
             DCL        VAR(&CUR) TYPE(*CHAR) LEN(3)
             DCL        VAR(&MOD) TYPE(*CHAR) LEN(1) /* +
                          1=Packingliste, 2=Proforma faktura +
                          (komplimerte), 3=Proforma faktura (ikke +
                          kompilerte), 4=Fakturagrunnlag, 5=Final +
                          packinglist*/
             DCL        VAR(&COMM) TYPE(*CHAR) LEN(1)
             DCL        VAR(&KNC) TYPE(*CHAR) LEN(8)
             CHGDTAARA  DTAARA(*LDA (501 500)) VALUE(*BLANKS)
             CALL       PGM(GSS034R) PARM(&CNO2 &UTFAX &MOD)
             IF         COND(&UTFAX = 'J') THEN(DO)
             IF         COND(&MOD = '1') THEN(SBMJOB CMD(CALL +
                          PGM(GSS022C) PARM(&TNO &TYPE)) +
                          JOB(FAXPACKING) JOBQ(SYJOBQNMAX))
/*S01        IF         COND(&MOD = '2') THEN(SBMJOB CMD(CALL +      */
/*S01                     PGM(GSS031C) PARM(&TNO &TYPE &CUR &COMM +  */
/*S01                     &KNC)) JOB(FAXINVSHIP) JOBQ(SYJOBQNMAX))   */
/*N01*/      IF         COND(&MOD = '2') THEN(SBMJOB CMD(CALL +
                          PGM(GSS031C) PARM(&TNO &TYPE &ORDTYP &CUR &COMM +
                          &KNC)) JOB(FAXINVSHIP) JOBQ(SYJOBQNMAX))
/*S01        IF         COND(&MOD = '3') THEN(SBMJOB CMD(CALL +      */
/*S01                     PGM(GSS033C) PARM(&TNO &TYPE)) +           */
/*S01                     JOB(FAXPROSHIP) JOBQ(SYJOBQNMAX))          */
/*N01*/      IF         COND(&MOD = '3') THEN(SBMJOB CMD(CALL +
                          PGM(GSS033C) PARM(&TNO &TYPE &ORDTYP)) +
                          JOB(FAXPROSHIP) JOBQ(SYJOBQNMAX))
             IF         COND(&MOD = '4') THEN(SBMJOB CMD(CALL +
                          PGM(GSS041C) PARM(&TNO &TYPE)) +
                          JOB(FAXINVBAS) JOBQ(SYJOBQNMAX))
             IF         COND(&MOD = '5') THEN(SBMJOB CMD(CALL +
                          PGM(GSS026C) PARM(&TNO)) JOB(FAXPACKING) +
                          JOBQ(SYJOBQNMAX))
             ENDDO
             ENDPGM
