      * Liste Ærende
     c     Error         cabne     *blanks       Utliste
     C     Key           SETLL     TRACKL01
     C     Key           READE     TRACKL01                               55
     C     *IN55         DOWEQ     '0'
     C                   Add       1             AntLest
N01  C                   EVAL      UTTEXT = TTTEXT
N01  C                   EVAL      UTTEXL = TTTEXL
N01   * manuell tekst / begge linjer uavh. av språk (*LE = Local+English )
N01  c                   if        TTEDEV = '*LE'
N01  C                   EVAL      UTTEXT = %trim(TTTEXL)+' ' + TTTEXT
N01  C                   EVAL      UTTEXL = %trim(TTTEXL)+' ' + TTTEXT
N01  c                   endif
N02   * EDITERING AV LAT/LONG - BLANK HVIS 0
N02  C                   if        TTBRE = *zeros
N02  C                   eval      TTBREX=*blanks
N02  C                   else
N02  C                   eval      TTBREX=%trim(%editw(TTBRE:'  .      '))
N02  C                   endif
N02  C                   if        TTLEN = *zeros
N02  C                   eval      TTLENX=*blanks
N02  C                   else
N02  C                   eval      TTLENX=%trim(%editw(TTLEN:'   .      '))
N02  C                   endif
      * ............................................................................................
      * Skriv en JSON-Array-linje pr menypunkt - komma før hvert nytt element
      * ............................................................................................
      /free
       if AntLest=1;
          JSONstring=*blanks;
          else;
          JSONstring=', ';
          endif;
       JSONstring=%trim(JSONstring)+'{'
          +'¬frBrev¬ : ¬'+%trim(%editc(TTFBNR:'Z'))+'¬, '
          +'¬event¬ : ¬'+%trim(TTACTI)+'¬, '
s02       //+'¬date¬ : ¬'+%trim(%editc(TTDATE:'Z'))+'¬, '
s02       //+'¬time¬ : ¬'+%trim(%editc(TTTIM4:'Z'))+'¬, '
S01       //+'¬date¬ : ¬'+%trim(%editc(TTDATE:'Z'))+'¬, '
S01       //+'¬time¬ : ¬'+%trim(%editc(TTTIM4:'Z'))+'¬, '
S01       //+'¬textEng¬ : ¬'+%trim(TTTEXT)+'¬, '
S01       //+'¬textLoc¬ : ¬'+%trim(TTTEXL)+'¬, '
n01       +'¬date¬ : ¬'+%trim(%editc(TTDATE:'X'))+'¬, '
n01       +'¬time¬ : ¬'+%trim(%editc(TTTIM4:'X'))+'¬, '
N01       +'¬textEng¬ : ¬'+%trim(UTTEXT)+'¬, '
N01       +'¬textLoc¬ : ¬'+%trim(UTTEXL)+'¬, '
n02       +'¬tttime¬ : ¬'+%trim(%editc(TTTIME:'X'))+'¬, '
N02       +'¬tttext¬ : ¬'+%trim(TTTEXT)+'¬, '
N02       +'¬tttexl¬ : ¬'+%trim(TTTEXL)+'¬, '
          +'¬ediCode¬ : ¬'+%trim(TTEDEV)+'¬, '
          +'¬ediReason¬ : ¬'+%trim(TTEDRE)+'¬, '
          +'¬depot¬ : ¬'+%trim(TTDEPO)+'¬, '
          +'¬name¬ : ¬'+%trim(TTNAME)+'¬, '
          +'¬status¬ : ¬'+%trim(TTMANU)+'¬, '
N02       +'¬tripNo¬ : ¬'+%trim(%editc(TTTUR:'Z'))+'¬, '
N02       +'¬locCode¬ : ¬'+%trim(TTLOCC)+'¬, '
N02       +'¬locText¬ : ¬'+%trim(TTLOCT)+'¬, '
N02       +'¬latitude¬ : ¬'+%trim(TTBREX)+'¬, '
N02       +'¬longitude¬ : ¬'+%trim(TTLENX)+'¬, '
N02       +'¬eventURL¬ : ¬'+%trim(TTURL)+'¬, '
N02       +'¬manualCode¬ : ¬'+%trim(TTMAN2)+'¬, '
          +'¬user¬ : ¬'+%trim(TTUSER)+'¬, '
          +'¬createDate¬ : ¬'+%trim(%editc(TTDATL:'Z'))+'¬, '
          +'¬createTime¬ : ¬'+%trim(%editc(TTTIML:'Z'))+'¬}';
      /end-free
