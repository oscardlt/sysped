             PGM
             ALCOBJ     OBJ((STATHFL1 *FILE *EXCL))
             MONMSG     MSGID(CPF1002) EXEC(DO)
             SNDPGMMSG  MSGID(YBC0450) MSGF(SYMSGF) MSGTYPE(*INFO)
             GOTO       CMDLBL(SLUTT)
             ENDDO
             DLCOBJ     OBJ((STATHFL1 *FILE *EXCL))

             ALCOBJ     OBJ((STATREK *FILE *EXCL))
             MONMSG     MSGID(CPF1002) EXEC(DO)
             SNDPGMMSG  MSGID(YBC0450) MSGF(SYMSGF) MSGTYPE(*INFO)
             GOTO       CMDLBL(SLUTT)
             ENDDO
             DLCOBJ     OBJ((STATREK *FILE *EXCL))

             SBMJOB     CMD(CALL PGM(SYFA44CLJ)) JOB(REKONSTAT) +
                          JOBQ(SYJOBQPRT)
 SLUTT:      ENDPGM
