     C                   EVAL      PABRID = '*KUNDFT*' + %TRIM(FIFIRM)
     C                   EVAL      PAID   = 'OSOTY'
     C*
      * Hent parametre for innland uvh. av om der er INL
     C     *LOVAL        SETLL     FIRINL
     C                   READ      FIRINL                                 33
     C     ZUSER         CHAIN     USRDFT                             33
     C                   MOVE      ZZTERM        RITERM
     C                   MOVE      'N'           INNLAN            1
     C     CALLFR        IFEQ      'I'
     C     CALLFR        OREQ      'U'
