     C***********************************************
     C     INIT          BEGSR
     C***********************************************
     C     SAEHK         KLIST
     C                   KFLD                    UAVD
     C                   KFLD                    UTDN
     C     EXTREFK1      KLIST
     C                   KFLD                    UAVD
     C                   KFLD                    UTDN
     C                   KFLD                    WSKODE            3
s01  C*                  MOVEL     '*CR'         WSKODE
n01  C                   MOVEL     '*CE'         WSKODE
     C*
     C     *ENTRY        PLIST
     C                   PARM                    UAVD              4 0
     C                   PARM                    UTDN              7 0
     C                   PARM                    UFLAGG            1 0
     C                   Z-ADD     0             UFLAGG
     C*
     C                   MOVE      *ZEROS        WSDT
     C                   MOVE      *ZEROS        SEDT
     C     SAEHK         CHAIN     SAEH                               29
N03  C                   IF        UTDN < 0
N03  C     SAEHK         CHAIN     SAEHOM                             29
N03  C                   END
     C   29              SETON                                        LR
     C   29              RETURN
N02  C     SAEHK         CHAIN     SAEHTL                             33
N02  C   33              EVAL      SESAM = 'J'
N02  C   33              EVAL      SEFVK = 0,9
     C                   MOVE      SEDTD         WSDTD
     C                   MOVE      SEDTM         WSDTM
     C                   MOVE      SEDTÅ         WSDTÅ
     C                   MOVE      SEDTDF        WSDTDF
     C                   MOVE      SEDTMF        WSDTMF
     C                   MOVE      SEDTÅF        WSDTÅF
     C     EXTREFK1      CHAIN     FRISOK                             33
     C  N33              MOVEL     FSSOK         WSTR17
     C*
     C                   ENDSR
