             PGM        PARM(&LIB &PRENAVN)
/* EKSAMPEL                 */
/* &LIB     = A01SYEND      */
/* &PRENAVN = A01           */
/* DA BLIR DELSYS SYSBS_A01 */

             DCL        VAR(&LIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&PRENAVN) TYPE(*CHAR) LEN(3)
             DCL        VAR(&DELSYS) TYPE(*CHAR) LEN(10)
             DCL        VAR(&DELSYSD) TYPE(*CHAR) LEN(70)

/* TEST BIBLIOTEK */
             CHKOBJ     OBJ(&LIB) OBJTYPE(*LIB)
             MONMSG     MSGID(CPF9801) EXEC(DO)
             SNDPGMMSG  MSG('BIBLIOTEK FINNES IKKE.')
             GOTO       CMDLBL(SLUTT)
             ENDDO

/* DELSYSTEM */
             CHGVAR     VAR(&DELSYS) VALUE('SYSBS_' *CAT &PRENAVN)
             CHGVAR     VAR(&DELSYSD) VALUE('Delsystem for ' *CAT &LIB)
             CRTSBSD    SBSD(&LIB/&DELSYS) POOLS((1 *BASE)) +
                          TEXT(&DELSYSD)
             MONMSG     MSGID(CPF0000) EXEC(GOTO CMDLBL(SLUTT))

/* KLASSE */
             CRTCLS     CLS(&LIB/SY400CLS) RUNPTY(21) DFTWAIT(120) +
                          TEXT('SYSPED400 class')
             MONMSG     MSGID(CPF0000)

/* AUTOSTART JOBB */
             ADDAJE     SBSD(&LIB/&DELSYS) JOB(SY400AUTO) +
                          JOBD(&LIB/SY400AUTJD)

/* AUTOSTART JOBB */
             ADDRTGE    SBSD(&LIB/&DELSYS) SEQNBR(9999) CMPVAL(*ANY) +
                          PGM(QSYS/QCMD) CLS(&LIB/SY400CLS)

/* JOBBKØ */
             CRTJOBQ    JOBQ(&LIB/SY400JOBQ)
             MONMSG     MSGID(CPF0000)
             CRTJOBQ    JOBQ(&LIB/SYJOBQNMAX)
             MONMSG     MSGID(CPF0000)
             CRTJOBQ    JOBQ(&LIB/SYJOBQPRT)
