             PGM
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1) /* Hvis &in01 +
                          på, betyr det avslutt program fra +
                          avdelingsvalg. */
             DCL        VAR(&PRT2) TYPE(*CHAR) LEN(10) /* Printernavn +
                          til merkelapp-1. */
             DCL        VAR(&PT2) TYPE(*CHAR) LEN(10) /* Papirtype +
                          til merkelapp-1. */
                                                          /**/
             CALL       PGM(SYEFPM) PARM(&IN01)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT)) /* +
                          Hvis &IN01 på, avslutter program. */
                                                          /**/
             RTVDTAARA  DTAARA(*LDA (601 10)) RTNVAR(&PRT2)
             RTVDTAARA  DTAARA(*LDA (611 10)) RTNVAR(&PT2)
             OVRPRTF    FILE(MERKL1) OUTQ(&PRT2) PAGESIZE(18 80) +
                          CPI(10) OVRFLW(18) FORMTYPE(&PT2)
             CALL       PGM(SYEF17)
                                                          /**/
SLUTT:       ENDPGM
