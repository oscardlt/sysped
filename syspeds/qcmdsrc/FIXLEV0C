             CMD        PROMPT('INAK-merke lev. m 0 i sal/resk')
             PARM       KWD(UPDAT) TYPE(*CHAR) LEN(1) RSTD(*NO) +
                          DFT(N) PROMPT('Oppdater?  J=Ja  N=Kun liste')
             PARM       KWD(DELNY) TYPE(*CHAR) LEN(1) RSTD(*NO) +
                          DFT(N) PROMPT('Tillat slett nye(1 år) N=Nye')
             PARM       KWD(PRINTER) TYPE(*CHAR) LEN(10) RSTD(*NO) +
                          DFT(*JOB) PROMPT('Printer')
