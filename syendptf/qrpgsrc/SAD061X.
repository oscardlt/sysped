     fsadwko    if a e           k disk
     ‚*  Prototype for sad061x
     d sad061x         pr
     d uavd_                               like(uavd)
     d utdn_                               like(utdn)
     ‚*  *ENTRY Interface for Main Procedure
     d sad061x         pi
     d uavd                           4  0
     d utdn                           7  0

      /free

       chain (uavd: utdn) sadhko;
       if not %found;
         chain (uavd: utdn) sadh;
         if %found;
           write sadhkor;
         endif;
       endif;

       chain (uavd: utdn) sadfko;
       if not %found;
         setll (uavd: utdn) sadf;
         reade (uavd: utdn) sadf;
         dow not %eof;
           write sadfkor;
           reade (uavd: utdn) sadf;
         enddo;
       endif;

       chain (uavd: utdn) sadvko;
       if not %found;
         setll (uavd: utdn) sadv;
         reade (uavd: utdn) sadv;
         dow not %eof;
           write sadvkor;
           reade (uavd: utdn) sadv;
         enddo;
       endif;

       chain (uavd: utdn) sadvjko;
       if not %found;
         setll (uavd: utdn) sadvj;
         reade (uavd: utdn) sadvj;
         dow not %eof;
           write sadvjkor;
           reade (uavd: utdn) sadvj;
         enddo;
       endif;

       chain (uavd: utdn) sadwko;
       if not %found;
         setll (uavd: utdn) sadw;
         reade (uavd: utdn) sadw;
         dow not %eof;
           write sadwkor;
           reade (uavd: utdn) sadw;
         enddo;
       endif;

       *inlr = *on;
       return;

      /end-free
