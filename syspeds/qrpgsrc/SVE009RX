      * RETTINGER I DETTE PROGRAM:
PTFnr:*Sek Sig Vers  Beskrivelse...........................
""""""*"""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""
10XXX * 01 SH  PTF10 FEIL VED UPDATE   ......
********************************************************************
     FSADH      IF   E           K DISK
     FSVTVK     IF   E           K DISK
     FSVEV      UF   E           K DISK
      *//////////////
      *  Hodelogikk /
      *//////////////
     C                   EXSR      SRA
     C                   EXSR      SRB
     C                   MOVE      '1'           *INLR
      *////////////////////////////////////////////////////////////
      *  Initiering                                           SRA /
      *////////////////////////////////////////////////////////////
     C     SRA           BEGSR
      *_________________
      * Input parametrer
      *"""""""""""""""""
     C     *ENTRY        PLIST
     C                   PARM                    UVEV_SYAV
     C                   PARM                    UVEV_SYOP
      *___________________
      * Definiere Workfelt
      *"""""""""""""""""""
     C     *LIKE         DEFINE    SVEV_SYAV     UVEV_SYAV
     C     *LIKE         DEFINE    SVEV_SYOP     UVEV_SYOP
     C     *LIKE         DEFINE    SVEV_SYAV     USVAVD
     C     *LIKE         DEFINE    SVEV_SYOP     USVTDN
     C     *LIKE         DEFINE    SIAVD         Z_SIAVD
     C     *LIKE         DEFINE    SITDN         Z_SITDN
     C     *LIKE         DEFINE    SVVK_KD       ZVVK_KD
     C     *LIKE         DEFINE    SVVK_DTS      ZVVK_DTS
     C                   EVAL      USVAVD=UVEV_SYAV
     C                   EVAL      USVTDN=UVEV_SYOP
     C                   EVAL      Z_SIAVD=UVEV_SYAV
     C                   EVAL      Z_SITDN=UVEV_SYOP
      *__________________
      * Definiere Nøkkler
      *""""""""""""""""""
     C     SVKEY         KLIST
     C                   KFLD                    USVAVD
     C                   KFLD                    USVTDN
     C     SADKEY        KLIST
     C                   KFLD                    Z_SIAVD
     C                   KFLD                    Z_SITDN
     C     SVTVKKEY      KLIST
     C                   KFLD                    ZVVK_KD
     C                   KFLD                    ZVVK_DTS
      *_________________________________
      * Hæmta dagens datum och klockslag
      *"""""""""""""""""""""""""""""""""
N01  C                   CALL      'SYGE15C'
N01  C                   PARM                    WDT               8
N01  C                   PARM                    WTM               6
     C                   MOVEL     WDT           ZVVK_DTS
     C                   ENDSR
      *////////////////////////////////////////////////////////////
      *  Bearbeide Formater                                   SRB /
      *////////////////////////////////////////////////////////////
     C     SRB           BEGSR
     C     SADKEY        CHAIN     SADH
      * Tell antal varelinjer
     C     SVKEY         SETLL     SVEV
     C     SVKEY         READE(N)  SVEV                                   53
     C     *IN53         DOWEQ     *OFF
     C                   ADD       1             ANTLIN            7 0
     C     SIVAL3        IFNE      *BLANKS
     C                   EVAL      ZVVK_KD=SIVAL3
     C     SVTVKKEY      SETGT     SVTVK
     C                   READP     SVTVK                                  64
     C     *IN64         IFEQ      *OFF
     C     SVEV_FABL     MULT      SVVK_KRS      SVEV_STVA
     C     SVEV_STVA     DIV       SVVK_OMR      SVEV_STVA
     C                   ENDIF
     C                   ENDIF
     C     SVKEY         READE     SVEV                                   53
     C                   ENDDO
      * Hent antall kolli fra SADH
     C     SADKEY        CHAIN     SADH
      * Oppdater første varelinje
     C     SINTK         SUB       ANTLIN        firslin           7 0
     C                   ADD       1             firslin
      * Oppdater første linje
     C     SVKEY         SETLL     SVEV
     C     SVKEY         READE     SVEV                                   53
N01  C     *IN53         IFEQ      '0'
     C                   EVAL      SVEV_KOTA=firslin
     C                   UPDATE    SVEVR
N01  C                   END
     C                   ENDSR
