     C*
     C                   OPEN      SAD052FM
     C                   EXSR      INIT
N20  C                   IF        WSTDN < 0
N20  C                   EXSR      OPNFIL
N35  C                   EVAL      WSTDN = WSTDN * -1
N35  C     SAEHK         CHAIN(N)  SAEH                               33
N35  C                   MOVE      SETLL         X_SETLL          10
N35  C                   EVAL      WSTDN = WSTDN * -1
N20  C     SAEHK         CHAIN(N)  SAEH                               33
N20  C                   SELECT
N20  C                   WHEN      SEST = 'Q'
N20  C     TVINMK        SETGT     EDIM1
N20  C     TVINMK        READPE    EDIM1                                  33
N20  C  N33MSR           IFEQ      'S'
N20  C     M1001         ANDEQ     '830'
N20  C                   DELETE    EDIMR
N20  C                   EVAL      SEST = ' '
N20  C                   GOTO      START0
N20  C                   END
N35  C                   CLOSE     SAD052FM
N20  C                   EVAL      *INLR = *ON
N20  C                   RETURN
N20  C                   WHEN      SEST <> ' ' AND SEST <> 'M'
N24  C                             AND SEST <> 'P'
N35  C                   CLOSE     SAD052FM
N20  C                   EVAL      *INLR = *ON
N20  C                   RETURN
N24  C                   WHEN      SEST = 'P' AND SEMF <> *BLANKS
N35  C                   IF        SETLL <> X_SETLL
N35  C                   CLOSE     SAD052FM
N24  C                   EVAL      *INLR = *ON
N24  C                   RETURN
N35  C                   END
N20  C                   ENDSL
N20  C     START0        TAG
N23  C                   EXSR      HENTSADDT
N20  C     SAEHK         CHAIN(N)  SAEHOM                             33
N20  C                   EXSR      MOVW2
N20  C                   GOTO      START2
N20  C                   END
