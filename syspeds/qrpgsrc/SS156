     C                   PARM                    UMM
     C                   PARM                    U≈≈
     C                   PARM                    UFLAGG            1 0
     C     UFLAGG        IFEQ      1
     C                   MOVEL     FEIL08        FEILMELDING
     C                   MOVE      '08'          FMELDNR
     C                   GOTO      UtTest
     C                   ENDIF
      *
     C     UTTEST        endsr
      *=====================================================================******
      *  OPPDATER DATABASE
      *=====================================================================******
     C     OPDDATA       begsr
     C     W2ONO         CHAIN     GSSORF                             33
     C                   IF        *IN33 = *OFF
     C                   EXSR      OPDLOGG1
     C                   EVAL      ORSNM  = WSSNM
     C                   EVAL      ORDTL  = W2DTL
     C                   EVAL      ORCLI  = W2CLI
     C                   EVAL      ORWT   = W2WT
     C                   EVAL      ORVOL  = W2VOL
     C                   MOVE      W2LM          ORLMA
     C                   EVAL      ORAREA = WSAREA
     C                   MOVEL     W2TYM         ORAAR
     C                   MOVE      W2TYM         ORUKE
     C                   EVAL      ORDTWL = W2DTWL
     C                   UPDATE    GSSORFR
      *
     C     W2ONO         CHAIN     GSSPOF                             33
     C                   IF        *IN33 = *OFF
     C                   EXSR      OPDLOGG2
     C                   EVAL      POCONO = WSCONO
     C                   UPDATE    GSSPOFR
     C                   END
      *
     C     W2ONO         SETLL     GSSRFF
     C     W2ONO         READE     GSSRFF                                 33
     C                   DOW       *IN33 = *OFF
     C                   IF        RFKD  = 'CPO'
     C                   EXSR      OPDLOGG3
     C                   EVAL      RFREF = WSACC
     C                   END
     C                   UPDATE    GSSRFFR
     C     W2ONO         READE     GSSRFF                                 33
     C  N33              ENDDO
      *
     C     W2ONO         SETLL     GSSRFF
     C     W2ONO         READE     GSSRFF                                 33
     C                   DOW       *IN33 = *OFF
     C                   IF        RFKD  = 'SAL'
     C                   EXSR      OPDLOGG4
     C                   EVAL      RFREF = WSSAL
     C                   END
     C                   UPDATE    GSSRFFR
     C     W2ONO         READE     GSSRFF                                 33
     C  N33              ENDDO
      *
     C                   END
