             PGM
             DCL        VAR(&IEJ) TYPE(*CHAR) LEN(2) VALUE('AI') /* +
                          AI = fly-innland.  AE = fly-eksport.  AJ = +
                          jetpakke. */
    /* */
    /* VEDLIKEHOLD AV AWBNR. FOR FLY-INNLAND */
             CALL       PGM(SYFA42) PARM(&IEJ)
    /* */
             ENDPGM
