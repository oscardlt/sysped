     C                   END
     C                   MOVE      TEGN          FE(NR)
     C                   ENDDO
     C                   MOVEL     FELT          SVAVTS
     C     TEGN          CABEQ     LOGAPP        SLSRTAXV
      * LES FORBI '+'
     C                   MOVE      *BLANKS       TEGN
     C                   DOW       TEGN <> LOGPLU
     C                   EXSR      GETCH
     C   LR              GOTO      ENDTAXV
     C                   ENDDO
      * LES GRUNNLAG
     C     1             DO        15            NR
     C                   EXSR      GETCH
     C   LR              GOTO      ENDTAXV
     C                   IF        TEGN = LOGPLU
     C                   LEAVE
     C                   END
     C                   MULT      10            SVAVTG
     C                   MOVE      TEGN          SVAVTG
     C                   ENDDO
     C     TEGN          CABEQ     LOGAPP        SLSRTAXV
      *
      * LES SATS
     C                   MOVE      *ZEROS        XSATS
     C     1             DO        15            NR
     C                   EXSR      GETCH
     C   LR              GOTO      ENDTAXV
     C                   IF        ((TEGN = LOGAPP) OR (TEGN = ','))
     C                   LEAVE
     C                   END
     C                   MULT      10            XSATSH
     C                   MOVE      TEGN          XSATSH
     C                   ENDDO
      *
     C                   IF        TEGN = ','
     C                   EXSR      GETCH
     C                   MOVE      TEGN          XSATSD1
     C                   EXSR      GETCH
     C                   IF        ((TEGN >= '0') AND (TEGN <= '9'))
     C                   MOVE      TEGN          XSATSD2
     C                   EXSR      GETCH
     C                   END
     C                   END
     C                   Z-ADD     XSATS         SVAVTP
      *
     C     SLSRTAXV      TAG
     C                   EXSR      GETSEG
     C*    SEGNAM        CABEQ     'TAX'         STSRTAXV
      *
     C     ENDTAXV       ENDSR
