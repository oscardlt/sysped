     C*** OPPDATERE PÅ REGISTER OG SUBFIL
     C     KODARK        CHAIN     KODTAR                             33
     C   33
     CAN 40
     CORN33
     CANN40              GOTO      SLTB
     C                   EXSR      MOVWAR
     C  N33              UPDATE    KODTARR
     C   33              WRITE     KODTARR
     C   40              UPDATE    SUBFIL
     C  N40              ADD       1             YRECNO
     C  N40              Z-ADD     YRECNO        ZRECNO
     C  N40              WRITE     SUBFIL
     C                   WRITE     SUBCTL
     C                   SETOFF                                       40
     C                   EXSR      MOVB2
     C*
     C     SLTB          ENDSR
     C*
     C***********************************************
     C     HENT          BEGSR
     C***********************************************
     C                   MOVEA     '00'          *IN(91)
     C     *IN98         DOWEQ     '0'
     C                   READC     SUBFIL                                 98
     C  N98WSNR          IFEQ      '2'
     C     KARRB1        COMP      '*DLT  '                               98
     C   98              GOTO      SLHENT
     C                   EXSR      MOVARW
     C                   MOVE      ' '           WSNR
     C                   SETON                                        40
     C                   GOTO      SLHENT
     C                   END
     C  N98WSNR          IFEQ      '4'
     C     KARRB1        COMP      '*DLT  '                               98
     C                   GOTO      SLHENT
     C                   END
     C                   END
     C*
     C     SLHENT        ENDSR
