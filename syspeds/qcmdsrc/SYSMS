             CMD        PROMPT('Send SMS via Clickatell.com')
             PARM       KWD(SMSNR) TYPE(*CHAR) LEN(200) MIN(1) +
                          PROMPT('Til nr (47 foran, komma v/fl):')
             PARM       KWD(SMSTXT) TYPE(*CHAR) LEN(160) MIN(1) +
                          PROMPT('SMS tekst:')
