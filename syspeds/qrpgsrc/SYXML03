      ***********************************************
     C     SRORVEKT      BEGSR
      ***********************************************
     C                   MOVE      *ZEROS        XORVEKT           9 0
     C     1             DO        10            XN2               3 0
     C                   SELECT
     C                   WHEN      FS10(XN2) = ' '
     C                   LEAVE
     C                   WHEN      ((FS10(XN2) >= '0') AND (FS10(XN2) <= '9'))
     C                   MULT      10            XORVEKT
     C                   MOVE      FS10(XN2)     XORVEKT
     C                   ENDSL
     C                   ENDDO
     C                   ADD       XORVEKT       XTOTORVEKT
      *
     C                   ENDSR
      *
      ***********************************************
     C     SRSUMGEB      BEGSR
      ***********************************************
     C     FRISOKK5      CHAIN     FRISOK                             33
     C                   IF        *IN33
     C                   Z-ADD     200000428     XKN               8 0
     C                   ELSE
     C                   MOVEL     FSSOK         XKN
     C                   END
      *
     C                   MOVE      *ZEROS        XANTGEB           3 0
     C     FAK2K         SETLL     FAK2
     C     FAK2K         READE     FAK2                                   33
     C                   DOW       *IN33 = *OFF
     C                   IF        (((FAKDA <> 'K') AND
     C                               (FASK <> 'I')) AND
     C                               (FABELV <> 0))
     C                   IF        ((FAKUNR <> XKN) AND (HEDTOP <= 20081023))
     C                   EVAL      XKN = FAKUNR
     C                   END
      *
     C                   IF        FAKUNR = XKN
     C                   ADD       1             XANTGEB
     C                   MOVE      FAVK          GEB1(XANTGEB)
     C                   MOVE      FAFAKT        FNUM(XANTGEB)
     C     KODTGEK       CHAIN     KODTGE                             33
     C                   EVAL      XAN41 = KGENOT
     C                   IF        FAVT <> *BLANKS
     C                   CAT       ',':0         XAN41            41
     C                   CAT       FAVT:0        XAN41
     C                   END
     C                   MOVE      XAN41         GEB2(XANTGEB)
     C                   MOVE      FABELV        GEB3(XANTGEB)
     C                   MOVE      FAVAL         GEB4(XANTGEB)
     C                   END
     C                   END
      *
     C     FAK2K         READE     FAK2                                   33
     C                   ENDDO
      *
     C                   ENDSR
      *
      ***********************************************
     C     OPDEDIM       BEGSR
      ***********************************************
     C     1             CHAIN     EDIC                               33
     C                   ADD       1             CMN
     C                   UPDATE    EDICR
      *
     C                   EVAL      M0004 = S0004
     C                   EVAL      M0010 = S0010
     C                   MOVEL     CMN           M0062
     C                   EVAL      M0065 = 'XMLINH'
     C                   MOVEL     SYSPEDREF     M0068
     C                   MOVEL     'KVI'         M1001
     C                   MOVEL     HERFA         M1004
     C                   MOVE      'KVI'         M1225
     C                   EVAL      MSN = SSN
     C                   EVAL      MMN = CMN
     C                   EVAL      MSR = 'S'
     C                   EVAL      MST = 'C'
     C                   MOVE      WDT           MDT
     C                   MOVE      WTM           MTM
     C                   EVAL      MAVD = HEAVD
     C                   EVAL      MTDN = HEOPD
     C                   WRITE     EDIMR
      *
     C                   ENDSR
      *
      ***********************************************
