     C     UTYPE         IFEQ      'CPY'
N47  C     UTYPE         OREQ      'OPG'
N47  C                   IF        UTYPE = 'OPG'
N47  C                   EVAL      WSDP = 30
N51  C                   EVAL      WSDP2 = 0
N47  C                   EVAL      WSTLE1 = WSTLE
N47  C                   EVAL      WSTLL1 = WSTLL
N47  C                   EVAL      WSDTG1 = WSDTG
N47  C                   EVAL      WSAVD_O = SEAVD_O
N47  C                   EVAL      WSTDN_O = SETDN_O
N47  C                   END
     C                   MOVE      *ZEROS        WSTDN
     C                   MOVE      *ZEROS        SETDN
     C                   MOVE      *BLANKS       WSDTG
     C                   MOVE      *BLANKS       WSTLL
     C                   MOVE      *BLANKS       WSTLE
     C                   MOVEL     WSFIF         WSFIF3            3
     C     WSFIF3        IFEQ      'F15'
     C                   MOVE      *BLANKS       WSFIF
     C                   END
     C                   END
N47   *
