/* TXTF(*NONE) -> TXTF(ARKSRC) TXTMBR(BLANKSRC) EDTMBR(*NO) */
             PGM        PARM(&NAVNF &EPOSTF &NAVNT &EPOSTT &CVP)
             DCL        VAR(&LIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&CVP) TYPE(*CHAR) LEN(70)
             DCL        VAR(&NAVNF) TYPE(*CHAR) LEN(3)
             DCL        VAR(&EPOSTF) TYPE(*CHAR) LEN(70)
             DCL        VAR(&NAVNT) TYPE(*CHAR) LEN(3)
             DCL        VAR(&EPOSTT) TYPE(*CHAR) LEN(70)

             RTVDTAARA  DTAARA(FIRMDT (141 10)) RTNVAR(&LIB)
             ADDPFM     FILE(ARKSRC) MBR(BLANKSRC)
             MONMSG     MSGID(CPF0000)
             ADDLIBLE   LIB(MMAIL)
             MONMSG     MSGID(CPF0000)
             EMLMSG     SUBJECT(&CVP) FROMNAME(&NAVNF) +
                          FROMADDR(&EPOSTF) TO(&EPOSTT/&NAVNT/*TO) +
                          TXTF(&LIB/ARKSRC) TXTMBR(BLANKSRC) +
                          EDTMBR(*NO)
             RMVLIBLE   LIB(MMAIL)
             MONMSG     MSGID(CPF0000)
             CALL       PGM(ARC006E) PARM(&NAVNF &EPOSTF &NAVNT &EPOSTT &CVP)
             ENDPGM
