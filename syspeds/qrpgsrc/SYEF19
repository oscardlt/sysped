     D  ST03                   3      3
     D  ST45                   4      5
     C*
     C     START         TAG
     c                   exfmt     BILDE01
     c     *in03         ifeq      '1'
     c                   seton                                        lr
     c                   return
     c                   end
     C     WSLAND        IFNE      *BLANKS
     C                   EXSR      OPPDAT
     C                   END
     C                   MOVE      *BLANKS       WSLAND
     C                   MOVE      *BLANKS       WSSONE
     C                   GOTO      START
     C*
     C***********************************************
     C     OPPDAT        BEGSR
     C***********************************************
     C                   SETOFF                                       33
     C     WSLAND        SETLL     STED2D
     C     *IN33         DOWEQ     '0'
     C     WSLAND        READE     STED2D                                 33
     C  N33ST01          IFGE      'A'
     C     ST01          ANDLE     'Z'
     C     ST02          ANDGE     'A'
     C     ST02          ANDLE     'Z'
     C     ST03          ANDGE     'A'
     C     ST03          ANDLE     'Z'
