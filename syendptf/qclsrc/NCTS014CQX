             PGM
             DCL        VAR(&PRTTYP) TYPE(*CHAR) LEN(1) VALUE(' ')
             DCL        VAR(&AVD) TYPE(*CHAR) LEN(4) VALUE('0001')
             DCL        VAR(&TDN) TYPE(*CHAR) LEN(7) VALUE('0500117')

             CALL       PGM(NCTS014R) PARM(&PRTTYP &AVD &TDN)
             ENDPGM
