             PGM
             DCL        VAR(&VAR1) TYPE(*DEC) LEN(10 0)
             DCL        VAR(&VAR2) TYPE(*DEC) LEN(10 0)
             DCL        VAR(&VAR11) TYPE(*CHAR) LEN(10)
             DCL        VAR(&VAR21) TYPE(*CHAR) LEN(10)
             RTVMBRD    FILE(FAKT) DTASPCSIZ(&VAR1) ACCPTHSIZ(&VAR2)
             CHGVAR     VAR(&VAR11) VALUE(&VAR1)
             CHGVAR     VAR(&VAR21) VALUE(&VAR2)
             CHGDTAARA  DTAARA(*LDA (1 10)) VALUE(&VAR11)
             CHGDTAARA  DTAARA(*LDA (20 10)) VALUE(&VAR21)
             DSPDTAARA  DTAARA(*LDA)
             ENDPGM
