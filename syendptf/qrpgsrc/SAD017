     C* SR for å behandle DMS-segmentet                   SRDMSH
      *""""""""""""""""""""""""""""""""""""""""""""""""""""""""""
     C     SRDMSH        BEGSR
      *
     C                   EVAL      SFAVD  = SEAVD
     C                   EVAL      SFOPDN = SETDN
     C                   MOVE      *BLANKS       SEFIF
     C                   MOVE      *ZEROS        SEFID
     C     STSRDMSH      TAG
      * LES FAKTURANR
     C                   MOVE      *BLANKS       FE
     C     1             DO        35            NR
     C                   EXSR      GETCH
     C   LR              GOTO      ENDDMSH
     C                   IF        TEGN = LOGPLU
     C                   LEAVE
     C                   END
     C                   MOVE      TEGN          FE(NR)
     C                   ENDDO
      *
     C                   SELECT
     C                   WHEN      SEFID = 0
     C                   MOVEL     FELT          SEFIF
     C                   WHEN      SEFID = 99999999
     C                   MOVEL     FELT          SFTXT
     C                   OTHER
     C                   MOVEL     SEFIF         SFTXT
     C                   MOVEL     SEFID         SFDT
     C                   WRITE     SAEFEDIR
     C                   MOVEL     FELT          SFTXT
     C                   EVAL      SEFID = 99999999
     C                   EVAL      SEFIF = 'F15  ER  BENYTTET'
     C                   ENDSL
      * LES FORBI ':'
     C                   MOVE      *BLANKS       TEGN
     C                   DOW       TEGN <> LOGKOL
     C                   EXSR      GETCH
     C   LR              GOTO      ENDDMSH
     C                   ENDDO
      * LES DATO
     C                   MOVE      *BLANKS       XN08              8 0
     C     1             DO        10            NR
     C                   EXSR      GETCH
     C   LR              GOTO      ENDDMSH
     C                   IF        (((TEGN = LOGAPP) OR
     C                               (TEGN = LOGPLU)) OR
     C                               (TEGN = LOGKOL))
     C                   LEAVE
     C                   END
     C                   MULT      10            XN08
     C                   MOVE      TEGN          XN08
     C                   ENDDO
      *
     C                   IF        SEFID = 0
     C                   MOVE      XN08          SEFID
     C                   ELSE
     C                   MOVEL     XN08          SFDT
     C                   WRITE     SAEFEDIR
     C                   END
      *
     C                   EXSR      GETSEG
     C     SEGNAM        CABEQ     'DMS'         STSRDMSH
      *
     C     ENDDMSH       ENDSR
      *
