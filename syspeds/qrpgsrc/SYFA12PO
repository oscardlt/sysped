N01   * +
N01  c     '+'           scan      D29TXT        X                 2 0    33
N01  c     *in33         doweq     '1'
N01  C                   SUB       1             FTXLEN
N01  C                   add       1             X
N01  c     '+'           scan      D29TXT:X      X                        33
N01  c                   enddo
N01   * :
N01  c     ':'           scan      D29TXT        X                 2 0    33
N01  c     *in33         doweq     '1'
N01  C                   SUB       1             FTXLEN
N01  C                   add       1             X
N01  c     ':'           scan      D29TXT:X      X                        33
N01  c                   enddo
N01   * ?
N01  c     '?'           scan      D29TXT        X                 2 0    33
N01  c     *in33         doweq     '1'
N01  C                   SUB       1             FTXLEN
N01  C                   add       1             X
N01  c     '?'           scan      D29TXT:X      X                        33
N01  c                   enddo
N01   * ' (appostrof)
N01  c     Ap            scan      D29TXT        X                 2 0    33
N01  c     *in33         doweq     '1'
N01  C                   SUB       1             FTXLEN
N01  C                   add       1             X
N01  c     Ap            scan      D29TXT:X      X                        33
N01  c                   enddo
N01  C                   ENDSR
