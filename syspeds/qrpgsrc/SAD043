     C                   EXFMT     BILDE02
      *
     C   05              DO
     C     START2        TAG
     C                   TIME                    XTIME
     C                   MOVE      XAAR          SSDTÅ
     C                   MOVE      XMND          SSDTM
     C                   MOVE      XDAG          SSDTD
     C                   MOVE      XTID          SSTM
     C     STSK          CHAIN     STS                                99
     C  N99              GOTO      START2
     C                   MOVE      WSAVD         SSAVD
     C                   MOVE      WSOPD         SSOPD
     C                   MOVE      WSUSER        SSUSR
     C                   MOVE      WSSG          SSSG
     C                   MOVE      'N'           SSRTYP
     C                   MOVE      SEST          SSSTG
     C                   MOVE      WSST          SSSTN
     C                   WRITE     STSR
      *
     C   05              MOVE      WSST          SEST
     C                   UPDATE    SADTRHR
     C                   MOVE      *ZEROS        WSOPD
     C                   MOVE      *BLANKS       WSST
     C                   SETOFF                                       99
     C                   END
     C                   GOTO      START
