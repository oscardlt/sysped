     C     STSMAX2       TAG
     C     AF(NR)        IFEQ      ' '
     C                   MOVE      '9'           AF(NR)
     C                   SUB       1             NR
     C     NR            ifne      0
     C                   GOTO      STSMAX2
     C                   END
     C                   END
     C                   END
     C                   MOVE      Wsfor2        Wsfor2k          14
     C                   ENDSR
     C*
     C***********************************************
     C     CUNDCA        BEGSR
     C***********************************************
      * ADD POST I CUNDC HVIS MANGLER
     C     KUNKEY        SETLL     CUNDC01                                51
     C     *IN51         IFEQ      *OFF
     C                   MOVEL     FIRMA         CFIRMA
     C                   Z-ADD     KUNDNR        CCOMPN
     C                   MOVEL     KPERS         CCONTA
     C     TFAXNR        IFNE      *BLANKS
     C                   MOVEL     TFAXNR        CFAX
     C                   ELSE
     C                   MOVEL     TLF           CFAX
     C                   ENDIF
     C                   WRITE     CUNDCR
     C                   ENDIF
     C                   ENDSR
