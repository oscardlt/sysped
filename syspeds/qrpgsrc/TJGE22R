     C     DOKUFMK       KLIST
     C                   KFLD                    FVRI
     C                   KFLD                    FVAVD
     C                   KFLD                    FVOPD
     C                   KFLD                    FVFBNR
     C                   KFLD                    FVLINR
N01  C                   KFLD                    FMMKNR
N01  C                   KFLD                    FMLONR
N01  C                   EVAL      FMMKNR=1
N01  C                   EVAL      FMLONR=1
     C     DOKUFVFK      KLIST
     C                   KFLD                    FVAVD
     C                   KFLD                    FVOPD
     C                   KFLD                    FVFBNR
     C                   KFLD                    FVLINR
N01  C                   KFLD                    FFLIN2
     C     *LIKE         DEFINE    FVRI          WFVRI
     C                   EVAL      WFVRI='F'
N01  C                   EVAL      FFLIN2=1

     C   50              EVAL      Error = 'Invalid userID'
