             PGM
             DCL        VAR(&AVD) TYPE(*CHAR) LEN(4) VALUE('0001')
             DCL        VAR(&OPD) TYPE(*CHAR) LEN(7) VALUE('0155470')
             DCL        VAR(&LOP) TYPE(*CHAR) LEN(3) VALUE('001')
             DCL        VAR(&SAML) TYPE(*CHAR) LEN(6) VALUE('LON009')
    /* */
    /* SKRIVER UT LABEL  FLYFRAKTBREV     */
    /* */
             OVRPRTF    FILE(SYEF14MP) DEVTYPE(*AFPDS) PAGESIZE(70 +
                          80) CPI(10) OVRFLW(70) OUTQ(PDFDIR)

    /*                                    */
             CALL       PGM(SYEF14M) PARM(&AVD &OPD &LOP &SAML)
             ENDPGM
