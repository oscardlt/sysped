          +'¬dkse_442c¬ : ¬'+%trim(KC)+'¬, '
          +'¬dkse_4424¬ : ¬'+%trim(K4)+'¬, '
          +'¬dkse_442d¬ : ¬'+%trim(KD)+'¬, '
          +'¬dkse_4425¬ : ¬'+%trim(K5)+'¬, '
          +'¬dkse_442e¬ : ¬'+%trim(KE)+'¬, '
          +'¬dkse_4426¬ : ¬'+%trim(K6)+'¬, '
          +'¬dkse_442f¬ : ¬'+%trim(KF)+'¬, '
          +'¬dkse_4427¬ : ¬'+%trim(K7)+'¬, '
          +'¬dkse_442g¬ : ¬'+%trim(KG)+'¬, '
          +'¬dkse_4428¬ : ¬'+%trim(K8)+'¬, '
          +'¬dkse_442h¬ : ¬'+%trim(KH)+'¬, '
          +'¬dkse_4429¬ : ¬'+%trim(K9)+'¬, '
          +'¬dkse_442i¬ : ¬'+%trim(KI)+'¬}';
