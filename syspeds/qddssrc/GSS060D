     A                                      DSPSIZ(24 80 *DS3)
     A                                      PRINT
     A                                      ALTHELP
     A                                      HELP
     A          R GSS060DA
     A                                      CA03(03 'Avbryt')
     A                                  1  2'SHIPSYS/GSS060D'
     A                                      COLOR(BLU)
     A                                  1 21'Change status on trip or order'
     A                                      COLOR(WHT)
     A                                  1 62DATE
     A                                      EDTCDE(Y)
     A                                  1 72TIME
     A                                  5  2'Trip No.  . . . . . .:'
     A            WSTNO         11Y 0B  5 26DSPATR(UL)
     A  99                                  DSPATR(PC)
     A  99                                  DSPATR(HI)
     A                                      EDTCDE(4)
     A                                  6  4'New status for trip:'
     A            WSSTT          1A  O  6 26DSPATR(UL)
     A                                      COLOR(WHT)
     A                                  8  2'Order No. . . . . . .:'
     A            WSONO          7Y 0B  8 26DSPATR(UL)
     A  98                                  DSPATR(PC)
     A  98                                  DSPATR(HI)
     A                                      EDTCDE(4)
     A                                  9  4'New status for ord.:'
     A            WSSTO          1A  O  9 26DSPATR(UL)
     A                                      COLOR(WHT)
     A                                 23  2'F3=End'
     A                                      COLOR(BLU)
     A  30        MSGTXT        78A  O 24  2MSGID(&MSGNR SYMSGF)
     A                                      COLOR(WHT)
     A            MSGNR          7A  P
