             PGM        PARM(&SSN &LIB &FILE &MBR &NETW &IID &IN30)
             DCL        VAR(&SSN) TYPE(*DEC) LEN(9 0)
             DCL        VAR(&LIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&FILE) TYPE(*CHAR) LEN(10)
             DCL        VAR(&MBR) TYPE(*CHAR) LEN(10)
             DCL        VAR(&IN30) TYPE(*LGL) LEN(1)
             DCL        VAR(&IID) TYPE(*CHAR) LEN(35)
             DCL        VAR(&NETW) TYPE(*CHAR) LEN(1)
             CHGVAR     VAR(&IN30) VALUE('0')
             CHKOBJ     OBJ(&LIB/&FILE) OBJTYPE(*FILE) MBR(&MBR)
             MONMSG     MSGID(CPF9810 CPF9801 CPF9815) EXEC(CHGVAR +
                          VAR(&IN30) VALUE('1'))
             ENDPGM
