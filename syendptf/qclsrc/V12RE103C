             PGM        PARM(&FRALIB &TILLIB)
             DCL        VAR(&FRALIB) TYPE(*CHAR) LEN(10) /* Fra +
                          bibliotek V11 */
             DCL        VAR(&TILLIB) TYPE(*CHAR) LEN(10) /* Til +
                          bibliotek for datafiler V12 */

             CLRPFM     FILE(&TILLIB/SADV)
             MONMSG     MSGID(CPF0000)
             CLRPFM     FILE(&TILLIB/V12RE103F)
             MONMSG     MSGID(CPF0000)
             OVRDBF     FILE(SADV4) TOFILE(&FRALIB/SADV4)
             OVRDBF     FILE(SADV) TOFILE(&TILLIB/SADV)
             CALL       PGM(V12RE103)
             DLTOVR     FILE(SADV)
             DLTOVR     FILE(SADV4)

             OVRDBF     FILE(ZADVV11) TOFILE(&FRALIB/ZADV)
             OVRDBF     FILE(ZADV) TOFILE(&TILLIB/ZADV)
             CALL       PGM(V12RE103Z)
             DLTOVR     FILE(ZADV)
             DLTOVR     FILE(ZADVV11)

             CALL       PGM(V12RE104C) PARM(&FRALIB &TILLIB)

 /*          CALL       PGM(V12RE107C) PARM(&FRALIB &TILLIB) */

             ENDPGM
