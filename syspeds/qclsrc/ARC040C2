             PGM        PARM(&FLD &FRAPER &TILPER)
             DCL        VAR(&FLD) TYPE(*CHAR) LEN(40)
             DCL        VAR(&FROMPF) TYPE(*CHAR) LEN(50)
             DCL        VAR(&FRAPER) TYPE(*CHAR) LEN(8)
             DCL        VAR(&TILPER) TYPE(*CHAR) LEN(8)
/* LAGER FILEN Q  */
             DLTF       FILE(QTEMP/QL1)
             MONMSG     MSGID(CPF0000)
             CRTPF      FILE(QTEMP/QL1) RCDLEN(199) SIZE(10000000)
/* KOPIERER FØRST TIL  Q  */
             CPYF       FROMFILE(IBMENDA/SYARKIVF) TOFILE(QTEMP/QL1) +
                          MBROPT(*ADD) INCREL((*IF SYDATO *GE +
                          &FRAPER) (*AND SYDATO *LE &TILPER)) +
                          FMTOPT(*NOCHK)
/* DEFINERE  Q  */
             CHGVAR     VAR(&FROMPF) +
                          VALUE('/QSYS.LIB/QTEMP.LIB/QL1.FILE/QL1+
                          .MBR')
/* KOPIERER FRA Q TIL STREAMFIL */
             CPYTOSTMF  FROMMBR(&FROMPF) TOSTMF(&FLD) STMFOPT(*ADD) +
                          STMFCODPAG(*PCASCII)
             ENDPGM
