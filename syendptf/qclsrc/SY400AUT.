             MONMSG     MSGID(CPF0000)

/*N11*/      CRTDTAQ    DTAQ(&FILIBF/EPOSTDIR) MAXLEN(132) +
                          TEXT('EPOSTDRIVER') AUT(*ALL) /*N15*/
/*N11*/      MONMSG     MSGID(CPF0000)

             CRTDTAQ    DTAQ(&FILIBF/SAD41PQ) MAXLEN(30) +
