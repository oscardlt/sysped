             PGM
             DCL        VAR(&USRPRF) TYPE(*CHAR) LEN(10)
             DCL        VAR(&JOBNM) TYPE(*CHAR) LEN(10)
             RTVUSRPRF  RTNUSRPRF(&USRPRF)
             CHGDTAARA  DTAARA(*LDA (101 10)) VALUE(&USRPRF)
             RTVJOBA    JOB(&JOBNM)
             RMVM       FILE(GSS022W) MBR(&JOBNM)
             MONMSG     MSGID(CPF0000)
             ADDPFM     FILE(GSS022W) MBR(&JOBNM)
             OVRDBF     FILE(GSS022W) TOFILE(GSS022W) MBR(&JOBNM)
             RMVM       FILE(GSS031W) MBR(&JOBNM)
             MONMSG     MSGID(CPF0000)
             ADDPFM     FILE(GSS031W) MBR(&JOBNM)
             OVRDBF     FILE(GSS031W) TOFILE(GSS031W) MBR(&JOBNM)
             CALL       PGM(GSS030R)
             RCLRSC
             ENDPGM
