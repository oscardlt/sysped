PGM
             DCL        VAR(&GMLLDA) TYPE(*CHAR) LEN(1024)
             DCL        VAR(&SYENDRE) TYPE(*CHAR) LEN(10)

 /*  TA VARE PÅ GAMMEL LDA */
             CHGVAR     VAR(&GMLLDA) VALUE(%SST(*LDA 1 1024))
             CHGDTAARA  DTAARA(*LDA *ALL) VALUE(' ')

 /*  FINN MILJØ UTFRA LIBL */
  CHGVAR     VAR(&SYENDRE) VALUE(SYENDRECE)
  RMVLIBLE   LIB(&SYENDRE)
  MONMSG     MSGID(CPF2104) EXEC(DO)
     CHGVAR     VAR(&SYENDRE) VALUE(SYENDRESL)
     RMVLIBLE   LIB(&SYENDRE)
     MONMSG     MSGID(CPF2104) EXEC(DO)
        CHGVAR     VAR(&SYENDRE) VALUE(SYENDRE)
        RMVLIBLE   LIB(&SYENDRE)
        MONMSG     MSGID(CPF2104) EXEC(DO)
             /*MISS PÅ ALLE= KOM IKKE FRA SYSPED I DET HELE TATT*/
             CHGVAR     VAR(&SYENDRE) VALUE('          ')
        ENDDO
     ENDDO
  ENDDO

 /* KJØR L1 */
             CALL       PGM(L1STD/MC030)

 /* DERSOM EN KOM FRA SYSPED  RETURNER TIL RETT MILJØ */
             IF         COND(&SYENDRE *NE ' ') THEN(CALL +
                          PGM(&SYENDRE/INLIBL))

 /*  LEGG TILBAKE GAMMEL LDA */
             CHGDTAARA  DTAARA(*LDA (1 1024)) VALUE(&GMLLDA)

ENDPGM
