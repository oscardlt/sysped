             PGM        PARM(&AAR)
             DCL        VAR(&AAR) TYPE(*CHAR) LEN(4)
             DCL        VAR(&BATCH) TYPE(*CHAR) LEN(1) VALUE('J')
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             SBMJOB     CMD(CALL PGM(ARCHST01) PARM(&BATCH &AAR)) +
                          JOBD(SY400JOBD) JOBQ(QCTL)
             DMPCLPGM
             ENDPGM
