             PGM        PARM(&FAKO &WS)
             DCL        VAR(&FAKO) TYPE(*CHAR) LEN(1)
             DCL        VAR(&WS) TYPE(*CHAR) LEN(10)
/* OVERRIDER UTGÅENDE                                    */
             IF         COND(&FAKO = 'F') THEN(DO)
             OVRDBF     FILE(VISTRAFAK) MBR(&WS)
             ENDDO
/*********************************************************/
/* KOSTNADSFØRING KAN OGSÅ VÆRE DIREKTE HOVEDBOK         */
/*********************************************************/
/* OVERRIDER INNG/HOVEDBOK                               */
             IF         COND(&FAKO = 'K') THEN(DO)
             OVRDBF     FILE(VISTRAKOS) MBR(&WS)
             OVRDBF     FILE(VISTRAHOV) MBR(&WS)
             ENDDO
/* OVRRIDE HOVEDBOK                                      */
             IF         COND(&FAKO = 'H') THEN(DO)
             OVRDBF     FILE(VISTRAHOV) MBR(&WS)
             ENDDO
/* KALLER BÅDE MED FAKO OG H                             */
             CALL       PGM(VISM35) PARM(&FAKO)
             IF         COND(&FAKO *NE 'H') THEN(DO)
             CALL       PGM(VISM35) PARM('H')
             ENDDO
             IF         COND(&FAKO = 'F') THEN(DO)
             CPYF       FROMFILE(VISTRANSK) TOFILE(VISTRANSKB) +
                          MBROPT(*REPLACE) FMTOPT(*MAP *DROP)
             CALL       PGM(VISUTGFACL)
             ENDDO
             IF         COND(&FAKO = 'K') THEN(DO)
             CPYF       FROMFILE(VISTRANSL) TOFILE(VISTRANSLB) +
                          MBROPT(*REPLACE) FMTOPT(*MAP *DROP)
             CALL       PGM(VISINNGACL)
             ENDDO
/* HOVEDBOK KJØRES UANSETT */
             CPYF       FROMFILE(VISTRANSH) TOFILE(VISTRANSHB) +
                          MBROPT(*REPLACE) FMTOPT(*MAP *DROP)
             MONMSG     MSGID(CPF0000)
             CALL       PGM(VISHOVEDCL)
             ENDPGM
