             PGM        PARM(&FIRMA &GRUPP &KJEDE &DT &PRT &AG &EIER)
             DCL        VAR(&FIRMA) TYPE(*CHAR) LEN(2)
             DCL        VAR(&GRUPP) TYPE(*CHAR) LEN(8)
             DCL        VAR(&KJEDE) TYPE(*CHAR) LEN(8)
             DCL        VAR(&DT) TYPE(*CHAR) LEN(8)
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10)
             DCL        VAR(&AG) TYPE(*CHAR) LEN(25)
             DCL        VAR(&EIER) TYPE(*CHAR) LEN(5)

             OVRPRTF    FILE(LL044P) PAGESIZE(72 80) OVRFLW(64) +
                          OUTQ(&PRT)

             CALL       PGM(LL044R) PARM(&FIRMA &GRUPP &KJEDE &DT +
                          &AG &EIER)

             ENDPGM
