/* FROM "A BETTER DEBUGGER," NEWS 3X/400, JANUARY 1993  */
/* KOMMANDOEN MÅ KREERES MED FØLGENDE KOMMANDO: */
/*                                              */
/* CRTCMD CMD(STRSRCDBG) PGM(SRCDBGC01) MODE(*PROD)   */
/*                                                      */
             CMD     PROMPT('Start Source Debug')

             PARM    KWD(PGM)      TYPE(QUAL1)  MIN(1)          +
                     PROMPT('Program')

             PARM    KWD(FILE)     TYPE(QUAL2)                  +
                     PROMPT('Source File')

             PARM    KWD(FIRSTBKP) TYPE(*CHAR)  LEN(10)         +
                     DFT(*FIRST)   SPCVAL((*FIRST '/1'))        +
                     EXPR(*YES)    PROMPT('First Breakpoint')

 QUAL1:      QUAL       TYPE(*NAME) LEN(10)
             QUAL       TYPE(*NAME) LEN(10) DFT(*LIBL)          +
                        SPCVAL((*LIBL))     EXPR(*YES)          +
                        PROMPT('Library')

 QUAL2:      QUAL       TYPE(*NAME) LEN(10) DFT(*PGM)           +
                        SPCVAL((*PGM))      EXPR(*YES)

             QUAL       TYPE(*NAME) LEN(10) DFT(*LIBL)          +
                        SPCVAL((*LIBL)  (*CURLIB)) EXPR(*YES)   +
                        PROMPT('Library')
             QUAL       TYPE(*NAME) LEN(10) DFT(*PGM)           +
                        SPCVAL((*PGM))             EXPR(*YES)   +
                        PROMPT('Member')
