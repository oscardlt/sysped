     C                   EXSR      INIT
      *
     C                   EXFMT     SVX033D1
      *
     C                   IF        *IN12 = *OFF
     C     UGNR          CHAIN     SVXGH                              33
     C                   MOVE      'Q'           TGST
     C                   UPDATE    SVXGHR
     C                   EXSR      KOMLIN
     C                   END
      *
     C                   MOVE      *ON           *INLR
     C                   RETURN
