     C     START2        TAG
      *
     C  N94              DO
E02  C                   DO        18
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
     C     ST21          TAG
     C     WSKUNR        READE     SADL                                   94
     C  N94              UPDATE    SADLR
     C  N94              MOVE      SLALFA        YALFA
     C  N94              END
     C                   END
      *
     C                   Z-ADD     ZRECNO        YRECNO            5 0
     C     START3        TAG
     C                   SETOFF                                       98
     C                   MOVEA     '11'          *IN(91)
     C     ZRECNO        IFEQ      0
     C                   WRITE     NYVARE
     C                   GOTO      ST41
     C                   ELSE
     C                   END
     C                   WRITE     SUBCTL
      *
      *** AVSLUTT
     C     ST41          TAG
     C                   EXFMT     BUNN
     C                   SETOFF                                       9830
     C   03              GOTO      SLUTT
      *** FRISK OPP
     C   05              GOTO      START
      *** LAGE NY VARE
     C   06              GOTO      START5
      *** SØK FRA VARE
     C   08              GOTO      START6
      *** TIL BILDE SOM HENTER KUNDENR
     C   12              GOTO      ST
      *
      *** BLA VIDERE
     C   25              DO
     C                   Z-ADD     YRECNO        ZRECNO
     C                   SETOFF                                       25
     C  N94SADLK3        SETLL     SADL
     C  N94WSKUNR        READE     SADL                                   94
     C                   GOTO      START2
     C                   END
      *
     C     ZRECNO        IFEQ      0
     C                   GOTO      START3
     C                   ELSE
     C                   EXSR      HENT
     C   98              GOTO      START3
     C                   END
      *
      *** SLETT VARE
     C     WSNR          IFEQ      '4'
     C                   MOVE      ' '           WSNR
     C                   EXFMT     VAREL3
     C  N12              DO
     C     SADLK2        CHAIN     SADL                               33
     C  N33              DELETE    SADLR
     C                   EXSR      MOVB1
     C                   MOVEL     '*DLT'        SLALFA
     C                   END
     C                   UPDATE    SUBFIL
     C                   GOTO      START3
     C                   END
      *
     C*** LAGE/ENDRE VARE
     C     START5        TAG
     C                   EXFMT     VARELI
     C                   MOVEA     '0000'        *IN(81)
     C                   MOVEA     '000'         *IN(87)
     C                   SETOFF                                       30
     C   03              GOTO      SLUTT
     C   12              DO
     C   40              UPDATE    SUBFIL
     C                   SETOFF                                       40
     C                   EXSR      MOVB2
     C                   GOTO      START3
     C                   END
     C                   EXSR      TESTB1
     C   30              GOTO      START5
     C                   GOTO      START3
