     FKODTS8    IF   E           K DISK
     FKODTS8LF  IF   E           K DISK
     F                                     RENAME(KOS8R:KOS8RL)
     FSAD061FO  CF   E             WORKSTN
     F                                     SFILE(SUBFIL:ZRECNO)
     D                 DS
     D  KS8FTX                 1     52
     D  WS8FTX                 1     20
     D                 DS
     D  W2FTX                  1     52
     D  W2SØK                  1     20
     C*
     C                   EXSR      INIT
     C*
     C     START         TAG
     C                   MOVE      '1'           *IN93
     C                   WRITE     SUBCTL
     C                   MOVEA     '00'          *IN(93)
     C                   Z-ADD     *ZEROS        ZRECNO
      *_____________
      * Fyll Subfile
      *"""""""""""""
     C  N27KODS8K        SETLL     KODTS8
     C   27KOS8LK        SETLL     KODTS8LF
     C     START1        TAG
     C  N27              READ      KODTS8                                 94
     C   27              READ      KODTS8LF                               94
N02  C  N94              SELECT
N02  C     UMOD          WHENEQ    'A'
N02  C     KS8SKV        ANDNE     *BLANKS
N02  C                   GOTO      START1
N02  C     UMOD          WHENEQ    'S'
N02  C     KS8SKV        ANDEQ     *BLANKS
N02  C                   GOTO      START1
N02  C                   ENDSL
      *
     C     START2        TAG
      *
     C  N94              DO
     C                   DO        5
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
N02  C     ST21          TAG
     C  N27              READ      KODTS8                                 94
     C   27              READ      KODTS8LF                               94
N02  C  N94              SELECT
N02  C     UMOD          WHENEQ    'A'
N02  C     KS8SKV        ANDNE     *BLANKS
N02  C                   GOTO      ST21
N02  C     UMOD          WHENEQ    'S'
N02  C     KS8SKV        ANDEQ     *BLANKS
N02  C                   GOTO      ST21
N02  C                   ENDSL
     C  N94              END
     C                   END
     C*
     C                   Z-ADD     ZRECNO        YRECNO            4 0
     C     *IN29         IFEQ      '0'
     C                   Z-ADD     4             LINNBR
     C                   Z-ADD     12            POSNBR
     C                   MOVE      '1'           *IN29
     C                   ELSE
     C                   Z-ADD     5             LINNBR
     C                   Z-ADD     2             POSNBR
     C                   END
     C     START3        TAG
     C                   MOVEA     '11'          *IN(91)
     C     ZRECNO        IFEQ      0
     C                   EXFMT     NYWRK
     C                   ELSE
     C                   WRITE     BUNN
     C                   EXFMT     SUBCTL
     C                   END
     C*
     C                   SETOFF                                       9830
     C   03
     COR 12              GOTO      SLUTT
     C*** FRISK OPP
     C   05              GOTO      START
     C*** LAGE NY
     C   06              DO
     C                   CALL      'SAD002'
     C                   GOTO      START
     C                   END
N02   *** ENDRE MODULS
N02  C   07              DO
N02   * HENT AVGIFTSKODE FRA SKJERMENBILDE
N02  C     W2KOD         IFEQ      *BLANKS
N02  C     ZRECNO        IFEQ      0
N02  C                   MOVE      UKOD          W2KOD
N02  C                   ELSE
N02  C                   ADD       4             ZRECNO
N02  C                   DIV       5             ZRECNO
N02  C                   MULT      5             ZRECNO
N02  C                   SUB       4             ZRECNO
N02  C     ZRECNO        CHAIN     SUBFIL                             33
N02  C  N94              MOVE      KS8AVG        W2KOD
N02  C                   END
N02  C                   END
N02  C     UMOD          IFEQ      'A'
N02  C                   MOVE      'S'           UMOD
N02  C                   ELSE
N02  C                   MOVE      'A'           UMOD
N02  C                   END
N02  C                   GOTO      START
N02  C                   END
     C*
     C*** BLA VIDERE
     C   25              DO
     C                   Z-ADD     YRECNO        ZRECNO
     C                   SETOFF                                       25
     C                   GOTO      START2
     C                   END
      *
     C     W2NR          IFEQ      '1'
     C     W2KOD         ANDNE     *BLANKS
     C                   MOVE      W2KOD         UKOD
N02  C                   MOVE      W2KOD2        UKOD2
     C                   GOTO      SLUTT
     C                   END
     C     W2KOD         IFNE      *BLANKS
     C                   MOVE      '0'           *IN27
     C                   GOTO      START
     C                   END
     C     W2SØK         IFNE      *BLANKS
     C                   MOVE      '1'           *IN27
     C                   GOTO      START
     C                   END
     C*
     C     ZRECNO        IFEQ      0
     C                   GOTO      START3
     C                   ELSE
     C                   EXSR      HENT
     C   98              GOTO      START3
     C                   GOTO      SLUTT
     C                   END
     C*
     C     SLUTT         TAG
     C                   SETON                                        LR
     C                   RETURN
     C*
     C***********************************************
     C     HENT          BEGSR
     C***********************************************
     C                   MOVEA     '00'          *IN(91)
     C     *IN98         DOWEQ     '0'
     C                   READC     SUBFIL                                 98
     C  N98WSNR          IFEQ      '1'
     C                   MOVE      KS8AVG        UKOD
N02  C                   MOVE      KS8SKV        UKOD2
     C                   GOTO      SLHENT
     C                   END
     C                   END
     C*
     C                   MOVE      'DIV0012'     MSGNR
     C                   SETON                                            30
     C*
     C     SLHENT        ENDSR
     C*
     C***********************************************
     C     INIT          BEGSR
     C***********************************************
     C     KODS8K        KLIST
S02  C*                    KFLD           KS8UNI
     C                   KFLD                    W2KOD
N02  C                   KFLD                    W2KOD2
     C     KOS8LK        KLIST
     C                   KFLD                    W2FTX
     C*
     C     *ENTRY        PLIST
     C                   PARM                    UKOD              2
N02  C                   PARM                    UKOD2             3
N02  C                   PARM                    UMOD              1
      *
     C                   MOVE      *BLANKS       W2FTX
     C     UKOD          IFEQ      *BLANKS
     C                   MOVE      '1'           *IN27
     C                   ELSE
     C                   MOVE      UKOD          W2KOD
N02  C     UMOD          IFEQ      'S'
     C                   MOVE      UKOD2         W2KOD2
N02  C                   END
     C                   END
S02  C*                    MOVE 'S8 '     KS8UNI
      *
     C                   WRITE     WINDOWSZ
      *
     C                   ENDSR
     C*
