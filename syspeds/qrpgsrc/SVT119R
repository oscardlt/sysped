     C                   EVAL      SVTX19_11C=W11
     C                   EVAL      SVTX19_12C=W12
     C                   EVAL      SVTX19_13C=W13
     C                   EVAL      SVTX19_14C=W14
     C                   EVAL      SVTX19_15C=W15
     C                   EVAL      SVTX19_16C=W16
