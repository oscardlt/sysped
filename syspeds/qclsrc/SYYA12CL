             PGM
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1) /* Hvis &in01 +
                          på, betyr det avslutt program fra +
                          avdelingsvalg. */
             DCL        VAR(&AVDA) TYPE(*CHAR) LEN(4) /* AVD PRT */
             DCL        VAR(&AVD) TYPE(*DEC) LEN(4 0) /* AVD PRT */
             DCL        VAR(&UTNR) TYPE(*DEC) LEN(3 0) VALUE(69) /* +
                          UTSK.NR */
             DCL        VAR(&DEV) TYPE(*CHAR) LEN(10) /* PRINTER */
             DCL        VAR(&FORMS) TYPE(*CHAR) LEN(10) /* FORMSTYP*/
             DCL        VAR(&HEAD) TYPE(*CHAR) LEN(1) /* HEADING */
             DCL        VAR(&KOPFM) TYPE(*CHAR) LEN(5)
        /* */
             CALL       PGM(SYFA00) PARM(*IN01)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT)) /* +
                          Hvis &IN01 på, avslutter program. */
             RTVDTAARA  DTAARA(*LDA (15 4)) RTNVAR(&AVDA)
             CHGVAR     VAR(&AVD) VALUE(&AVDA) /*N01*/
             CALL       PGM(SYGE01) PARM(&AVD &UTNR &DEV &FORMS +
                          &HEAD &KOPFM)
        /* */
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             OVRPRTF    FILE(SYYA12PF) OUTQ(&DEV) PAGESIZE(72 80) +
                          CPI(10) OVRFLW(68) +
                          FORMTYPE(&FORMS)
             CALL       PGM(SYYA12)
        /* */
 SLUTT:      ENDPGM
