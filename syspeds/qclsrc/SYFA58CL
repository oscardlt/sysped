             PGM        PARM(&UPDATE)
             DCL        VAR(&UPDATE) TYPE(*CHAR) LEN(1)
             DCL        VAR(&ALLWS) TYPE(*CHAR) LEN(1) VALUE('J')
             DCL        VAR(&AVD) TYPE(*DEC) LEN(4 0) VALUE(0)
             DCL        VAR(&OPD) TYPE(*DEC) LEN(7 0) VALUE(0)
             CALL       PGM(SYFA58) PARM(&AVD &OPD &ALLWS &UPDATE)
             ENDPGM
