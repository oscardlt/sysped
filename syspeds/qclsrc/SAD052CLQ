             PGM
             DCL        VAR(&PRTTYP) TYPE(*CHAR) LEN(1) VALUE('P')
             DCL        VAR(&AVD) TYPE(*DEC) LEN(4 0)
             DCL        VAR(&TDN) TYPE(*DEC) LEN(7 0)
             DCL        VAR(&PRT1) TYPE(*CHAR) LEN(10)
             DCL        VAR(&PRT2) TYPE(*CHAR) LEN(10)
             DCL        VAR(&PRT3) TYPE(*CHAR) LEN(10)
             CALL       PGM(SAD52P) PARM(&PRTTYP &AVD &TDN &PRT1 +
                          &PRT2 &PRT3)
             ENDPGM
