     C* SKRIVER HEADING
     C*
     C     TROPD0        COMP      *ZEROS                             5050        DUP AV
     C     HEPRO         COMP      *ZEROS                             5151        PÅ TUR
      * Hvis DUP-oppdr. hent evt. løpenr fra overordnet.
     C                   SETOFF                                       52
     C     HETLL         IFEQ      *BLANKS
     C     TROPD0        ANDNE     *ZEROS
     C                   MOVE      TRAVD0        XXAVD0            4 0
     C                   MOVE      TROPD0        XXOPD0            7 0
     C                   MOVE      *BLANKS       XXPADS          256
     C                   CALL      'SYTR99'
     C                   PARM                    XXAVD0
     C                   PARM                    XXOPD0
     C                   PARM                    XXPADS
     C                   MOVE      XXPADS        DUPADS
     C                   MOVE      DATLE         HETLE
     C                   MOVE      DATLL         HETLL
     C                   MOVE      DAGN          HEGN
     C                   MOVE      DAPOS1        HEPOS1
     C                   Z-ADD     DAPOS2        HEPOS2
     C     HETLL         COMP      *BLANKS                            5252      "SE DUP AV"
     C                   END
     C                   WRITE     HEAD1A
     C     HEKNA         IFNE      *ZEROS
     C                   WRITE     FORT
     C                   END
     C   90              DO
     C     TRVERB        IFNE      *ZEROS
     C                   WRITE     VARE
     C                   END
     C                   END
     C     TRETTB        IFNE      *ZEROS
     C                   WRITE     ETT
     C                   END
     C   90              WRITE     FRAKT
     C                   WRITE     HEAD1B
      * Parameter for bruk at arb-ordre som skilleark (men kun intern ordre..)
     c     SkilKode      ifne      *blanks
     c     *in90         andeq     '1'
     c     *in91         andeq     '1'
     c                   movel     SkilKode      Skille           13
     c                   movel     HEAVD         SkiAvOp
     c                   move      HEOPD         SkiAvOp          11
     c                   move      SkiAvOp       Skille
     c                   movel     Skille        SkilleT          15
     C                   WRITE     SKILLBAR
     c                   end
     C     TRFA11        IFNE      *BLANKS
     C     TRFA21        ORNE      *BLANKS
     C                   WRITE     FARL
     C                   END
     C*
      * MELLOM B.L. OG   MERKN....
      * FRIE SØKEVEIER (KUN DE SOM HAR DOK.KODE LIK K)
     C     HEADFK        SETLL     FRISOK
     C     HEADFK        READE     FRISOK                                 33
     C     *IN33         DOWEQ     '0'
     C                   SORTA     DFS
      * VED INTERN SKRIVES ALLE SØKEVEIER (SIMULER AT DET STÅR K)
      * VED EKSTERN KUN HVIS S/K
     C     INT           IFEQ      'J'
     C                   MOVE      'K'           DFS(10)
     C                   END
     C     DFS(10)       IFNE      ' '
     C     'K'           LOOKUP    DFS                                    33
     C  N33'S'           LOOKUP    DFS                                    33
     C     *IN33         IFEQ      '1'
     C     FSKODE        CHAIN     KODFRI                             33
     C     *IN33         IFEQ      '0'
     C     *IN64         IFEQ      '1'
     C     UHEAD         IFEQ      'J'
     C     UHEAD         OREQ      'Y'
     C                   WRITE     HEADP
     C                   ELSE
     C                   WRITE     HEAD0
     C                   END
     C                   SETOFF                                       64
     C                   END
     C                   WRITE     KJØPFS
     C                   END
     C                   END
     C                   END
     C*
     C     HEADFK        READE     FRISOK                                 33
     C                   END
