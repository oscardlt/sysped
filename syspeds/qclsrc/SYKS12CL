             PGM
             DCL        VAR(&PARM) TYPE(*CHAR) LEN(256) +
                          VALUE('N00000000000000000000000000000000000+
                          00000000')
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CALL       PGM(SYKS12) PARM(&PARM)
             ENDPGM
