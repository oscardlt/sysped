             PGM
             DCL        VAR(&GEKODE) TYPE(*CHAR) LEN(1) VALUE('B')
             DCL        VAR(&GEUNIK) TYPE(*CHAR) LEN(9)
             DCL        VAR(&UNIK) TYPE(*CHAR) LEN(10)
 /* LAGER UNIK VERDI FOR BACKUPMEMBER */
             CALL       PGM(SYGE52) PARM(&GEKODE &GEUNIK)
             CHGVAR     VAR(&UNIK) VALUE(&GEKODE *CAT &GEUNIK)
 /* LAGER EVENTUELT BACKUPFIL         */
             CRTDUPOBJ  OBJ(FRTIL) FROMLIB(*LIBL) OBJTYPE(*FILE) +
                          NEWOBJ(FRTILCOPY)
             MONMSG     MSGID(CPF0000)
             CHGPF      FILE(FRTILCOPY) MAXMBRS(*NOMAX) SIZE(50000000)
             CPYF       FROMFILE(FRTIL) TOFILE(FRTILCOPY) +
                          TOMBR(&UNIK) MBROPT(*REPLACE)
             ENDPGM
