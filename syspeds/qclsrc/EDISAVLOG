             PGM
             DCL        VAR(&USR)   TYPE(*CHAR) LEN(10)
             RTVUSRPRF  RTNUSRPRF(&USR)
             IF         COND(&USR *NE 'QSECOFR') THEN(DO)
             SNDPGMMSG  MSG('Du har ikke autorisasjon til å utføre +
                          denne jobben.')
             GOTO       CMDLBL(SLUTT)
             ENDDO
             ?          EDISAVLOG2
             MONMSG     MSGID(CPF0000)
 SLUTT:      ENDPGM
