      *
     C  N50              WRITE     HEADFRCMP
     C                   DELETE    HEADFR
      *
     C                   ADD       1             WSANT
     C                   ADD       1             XANT              3 0
     C                   IF        WSANT <= 1000
     C                   MOVEA     '01'          *IN(94)
     C     XANT          COMP      100                                    97
     C                   ELSE
     C                   MOVEA     '10'          *IN(94)
     C     XANT          COMP      1000                                   97
     C                   END
     C                   IF        *IN97
     C                   MOVE      *ZEROS        XANT
     C                   TIME                    WSTDN≈
     C                   READ      REOPDAFM                             3333
     C                   WRITE     BILDE
     C                   READ      REOPDAFM                             3333
     C                   END
      *
     C                   GOTO      STDLORDRE
      *
     C     SLDLORDRE     ENDSR
      *
     C***********************************************
     C     UPDFAKT       BEGSR
     C***********************************************
     C                   IF        NYLINJE = ' '
     C                   EXSR      MOVDTA
     C                   ENDIF
     C                   IF        (((FAFAKT = XFAKT)  AND
     C                               (FAKUNR = XKUNR)) AND
     C                               (FASK = XSK))
     C                   EXSR      SUMLINJE
     C                   ELSE
     C                   EXSR      UTLINJE
     C                   ENDIF
      *
     C                   ENDSR
      *
     C***********************************************
     C     MOVDTA        BEGSR
     C***********************************************
     C                   MOVE      FAOPKO        XOPKO             1
     C                   MOVE      FASK          XSK               1
     C                   MOVE      FAVAL         XVAL              3
     C                   MOVE      *ZEROS        XBELV            13 2
     C                   MOVE      *ZEROS        XBELN            13 2
     C                   MOVE      *ZEROS        XBELU            13 2
     C                   MOVE      FAKUNR        XKUNR             6 0
     C                   MOVE      FAFAKT        XFAKT             6 0
     C                   MOVE      FAJOUR        XJOUR             7 0
     C                   MOVE      FACC          XCC               2 0
     C                   MOVE      FA≈R          X≈R               2 0
     C                   MOVE      FAMND         XMND              2 0
     C                   MOVE      FADATO        XDATO             8 0
     C                   MOVE      FADAFF        XDAFF             8 0
     C                   MOVE      *ZEROS        FAMOMS
     C                   MOVE      'Y'           NYLINJE           1
     C                   MOVE      'J'           XUTVK             1
      *
     C                   ENDSR
      *
     C***********************************************
     C     SUMLINJE      BEGSR
     C***********************************************
     C                   ADD       FABELV        XBELV
     C                   ADD       FABELN        XBELN
     C                   ADD       FABELU        XBELU
     C                   IF        FAKDM = 'J'
     C                   ADD       FABELN        FAMOMS
     C                   END
     C                   IF        FAVAL <> XVAL
     C                   MOVE      'N'           XUTVK
     C                   END
      *
     C                   ENDSR
      *
     C***********************************************
     C     UTLINJE       BEGSR
     C***********************************************
     C                   MOVE      FAOPKO        ZOPKO             1
     C                   MOVE      FASK          ZSK               1
     C                   MOVE      FAVAL         ZVAL              3
     C                   MOVE      FABELV        ZBELV            13 2
     C                   MOVE      FABELN        ZBELN            13 2
     C                   MOVE      FABELU        ZBELU            13 2
     C                   MOVE      FAKUNR        ZKUNR             6 0
     C                   MOVE      FAFAKT        ZFAKT             6 0
     C                   MOVE      FAJOUR        ZJOUR             7 0
     C                   MOVE      FACC          ZCC               2 0
     C                   MOVE      FA≈R          Z≈R               2 0
     C                   MOVE      FAMND         ZMND              2 0
     C                   MOVE      FADATO        ZDATO             8 0
     C                   MOVE      FADAFF        ZDAFF             8 0
      *
     C                   ADD       1             XLI
     C                   MOVE      XLI           FALI
     C                   MOVE      HEAVD         FAAVD
     C                   MOVE      HEOPD         FAOPD
     C                   MOVE      FITAX         FITAXB            4 4
     C                   IF        (((FITAXD <> 0)  AND
     C                               (FITAX2 <> 0)) AND
     C                               (FADATO < FITAXD))
     C                   MOVE      FITAX2        FITAXB            4 4
     C                   END
      *
     C                   EVAL(H)   FAMOMS = FAMOMS * FITAXB
     C                   MOVE      XOPKO         FAOPKO
     C                   MOVE      XSK           FASK
     C                   MOVE      XVAL          FAVAL
     C                   MOVE      XBELV         FABELV
     C                   MOVE      XBELN         FABELN
     C                   MOVE      XBELU         FABELU
     C                   MOVE      XKUNR         FAKUNR
     C                   MOVE      XFAKT         FAFAKT
     C                   MOVE      XJOUR         FAJOUR
     C                   MOVE      XCC           FACC
     C                   MOVE      X≈R           FA≈R
     C                   MOVE      XMND          FAMND
     C                   MOVE      XDATO         FADATO
     C                   MOVE      XDAFF         FADAFF
     C                   IF        XUTVK = 'N'
     C                   MOVE      '   '         FAVAL
     C                   MOVE      *ZEROS        FABELV
     C                   END
     C                   WRITE     FAKTRCMP
      *
     C                   MOVE      ZSK           XSK
     C                   MOVE      ZOPKO         XOPKO
     C                   MOVE      ZVAL          XVAL
     C                   MOVE      ZBELV         XBELV
     C                   MOVE      ZBELN         XBELN
     C                   MOVE      ZBELU         XBELU
     C                   MOVE      ZKUNR         XKUNR
     C                   MOVE      ZFAKT         XFAKT
     C                   MOVE      ZJOUR         XJOUR
     C                   MOVE      ZCC           XCC
     C                   MOVE      Z≈R           X≈R
     C                   MOVE      ZMND          XMND
     C                   MOVE      ZDATO         XDATO
     C                   MOVE      ZDAFF         XDAFF
     C                   IF        FAKDM = 'J'
     C                   Z-ADD     ZBELN         FAMOMS
     C                   ELSE
     C                   MOVE      *ZEROS        FAMOMS
     C                   END
     C                   MOVE      'Y'           NYLINJE           1
     C                   MOVE      'J'           XUTVK             1
      *
     C                   ENDSR
      *
     C***********************************************
     C     INIT          BEGSR
