    /* ----------------------------------------------------------- */
    /* SKRIVER UT MERKELAPPER MANUELT */
    /* ----------------------------------------------------------- */
             PGM
    /* */
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1) VALUE('0') /* +
                          Hvis &in01 på, betyr det avslutt program +
                          fra valgbilde. */
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10) /* Printernavn +
                          til merkelapp-3. */
             DCL        VAR(&PT) TYPE(*CHAR) LEN(10) /* Papirtype +
                          til merkelapper. */
    /* */
    /* HENTE OPPLYSNINGER OM HVOR UTSKRIFTEN ØNSKES */
    /* */
             CALL       PGM(SYFAMK) PARM(&IN01)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT)) /* +
                          Hvis &IN01 på, avslutter program. */
    /* */
             RTVDTAARA  DTAARA(*LDA (681 10)) RTNVAR(&PRT)
             RTVDTAARA  DTAARA(*LDA (691 10)) RTNVAR(&PT)
    /* */
             OVRPRTF    FILE(MLAPP) OUTQ(&PRT) PAGESIZE(24 70) +
                          CPI(12) OVRFLW(24) FORMTYPE(&PT)
    /* */
             CALL       PGM(SYFA17)
    /* */
 SLUTT:      ENDPGM
