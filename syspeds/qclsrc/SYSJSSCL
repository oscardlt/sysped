             PGM
             DCL        VAR(&SG) TYPE(*CHAR) LEN(3)
             DCL        VAR(&SD) TYPE(*CHAR) LEN(5)
             DCL        VAR(&DT) TYPE(*CHAR) LEN(6)
             DCL        VAR(&MN) TYPE(*CHAR) LEN(17)
             DCL        VAR(&DTS) TYPE(*CHAR) LEN(6)
             DCL        VAR(&PRT35) TYPE(*CHAR) LEN(10)
             DCL        VAR(&PT35) TYPE(*CHAR) LEN(10)
             DCL        VAR(&JOBK) TYPE(*CHAR) LEN(1)
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1) VALUE('0') /* +
                          Hvis &in01 på, betyr det avslutt program +
                          fra valgbilde. */
       /* */
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
       /* */
             CALL       PGM(SYFA00) PARM(&IN01)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT)) /* +
                          Hvis &IN01 på, avslutter program. */
       /* */
             CALL       PGM(SYSJSS) PARM(&IN01 &SG &SD &DT &MN &DTS +
                          &PRT35 &PT35 &JOBK)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT)) /* +
                          Hvis &IN01 på, avslutter program. */
       /* */
             CALL       PGM(SYSJ58CL) PARM(&SG &SD &DT &MN &DTS +
                          &PRT35 &PT35 &JOBK)
       /* */
 SLUTT:      ENDPGM
