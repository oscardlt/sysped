     C***********************************************
     C     INIT          BEGSR
     C***********************************************
     C     TRANK         KLIST
     C                   KFLD                    FIFIRM
     C                   KFLD                    W2KOD
     C     TRAN1K        KLIST
     C                   KFLD                    FIFIRM
     C                   KFLD                    W2SØK
      *
     C     *ENTRY        PLIST
     C                   PARM                    UKOD              8 0
     C                   PARM                    UDATO             6 0
     C                   PARM                    UAVD              4 0
     C                   PARM                    UF4KD             1
      *
     C                   READ      FIRM                                   33
     C     TURK16        KLIST
     C                   KFLD                    ZAVRE             1
     C                   KFLD                    VMTRAN
      * TOM DATO
     C                   MOVE      UDATO         TMDT
NV5  C     TMDT          IFEQ      0
NV5  C                   MOVE      *ZEROS        TOMDT
NV5  C                   ELSE
     C                   Z-ADD     TMDTD         TOMDTD
     C                   Z-ADD     TMDTM         TOMDTM
SV5  C*                    Z-ADDTMDTÅ     TOMDTÅ
NV5  C     TMDTÅ         IFGT      50
NV5  C     TMDTÅ         ADD       1900          TOMDTÅ
NV5  C                   ELSE
NV5  C     TMDTÅ         ADD       2000          TOMDTÅ
NV5  C                   END
NV5  C                   END
