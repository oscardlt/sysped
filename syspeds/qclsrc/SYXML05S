             PGM        PARM(&MBR)
             DCL        VAR(&MBR) TYPE(*CHAR) LEN(16)
             DCL        VAR(&FIL) TYPE(*CHAR) LEN(70)
             DCL        VAR(&FILNAVN) TYPE(*CHAR) LEN(20)

             CHGVAR     VAR(&FIL) VALUE('/TilFmsXml/' *CAT &MBR +
                          *TCAT '.xml')
             CTOPCF     FIL(XMLTILP) LIB(QTEMP) STMF(&FIL)
             MONMSG     MSGID(CPF0000)
             CHGAUT     OBJ(&FIL) USER(*PUBLIC) DTAAUT(*RWX) +
                          OBJAUT(*ALL)
             MONMSG     MSGID(CPF0000)
             DLTF       FILE(QTEMP/XMLTILP)

/*NC KALLER PROGRAM FOR Å HENTE FIL FRA IFS TIL EGET SYSTEM */
/*NC         CHGVAR     VAR(&FILNAVN) VALUE(&MBR *TCAT '.xml')     */
/*ND         CALL       PGM(EDIFRAEDF/NCSYXMSCA1) PARM(&FILNAVN)   */

             ENDPGM
