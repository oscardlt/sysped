             PGM
             DCL        VAR(&WAIT) TYPE(*DEC) LEN(5 0) VALUE(-99999)
             DCL        VAR(&JON) TYPE(*CHAR) LEN(6) /* jobnr */
             DCL        VAR(&USE) TYPE(*CHAR) LEN(10) /* bruke */
             DCL        VAR(&TLF) TYPE(*CHAR) LEN(21) /* faxnr */
             DCL        VAR(&KOD) TYPE(*CHAR) LEN(2) /* tr.KOD */
             DCL        VAR(&SUC) TYPE(*CHAR) LEN(1) /* kode U/S */
             DCL        VAR(&NAM) TYPE(*CHAR) LEN(30) /* navn */
             DCL        VAR(&LNG) TYPE(*DEC) LEN(5 0) VALUE(110)
             DCL        VAR(&DTA) TYPE(*CHAR) LEN(110)
             DCL        VAR(&L1) TYPE(*CHAR) LEN(1) VALUE('J')
                                                          /**/
 RUNDE:      CALL       PGM(QRCVDTAQ) PARM(FAXDTAQ *LIBL &LNG +
                          &DTA &WAIT)
             CHGVAR     VAR(&USE) VALUE(%SST(&DTA 11 10))
             CHGVAR     VAR(&SUC) VALUE(%SST(&DTA 75 1))
             CHGVAR     VAR(&KOD) VALUE(%SST(&DTA 73 2))
             CHGVAR     VAR(&JON) VALUE(%SST(&DTA 21 6))
             IF         COND(&KOD = '00') THEN(DO)
/*           OVRDBF     FILE(MSGKL01) TOFILE(*LIBL/XSGKL01)   */
             CALL       PGM(FAX013B) PARM(&JON &NAM &TLF)


             IF         COND(&SUC = 'U') THEN(DO)
             SNDPGMMSG  MSG('Unsucessful Faxtransmission to:' *BCAT +
                          &NAM *BCAT 'Phone' *BCAT &TLF *BCAT '.') +
                          MSGTYPE(*INFO) TOUSR(&USE)
             ENDDO
             IF         COND(&SUC = 'S') THEN(DO)
             SNDMSG  MSG('Sucessful Faxtransmission to:' *BCAT +
                          &NAM *BCAT 'Phone' *BCAT &TLF *BCAT '.') +
                          MSGTYPE(*INFO) TOUSR(&USE)
             ENDDO
             ENDDO
             GOTO       CMDLBL(RUNDE)
             ENDPGM
