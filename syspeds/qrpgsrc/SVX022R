     C     W2EH          IFNE      *BLANKS
     C                   MOVE      '017'         YUNIK
     C                   MOVE      *BLANKS       YKODE
     C                   MOVEL     W2EH          YKODE
     C     SVK01K        CHAIN     SVXKODL01                          88
     C     *IN88         IFEQ      '1'
     C                   MOVE      'SVX0816'     MSGNR
     C                   MOVE      '1'           *IN30
     C                   GOTO      SLCRT
     C                   END
     C                   END
      *
     C  N88W2EH          IFEQ      'NE'
     C     W2EH          OREQ      'VQ'
     C     W2EH          OREQ      'VG'
     C     W2EH          OREQ      'VL'
     C     W2EH          OREQ      'VY'
     C     W2EH          OREQ      'VR'
     C     W2EH          OREQ      'VO'
     C                   MOVE      *ZEROS        W2NT
     C                   END
      *
     C     W2EH          IFEQ      'NE'
     C     W2NTEH        COMP      0                                      86
     C     *IN86         IFEQ      '1'
     C     *IN30         ANDEQ     '0'
     C                   MOVE      'SVX0825'     MSGNR
     C                   MOVE      '1'           *IN30
     C                   GOTO      SLCRT
     C                   END
     C                   ELSE
     C                   MOVE      *ZEROS        W2NTEH
     C                   END
      *
     C                   MOVE      W2MN          TVMN
     C                   MOVE      W2MNSK        TVMNSK
     C                   MOVE      W2EH          TVEH
     C                   MOVE      W2NT          TVNT
     C                   MOVE      W2NTEH        TVNTEH
     C                   ADD       1             YRECNO
     C                   MOVE      YRECNO        ZRECNO
     C                   WRITE     SUBFIL
     C                   WRITE     SUBCTL
     C                   MOVE      *BLANKS       W2NR
     C                   MOVE      *BLANKS       W2MN
     C                   MOVE      *BLANKS       W2MNSK
     C                   MOVE      *BLANKS       W2EH
     C                   MOVE      *ZEROS        W2NT
     C                   MOVE      *ZEROS        W2NTEH
