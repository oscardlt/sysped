********************************************************************
      * RETTINGER I DETTE PROGRAM:
PTFnr:*Sek Sig Vers  Beskrivelse...........................
""""""*"""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""
10XXX * 01 SH  PTF10 sende sdvfil        ....
10XXX * 02 SH  PTF10 epost adresse inn   ....
10XXX * 03 SH  PTF10 emnerubrikk utvidet til 70
********************************************************************
      *
      *
      **************************************************************
      *** PROGRAM FOR PRINTERVALG M.M.  GODSJOURNAL              ***
      **************************************************************
      *
     FFIRM      IF   E             DISK
     FCUNDC     IF   E             DISK
     FGODSFI    IF   E           K DISK
     FSYTL04FM  CF   E             WORKSTN
     F                                     INFDS(FBAREA)
     D FBAREA          DS
     D  WSCLOC               370    371B 0
     D  WSSUBN               378    379B 0
     D UP              C                   CONST('ABCDEFGHIJKLMNOPQRST-
     D                                     UVWXYZÆØÅ')
     D LO              C                   CONST('abcdefghijklmnopqrst-
     D                                     uvwxyzæøå')
     D                 DS
     D  DSTID                  1     14  0
     D  DSDAG                  7      8  0
     D  DSMND                  9     10  0
     D  DSAAR                 13     14  0
     D  DSCCAA                11     14  0
     D                 DS
     D  WSDATO                 1      6  0
     D  UDAG                   1      2  0
     D  UMND                   3      4  0
     D  UAAR                   5      6  0
     D                 DS
     D  USØKKN                 1     30
     D  USØKKO                 1      2
     D  URRN                   3     12
     D  epofax                13     13
     C     *ENTRY        PLIST
     C                   PARM                    WSPRT
     C                   PARM                    UIN01             1
     C                   PARM                    GFLBKO
     C                   PARM                    GFENH
     C                   PARM                    UUDATO            6
     C                   PARM                    UDAGNR            3
     C                   PARM                    Uepost           70
s03  C*                  PARM                    UCOV01           40
n03  C                   PARM                    UCOV01           70
     C                   PARM                    uCOV02           40
     C                   PARM                    uCOV03           40
     C                   PARM                    uCOV04           40
     C                   PARM                    uCOV05           40
     C                   PARM                    uCOV06           40
     C                   PARM                    uCOV07           40
     C                   PARM                    uCOV08           40
     C                   PARM                    EPOSJN            1
      *
n02  c                   move      uepost        epost
     C                   read      firm                                   33
     c                   move      'N'           eposjn
N01  c                   move      'N'           wsepjn
      *
     C                   MOVE      '0'           UIN01
     C     *LOVAL        SETLL     GODSFI
     C                   READ      GODSFI                                 50
     C                   READ      GODSFI                                 50
     C     *IN50         IFEQ      '0'
     C                   MOVE      *BLANKS       UULBKO            5
     C                   MOVE      *BLANKS       UUENH             1
     C                   MOVE      'N'           UUPDAT            1
     C                   CALL      'SYTL03B'
     C                   PARM                    UULBKO
     C                   PARM                    UUENH
     C                   PARM                    UUDATO            6
     C                   PARM                    UDAGNR            3
     C                   PARM                    UUPDAT            1
     C     BEVKEY        KLIST
     C                   KFLD                    UULBKO
     C                   KFLD                    UUENH
     C     BEVKEY        CHAIN     GODSFI                             50
     C   50              MOVE      '1'           UIN01
     C   50              GOTO      SLUTT
     c                   else
     C                   TIME                    DSTID
     C                   MOVE      DSAAR         UAAR
     C                   MOVE      DSMND         UMND
     C                   MOVE      DSDAG         UDAG
     C                   MOVE      WSDATO        UDATO             6
     C                   CALL      'SYGE25CL'
     C                   PARM                    UDATO
     C                   PARM                    UDAGNR            3
     c                   move      udato         uudato
     C                   END
     C                   MOVE      GFLBKO        WSLBKO
     C                   MOVE      GFLBS1        WSLBS1
     C                   MOVE      GFLBS2        WSLBS2
     C                   MOVE      GFLBS3        WSLBS3
     C                   MOVE      GFLBS4        WSLBS4
     C                   MOVE      GFPRT         WSPRT
     C     START         TAG
     C                   EXFMT     BILDE1
     C     WSCLOC        DIV       256           LINNBR
     C                   MVR                     POSNBR
      **  AVSLUTT
     C     *IN03         IFEQ      *ON
     C                   MOVE      '1'           UIN01
     C                   GOTO      SLUTT
     C                   ENDIF                                                  <*IN03
      *
      * søk på epostadresse
      *
     C   04              DO
     C     LINNBR        IFeq      15
     C     LO:UP         XLATE     EPOST         usok
     C                   CALL      'VCUNDC2R'
     C                   PARM                    FIFIRM
     C                   PARM                    USOK             30
     C                   PARM                    URRN2            10 0
     c                   parm                    epofax            1
     C     URRN2         IFNE      *ZEROS
     C                   MOVE      URRN2         URRN
     C                   MOVE      'KP'          USØKKO
     c                   else
     c                   movel     usok          epost
     C                   END
     c                   end
     C* RETUR MED KONTAKTPERS
     C     USØKKO        IFEQ      'KP'
     C                   MOVE      URRN          RRN              10 0
     C     RRN           CHAIN     CUNDC                              31
     C  N31              MOVEL     CEMAIL        EPOST
     C                   END
     C                   GOTO      START
     C                   END
N01  c     wsepjn        ifne      'N'
N01  c     epost         andeq     *blanks
N01  C                   GOTO      START
N01  C                   END
     c     epost         ifne      *blanks
N01  c     wsepjn        andeq     'J'
     C                   CALL      'EPOCVP'
     C                   PARM                    COV01            40
     C                   PARM                    COV02            40
     C                   PARM                    COV03            40
     C                   PARM                    COV04            40
     C                   PARM                    COV05            40
     C                   PARM                    COV06            40
     C                   PARM                    COV07            40
     C                   PARM                    COV08            40
     C                   PARM                    FUNK              1
     c                   END
     C*
      **
     C                   MOVE      '0'           *IN30
     C*
     C     SLUTT         TAG
s03  c*                  move      cov01         ucov01
n03  c                   movel     cov01         ucov01
     c                   move      cov02         ucov02
     c                   move      cov03         ucov03
     c                   move      cov04         ucov04
     c                   move      cov05         ucov05
     c                   move      cov06         ucov06
     c                   move      cov07         ucov07
     c                   move      cov08         ucov08
     c                   move      epost         uepost
     c     epost         ifne      *blanks
     c                   move      *blanks       wsprt
     c                   movel     'OUTMAIL'     wsprt
s01  c*                  move      'J'           eposjn
     c                   end
n01  c                   move      WSEPJN        eposjn
     C                   SETON                                        LR
