      *
     C                   EXSR      INIT
      *
     C     XMLINHL1K     CHAIN     XMLINHL1                           50
     C                   IF        *IN50 = *OFF
     C                   IF        (((XMST <> ' ') AND
     C                               (XMST <> 'A')) AND
     C                               (XMST <> 'C'))
     C                   UPDATE    XMLINHR
     C                   GOTO      SLUTT
     C                   END
     C     XMLINHL1K     CHAIN     HEADF                              50
     C  N50HXEDIK        COMP      'X'                                5050
     C                   IF        *IN50
     C                   CALL      'SYGE15C'
     C                   PARM                    WDT
     C                   PARM                    WTM
     C                   MOVE      'E'           XMST
     C                   MOVE      WDT           XMDTS
     C                   MOVE      WTM           XMTMS
     C                   UPDATE    XMLINHR
     C                   END
     C                   END
     C   50              GOTO      SLUTT
      *
     C                   EXSR      SRINIT
     C                   EXSR      SRBYGGF
      *
     C                   EXSR      SRSTART
      *
     C                   EXSR      SRCONSTR
      *
     C                   EXSR      SRPARTYS
      *
     C                   EXSR      SRDATSTR
      *
     C                   EXSR      SRSLUTT
     C                   EXSR      SRCLOSE
     C                   EXSR      SRSEND
      * Oppdater XMLUFF
     C                   MOVE      'C'           XMST
     C                   MOVE      WDT           XMDTS
     C                   MOVE      WTM           XMTMS
     C                   UPDATE    XMLINHR
      *
     C     SLUTT         TAG
     C                   MOVE      *ON           *INLR
     C                   RETURN
      *
      ***********************************************
     C     SRINIT        BEGSR
      ***********************************************
     C                   OPEN      ECMSG1
     C                   OPEN      FRISOK
     C                   OPEN      DOKUF
     C                   OPEN      DOKUFM
     C                   OPEN      DOKUFV
     C                   OPEN      FAK2
     C                   OPEN      KODTGE
     C                   OPEN      EDIC
     C                   OPEN      EDIS
     C                   OPEN      EDIM
     C                   OPEN      EDISE
      *
     C                   MOVE      'INS'         XFSKODEINS        3
     C     FRISOKK6      CHAIN     FRISOK                             33
     C   33              MOVE      'INB'         XFSKODEINS
     C   33FRISOKK6      CHAIN     FRISOK                             33
     C                   MOVEL     FSSOK         XAN15            15
     C                   EVAL      ECIIDC = XAN15
      *
     C     ECMSG1K       CHAIN     ECMSG1                             33
     C     1             CHAIN     EDIC                               33
     C                   ADD       1             CSN
     C                   UPDATE    EDICR
      *
     C                   MOVE      *BLANKS       XMBR
     C                   MOVE      'F'           XMBRA
     C                   MOVE      CSN           XMBRB
     C                   MOVE      XMBR          SEMBR
     C                   MOVE      *BLANKS       SDATA
     C                   CALL      'SYGE15C'
     C                   PARM                    WDT
     C                   PARM                    WTM
      *
     C                   EVAL      S0004 = ECIIDI
     C                   EVAL      S0010 = ECIIDC
     C                   EVAL      S0020 = 'INHPK'
     C                   EVAL      S0026 = 'XML-INH INSINF'
     C                   CAT       XHEAVD:0      S0020
     C                   CAT       XHEOPD:0      S0020
     C                   EVAL      SSN   = CSN
     C                   EVAL      SSR   = 'S'
     C                   EVAL      SST   = 'C'
     C                   MOVE      WDT           SDT
     C                   MOVE      WTM           STM
     C                   EVAL      SLIB  = '*LIBL'
     C                   EVAL      SFILE = 'EDISE'
     C                   EVAL      SMBR  = SEMBR
     C                   EVAL      SMBRS = 0
     C                   EVAL      S0036 = 1
      *
     C                   ENDSR
      *
