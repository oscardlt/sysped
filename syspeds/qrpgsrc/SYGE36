     c     *ENTRY        plist
     c                   parm                    firma             2
     c                   parm                    kundnr            8 0
     c                   parm                    UUPB             10
     c                   parm                    UPB               4 0
     c     key           klist
     c                   kfld                    firma
     c                   kfld                    kundnr
     c                   MOVE      *BLANKS       UUPB
     c                   MOVE      *BLANKS       UPB
     c     key           chain     kundl01                            33
     C     *IN33         IFEQ      '0'
     C                   MOVEL     PNPBKU        PN4A              4
     C                   MOVE      PNPBKU        PN6A              6
     C* SEKS SISTE UTFYLLT
     C     PN6A          IFNE      *BLANKS
     C                   MOVEL     PNPBKU        UUPB
     C                   END
     C* FIRE FØRTSE UTFYLLT
     C     PN6A          IFEQ      *BLANKS
     C     PN4A          ANDNE     *BLANKS
N03  C                   testn                   PN4A                 34
     C     *IN34         IFEQ      '1'
     C                   MOVE      PN4A          UPB
     C                   ELSE
     C                   MOVEL     PN4A          UUPB
     C                   END
     C                   END
     C                   END
     c                   seton                                        lr
     c                   return
