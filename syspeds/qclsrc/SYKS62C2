             PGM        PARM(&WSUSER &WSSTAT &UUFDTO &UUFTID &UUTDTO +
                          &UUTTID &WSPRIN &WSA4)
  /* */
             DCL        VAR(&WSUSER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&WSSTAT) TYPE(*CHAR) LEN(1)
             DCL        VAR(&UUFDTO) TYPE(*CHAR) LEN(6)
             DCL        VAR(&UUFTID) TYPE(*CHAR) LEN(4)
             DCL        VAR(&UUTDTO) TYPE(*CHAR) LEN(6)
             DCL        VAR(&UUTTID) TYPE(*CHAR) LEN(4)
             DCL        VAR(&WSPRIN) TYPE(*CHAR) LEN(10)
             DCL        VAR(&WSA4)   TYPE(*CHAR) LEN(1)
 /*  */
             IF         COND(&WSPRIN *EQ '*JOB') THEN(RTVJOBA +
                          PRTDEV(&WSPRIN))
 /*  */
             SBMJOB     CMD(CALL PGM(SYKS63) PARM(&WSUSER &WSSTAT +
                          &UUFDTO &UUFTID &UUTDTO &UUTTID &WSPRIN +
                          &WSA4)) JOB(BILKONTR) +
                          JOBQ(SYJOBQPRT) OUTQ(&WSPRIN)
 /*  */
             ENDPGM
