N08  C                   kfld                    PABRID
N08  C                   kfld                    PAKUNR
N08  C                   kfld                    PAID
N08  C                   MOVE      BIBRID        PABRID
N08  C                   MOVE      *zeros        PAKUNR
      *
N01  C                   EVAL      WFMMKNR=1
N01  C                   EVAL      WFMLONR=1
     C     *LIKE         DEFINE    XFMMKNR       WFMMKNR
     C     *LIKE         DEFINE    XFMLONR       WFMLONR
     C     *LIKE         DEFINE    XFVRI         WFVRI
     C                   EVAL      WFVRI='F'
N01  C     UnNrKey       KLIST
N01  C                   KFLD                    FFUNNR
N01  C                   KFLD                    FFEMBG
N03  C                   KFLD                    FFINDX
      *
     C   50              eval      Error = 'Invalid userID'
N08  C  N50              OPEN      BRPARF
     C  N50              OPEN      DOKUFV
     C  N50              OPEN      DOKUFVF
N07  C  N50              OPEN      ZOKUFV2
N07  C  N50              OPEN      ZOKUFVF2
N07  C  N50              OPEN      ZOKUFM2
N01  C  N50              OPEN      ADRUNNRF
     C  N50              OPEN      DOKUFM

     c                   endsr
      *
      *______________________________________________________
      * TEST                                             TEST
      *""""""""""""""""""""""""""""""""""""""""""""""""""""""
     C     TEST          BEGSR
S02  C*    WMODE         IFNE      'A'
S02  C*    DOKUFVK       CHAIN     DOKUFV                             55
S02  C*                  ENDIF
      * MODE A=Add  D=Delete  U=Update
     C                   SELECT
     C     WMODE         WHENEQ    'A'
N07  c     OPD           ifne      *zeros
N04  C     DOKUFVK       CHAIN(N)  DOKUFV                             55
N07  c                   else
N07  C     ZOKUFVK       CHAIN(N)  ZOKUFV2                            55
N07  c                   endif
N04  C                   if        *in55=*off or LIN = 0
N04  C                   eval      Error = 'Ugyldig linjenummer '
N04  C                             + '(tips gå ut /inn igjen)'
N04  C                   else
     C                   eval      Error = *blanks                              kun ved booking
N09  c                   if        OPD = *zeros
N08  c                   exsr      TstUsrKrav
N09  c                   endif
N08  C                   if        Error = *blanks
     C                   EXSR      SRA
N08  C                   endif
N04  C                   endif
     C     WMODE         WHENEQ    'D'
N07  c     OPD           ifne      *zeros
N02  C     DOKUFVK       CHAIN(N)  DOKUFV                             55
N07  c                   else
N07  C     ZOKUFVK       CHAIN(N)  ZOKUFV2                            55
N07  c                   endif
     C                   IF        *IN55=*ON
N02  C*                  eval      Error = 'AwbLine not found'
N02  C                   eval      Error = 'OrderLine not found'
     C                   ELSE
     C                   eval      Error = *blanks
S02  C*                  EXSR      SRD
N07  c     OPD           ifne      *zeros
N02  c                   call      'SYGE85'
N02  C                   parm                    XFVAVD
N02  C                   parm                    XFVOPD
N02  C                   parm                    XFVFBNR
N02  C                   parm                    XFVLINR
N07  C                   else
N07  c                   call      'SYGE85Z'
N07  C                   parm                    XFVUNIK
N07  C                   parm                    XFVFBNR
N07  C                   parm                    XFVLINR
N07  C                   endif
     C                   ENDIF
     C     WMODE         WHENEQ    'U'
N07  c     OPD           ifne      *zeros
N02  C     DOKUFVK       CHAIN     DOKUFV                             55
N07  c                   else
N07  C     ZOKUFVK       CHAIN     ZOKUFV2                            55
N07  c                   endif
     C                   IF        *IN55=*ON
S02  C*                  eval      Error = 'AwbLine not found'
N02  C                   eval      Error = 'OrderLine not found'
     C                   ELSE
     C                   eval      Error = *blanks
N08  c                   exsr      TstUsrKrav
N08  C                   if        Error = *blanks
     C                   EXSR      SRU
N08  C                   endif
     C                   ENDIF
     C                   OTHER
     C                   eval      Error = 'Wrong mode'
     C                   ENDSL
     C                   ENDSR
      *______________________________________________________
      * Add                                               SRA
      *""""""""""""""""""""""""""""""""""""""""""""""""""""""
     C     SRA           BEGSR
     C                   CLEAR                   DOKUFVR
N07  C                   CLEAR                   ZOKUFVR
     C                   EVAL      XFVRI='F'
     C                   EVAL      XFVAVD=AVD
     C                   EVAL      XFVOPD=OPD
     C                   EVAL      XFVFBNR=FBN
     C                   EVAL      XFVLINR=LIN

     C                   EVAL      XFVANT=FVANT
     C                   EVAL      XFVPAKN=FVPAKN
     C                   EVAL      XFVVT=FVVT
     C                   EVAL      XFVVKT=FVVKT
     C                   EVAL      XFVVOL=FVVOL
     C                   EVAL      XFVLM=FVLM
TMS  C                   EVAL      XFVLM2=FVLM2
TMS  C                   EVAL      XFVLEN=FVLEN
TMS  C                   EVAL      XFVBRD=FVBRD
TMS  C                   EVAL      XFVHOY=FVHOY

N07  c     OPD           ifne      *zeros
     C                   WRITE     DOKUFVR
N07  c                   else
N07  C                   EVAL      XFVREFF=REFF
N07  C                   EVAL      XFVUNIK=UNIK
N07  C                   WRITE     ZOKUFVR
N07  c                   endif
      * DOKUFM
N07  c     OPD           ifne      *zeros
     C     DOKUFMK       CHAIN     DOKUFM                             56
N07  c                   else
N07  C     ZOKUFMK       CHAIN     ZOKUFM2                            56
N07  c                   endif
     C                   IF        *IN56
     C                   CLEAR                   DOKUFMR
N07  C                   CLEAR                   ZOKUFMR
     C                   EVAL      XFMRI='F'
N01  C                   EVAL      XFMMKNR=1
N01  C                   EVAL      XFMLONR=1
     C                   EVAL      XFMAVD=AVD
     C                   EVAL      XFMOPD=OPD
     C                   EVAL      XFMFBNR=FBN
     C                   EVAL      XFMLINR=LIN
     C                   EVAL      XFMMRK1=FMMRK1
N07  c     OPD           ifne      *zeros
     C                   WRITE     DOKUFMR
N07  c                   else
N07  C                   EVAL      XFMREFF=REFF
N07  C                   EVAL      XFMUNIK=UNIK
N07  C                   WRITE     ZOKUFMR
N07  c                   endif
     C                   ELSE
     C                   EVAL      XFMMRK1=FMMRK1
N07  c     OPD           ifne      *zeros
     C                   UPDATE    DOKUFMR
N07  c                   else
N07  C                   UPDATE    ZOKUFMR
N07  c                   endif
     C                   ENDIF
      * DOKUFVF
      *  - farlig gods - skapes/endres dersom FFUNR ulik blank / slettes hvis lik
N01  C                   IF        FFUNNR <> *blanks
N01  C     UnNrKey       chain     ADRUNNRF                           33
N01  C                   ENDIF
N07  c     OPD           ifne      *zeros
     C     DOKUFVFK      CHAIN     DOKUFVF                            56
N07  c                   else
N07  C     ZOKUFVFK      CHAIN     ZOKUFVF2                           56
N07  c                   endif
     C                   IF        *IN56
N01  C                   IF        FFUNNR <> *blanks
     C                   CLEAR                   DOKUFVFR
N07  C                   CLEAR                   ZOKUFVFR
     C                   EVAL      XFFAVD=AVD
     C                   EVAL      XFFOPD=OPD
     C                   EVAL      XFFFBNR=FBN
     C                   EVAL      XFFLINR=LIN
N01  C                   EVAL      XFFLIN2=1
     C                   EVAL      XFFUNNR=FFUNNR
     C                   EVAL      XFFEMBG=FFEMBG
N03  C                   EVAL      XFFINDX=FFINDX
s01  C                   EVAL      XFFKLAS=ADKLAS
s01  C                   EVAL      XFFSEDD=ADSEDD
