             PGM
             DCL        VAR(&AVD) TYPE(*CHAR) LEN(4)
             CRTDTAARA  DTAARA(SYSPED/INNUT) TYPE(*CHAR) LEN(512)
             MONMSG     MSGID(CPF1023)
             RTVDTAARA  DTAARA(INNUT (15 4)) RTNVAR(&AVD)
             IF         COND(&AVD *EQ '    ') THEN(CHGDTAARA +
                          DTAARA(INNUT (15 4)) VALUE('0000'))
             CALL       PGM(SYFA00)
             ENDPGM
