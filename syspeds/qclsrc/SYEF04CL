             PGM
             DCL        VAR(&KUND) TYPE(*DEC) LEN(8 0)
             DCL        VAR(&LAND) TYPE(*CHAR) LEN(2)
             DCL        VAR(&STED) TYPE(*CHAR) LEN(5)
             DCL        VAR(&FLAGG) TYPE(*DEC) LEN(1 0) VALUE(1)
             DCL        VAR(&DELSY) TYPE(*CHAR) LEN(1)
                                                          /**/
             CALL       PGM(SYEF04) PARM(&KUND &LAND &STED &FLAGG +
                          &DELSY)
SLUTT:       ENDPGM
