             PGM
             CHKOBJ     OBJ(QSYS/MMAIL) OBJTYPE(*LIB) /* Test om +
                          MMAIL er installert. */
             MONMSG     MSGID(CPF9801 CPF9810) EXEC(DO)
             RSTLIB     SAVLIB(MMAIL) DEV(*SAVF) SAVF(SYSPEDV8/MMAIL)
             STRREXPRC  SRCMBR(INSTALL) SRCFILE(MMAIL/QREXSRC)
             ENDDO

             ENDPGM
