     FKODTP     IF   E           K DISK
     C     *ENTRY        PLIST
     C     WAVD          PARM                    AVD               4 0
     C     WTSKNR        PARM                    UTSKNR            3 0
     C                   PARM                    KOPNVN
     C                   PARM                    KOPTY
     C                   PARM                    KOPHEA
     C                   PARM                    KOPFM
     C*
     C     KEY           KLIST
     C                   KFLD                    KOPUNI
     C                   KFLD                    WAVD
     C                   KFLD                    WTSKNR
     C*
     C     *LIKE         DEFINE    AVD           WAVD
     C     *LIKE         DEFINE    UTSKNR        WTSKNR
     C                   MOVE      'P'           KOPUNI
     C*
     C     KEY           CHAIN     KODTP                              33
     C     *IN33         IFEQ      *OFF
     C     KOPNVN        IFEQ      *BLANKS
     C                   MOVEL     'QPRINT '     KOPNVN
     C                   ENDIF
     C     KOPTY         IFEQ      *BLANKS
     C                   MOVEL     '*STD   '     KOPTY
     C                   ENDIF
     C     KOPHEA        IFEQ      ' '
     C                   MOVE      'N'           KOPHEA
     C                   ENDIF
     C                   ELSE
     C                   MOVEL     'QPRINT '     KOPNVN
     C                   MOVEL     '*STD   '     KOPTY
     C                   MOVE      'N'           KOPHEA
     C                   MOVE      *BLANKS       KOPFM
     C                   ENDIF
     C                   RETURN
