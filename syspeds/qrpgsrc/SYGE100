     H DFTACTGRP(*NO)
     ‚*  Prototype for syge100
     d syge100         pr
     d ua_                                 like(ua)
     d ugs1_                               like(ugs1)
     ‚*  *ENTRY Interface for Main Procedure
     d syge100         pi
     d ua                            35
     d ugs1                           1

     d                 ds
     d a                             35
     d  a2                            2    overlay(a)
     d  a18                           1    overlay(a:18)
     d  a19                           1    overlay(a:19)
     d  a20                           1    overlay(a:20)
     d  a21                           1    overlay(a:21)
     d  af20                         20    overlay(a)
     d   af18                        18    overlay(af20)

     d  pos            s              2  0

      /free
       a=ua;
       ugs1=*blanks;

       pos = %check( '0123456789' : af18 );
       if pos > *zero;
         ugs1='X';
       endif;

       if a19 <> *blanks and a20 <> *blanks;
         pos = %check( '0123456789' : af20 );
           if pos > *zero;
             ugs1='X';
           endif;
       endif;

       if a2 < '00';
         ugs1='X';
       endif;

       if a21 > *blanks;
         ugs1='X';
       endif;

       if a2 > '00' and a19 <> *blanks;
         ugs1='X';
       endif;

       if a2 = '00' and a20 = *blanks;
         ugs1='X';
       endif;

       if a18 = *blanks;
         ugs1='X';
       endif;

       *inlr = *on;
      /end-free
