N21   ***********************************************
N21  C     SR8AVG        BEGSR
N21   ***********************************************
N21  C                   EVAL      WSSPLITT = 'N'
N21  C                   MOVE      *BLANKS       AVG
N21  C                   MOVE      *ZEROS        ALI
N21  C                   MOVE      'N'           X8AVG             1
N21  C                   MOVE      'N'           X8AVG2            1
N21  C                   MOVE      'N'           X8TL              1
N21  C                   MOVE      'N'           X8MV              1
N21  C     SADHK         SETLL     SADV
N21  C     SADHK         READE(N)  SADV                                   33
N21  C                   DOW       *IN33 = *OFF
N21  C*                  IF        SVVNT >= 20090000 AND SVVNT <= 22099999
N21  C                   IF        SVTN = 'S' AND
N21  C                             (SVKDAÆ = 'GG' OR SVKDAÆ = 'MG')
N21  C                   MOVE      'J'           X8AVG2            1
N21  C     1             DO        5000          XN3               5 0
N21  C                   SELECT
N21  C                   WHEN      ALI(XN3) = 0
N21  C                   IF        SVREFL <= 0
N21  C                   EVAL      ALI(XN3) = SVLI
N21  C                   ELSE
N21  C                   EVAL      ALI(XN3) = SVREFL
N21  C                   END
N21  C                   LEAVE
N21  C                   WHEN      SVREFL <= 0
N21  C                   IF        ALI(XN3) = SVLI
N21  C                   LEAVE
N21  C                   END
N21  C                   OTHER
N21  C                   IF        ALI(XN3) = SVREFL
N21  C                   LEAVE
N21  C                   END
N21  C                   ENDSL
N21  C                   ENDDO
N21  C                   END
N21  C                   IF        SVPVA <> 'F'
N21  C                   EVAL      X8TL = 'J'
N21  C                   END
N21  C                   IF        SVMFR <> 'F'
N21  C                   EVAL      X8MV = 'J'
N21  C                   END
N21  C                   IF        SVKDAÆ <> *BLANKS
N21  C     1             DO        9             XN
N21  C                   SELECT
N21  C                   WHEN      AVG(XN) = *BLANKS
N21  C                   EVAL      AVG(XN) = SVKDAÆ
N21  C                   LEAVE
N21  C                   WHEN      AVG(XN) = SVKDAÆ
N21  C                   LEAVE
N21  C                   WHEN      XN >= 6 AND X8TL = 'J' AND X8MV = 'J'
N21  C                   EVAL      X8AVG = 'J'
N21  C                   LEAVE
N21  C                   WHEN      (XN >= 7 AND X8TL = 'J') OR
N21  C                             (XN >= 7 AND X8MV = 'J')
N21  C                   EVAL      X8AVG = 'J'
N21  C                   LEAVE
N21  C                   WHEN      XN >= 8
N21  C                   EVAL      X8AVG = 'J'
N21  C                   LEAVE
N21  C                   ENDSL
N21  C                   ENDDO
N21  C                   END
N21  C     SADHK         READE(N)  SADV                                   33
N21  C                   ENDDO
N21   *
N21  C                   IF        X8AVG = 'J' AND X8AVG2 = 'J'
N21  C                   EVAL      WSSPLITT = 'N'
N21  C                   EXFMT     SPLITT
N21  C                   IF        WSSPLITT = 'J'
N21  C                   EXSR      SRSPLITT
N21  C                   END
N21  C                   END
N21   *
N21  C                   ENDSR
N21   *
N21   ***********************************************
N21  C     SRSPLITT      BEGSR
N21   ***********************************************
N21  C     UAVD          CHAIN     STANDI                             33
N21  C                   MOVE      SITDN         XTDN              7 0
N21  C                   ADD       1             SITDN
N21  C                   UPDATE    STANDIR
S29  C*                  MOVE      *ZEROS        XVKT              9 0
N29  C                   MOVE      *ZEROS        XVKT             12 3
N21  C                   MOVE      *ZEROS        XBL1             13 2
N21  C                   MOVE      *ZEROS        XBL2             13 2
N21  C                   MOVE      *ZEROS        XBL3             13 2
N21   * SPLITT LINJER
N21  C     SADHK         SETLL     SADV
N21  C     SADHK         READE     SADV                                   33
N21  C                   DOW       *IN33 = *OFF
N21  C     1             DO        5000          XN3
N21  C                   IF        ALI(XN) = 0
N21  C                   MOVE      'N'           XSPLITT           1
N21  C                   LEAVE
N21  C                   END
N21  C                   IF        SVREFL <= 0
N21  C                   IF        ALI(XN3) = SVLI
N21  C                   MOVE      'J'           XSPLITT           1
N21  C                   LEAVE
N21  C                   END
N21  C                   ELSE
N21  C                   IF        ALI(XN3) = SVREFL
N21  C                   MOVE      'J'           XSPLITT           1
N21  C                   LEAVE
N21  C                   END
N21  C                   END
N21  C                   ENDDO
N21  C                   IF        XSPLITT = 'J'
N21  C                   EVAL      XVKT = XVKT + SVVKTB
N21  C                   EVAL      XBL3 = XBL3 + SVBELT
N21  C                   DELETE    SADVR
N21  C                   EVAL      SVTDN = XTDN
N21  C                   WRITE     SADVR
N21  C                   ELSE
N21  C                   UPDATE    SADVR
N21  C                   END
N21  C     SADHK         READE     SADV                                   33
N21  C                   ENDDO
N21   * OPPDATER HEADING
N21  C     SADHK         CHAIN     SADH                               33
N21  C                   IF        XVKT >= SIVKB
N21  C                   EVAL      XVKT = SIVKB - 1
N21  C                   END
N21  C                   EVAL      XBL1 = XVKT / SIVKB * SIBEL1
N21  C                   EVAL      XBL2 = XBL3 / SIBEL3 * SIBEL2
N21  C                   EVAL      SIBEL1 = SIBEL1 - XBL1
N21  C                   EVAL      SIBEL2 = SIBEL2 - XBL2
N21  C                   EVAL      SIBEL3 = SIBEL3 - XBL3
N21  C                   EVAL      SIVKB  = SIVKB  - XVKT
N21  C                   EVAL      SINTK  = SINTK  - 1
N21  C                   EVAL      SIST   = ' '
N21  C                   EVAL      SIFT3  = 'Oppdraget er splittet til '
N21  C                             + %TRIM(%EDITC(SIAVD:'Z')) + '/'
N21  C                             + %TRIM(%EDITC(XTDN:'Z'))
N21  C                   UPDATE    SADHR
N21  C                   EVAL      SIFT3  = 'Oppdraget er splittet fra '
N21  C                             + %TRIM(%EDITC(SIAVD:'Z')) + '/'
N21  C                             + %TRIM(%EDITC(SITDN:'Z'))
N21  C                   EVAL      SITDN  = XTDN
N21  C                   EVAL      SIBEL1 = XBL1
N21  C                   EVAL      SIBEL2 = XBL2
N21  C                   EVAL      SIBEL3 = XBL3
N21  C                   EVAL      SIVKB  = XVKT
N21  C                   EVAL      SINTK  = 1
N21  C                   WRITE     SADHR
N21   * OPPDATER RUBR. 28
N21  C     SADHK         SETLL     SADF
N21  C     SADHK         READE     SADF                                   33
N21  C                   DOW       *IN33 = *OFF
N21  C                   EVAL      SFOPDN = XTDN
N21  C                   WRITE     SADFR
N21  C     SADHK         READE     SADF                                   33
N21  C                   ENDDO
N21   *
N21  C                   EVAL      UFLAGG = 5
N21   *
N21  C                   ENDSR
N21   *
