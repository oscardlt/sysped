             PGM
          /*___________________*/
          /* Declare variables */
          /*"""""""""""""""""""*/
             DCLF       FILE(EDIFRCF)
             DCL        VAR(&ODOBNM) TYPE(*CHAR) LEN(10)
             DCL        VAR(&ODOBTP) TYPE(*CHAR) LEN(8)
             DCL        VAR(&ODOBAT) TYPE(*CHAR) LEN(10)
             DCL        VAR(&FILIBF) TYPE(*CHAR) LEN(10)
             DCL        VAR(&FILIBO) TYPE(*CHAR) LEN(10)
          /*______________________*/
          /* HENT FILBIBLIOTEKER  */
          /*""""""""""""""""""""""*/
             RTVDTAARA  DTAARA(EDIDTA (171 10)) RTNVAR(&FILIBF)
             RTVDTAARA  DTAARA(EDIDTA (181 10)) RTNVAR(&FILIBO)
          /*___________________________________________________*/
          /* Retreive Physical files in *libl who start with E*/
          /*"""""""""""""""""""""""""""""""""""""""""""""""""""*/
             DSPOBJD    OBJ(&FILIBF/E*) OBJTYPE(*FILE) +
                          OUTPUT(*OUTFILE) OUTFILE(EDIFRCF)
          /*_______________*/
          /* Read workfile */
          /*"""""""""""""""*/
 READ:       RCVF       RCDFMT(QLIDOBJD)
             MONMSG     MSGID(CPF0000) EXEC(GOTO CMDLBL(EXIT))
          /*_________________*/
          /* Reorganize file */
          /*"""""""""""""""""*/
             IF         COND(&ODOBTP *NE '*FILE   ') THEN(GOTO +
                          CMDLBL(READ))
             IF         COND(&ODOBAT *NE 'PF        ') THEN(GOTO +
                          CMDLBL(READ))
             CHGPF      FILE(&ODOBNM) FRCRATIO(1)
             MONMSG     MSGID(CPF0000)
             GOTO READ
 EXIT:       ENDPGM
