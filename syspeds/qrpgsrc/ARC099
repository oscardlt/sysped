     c     key           setll     arkivl03
     c     *in34         doweq     '0'
     c     key           reade     arkivl03                               34
     c     *in34         ifeq      '0'
     c                   exsr      genbane
     c                   call      'SYGE98CL'
     C                   PARM                    ubane
     C                   PARM                    nyobj
     c                   parm                    fins              1
     c     *inof         ifeq      '1'
     c                   except    head
     c                   move      '0'           *inof
     c                   end
     c     fins          ifeq      'J'
     c                   add       1             antfin            9 0
     c                   else
     c                   add       1             antman            9 0
     c                   end
     c                   except    det
     c                   exsr      srsj
     c                   exsr      srsk
     c                   end
     c                   end
     c     *inof         ifeq      '1'
     c                   except    head
     c                   move      '0'           *inof
     c                   end
     c                   except    tot
     c                   seton                                        lr
     c                   return
