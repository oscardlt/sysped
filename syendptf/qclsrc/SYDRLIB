             PGM        PARM(&SYSBS)
             DCL        VAR(&SYSBS) TYPE(*CHAR) LEN(10)
             DCL        VAR(&SYSBS3) TYPE(*CHAR) LEN(3)
             DCL        VAR(&LIB) TYPE(*CHAR) LEN(10)
             CHGVAR     VAR(&SYSBS3) VALUE(%SUBSTRING(&SYSBS 7 3))
             CHGVAR     VAR(&LIB) VALUE(&SYSBS3 *TCAT 'SYEND')
             CALL       PGM(&LIB/INLIBL)
             ENDPGM
