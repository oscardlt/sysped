     C***********************************************
     C     MOVWST        BEGSR
     C***********************************************
     C                   MOVE      YKUNIK        TKUNIK
     C                   MOVE      WSKODE        TKKODE
     C                   IF        WSUNIK = 17
     C                   EVAL      TKTXTN = WSTXTN22
     C                   EVAL      TKTXTE = WSTXTE22
     C                   ELSE
     C                   MOVE      WSTXTN        TKTXTN
     C                   MOVE      WSTXTE        TKTXTE
     C                   END
     C                   IF        *IN28
     C                   MOVE      WSAVG         TKAVG
     C                   MOVE      WSANK         TKANK
     C                   MOVE      WSTRS         TKTRS
     C                   ELSE
     C                   MOVE      ' '           TKAVG
     C                   MOVE      ' '           TKANK
     C                   MOVE      ' '           TKTRS
     C                   END
      *
     C                   ENDSR
      *
     C***********************************************
     C     MOVTWS        BEGSR
     C***********************************************
     C                   MOVE      TKKODE        WSKODE
