             PGM        PARM(&BNR &BNR2 &FLAGG &VIS)
             DCL        VAR(&BNR) TYPE(*DEC) LEN(7 0)
             DCL        VAR(&BNR2) TYPE(*DEC) LEN(7 0)
             DCL        VAR(&FLAGG) TYPE(*DEC) LEN(1 0)
             DCL        VAR(&VIS) TYPE(*CHAR) LEN(1)
             OVRDBF     FILE(KOSTBUPD) TOFILE(KOSTB)
             CALL       PGM(SYKS03L1) PARM(&BNR &BNR2 &FLAGG &VIS)
             ENDPGM
