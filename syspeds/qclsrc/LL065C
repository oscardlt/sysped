             PGM
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1)
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10)
             DCL        VAR(&OK) TYPE(*CHAR) LEN(1)
             DCL        VAR(&GRUP) TYPE(*CHAR) LEN(8)
             DCL        VAR(&PNR) TYPE(*CHAR) LEN(8)
             DCL        VAR(&LT) TYPE(*CHAR) LEN(50)

             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CALL       PGM(SYFA00) PARM(&IN01)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT))

             RTVJOBA    OUTQ(&PRT)
             CALL       PGM(LL065R) PARM(&PRT &OK &GRUP &PNR &LT)
             IF         COND(&OK = 'N') THEN(GOTO CMDLBL(SLUTT))

             CALL       PGM(LL066C) PARM(&PRT &GRUP &PNR &LT)

 SLUTT:      ENDPGM
