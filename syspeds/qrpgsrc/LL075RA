     C     DORD9K        KLIST
     C                   KFLD                    UFIRM
     C                   KFLD                    OTBKOD
     C                   KFLD                    XAAR
     C                   KFLD                    XPSDN
     C     VARE1K        KLIST
     C                   KFLD                    UFIRM
     C                   KFLD                    VARENR
      *
     C                   MOVE      UAAR          XAAR
     C                   MOVE      UPSDN         XPSDN
     C                   MOVE      'O'           OTBKOD
     C                   WRITE     WINDOWSZ
      *
     C                   ENDSR
      *
      *////////////////////////////////////////////////////////////
     C     HOVED         BEGSR
      *////////////////////////////////////////////////////////////
     C     START         TAG
     C                   MOVE      '1'           *IN93
     C                   WRITE     SUBCTL
     C                   MOVEA     '00'          *IN(93)
     C                   Z-ADD     *ZEROS        ZRECNO
     C     DORD9K        SETLL     DORDL09
     C     DORD9K        READE     DORDL09                                94
     C     *IN33         DOWEQ     '0'
     C     VARE1K        CHAIN     VAREL01                            33
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
     C     DORD9K        READE     DORDL09                                94
     C  N94              ENDDO
      *
     C     START3        TAG
     C                   MOVEA     '11'          *IN(91)
     C     ZRECNO        IFEQ      0
     C                   EXFMT     NYWRK
     C                   ELSE
     C                   WRITE     SUBCTL
     C                   EXFMT     BUNN
     C                   END
