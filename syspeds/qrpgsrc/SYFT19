     C***********************************************
     C     TESTB1        BEGSR
     C***********************************************
     C*
     C* TEST OM BRUKER HAR TASTET LIKVIDITETSKODER
     C     WLIKOD        COMP      ' '                                    90
     C   90              MOVE      'SPL0009'     MSGNR
     C  N40
     CANN90              DO
     C     KODLIK        CHAIN     KODTLIK                            33
     C  N33              DO
     C                   UPDATE    KOLIKR
     C                   SETON                                        90
     C                   MOVE      'SPL0010'     MSGNR
     C                   END
     C                   END
     C   90              Z-ADD     17            POSNBR
     C   90              SETON                                        30
     C*
     C* TEST OM VARSLINGSTEKST = BLANK
     C     WLINAV        COMP      *BLANKS                                89
     C   89
     CANN30              MOVE      'SPN0013'     MSGNR
     C   89
     CANN30              Z-ADD     29            POSNBR
     C   89              SETON                                        30
     C*
     C* TEST OM BRUKER HAR TASTET STOPPEKODE
     C     WLISTO        COMP      ' '                                    88
     C   88
     CANN30              MOVE      'SPL0011'     MSGNR
     C   88
     CANN30              Z-ADD     65            POSNBR
     C   88              SETON                                        30
     C*
     C   30              GOTO      SLTB
