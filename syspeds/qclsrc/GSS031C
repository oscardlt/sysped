/* *********************************************************************   */
/*       * RETTINGER I DETTE PROGRAM:                                      */
/* PTFnr:*Sek Sig Vers  Beskrivelse..........................              */
/* """"""*"""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""    */
/* 10027 * 01 YBC PTF10 ORDRETYPE                                          */
/* ********************************************************************    */
/*S01        PGM        PARM(&TNO &TYPE &CUR &COMM &KNC)   */
/*N01*/      PGM        PARM(&TNO &TYPE &ORDTYP &CUR &COMM &KNC)
             DCL        VAR(&TNO) TYPE(*CHAR) LEN(11)
             DCL        VAR(&TYPE) TYPE(*CHAR) LEN(3)
/*N01*/      DCL        VAR(&ORDTYP) TYPE(*CHAR) LEN(1)
             DCL        VAR(&CUR) TYPE(*CHAR) LEN(3)
             DCL        VAR(&COMM) TYPE(*CHAR) LEN(1)
             DCL        VAR(&KNC) TYPE(*CHAR) LEN(8)
             DCL        VAR(&UTFAX) TYPE(*CHAR) LEN(1)
             DCL        VAR(&QJOB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QUSER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QNBR) TYPE(*CHAR) LEN(6)
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10)
             DCL        VAR(&CPY) TYPE(*CHAR) LEN(1)
             DCL        VAR(&LOC1) TYPE(*CHAR) LEN(5)
             DCL        VAR(&LOC2) TYPE(*CHAR) LEN(5)
             DCL        VAR(&LOC3) TYPE(*CHAR) LEN(5)
             DCL        VAR(&UNIK) TYPE(*CHAR) LEN(7)

             RTVJOBA    JOB(&QJOB) USER(&QUSER) NBR(&QNBR)
             CHGVAR     VAR(&UNIK) VALUE('W' *CAT &QNBR)
             RMVM       FILE(GSS031W) MBR(&UNIK)
             MONMSG     MSGID(CPF0000)
             ADDPFM     FILE(GSS031W) MBR(&UNIK)
             OVRDBF     FILE(GSS031W) TOFILE(GSS031W) MBR(&UNIK)
             CHGVAR     VAR(&UTFAX) VALUE('J')
/*S01        CALL       PGM(GSS031R) PARM(&TNO &TYPE &PRT &CUR &CPY +   */
/*S01                     &UTFAX &COMM &KNC &QJOB &QUSER &QNBR &LOC1 +  */
/*S01                     &LOC2 &LOC3)                                  */
/*N01*/      CALL       PGM(GSS031R) PARM(&TNO &TYPE &PRT &ORDTYP +
                          &CUR &CPY &UTFAX &COMM &KNC &QJOB &QUSER +
                          &QNBR &LOC1 &LOC2 &LOC3)
             RMVM       FILE(GSS031W) MBR(&UNIK)
             MONMSG     MSGID(CPF0000)
             ENDPGM
