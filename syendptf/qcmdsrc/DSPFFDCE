             CMD        PROMPT('DSP Short File Desc')
             PARM       KWD(FILE) TYPE(*CHAR) LEN(10) RSTD(*NO) +
                          PROMPT('File')
             PARM       KWD(LIB) TYPE(*CHAR) LEN(10) RSTD(*NO) +
                          DFT(*LIBL) PROMPT('Library')
             PARM       KWD(COLHDG) TYPE(*CHAR) LEN(1) RSTD(*NO) +
                          DFT(N) CHOICE('N=No Y=Yes') PROMPT('Prt +
                          ColHdg')
