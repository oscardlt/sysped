             CMD        PROMPT('Bygg logiske filer V12-ptf')
             PARM       KWD(TILLIB) TYPE(*CHAR) LEN(10) RSTD(*NO) +
                          DFT('SYSPEDFxx') PROMPT('Prod.bibliotek:')
             PARM       KWD(SRCLIB) TYPE(*CHAR) LEN(10) RSTD(*NO) +
                          DFT('SYENDPTF') PROMPT('Kildebibliotek:')
