/* KUN 1 JOBB AV GANGEN  */
             PGM        PARM(&AV &OP &TY &DA &LI)
             DCL        VAR(&AV) TYPE(*CHAR) LEN(4)
             DCL        VAR(&OP) TYPE(*CHAR) LEN(7)
             DCL        VAR(&TY) TYPE(*CHAR) LEN(2)
             DCL        VAR(&DA) TYPE(*CHAR) LEN(8)
             DCL        VAR(&LI) TYPE(*CHAR) LEN(40)
             SBMJOB     CMD(CALL PGM(ARC044) PARM(&AV &OP &TY &DA +
                          &LI)) JOB(ARKIV) JOBD(*USRPRF) +
                          JOBQ(SYJOBQARK)
 SLUTT:      ENDPGM
