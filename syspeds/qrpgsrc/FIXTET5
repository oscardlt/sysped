nt   FTRISTD    UF   E           K DISK
     FTRUSTD    UF   E           K DISK
      *
     C     *LOVAL        SETLL     TRISTD
     C                   READ      TRISTD                                 33
     C                   DOW       *IN33 = *OFF
     C                   IF        TITIN <> *BLANKS
     C                   MOVEL     TITIN         XAN02             2
     C                   IF        XAN02 <> 'NO'
EV5  C                   EVAL      TITIN = 'NO' + %TRIM(TITIN)
EV5  C                   END
EV5  C                   END
EV5  C                   UPDATE    TRISTDR
NV5  C                   READ      TRISTD                                 33
NV5  C                   ENDDO
NV5   *
NV5  C     *LOVAL        SETLL     TRUSTD
NV5  C                   READ      TRUSTD                                 33
     C                   DOW       *IN33 = *OFF
EV5  C                   IF        THTINA <> *BLANKS
EV5  C                   MOVEL     THTINA        XAN02             2
EV5  C                   IF        XAN02 <> 'NO'
EV5  C                   EVAL      THTINA = 'NO' + %TRIM(THTINA)
NV5  C                   END
NV5  C                   END
NV5  C                   IF        THTINS <> *BLANKS
NV5  C                   MOVEL     THTINS        XAN02             2
NV5  C                   IF        XAN02 <> 'NO'
     C                   EVAL      THTINS = 'NO' + %TRIM(THTINS)
     C                   END
     C                   END
     C                   IF        THTINK <> *BLANKS
     C                   MOVEL     THTINK        XAN02             2
     C                   IF        XAN02 <> 'NO'
     C                   EVAL      THTINK = 'NO' + %TRIM(THTINK)
     C                   END
     C                   END
     C                   UPDATE    TRUSTDR
     C                   READ      TRUSTD                                 33
     C                   ENDDO
      *
     C                   RETURN
     C                   EVAL      *INLR = *ON
