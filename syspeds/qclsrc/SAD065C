             PGM        PARM(&AVD &TDN &FIND &SSLAA)
             DCL        VAR(&AVD) TYPE(*DEC) LEN(4 0)
             DCL        VAR(&TDN) TYPE(*DEC) LEN(7 0)
             DCL        VAR(&FIND) TYPE(*CHAR) LEN(1)
             DCL        VAR(&SSLAA) TYPE(*CHAR) LEN(1)

             OVRDBF     FILE(SVEH) TOFILE(*LIBL/SVEH)
             OVRDBF     FILE(SVEF) TOFILE(*LIBL/SVEF)
             OVRDBF     FILE(SVEV) TOFILE(*LIBL/SVEV)

             CALL       PGM(SAD065R) PARM(&AVD &TDN &FIND &SSLAA)

             ENDPGM
