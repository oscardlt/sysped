             PGM
             DCL        VAR(&LIBFRA) TYPE(*CHAR) LEN(10) +
                          VALUE('SYSPEDF   ')
             DCL        VAR(&LIBTIL) TYPE(*CHAR) LEN(10) +
                          VALUE('SYSPEDF   ')
             DCL        VAR(&AVDFRA) TYPE(*CHAR) LEN(4) VALUE('0002')
             DCL        VAR(&AVDTIL) TYPE(*CHAR) LEN(4) VALUE('0001')
             DCL        VAR(&OPDFRA) TYPE(*CHAR) LEN(7) VALUE('7000034')
             DCL        VAR(&OPDTIL) TYPE(*CHAR) LEN(7) VALUE('0200000')
             DCL        VAR(&SIGN) TYPE(*CHAR) LEN(3) VALUE('CB ')

             CALL       PGM(FORT01C) PARM(&LIBFRA &LIBTIL &AVDFRA +
                          &AVDTIL &OPDFRA &OPDTIL &SIGN)


ENDPGM
