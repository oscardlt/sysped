     C                   MOVE      PFHT7         WSHT7
     C                   MOVE      PFPT1         WSPT1
     C                   MOVE      PFPT2         WSPT2
     C                   MOVE      PFPT3         WSPT3
     C                   MOVE      PFPT4         WSPT4
     C                   MOVE      PFPT5         WSPT5
     C                   MOVE      PFPT6         WSPT6
     C                   MOVE      PFPT7         WSPT7
     C                   MOVE      PFOT          WSOT
     C                   MOVE      PFFRAN        WSFRAN
     C                   ENDIF
     C     START         TAG
     C                   EXFMT     BILDE1
      **  AVSLUTT
     C     *IN03         IFEQ      *ON
     C                   GOTO      SLUTT
     C                   ENDIF                                                  <*IN03
