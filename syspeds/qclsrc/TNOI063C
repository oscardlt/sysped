             PGM
             DCL        VAR(&UNIK) TYPE(*CHAR) LEN(7)
             DCL        VAR(&NBR) TYPE(*CHAR) LEN(6)
             RTVJOBA    NBR(&NBR)
             CHGVAR     VAR(&UNIK) VALUE('W' *CAT &NBR)
             ADDPFM     FILE(SADAVGW) MBR(&UNIK)
             MONMSG     MSGID(CPF0000)
             CLRPFM     FILE(SADAVGW) MBR(&UNIK)
             MONMSG     MSGID(CPF0000)
             OVRDBF     FILE(SADAVGW) MBR(&UNIK) FRCRATIO(1) +
                          OVRSCOPE(*JOB)
