             PGM

 /* LAG LF -  SOURCE ENTEN I SYENDPTF ELLER I SYSPEDS */
             CRTLF      FILE(SYSPEDF/FAK99X) +
                          SRCFILE(SYENDPTF/QDDSSRC) LVLCHK(*NO)
             MONMSG     MSGID(CPF0000) EXEC(CRTLF +
                          FILE(SYSPEDF/FAK99X) +
                          SRCFILE(SYSPEDS/QDDSSRC) LVLCHK(*NO))

             CALL PGM(FIXFALI)

             DLTF       FILE(FAK99X)

             ENDPGM
