     FEDIMTVINF UF   E           K DISK
     d wmavd           S                   like(mavd)
     d wmtdn           S                   like(mtdn)
      /free

        read edimtvinf;
               dow not %eof;
                  if mavd = wmavd;
                  if mtdn = wmtdn;
                  delete edimtvinfr;
                  endif;
                  endif;
                  wmavd=mavd;
                  wmtdn=mtdn;
                  read edimtvinf;
               enddo;
         *inlr = *on;
      /end-free
