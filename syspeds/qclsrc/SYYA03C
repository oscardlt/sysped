             PGM
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1)
             DCL        VAR(&AVDA) TYPE(*CHAR) LEN(4)
             DCL        VAR(&AVD) TYPE(*DEC) LEN(4 0)
             DCL        VAR(&UTNR) TYPE(*DEC) LEN(3 0) VALUE(69)
             DCL        VAR(&DEV) TYPE(*CHAR) LEN(10)
             DCL        VAR(&FORMS) TYPE(*CHAR) LEN(10)
             DCL        VAR(&HEAD) TYPE(*CHAR) LEN(1)
             DCL        VAR(&KOPFM) TYPE(*CHAR) LEN(5)

             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CALL       PGM(SYFA00) PARM(*IN01)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT))

             RTVDTAARA  DTAARA(*LDA (15 4)) RTNVAR(&AVDA)
             CHGVAR     VAR(&AVD) VALUE(&AVDA)
             CALL       PGM(SYGE01) PARM(&AVD &UTNR &DEV &FORMS +
                          &HEAD &KOPFM)
             OVRPRTF    FILE(SYYA03P) PAGESIZE(70 120) CPI(15) +
                          OVRFLW(58) OUTQ(&DEV) FORMTYPE(&FORMS)
             OVRDBF     FILE(FRATFRA) TOFILE(FRATFRAW)
             CALL       PGM(SYYA03R)

 SLUTT:      ENDPGM
