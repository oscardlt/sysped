             PGM
             DCL        VAR(&MODE) TYPE(*CHAR) LEN(1)
             DCL        VAR(&UNNR) TYPE(*DEC) LEN(4 0)
             DCL        VAR(&LOP) TYPE(*DEC) LEN(3 0)
    /* */
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CALL       PGM(GSS078R) PARM(&MODE &UNNR &LOP)
    /* REORTGANISER LOPENUMMER  */
             CALL       PGM(GSS081R)
    /* */
             ENDPGM
