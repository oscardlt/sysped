             CMD        PROMPT('Fjerne EDI MSG-LOGG')
             PARM       KWD(WP1) TYPE(*CHAR) LEN(1) DFT(Y) +
                          CHOICE('Y, N') PROMPT('Slette hvis ikke i +
                          SAD ?')
             PARM       KWD(WP2) TYPE(*CHAR) LEN(1) DFT(N) +
                          CHOICE('Y, N') PROMPT('Slette t.o.m. dato +
                          uansett ?')
             PARM       KWD(WP3) TYPE(*CHAR) LEN(8) CHOICE(CCYYMMDD) +
                          PROMPT('Til og med dato')
