             PGM        PARM(&FIRM &ALN &KOPI &PRT &NR &TOT &LAS &STR &EPOST)
             DCL        VAR(&FIRM) TYPE(*CHAR) LEN(2)
             DCL        VAR(&ALN) TYPE(*CHAR) LEN(7)
             DCL        VAR(&KOPI) TYPE(*CHAR) LEN(1)
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10)
             DCL        VAR(&NR) TYPE(*CHAR) LEN(1)
             DCL        VAR(&TOT) TYPE(*CHAR) LEN(1)
             DCL        VAR(&LAS) TYPE(*CHAR) LEN(1)
             DCL        VAR(&STR) TYPE(*CHAR) LEN(5)
             DCL        VAR(&EPOST) TYPE(*CHAR) LEN(70)
             DCL        VAR(&QUSER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QJOB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QNBR) TYPE(*CHAR) LEN(6)
/*                         */
/*    LASER                */
/*                         */
             IF         COND(&LAS = 'A') THEN(DO)
             OVRPRTF    FILE(LL014P) PAGESIZE(70 120) CPI(10) +
                          OVRFLW(62) +
                          OUTQ(&PRT)
             ENDDO
             IF         COND(&LAS = 'J') THEN(DO)
             OVRPRTF    FILE(LL014P) PAGESIZE(70 120) CPI(15) +
                          OVRFLW(62) +
                          OUTQ(&PRT)
             ENDDO
/*                         */
/*    IKKE LASER           */
/*                         */
             IF         COND(&LAS = ' ') THEN(DO)
             OVRPRTF    FILE(LL014P) PAGESIZE(72 120) CPI(15) +
                          OVRFLW(64) PRTQLTY(*DRAFT) OUTQ(&PRT)
             ENDDO

/*                                                  */
             CALL       PGM(LL014R) PARM(&FIRM &ALN &KOPI &NR &TOT +
                        &LAS &STR &EPOST &QUSER &QJOB &QNBR)
             ENDPGM
