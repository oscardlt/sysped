n14   * vis google cloud benyttes - send det i skyen med en gang
n14  C                   read      FIRMGOOG
n14  C                   if        not %eof and GOBUID <> *blanks
n14  c                   eval      Companyid = GOBUID
n14  c                   eval      Uokmov    = 'N'
n13  c                   eval      Udelete   = 'J'
n14  c                   call      'ARC99WSU1'
n14  c                   parm                    Companyid        10
n14  c                   parm                    baneTIL         100
n14  c                   parm                    dokTIL          120
n13  c                   parm                    udelete           1
n13  c                   parm                    NEWDOC           60
n14  c                   parm                    uokmov            1
n14  c                   endif
     C                   ENDSR
