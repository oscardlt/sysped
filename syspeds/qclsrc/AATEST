             PGM        PARM(&DEV &JOBPR)
             DCL        VAR(&DEV)   TYPE(*CHAR) LEN(10)
             DCL        VAR(&JOBPR) TYPE(*CHAR) LEN(2)
             DCL        VAR(&LIBD) TYPE(*CHAR) LEN(10) +
                          VALUE('SYEDID    ')
             DCL        VAR(&LIB)   TYPE(*CHAR) LEN(10)
             DCL        VAR(&QYEAR)  TYPE(*CHAR) LEN(2)
             DCL        VAR(&QMONTH) TYPE(*CHAR) LEN(2)
             DCL        VAR(&QDAY)   TYPE(*CHAR) LEN(2)
             DCL        VAR(&FIL1)   TYPE(*CHAR) LEN(10)
             DCL        VAR(&FIL2)   TYPE(*CHAR) LEN(10)
             DCL        VAR(&FIL3)   TYPE(*CHAR) LEN(10)
             DCL        VAR(&FIL4)   TYPE(*CHAR) LEN(10)
             DCL        VAR(&FIL5)   TYPE(*CHAR) LEN(10)
             DCL        VAR(&FIL6)   TYPE(*CHAR) LEN(10)

             CHGJOB     RUNPTY(&JOBPR) LOG(4 99 *SECLVL) LOGCLPGM(*YES)

             RTVDTAARA  DTAARA(EDIDTA (181 10)) RTNVAR(&LIB)

             RTVSYSVAL  SYSVAL(QYEAR)  RTNVAR(&QYEAR)
             RTVSYSVAL  SYSVAL(QMONTH) RTNVAR(&QMONTH)
             RTVSYSVAL  SYSVAL(QDAY)   RTNVAR(&QDAY)

             CHGVAR     VAR(&FIL1) VALUE('E001' *CAT &QYEAR *CAT +
                          &QMONTH *CAT &QDAY)

             CHGVAR     VAR(&FIL2) VALUE('E002' *CAT &QYEAR *CAT +
                          &QMONTH *CAT &QDAY)

             CHGVAR     VAR(&FIL3) VALUE('E003' *CAT &QYEAR *CAT +
                          &QMONTH *CAT &QDAY)

             CHGVAR     VAR(&FIL4) VALUE('E004' *CAT &QYEAR *CAT +
                          &QMONTH *CAT &QDAY)

             CHGVAR     VAR(&FIL5) VALUE('E005' *CAT &QYEAR *CAT +
                          &QMONTH *CAT &QDAY)

             CHGVAR     VAR(&FIL6) VALUE('E006' *CAT &QYEAR *CAT +
                          &QMONTH *CAT &QDAY)

 /* FLYTTER EDIS */

             MOVOBJ     OBJ(&LIB/EDIS) OBJTYPE(*FILE) TOLIB(&LIBD)
             RNMOBJ     OBJ(&LIBD/EDIS) OBJTYPE(*FILE) NEWOBJ(&FIL1)
             CRTDUPOBJ  OBJ(&FIL1) FROMLIB(&LIBD) OBJTYPE(*FILE) +
                          TOLIB(&LIB) NEWOBJ(EDIS)

 /* FLYTTER EDIM */

             MOVOBJ     OBJ(&LIB/EDIM) OBJTYPE(*FILE) TOLIB(&LIBD)
             RNMOBJ     OBJ(&LIBD/EDIM) OBJTYPE(*FILE) NEWOBJ(&FIL2)
             CRTDUPOBJ  OBJ(&FIL2) FROMLIB(&LIBD) OBJTYPE(*FILE) +
                          TOLIB(&LIB) NEWOBJ(EDIM)

 /* FLYTTER EDIRE*/

             MOVOBJ     OBJ(&LIB/EDIRE) OBJTYPE(*FILE) TOLIB(&LIBD)
             RNMOBJ     OBJ(&LIBD/EDIRE) OBJTYPE(*FILE) NEWOBJ(&FIL3)
             CPYF       FROMFILE(&LIBD/&FIL3) TOFILE(&LIB/EDIRE) +
                          CRTFILE(*YES)
             RMVM       FILE(&LIB/EDIRE) MBR(*ALL)
             MONMSG     MSGID(CPF0000)

 /* FLYTTER EDIRE1K */

             MOVOBJ     OBJ(&LIB/EDIRE1K) OBJTYPE(*FILE) TOLIB(&LIBD)
             RNMOBJ     OBJ(&LIBD/EDIRE1K) OBJTYPE(*FILE) NEWOBJ(&FIL4)
             CPYF       FROMFILE(&LIBD/&FIL4) TOFILE(&LIB/EDIRE1K) +
                          CRTFILE(*YES)
             RMVM       FILE(&LIB/EDIRE1K) MBR(*ALL)
             MONMSG     MSGID(CPF0000)

 /* FLYTTER EDISE*/

             MOVOBJ     OBJ(&LIB/EDISE) OBJTYPE(*FILE) TOLIB(&LIBD)
             RNMOBJ     OBJ(&LIBD/EDISE) OBJTYPE(*FILE) NEWOBJ(&FIL5)
             CPYF       FROMFILE(&LIBD/&FIL5) TOFILE(&LIB/EDISE) +
                          CRTFILE(*YES)
             RMVM       FILE(&LIB/EDISE) MBR(*ALL)
             MONMSG     MSGID(CPF0000)


 /* FLYTTER EDISE1K */

             MOVOBJ     OBJ(&LIB/EDISE1K) OBJTYPE(*FILE) TOLIB(&LIBD)
             RNMOBJ     OBJ(&LIBD/EDISE1K) OBJTYPE(*FILE) NEWOBJ(&FIL6)
             CPYF       FROMFILE(&LIBD/&FIL6) TOFILE(&LIB/EDISE1K) +
                          CRTFILE(*YES)
             RMVM       FILE(&LIB/EDISE1K) MBR(*ALL)
             MONMSG     MSGID(CPF0000)


             OVRDBF     FILE(EDISOLD) TOFILE(&LIBD/&FIL1)
             CALL       PGM(EDISAVLD)

             ENDPGM
