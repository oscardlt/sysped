     C     W2NR          IFNE      0                                            1<-B
     C     W2PRO         ORNE      0
     C     W2UUID        ORNE      *BLANKS
     C     W2ATDD        ORNE      0
     C     W2ST          ORNE      *BLANKS
     C     W2DTR         ORNE      0
     C     W2NR          IFEQ      0                                             2<-B
     C                   EXSR      NYSØK
     C                   GOTO      START
     C                   ELSE                                                    2
     C     W2NR          COMP      1                                      06
     C   06              DO                                                       3<-B
     C     W2AVD         IFNE      0                                               4<-B
     C                   MOVE      W2AVD         WSAVD
     C                   END                                                       4<-E
     C                   MOVE      *ZEROS        W2NR
     C                   MOVE      *ZEROS        W2AVD
     C                   GOTO      START4
     C                   ENDDO                                                    3<-E
     C     W2PRO         IFEQ      0                                              3<-B
     C                   SETON                                        9830
     C                   MOVE      'YBC2034'     MSGNR
     C                   ELSE                                                     3
     C                   EXSR      NYTUR
     C                   END                                                      3<-E
     C                   MOVE      *ZEROS        W2PRO
     C                   MOVE      *BLANKS       W2UUID           36
     C                   MOVE      *ZEROS        W2ATDD
     C                   MOVE      *BLANKS       W2ST
     C                   MOVE      *ZEROS        W2DTR
     C                   GOTO      START3
     C                   END                                                     2<-E
     C                   END                                                    1<-E
