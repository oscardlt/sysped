     C***********************************************
     C     HENTST        BEGSR
     C***********************************************
     C     PRIKU         SETLL     AVFRD
     C     PRIKU         READE(N)  AVFRD                                  33
     C  N33              GOTO      UTSR
     C*
     C*   HENTER STANDARDVERDIER
     C*
     C*
     C                   MOVE      UFAVDE        UUAVDE
     C                   MOVE      AFPROD        UUPROD
      *
     C     CUSTKU        SETLL     CUSTT
     C     CUSTKU        READE     CUSTT                                  33
     C  N33CUSTKU        SETLL     CUSTT
     C   33              DO
     C                   MOVE      *ZEROS        UUAVDE
     C     CUSTKU        SETLL     CUSTT
     C     CUSTKU        READE     CUSTT                                  33
     C  N33CUSTKU        SETLL     CUSTT
     C                   END
     C   33              DO
     C                   MOVE      *BLANKS       UUPROD
     C     CUSTKU        SETLL     CUSTT
     C                   END
     C     STARTH        TAG
     C     CUSTKU        READE     CUSTT                                  33
     C  N33              DO
     C                   MOVE      PTVEKT        AFVEKT
     C                   MOVE      PTPRPR        AFPRPR
     C                   MOVE      PTADJU        AFADJU
     C                   MOVE      NUMTYP        AFCOBE
     C     PRISK2        CHAIN(N)  AVFRD                              34
     C   34              WRITE     AVFRDR
     C                   GOTO      STARTH
     C                   END
     C*
     C     UTSR          ENDSR
