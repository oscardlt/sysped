     FHEAD8     IF   E           K DISK
     FSYFF53FA  CF   E             WORKSTN
     F                                     SFILE(SUBFIL:ZRECNO)
     D                 DS
     D  HEPOS1                 1      7
     D  WSPOS1                 1      4
     D                 DS
     D  HEVKT                  1      9  0
     D  W2VKT                  5      9  0
     D                 DS
     D  HEM3                   1      7  3
     D  W2M3                   3      6  2
     C*
     C                   EXSR      INIT
     C*
     C     START         TAG
     C                   MOVE      '1'           *IN93
     C                   WRITE     SUBCTL
     C                   MOVEA     '000'         *IN(93)
     C                   Z-ADD     *ZEROS        ZRECNO
      *_____________
      * Fyll Subfile
      *"""""""""""""
     C     HEAD8K        SETLL     HEAD8
     C     START1        TAG
     C                   READ      HEAD8                                  94
     C  N94              DO                                                     1<-B
     C     HEPRO         IFLT      UPRO                                          2<-B
     C                   MOVE      HEAVD         YAVD
     C     HEAD8K        SETLL     HEAD8
     C                   GOTO      START1
     C                   END                                                     2<-E
     C     HEPRO         IFGT      UPRO                                          2<-B
     C     HEAVD         COMP      9999                                   94
     C   94              GOTO      START2
     C     HEAVD         ADD       1             YAVD
     C     HEAD8K        SETLL     HEAD8
     C                   GOTO      START1
     C                   END                                                     2<-E
     C                   END                                                    1<-E
      *
     C     START2        TAG
      *
     C  N94              DO
     C                   DO        7
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
     C     ST21          TAG
     C                   READ      HEAD8                                  94
     C  N94              DO                                                     1<-B
     C     HEPRO         IFLT      UPRO                                          2<-B
     C                   MOVE      HEAVD         YAVD
     C     HEAD8K        SETLL     HEAD8
     C                   GOTO      ST21
     C                   END                                                     2<-E
     C     HEPRO         IFGT      UPRO                                          2<-B
     C     HEAVD         COMP      9999                                   94
     C   94              GOTO      ST22
     C     HEAVD         ADD       1             YAVD
     C     HEAD8K        SETLL     HEAD8
     C                   GOTO      ST21
     C                   END                                                     2<-E
     C                   END                                                    1<-E
     C  N94              END
     C                   END
     C     ST22          TAG
     C*
     C                   Z-ADD     ZRECNO        YRECNO            5 0
     C     START3        TAG
     C                   MOVEA     '11'          *IN(91)
     C     START4        TAG
     C                   SETOFF                                       30
     C     ZRECNO        IFEQ      0
     C                   EXFMT     NYWRK
     C                   ELSE
     C                   WRITE     SUBCTL
     C                   EXFMT     BUNN
     C                   END
     C*
     C*** AVSLUTT
     C   03              Z-ADD     3             UFLAGG
     C   03              GOTO      SLUTT
     C*
     C*** FRISK OPP
     C   05              Z-ADD     1             YAVD
     C   05              GOTO      START
     C*
     C*** TILBAKE TIL VALGBILDE
     C   12              Z-ADD     9             UFLAGG
     C   12              GOTO      SLUTT
     C*
     C*** BLA VIDERE
     C   25              DO
     C                   SETOFF                                       25
     C                   Z-ADD     YRECNO        ZRECNO
     C                   GOTO      START2
     C                   END
     C*
     C     ZRECNO        IFNE      0
     C                   MOVEA     '00'          *IN(91)
     C     *IN98         DOWEQ     '0'
     C                   READC     SUBFIL                                 98
     C  N98WSNR          IFEQ      '5'
     C                   MOVE      ' '           WSNR
     C                   EXSR      VISOPD
     C   12              UPDATE    SUBFIL
     C   12              GOTO      START3
     C                   GOTO      SLUTT
     C                   END
     C                   END
     C                   END
     C                   GOTO      START4
     C*
     C     SLUTT         TAG
     C                   SETON                                        LR
     C                   RETURN
     C*
     C***********************************************
     C     VISOPD        BEGSR
     C***********************************************
     C                   Z-ADD     2             UFLAGG            1 0
     C                   CALL      'SYFM21A'
     C                   PARM                    UFIRM
     C                   PARM                    UAVD
     C                   PARM                    HEOPD
     C                   PARM                    UFLAGG
     C*          UFLAGG    COMP 1                        01
     C     UFLAGG        COMP      2                                      02
     C     UFLAGG        COMP      3                                      03
     C     UFLAGG        COMP      9                                      12
     C*
     C  N02
     CANN03
     CANN12              EXSR      UTFAKT
     C*
     C     SLVOPD        ENDSR
     C*
     C***********************************************
     C     UTFAKT        BEGSR
     C***********************************************
     C                   CALL      'SYFM21B'
     C                   PARM                    UFIRM
     C                   PARM                    UAVD
     C                   PARM                    HEOPD
     C                   PARM                    UFLAGG
     C     UFLAGG        COMP      9                                      12
     C*
     C                   ENDSR
     C*
     C***********************************************
     C     INIT          BEGSR
     C***********************************************
     C     HEAD8K        KLIST
     C                   KFLD                    YAVD
     C                   KFLD                    UPRO
     C                   KFLD                    YPOS1
     C*
     C     *ENTRY        PLIST
     C                   PARM                    UFIRM             2
     C                   PARM                    UAVD
     C                   PARM                    UPRO
     C                   PARM                    UFLAGG
     C*
     C                   MOVE      *ZEROS        YAVD              4 0
     C                   MOVE      *BLANKS       YPOS1             7
     C                   MOVE      *ZEROS        HEVKT
     C                   MOVE      *ZEROS        HEM3
     C                   MOVE      *BLANKS       WSPOS1
     C*
     C                   ENDSR
     C*
