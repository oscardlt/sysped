     C***********************************************
     C     TESTB1        BEGSR
     C***********************************************
     C  N40              DO
     C     WSSFSI        COMP      *BLANKS                                90
     C   90              MOVE      'SPS0024'     MSGNR
     C  N90KODSFK        CHAIN     KODTSF                             33
     C  N90
     CANN33              DO
     C                   UPDATE    KOSFR
     C                   SETON                                        90
     C                   MOVE      'SPS0025'     MSGNR
     C                   END
     C   90              Z-ADD     17            POSNBR
     C   90              SETON                                        30
     C                   END
     C*
     C     WSSFNV        COMP      *BLANKS                                89
     C   89
     CANN30              MOVE      'SPN0010'     MSGNR
     C   89
     CANN30              Z-ADD     39            POSNBR
     C   89              SETON                                        30
     C*
     C   30              GOTO      SLTB
     C*
      * Hvis WSUser er fylt ut sjekk/skriv til SYPARF
     c                   exsr      TstUsr
     C     *IN14         IFEQ      '1'
     C     WSUSER        ifeq      *BLANKS
     C                   MOVE      'JOV1486'     MSGNR
     c                   seton                                        3088
     C                   GOTO      SLTB
     C                   else
     C                   CALL      'SYPR03R'
     C                   PARM                    UKUNDA            8
     C                   PARM                    UAVDA             4
     C                   PARM      WSUSER        XXSER            10
     C                   PARM                    XXLAGG            1
     C                   end
     C                   END
     C*** OPPDATERE PÅ REGISTER OG SUBFIL
     C     KODSFK        CHAIN     KODTSF                             33
     C                   EXSR      MOVWSF
     C  N33              UPDATE    KOSFR
     C   33              WRITE     KOSFR
     C   40              UPDATE    SUBFIL
     C  N40              ADD       1             YRECNO
     C  N40              Z-ADD     YRECNO        ZRECNO
     C  N40              WRITE     SUBFIL
     C                   WRITE     SUBCTL
     C                   EXSR      MOVB2
     C*
     C     SLTB          ENDSR

     C***********************************************
     C     TstUsr        BEGSR
     C***********************************************
     C*
     c                   movel     WSSFSI        SYVRDA
     C     UsrKey        Chain     SYPARL3                            33
     c     WsUser        ifeq      *blanks
     c  N33              delete    syparfr
     c                   else
     c                   movel     WSUSER        SYUSER
N01  C     *in33         ifeq      '1'
N01  C     'SYPAR   '    CHAIN     TELLGE                             34
N01  C                   MOVE      GENO          SYRECN
N01  C                   ADD       1             GENO
N01  C                   UPDATE    TELLGER
N01  C                   END
     c   33              write     syparfr
     c  N33              update    syparfr
     c                   end
      *
     C                   ENDSR
      *
     C***********************************************
     C     HENT          BEGSR
     C***********************************************
     C                   Z-ADD     0             LINNBR
     C                   SETOFF                                       919299
     C     *IN98         DOWEQ     '0'
     C                   READC     SUBFIL                                 98
     C   98              DO
     C   99              SETOFF                                       98
     C                   Z-ADD     1             LINNBR
     C                   GOTO      SLHENT
     C                   END
     C     ZRECNO        DIV       14            LINNBR
     C                   MVR                     XLIN              2 0
     C     XLIN          IFEQ      0
     C                   Z-ADD     14            LINNBR
     C                   ELSE
     C                   Z-ADD     XLIN          LINNBR
     C                   END
     C   70WSNR          IFEQ      '1'                                          ENDRE
     c                   move      kosfsi        usign
     c                   seton                                        lr
     c                   return
     c                   end
      *
     C  N70WSNR          IFEQ      '2'                                          ENDRE
     C                   MOVE      ' '           WSNR
     C     KOSFN7        COMP      '*DELETE'                              98
     C   98              GOTO      SLHENT
     C                   EXSR      CHGSF
     C   12              GOTO      SLHENT
     C                   END
     C  n70WSNR          IFEQ      '4'                                          SLETTE
     C                   MOVE      ' '           WSNR
     C     KOSFN7        COMP      '*DELETE'                              98
     C   98              GOTO      SLHENT
     C                   EXSR      DLTSF
     C   12              GOTO      SLHENT
     C                   END
     C                   END
