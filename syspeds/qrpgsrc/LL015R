     C                   EXSR      INIT
      *
     C     START         TAG
     C                   MOVE      '1'           *IN93
     C                   WRITE     SUBCTL
     C                   MOVEA     '00'          *IN(93)
     C                   MOVE      '0'           *IN42
     C                   Z-ADD     *ZEROS        ZRECNO
      *_____________
      * Fyll Subfile
      *"""""""""""""
     C     LLAF4K        SETLL     LLAGJS04
     C     START1        TAG
     C     LLAF4K        READE     LLAGJS04                               94
      *
     C     START2        TAG
      *
     C     *IN94         DOWEQ     '0'                                          1<-B
     C     VASTAT        IFEQ      ' '
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
     C                   END
     C     LLAF4K        READE     LLAGJS04                               94
     C  N94              END                                                    1<-E
      *
     C     ZRECNO        IFEQ      1
     C                   MOVE      VASENR        USENR
     C                   MOVE      VAANR         UANR
     C                   MOVE      VASALD        USTK
     C                   GOTO      SLUTT
     C                   END
     C                   Z-ADD     ZRECNO        YRECNO            5 0
     C     ZRECNO        IFGT      1
     C                   Z-ADD     1             ZRECNO
     C                   END
      *
     C                   Z-ADD     5             LINNBR
     C                   Z-ADD     2             POSNBR
     C     START3        TAG
     C                   MOVEA     '11'          *IN(91)
     C                   WRITE     BUNN
     C                   EXFMT     SUBCTL
      *
     C                   MOVEA     '00'          *IN(30)
      *
     C   03
     COR 12              GOTO      SLUTT
      *** FRISK OPP
     C   05              GOTO      START
      *
     C                   EXSR      HENT
     C     *IN30         CABEQ     '1'           START3
      *
     C     SLUTT         TAG
     C                   SETON                                        LR
     C                   RETURN
      *
     C***********************************************
     C     HENT          BEGSR
     C***********************************************
     C                   MOVEA     '000'         *IN(90)
     C     *IN98         DOWEQ     '0'
     C                   READC     SUBFIL                                 98
     C     WSNR          IFEQ      '1'
     C                   MOVE      VASENR        USENR
     C                   MOVE      VAANR         UANR
     C                   MOVE      VASALD        USTK
     C                   MOVE      '1'           *INLR
     C                   RETURN
     C                   END
     C                   END
      *
     C                   MOVE      'DIV0012'     MSGNR
     C                   MOVE      '1'           *IN30
     C                   Z-ADD     5             LINNBR
     C                   Z-ADD     2             POSNBR
      *
     C                   ENDSR
