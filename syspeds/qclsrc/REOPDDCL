             PGM
             DCL        VAR(&LIBR) TYPE(*CHAR) LEN(10)
             CHGDSPF    FILE(REOPDDFM) WAITRCD(*IMMED)
             RTVDTAARA  DTAARA(FIRMDT (151 10)) RTNVAR(&LIBR)
        /*  */
             OVRDBF     FILE(HEADDCMP) TOFILE(&LIBR/HEADDCMP)
             OVRDBF     FILE(HEADFCMP) TOFILE(&LIBR/HEADFCMP)
             OVRDBF     FILE(FAK2CMP) TOFILE(&LIBR/FAK2CMP)
             OVRDBF     FILE(FAINCMP) TOFILE(&LIBR/FAINCMP)
        /*  */
             CALL       PGM(REOPDD)
             ENDPGM
