     FSVXVMN    UF A E           K DISK
     FSVXKODL01 IF   E           K DISK
     FSVX022D   CF   E             WORKSTN
     F                                     SFILE(SUBFIL:ZRECNO)
      *
     C                   EXSR      INIT
      *
     C     START         TAG
     C                   MOVE      '1'           *IN93
     C                   WRITE     SUBCTL
     C                   MOVEA     '00'          *IN(93)
     C                   Z-ADD     *ZEROS        ZRECNO
     C     UMN           IFNE      *BLANKS
     C                   MOVE      UMN           TVMN
     C                   MOVE      UMNSK         TVMNSK
     C                   MOVE      UEH           TVEH
     C                   MOVE      UNT           TVNT
     C                   MOVE      UNTEH         TVNTEH
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
     C                   END
      *_____________
      * Fyll Subfile
      *"""""""""""""
     C     SVVMNK        SETLL     SVXVMN
     C     SVVMNK        READE(N)  SVXVMN                                 94
      *
     C     *IN94         DOWEQ     '0'
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
     C     SVVMNK        READE(N)  SVXVMN                                 94
     C  N94              END
      *
     C                   Z-ADD     ZRECNO        YRECNO            5 0
     C     ZRECNO        IFGT      0
     C                   Z-ADD     1             ZRECNO
     C                   END
     C                   Z-ADD     4             LINNBR
     C                   Z-ADD     2             POSNBR
      *
     C     START3        TAG
     C                   MOVEA     '11'          *IN(91)
     C     ZRECNO        IFEQ      0
     C                   EXFMT     NYWRK
     C                   ELSE
     C                   WRITE     BUNN
     C                   EXFMT     SUBCTL
     C                   END
      *
     C                   SETOFF                                       9830
     C                   MOVE      '0'           *IN90
     C   03
     COR 12              GOTO      SLUTT
      *
     C   04              DO
     C                   MOVE      '017'         UUNIK             3
     C                   MOVE      *BLANKS       UKODE            10
     C                   MOVEL     W2EH          UKODE
     C                   CALL      'SVX002R'
     C                   PARM                    UUNIK
     C                   PARM                    UKODE
     C                   PARM                    UTYPE2            3
     C                   MOVEL     UKODE         W2EH
     C                   GOTO      START3
     C                   END
      *** FRISK OPP
     C   05              DO
     C  N27              EXSR      OPDFIL
     C                   GOTO      START
     C                   END
      *
     C     W2NR          IFEQ      '1'
     C                   EXSR      CRTMN
     C                   GOTO      START3
     C                   END
      *
     C     ZRECNO        IFNE      0
     C                   EXSR      HENT
     C                   END
     C                   GOTO      START3
      *
     C     SLUTT         TAG
     C  N27              EXSR      OPDFIL
     C                   SETON                                        LR
     C                   RETURN
      *
     C***********************************************
     C     OPDFIL        BEGSR
     C***********************************************
     C     SVVMNK        SETLL     SVXVMN
     C     SVVMNK        READE     SVXVMN                                 33
     C     *IN33         DOWEQ     '0'
     C                   DELETE    SVXVMNR
     C     SVVMNK        READE     SVXVMN                                 33
     C  N33              ENDDO
      *
     C                   MOVE      *BLANKS       UMN
     C                   MOVE      *BLANKS       UMNSK
     C                   MOVE      *BLANKS       UEH
     C                   MOVE      *ZEROS        UNT
     C                   MOVE      *ZEROS        UNTEH
     C     YRECNO        IFGT      0
     C     1             CHAIN     SUBFIL                             33
     C     TVMN          IFNE      '*DELETE'
     C                   MOVE      TVMN          UMN
     C                   MOVE      TVMNSK        UMNSK
     C                   MOVE      TVEH          UEH
     C                   MOVE      TVNT          UNT
     C                   MOVE      TVNTEH        UNTEH
     C                   END
     C     2             DO        YRECNO        ZRECNO
     C     ZRECNO        CHAIN     SUBFIL                             33
     C     TVMN          IFNE      '*DELETE'
     C     UMN           IFEQ      *BLANKS
     C                   MOVE      TVMN          UMN
     C                   MOVE      TVMNSK        UMNSK
     C                   MOVE      TVEH          UEH
     C                   MOVE      TVNT          UNT
     C                   MOVE      TVNTEH        UNTEH
     C                   ELSE
     C                   WRITE     SVXVMNR
     C                   END
     C                   END
     C                   ENDDO
     C                   END
      *
     C                   ENDSR
      *
     C***********************************************
     C     HENT          BEGSR
     C***********************************************
     C                   MOVEA     '00'          *IN(91)
     C                   MOVEA     '00'          *IN(98)
     C     *IN98         DOWEQ     '0'
     C                   READC     SUBFIL                                 98
     C   98              DO
     C   99              MOVE      '0'           *IN98
     C                   GOTO      SLHENT
     C                   END
     C  N98WSNR          IFEQ      '2'
     C     TVMN          COMP      '*DELETE'                              98
     C   98              GOTO      SLHENT
     C                   EXSR      CHGMN
     C                   END
     C  N98WSNR          IFEQ      '4'
     C     TVMN          COMP      '*DELETE'                              98
     C   98              GOTO      SLHENT
     C                   EXSR      DLTMN
     C                   END
     C                   END
      *
     C     SLHENT        TAG
     C   98              DO
     C                   MOVE      'DIV0012'     MSGNR
     C                   SETON                                            30
     C                   END
      *
     C                   ENDSR
      *
     C***********************************************
     C     CRTMN         BEGSR
     C***********************************************
     C     W2MN          COMP      *BLANKS                                90
     C     *IN90         IFEQ      '1'
     C                   MOVE      'SVX0826'     MSGNR
     C                   MOVE      '1'           *IN30
     C                   GOTO      SLCRT
     C                   END
      *
     C     W2MNSK        IFNE      *BLANKS
     C                   MOVE      '012'         YUNIK
     C                   MOVE      *BLANKS       YKODE
     C                   MOVEL     W2MNSK        YKODE
     C     SVK01K        CHAIN     SVXKODL01                          89
     C     *IN89         IFEQ      '1'
     C                   MOVE      'SVX0810'     MSGNR
     C                   MOVE      '1'           *IN30
     C                   GOTO      SLCRT
     C                   END
     C                   END
