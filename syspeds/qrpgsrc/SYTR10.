     C     WSSFFK        IFNE      *ZEROS
     C     WSSFFK        IFGT      2400
     C                   SETON                                        3074
     C                   END
     C                   END
     C*
     C*
     C* TESTER KODE FORFRAKT REKVIRERES
     C     WSSTAF        IFNE      ' '
     C     WSSTAF        ANDNE     'R'
     C     WSSTAF        ANDNE     'F'
     C     WSSTAF        ANDNE     'A'
     C     WSSTAF        ANDNE     'O'
     C                   SETON                                        3048
     C                   END
     C*** TEST DATO VIDERRE ND 71
     C     WSSVTD        IFEQ      *ZEROS
     C     WSSDVT        ANDNE     *BLANKS
     C     WSSDTD        ANDNE     *ZEROS
     C     *IN64         ANDNE     '1'
     C                   MOVE      WSSDTD        WSSVTD
     C                   END
     C     WSSVTD        IFNE      *ZEROS
     C     WSSVTD        IFLT      3999
     C                   MOVE      WSSVTD        TMPDAT
     C                   EXSR      FIXDAT
     C                   MOVE      TMPDAT        WSSVTD
     C                   END
     C                   MOVE      WSDTD4        UDT1              2 0
     C                   MOVE      WSDTM4        UDT2              2 0
     C                   MOVE      WSDTÅ4        UDT3              2 0
     C                   CALL      'TESTDATO'
     C                   PARM                    UDT1
     C                   PARM                    UDT2
     C                   PARM                    UDT3
     C                   PARM                    UFLAGG            1 0
     C     UFLAGG        COMP      1                                      71
     C  N71UDATOD        COMP      UDATOB                             71
     C  N71UDATOD        COMP      UDATOA                               71
     C   71              SETON                                        30
     C                   END
