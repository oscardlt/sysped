     C                   MOVE      W2FR01        YFR01
     C                   MOVE      W2LNR         YLNR
     C*
     C                   ENDSR
     C*
     C***********************************************
     C     MOVPY         BEGSR
     C***********************************************
     C                   MOVE      PKOT01        YOT01
     C                   MOVE      PKFR01        YFR01
     C                   MOVE      PKAVTN        YAVTN
     C                   MOVE      PKGEBY        YGEBY
     C                   MOVE      PKLNR         YLNR
     C*
     C                   ENDSR
     C*
     C***********************************************
     C     SØKDTA        BEGSR
     C***********************************************
     C     LINNBR        IFEQ      9
     C     POSNBR        ANDGE     20
     C     POSNBR        ANDLE     22
     C                   MOVE      'J'           USPED             1
     C                   CALL      'SYFI10G'
     C                   PARM                    WSFR01
     C                   PARM                    USPED
     C                   Z-ADD     11            LINNBR
     C                   GOTO      SLSØK
     C                   END
     C*
     C     LINNBR        IFEQ      11
     C     POSNBR        ANDGE     20
     C     POSNBR        ANDLE     21
     C                   CALL      'SYFI10C'
     C                   PARM                    WSOT01
     C                   Z-ADD     9             LINNBR
     C                   GOTO      SLSØK
     C                   END
     C*
     C                   MOVE      'DIV1037'     MSGNR
     C                   SETON                                        30
     C*
     C     SLSØK         ENDSR
     C*
