             PGM
             DCL        VAR(&KEY) TYPE(*CHAR) LEN(8) VALUE('FRATN   ')
             DCL        VAR(&NR) TYPE(*DEC) LEN(7 0)
             DCL        VAR(&NRA) TYPE(*CHAR) LEN(7)
             DCL        VAR(&MBR) TYPE(*CHAR) LEN(8)
             CALL       PGM(SYGE49) PARM(&KEY &NR)
             CHGVAR     VAR(&NRA) VALUE(&NR)
             CHGVAR     VAR(&MBR) VALUE('C' *CAT &NRA)
             CPYF       FROMFILE(FRATN) TOFILE(FRATNFOR) TOMBR(&MBR) +
                          MBROPT(*REPLACE)
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CALL       PGM(SYFA36P)
             ENDPGM
