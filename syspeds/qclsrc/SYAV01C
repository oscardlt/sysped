             PGM
     /*___________________*/
     /* Beskriv variabler */
     /*"""""""""""""""""""*/
             DCL        VAR(&USER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&ZAVDF) TYPE(*CHAR) LEN(4)
             DCL        VAR(&ZAVDT) TYPE(*CHAR) LEN(4)
             DCL        VAR(&ZFLYF) TYPE(*CHAR) LEN(3)
             DCL        VAR(&ZFLYT) TYPE(*CHAR) LEN(3)
             DCL        VAR(&ZDTF) TYPE(*CHAR) LEN(6)
             DCL        VAR(&ZDTT) TYPE(*CHAR) LEN(6)
             DCL        VAR(&ZEI) TYPE(*CHAR) LEN(1)
             DCL        VAR(&ZDEV) TYPE(*CHAR) LEN(10)
             DCL        VAR(&IN03) TYPE(*LGL) LEN(1)
             DCL        VAR(&IN12) TYPE(*LGL) LEN(1)
     /*_____________________________*/
     /* Hente bestillingsparametrer */
     /*"""""""""""""""""""""""""""""*/
             CALL       PGM(SYAV01R) PARM(&ZAVDF &ZAVDT &ZFLYF &ZFLYT +
                          &ZDTF &ZDTT &ZEI &ZDEV &IN03 &IN12)
     /*_____________________*/
     /* Annuller bestilling */
     /*"""""""""""""""""""""*/
             IF         COND(&IN03 *EQ '1') THEN(RETURN)
             IF         COND(&IN12 *EQ '1') THEN(RETURN)
     /*__________________*/
     /* Avregning i JOBQ */
     /*""""""""""""""""""*/
        /*   CALL       PGM(SYAV02C) PARM(&ZAVDF &ZAVDT &ZFLYF &ZFLYT +
                          &ZDTF &ZDTT &ZEI &ZDEV)    */
        /*   RETURN  */
             SBMJOB     CMD(CALL PGM(SYAV02C) PARM(&ZAVDF &ZAVDT +
                          &ZFLYF &ZFLYT &ZDTF &ZDTT &ZEI &ZDEV)) +
                          JOB(FLYAVREG) JOBQ(SYJOBQAV)
             RTVJOBA    USER(&USER)
             SNDMSG     MSG('Jobben er sendt til jobbkø !') +
                          TOUSR(&USER)

             ENDPGM
