             PGM        PARM(&UNIK &FAX &PDATE &PTIME &KNAVN &USER)
             DCL        VAR(&BPARM) TYPE(*CHAR) LEN(85)
             DCL        VAR(&PTIME) TYPE(*CHAR) LEN(8)
             DCL        VAR(&PDATE) TYPE(*CHAR) LEN(8)
             DCL        VAR(&USER) TYPE(*CHAR) LEN(8)
             DCL        VAR(&KNAVN) TYPE(*CHAR) LEN(30)
             DCL        VAR(&FAX) TYPE(*CHAR) LEN(21)
             DCL        VAR(&UNIK) TYPE(*CHAR) LEN(9)
             DCL        VAR(&E) TYPE(*CHAR) LEN(1) VALUE('E')
             DCLF       FILE(FAXSEN)
             RCVF       RCDFMT(*FILE)
             CHGVAR     VAR(&BPARM) VALUE(&UNIK *CAT &FAX *CAT +
                          &PTIME *CAT &PDATE *CAT &KNAVN *CAT &USER +
                          *CAT &E)
             /* FAXSYS = F FACSIMILIE-SUPPORTY 400 */
             IF         COND(&FAXSYS = 'F') THEN(DO)
             SBMJOB     CMD(CALL PGM(FAXR22CLJ) PARM(&BPARM)) +
                          JOB(SNDFAXMAN) JOBD(*USRPRF) +
                          JOBQ(QFAX/QFAXJOBQ)
                          ENDDO
             /* FAXSYS = B  BLUFAX                 */
             IF         COND(&FAXSYS = 'B') THEN(DO)
             SBMJOB     CMD(CALL PGM(FAXR22CLJB) PARM(&BPARM)) +
                          JOB(SNDFAXMAN) JOBD(*USRPRF) +
                          JOBQ(QFAX/QFAXJOBQ)
                          ENDDO
             ENDPGM
