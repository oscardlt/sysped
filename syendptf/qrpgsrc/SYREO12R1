     H DFTACTGRP(*NO)
     FBILLOF    UF   E           K DISK
     FBLV1      UF   E           K DISK
     FBLV1GATE  UF   E           K DISK
     FBLV2      UF   E           K DISK
     FBLVSD2    UF   E           K DISK
     FBILLOFOS  UF   E           K DISK
     FBILLOFTU  UF   E           K DISK
     FBLADRT    UF   E           K DISK
     FBLLOK     UF   E           K DISK
     FBOOKING   UF   E           K DISK
     FBOOKIN2   UF   E           K DISK
     FCCBLV1    UF   E           K DISK    PREFIX(CC_)
     FCCBLV2    UF   E           K DISK

     d xavd            S              4  0
     d xopd            S              7  0
     d xpro            S              8  0
      *  Prototype for syreo12r1
     d syreo12r1       pr
     d uavd_                               like(uavd)
     d uopd_                               like(uopd)
     d upro_                               like(upro)
      *  *ENTRY Interface for Main Procedure
     d syreo12r1       pi
     d uavd                           4
     d uopd                           7
     d upro                           8
      *
      /free

         exsr init;

         exec sql
         delete from billof
         where blavd = :xavd and blopd = :xopd
         with NC;

         exec sql
         delete from blv1
         where bvavd = :xavd and bvopd = :xopd
         with NC;

         exec sql
         delete from blv1gate
         where bvavd = :xavd and bvopd = :xopd
         with NC;

         exec sql
         delete from blv2
         where bvavd = :xavd and bvopd = :xopd
         with NC;

         exec sql
         delete from blvsd2
         where bvavd = :xavd and bvopd = :xopd
         with NC;

         exec sql
         delete from billofos
         where blavd = :xavd and blopd = :xopd
         with NC;

         if xpro > 0;
           exec sql
           delete from billoftu
           where blpro = :xpro
           with NC;
         endif;

         exec sql
         delete from bladrt
         where btavd = :xavd and btopd = :xopd
         with NC;

         exec sql
         delete from bllok
         where beavd = :xavd and beopd = :xopd
         with NC;

         exec sql
         delete from booking
         where boavd = :xavd and boopd = :xopd
         with NC;

         exec sql
         delete from bookin2
         where b2avd = :xavd and b2opd = :xopd
         with NC;

         exec sql
         delete from ccblv1
         where bvavd = :xavd and bvopd = :xopd
         with NC;

         exec sql
         delete from ccblv2
         where bvavd = :xavd and bvopd = :xopd
         with NC;

         *inlr = *on;
         return;

      //***********************************************
       begsr init;
      //***********************************************
       xavd = %float(uavd);
       xopd = %float(uopd);
       xpro = %float(upro);

       endsr;

      /end-free
