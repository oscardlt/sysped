     C                   EXSR      SRB
     C                   MOVE      '1'           *INLR
      *////////////////////////////////////////////////////////////
      *  Initiering                                           SRA /
      *////////////////////////////////////////////////////////////
     C     SRA           BEGSR
      *____________________
      * Definiere variabler
      *""""""""""""""""""""
     C     *LIKE         DEFINE    SVIX_SYAV     ZVIX_SYAV
     C     *LIKE         DEFINE    SVIX_SYOP     ZVIX_SYOP
      *_________________
      * Input parametere
      *"""""""""""""""""
     C     *ENTRY        PLIST
     C                   PARM                    ZVIX_SYAV
     C                   PARM                    ZVIX_SYOP
      *__________________
      * Definiere Nøkkler
      *""""""""""""""""""
     C     SVIX_KEY      KLIST
     C                   KFLD                    ZVIX_SYAV
     C                   KFLD                    ZVIX_SYOP
      *_________________________________
      * Hæmta dagens datum och klockslag
      *"""""""""""""""""""""""""""""""""
     C                   CALL      'SYGE15C'
     C                   PARM                    WDT               8
     C                   PARM                    WTM               6
     C                   ENDSR
