s01  D* FIFIRM                 1      2
     C*
     C                   EXSR      INIT
     C     UKNR          IFNE      0
     C                   GOTO      TSTPA
     C                   END
     C*
     C     BG1           TAG
     C  N30
     CANN04              Z-ADD     9             LINNBR
     C  N30
     CANN04              Z-ADD     20            POSNBR
     C                   EXFMT     BILDE01
     C                   SETOFF                                       309998
     C*
     C   03              GOTO      SLUTT
      *
n06   * sett ny dato alle avtaler
      *
n06  c     *in10         ifeq      '1'
n06  C                   exsr      nydatosr
n06  C                   goto      BG1
N06  c                   else
N06   * tastet ny dato men ikke tastet F10
n06  c     nydato        ifne      0
N06  C                   MOVE      'SHU1762'     MSGNR
N06  C                   SETON                                        3095
N06  C                   Z-ADD     21            LINNBR
N06  C                   Z-ADD     49            POSNBR
n06  C                   goto      BG1
n06  c                   end
n06  c                   end
      *
n06   * slett alle avtalelinjer med en gitt dato
      *
n06  c     *in09         ifeq      '1'
n06  C                   exsr      sldatosr
n06  C                   goto      BG1
N06  c                   else
N06   * tastet ny dato men ikke tastet F10
n06  c     sldato        ifne      0
N06  C                   MOVE      'SHU1768'     MSGNR
N06  C                   SETON                                        3095
N06  C                   Z-ADD     23            LINNBR
N06  C                   Z-ADD     49            POSNBR
n06  C                   goto      BG1
n06  c                   end
n06  c                   end
      *
n06   * kopier gamle avtaler til ny dato
      *
n06  c     *in11         ifeq      '1'
n06  C                   exsr      kodatosr
n06  C                   goto      BG1
N06  c                   else
N06   * tastet ny dato men ikke tastet F10
n06  c     kodato        ifne      0
n06  c     kfdato        orne      0
N06  C                   MOVE      'SHU1763'     MSGNR
N06  C                   SETON                                        309492
N06  C                   Z-ADD     22            LINNBR
N06  C                   Z-ADD     49            POSNBR
     c                   seton                                        30
n06  C                   goto      BG1
n06  c                   end
n06  c                   end
