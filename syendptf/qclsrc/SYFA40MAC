/*N01 - INDIKATOR &IN01 - HOPP UT HVIS IKKE INSTALLERT */
             PGM
             DCL        VAR(&TIME) TYPE(*CHAR) LEN(6)
/*N01*/      DCL        VAR(&IN01) TYPE(*LGL) LEN(1) VALUE('0')

 /*S01  START:      CALL       PGM(SYFA40MA) */

 /*N01  LEGG UT POSTER MED STAUS A=ANKOMSTMELDING I RUCKP  */
 START:      CALL       PGM(SYFA40MA) PARM(&IN01)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT)) +
                          /* Hopp ut hvis ikke installert */

 /*N01  SEND ANK-MELDING PÅ DE POSTER DER VILKÅR ER TILSTEDE */
             CALL       PGM(SYFA40MA2)

             RTVSYSVAL  SYSVAL(QTIME) RTNVAR(&TIME)
             IF         COND(&TIME *LT '230000') THEN(DO)
               DLYJOB     DLY(300)
               GOTO       CMDLBL(START)
             ENDDO

 SLUTT:      ENDPGM
