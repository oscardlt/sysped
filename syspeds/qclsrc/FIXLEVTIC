             PGM
             DCL        VAR(&DIR) TYPE(*CHAR) LEN(100) +
                          VALUE('/SYSPEDFX/DIVCSV/LEVTID')
             DCL        VAR(&PGM) TYPE(*CHAR) LEN(10) VALUE('FIXLEVTIC2')
             CALL       PGM(ARCA001R) PARM(&DIR &PGM)
             ENDPGM
