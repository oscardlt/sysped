     A            POSNBR         3S 0H
     A                                  1  2'SAD061Q'
     A                                      COLOR(BLU)
     A                                  1 10'Søking merke/modell (avgifts-mal)'
     A                                      COLOR(WHT)
     A            ZRECNO         4S 0B  1 42SFLRCDNBR
     A                                      DSPATR(ND)
     A                                  2  2'Velg option:  1 = Plukke   4= Slet-
     A                                      te'
     A                                      COLOR(BLU)
     A                                  3  2'Op Merke  Modell 1   Modell 2    V-
     A                                      ekt   Effekt'
     A                                      COLOR(WHT)
     A            W2NR           1A  I  4  2VALUES(' ' '1' '4')
     A                                      DSPATR(UL)
     A            W2MERK        15A  I  4  4DSPATR(UL)
     A            W2MOD1        10A  I  4 20DSPATR(UL)
