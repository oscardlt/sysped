             PGM        PARM(&SN)
             DCL        VAR(&SN) TYPE(*CHAR) LEN(9)
             DCL        VAR(&OK) TYPE(*CHAR) LEN(1)

             CLRPFM     FILE(TARINY)
             CALL       PGM(SYTI07R) PARM(&SN &OK)

             IF         COND(&OK = 'J') THEN(DO)
             CALL       PGM(SYTI07C2)
             ENDDO

             ENDPGM
