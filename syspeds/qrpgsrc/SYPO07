     C*
     C                   EXSR      INIT
     C*
     C     START0        TAG
     C*
     C                   Z-ADD     28            POSNBR
     C                   Z-ADD     5             LINNBR
     C   41              Z-ADD     7             LINNBR
     C                   MOVE      *OFF          *IN50
     C                   MOVE      ' '           WSSU
     C     STAR0B        TAG
     C                   EXFMT     BILDE01
     C     WSCLOC        DIV       256           LINNBR
     C                   MVR                     POSNBR
     C*
     C* SEARCH
     C   04              DO
     C                   EXSR      SØKDTA
     C                   GOTO      START0
     C                   END
     C                   SETOFF                                       30
     C   03              GOTO      SLUTT
     C*
     C* TEST CUSTOMER
     C*
     C  N41              DO
     C     KUNKEY        CHAIN     CUNDL01                            30
     C  N30STICUS        COMP      0                                      30
     C   30              MOVE      'SPK0001'     MSGNR
     C   30              GOTO      START0
     C                   END
     C*
     C* TEST COUNTRY
     C*
     C     KODLKK        CHAIN     KODTLK                             30
     C   30              Z-ADD     28            POSNBR
     C   30              Z-ADD     7             LINNBR
     C   30              MOVE      'SPL0001'     MSGNR
     C   30              GOTO      STAR0B
     C*
     C                   MOVE      *BLANKS       SPETAB
     C     STINLK        IFEQ      'NO'
     C     WSSU          ANDNE     'S'
     C     WSSU          ANDNE     'D'
     C     WSSU          ANDNE     'P'
     C                   MOVE      *ON           *IN50
     C                   GOTO      STAR0B
     C                   END
      * HVIS NO: SPESIAL KUN V/DOMESTIC / POSTEN - FIKTIV KUNDE...
     C                   SELECT
     C     STINLK        WHENEQ    'NO'
     C     WSSU          ANDEQ     'D'
     C                   MOVE      *ON           *IN50
     C                   Z-ADD     99999999      STICUS
     C                   MOVEL     DOMTXT        SPETAB
     C     STINLK        WHENEQ    'NO'
     C     WSSU          ANDEQ     'P'
     C                   MOVE      *ON           *IN50
     C                   Z-ADD     99999998      STICUS
     C                   MOVEL     POSTXT        SPETAB
     C                   OTHER
     C                   MOVE      *OFF          *IN50
     C                   MOVE      *ZEROS        STICUS
     C                   END
     C*
     C     START         TAG
     C                   MOVE      '1'           *IN93
     C                   WRITE     SUBCTL
     C                   MOVEA     '00'          *IN(93)
     C                   Z-ADD     *ZEROS        ZRECNO
     C                   MOVE      W2INBA        YTINBA            5
     C                   MOVE      W2INPF        YTINPF            5
