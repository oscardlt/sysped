/********************************************************************/
/* REORGANISERER MERGEDE DOKUMENTER  ELDRE ENN 1  DAGER             */
/********************************************************************/
             PGM
             DCL        VAR(&DIR) TYPE(*CHAR) LEN(100) /* +
                          Directory/mappe som leses inn */
             DCL        VAR(&DGR) TYPE(*CHAR) LEN(5) VALUE('00001') +
                          /*DAGER*/
/*______________*/
/* LES FIRMAFIL */
/*""""""""""""""*/
             DCLF       FILE(FIRMARC) RCDFMT(*ALL)
             RCVF       DEV(*FILE) RCDFMT(*FILE)
             CHGVAR     VAR(&DIR) VALUE(&ARCANE *TCAT '/mrg')
             CALL       PGM(REOIFSR) PARM(&DIR &DGR)
             ENDPGM
