             PGM PARM(&GRP &KNR &MRA)
             DCL        VAR(&GRP) TYPE(*CHAR) LEN(8)
             DCL        VAR(&KNR) TYPE(*CHAR) LEN(8)
             DCL        VAR(&MRA) TYPE(*CHAR) LEN(5)
  /* filtrer ut aktuell M-rapp */
  /*                                                                      */
             DLTF       FILE(QTEMP/X_MRAP)
             MONMSG     MSGID(CPF0000)
             CRTDUPOBJ  OBJ(LLAGJF) FROMLIB(*LIBL) OBJTYPE(*FILE) +
                          TOLIB(QTEMP) NEWOBJ(X_MRAP)
             CPYF       FROMFILE(LLAGJL07) TOFILE(QTEMP/X_MRAP) +
                          MBROPT(*REPLACE) FROMKEY(*BLDKEY (&GRP +
                          &KNR &MRA)) TOKEY(*BLDKEY (&GRP &KNR &MRA))
             ADDLIBLE   LIB(SYEXCEL)
             MONMSG     MSGID(CPF0000)

             XLCRT2     FILE(X_MRAP)

             RMVLIBLE   LIB(SYEXCEL)
             MONMSG     MSGID(CPF0000)
             ENDPGM
