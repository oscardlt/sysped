      *____
      * UNH
      *""""
     C                   EVAL      ADS='UNH+'+%trimr(S0020)
     C                             + 'A+AUTACK:4:1:UN:SEA02A'+WAPSTR
     C                   EVAL      AXV=1
     C     ' '           CHECKR(E) ADS           AXH
     C                   EVAL      WCHART='N'
