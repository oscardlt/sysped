             PGM
             DCL        VAR(&AVBRU) TYPE(*CHAR) LEN(1) VALUE('N')
             DCL        VAR(&AAR) TYPE(*CHAR) LEN(4)

/*______________*/
/* HENT ÅRSTALL */
/*""""""""""""""*/
             CALL       PGM(REOSTA) PARM(&AVBRU &AAR)
             IF         COND(&AVBRU = 'J') THEN(GOTO CMDLBL(SLUTT)) +
                          /* Hvis &AVBRU på, avslutter program. */
                                                          /**/
/*______________*/
/* STATHF       */
/*""""""""""""""*/
             CALL REOSTA2A  PARM(&AAR)
/*______________*/
/* STATDF       */
/*""""""""""""""*/
             CALL REOSTA2
/*______________*/
/* STATOHF      */
/*""""""""""""""*/
             CALL REOSTA3A  PARM(&AAR)
/*______________*/
/* STATODF      */
/*""""""""""""""*/
             CALL REOSTA3
SLUTT:       ENDPGM
