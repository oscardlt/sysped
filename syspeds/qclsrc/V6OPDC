             PGM
 /* LEGG UT LAVESTE PERI PR OPPDRAG I FIL QTEMP/OPDPER */
             RUNQRY     QRY(V6OPDQRY)
 /* KOPIER OVER I FIL-DEFN SOM HAR AVD/OPD SOM KEY */
             CPYF       FROMFILE(OPDPER) TOFILE(V6OPDP) +
                          MBROPT(*REPLACE) FMTOPT(*NOCHK)
 /* OPPDATER I HEADF, 00 00 00 PÅ DE SOM IKKE FINNES I FILA */
             CALL       PGM(V6OPD01)
             ENDPGM
