S02  C*    EPOHK2        CHAIN     EPOH                               33
N02  C     EPOHK2        CHAIN(N)  EPOH                               33
     C  N33              EVAL      VAKURE = EHLEVN
     C  N33VADR3K        CHAIN     VADR3                              33
     C   33              EVAL      VADRNA = EHLEVN
     C                   callp     updHTMLvar('VADRNA':VADRNA)
     C                   callp     updHTMLvar('ELART':IN(XN))
     C                   callp     updHtmlVar('WSAN':
     C                             %trim(%editc(AN(XN):'Z')))
     C                   callp     updHtmlVar('WSNT':
     C                             %trim(%editc(NT(XN):'Z')))
     C                   callp     updHtmlVar('WSVKB':
     C                             %trim(%editc(VKB(XN):'4')))
     C                   callp     updHtmlVar('WSVKN':
     C                             %trim(%editc(VKN(XN):'4')))
