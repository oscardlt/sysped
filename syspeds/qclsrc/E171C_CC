             PGM        PARM(&MNA)
             DCL        VAR(&MNA) TYPE(*CHAR) LEN(9)
             DCL        VAR(&KOPI) TYPE(*CHAR) LEN(1)
             DCL        VAR(&INFOP) TYPE(*CHAR) LEN(1)
             DCL        VAR(&BANEA) TYPE(*CHAR) LEN(70)
             DCL        VAR(&BANEB) TYPE(*CHAR) LEN(70)
             DCL        VAR(&QUSER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QJOB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QNBR) TYPE(*CHAR) LEN(6)
             DCL        VAR(&FILEN) TYPE(*CHAR) LEN(10)
/*______________*/
/* LES FIRMAFIL */
/*""""""""""""""*/
             DCLF       FILE(FIRMDAG) RCDFMT(*ALL)
             RCVF       DEV(*FILE) RCDFMT(*FILE)
             IF         COND(&DGPRIN = ' ') THEN(CHGVAR VAR(&DGPRIN) +
                          VALUE('QPRINT'))
             OVRPRTF    FILE(E171P) PAGESIZE(70 120) CPI(15) +
                          OVRFLW(64) OUTQ(&DGPRIN) SAVE(*YES)
             OVRPRTF    FILE(E171PARK) DEVTYPE(*AFPDS) PAGESIZE(70 +
                          120) CPI(15) OVRFLW(64) OUTQ(OUTMAIL) +
                          FORMTYPE(*STD) HOLD(*YES)
             OVRDBF     FILE(E171FGML) TOFILE(E171F)

             CALL       PGM(E171R_CC) PARM(&MNA &KOPI &INFOP &BANEA +
                          &BANEB)

             IF         COND(&BANEA *NE ' ') THEN(DO)
             ADDLIBLE   LIB(MMAIL) POSITION(*LAST)
             MONMSG     MSGID(CPF0000)
             RTVJOBA    JOB(&QJOB) USER(&QUSER) NBR(&QNBR)
             IF         COND(&INFOP = 'N') THEN(DO)
             CVTSPLFPDF SPLF(E171PARK) JOB(&QNBR/&QUSER/&QJOB) +
                          TOPDF(&BANEA)
             ENDDO
             IF         COND(&INFOP = 'J') THEN(DO)
             CHGVAR     VAR(&FILEN) VALUE('E171PARK')
             CALL       PGM(INF001CL) PARM(&FILEN &QNBR &QUSER &QJOB +
                          &BANEA)
             ENDDO
             RMVLIBLE   LIB(MMAIL)
             MONMSG     MSGID(CPF0000)
             ENDDO

             ENDPGM
