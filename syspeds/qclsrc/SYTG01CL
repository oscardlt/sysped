/* ********************************************************************    */
/* HENTE MYPACKADRESSE                               */
/* ********************************************************************    */
/*       * RETTINGER I DETTE PROGRAM:                                      */
/* PTFnr:*Sek Sig Vers  Beskrivelse..........................              */
/* """"""*"""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""    */
/* 10XXX * 01 JOV PTF10 SURR MED &ADR / &ADRBL                             */
/* 10XXX * 02 JOV PTF10 PARM FOR VELG FØRSTE, RETUR AV DISTANSE/ÅPNINGST.  */
/* ********************************************************************    */
/*S01        PGM        PARM(&NAV &ADRBL &POS &STE &KUNR)   */
/*S02        PGM        PARM(&NAV &ADR &POS &STE &KUNR)     */
/*N02*/      PGM        PARM(&NAV &ADR &POS &STE &KUNR &FØRST &DIST &OPEN)
             DCL        VAR(&KUNR)  TYPE(*CHAR) LEN(12)
             DCL        VAR(&ANTREC)  TYPE(*DEC) LEN(10 0)
             DCL        VAR(&NAV)  TYPE(*CHAR) LEN(30)
             DCL        VAR(&ADR)  TYPE(*CHAR) LEN(30)
             DCL        VAR(&POS)  TYPE(*CHAR) LEN(4)
             DCL        VAR(&STE)  TYPE(*CHAR) LEN(25)
/*S01        DCL        VAR(&ADRBL)  TYPE(*CHAR) LEN(25)      */
/*N01*/      DCL        VAR(&ADRBL)  TYPE(*CHAR) LEN(30)
/*N02*/      DCL        VAR(&FØRST)  TYPE(*CHAR) LEN(1)
/*N02*/      DCL        VAR(&DIST)  TYPE(*CHAR) LEN(9)
/*N02*/      DCL        VAR(&OPEN)  TYPE(*CHAR) LEN(91)
             DLTOVR     FILE(TPSERV) LVL(*JOB)
             MONMSG     MSGID(CPF0000)
             CRTDUPOBJ  OBJ(TPSERV) FROMLIB(*LIBL) OBJTYPE(*FILE) +
                          TOLIB(QTEMP) NEWOBJ(TPSERV)
             MONMSG     MSGID(CPF0000)
             CLRPFM     FILE(QTEMP/TPSERV)
             MONMSG     MSGID(CPF0000)
             OVRDBF     FILE(TPSERV) TOFILE(QTEMP/TPSERV) +
                          OVRSCOPE(*JOB)
    /* */
    /*                                                               */
    /* */
             CALL       PGM(SYTG01) PARM(&NAV &ADR &POS &STE)
             RTVMBRD    FILE(QTEMP/TPSERV) NBRCURRCD(&ANTREC)
/*           HVIS 0 REKORD PRØVER IGJEN MED BLANKT GATAADRESSE */
             IF         COND(&ANTREC = 0) THEN(CALL PGM(SYTG01) +
                          PARM(&NAV &ADRBL &POS &STE))
/*S02        CALL       PGM(SYTG02) PARM(&NAV &ADR &POS &STE &KUNR)   */
/*N02*/      CALL       PGM(SYTG02) PARM(&NAV &ADR &POS &STE &KUNR +
                          &FØRST &DIST &OPEN)
             ENDPGM
