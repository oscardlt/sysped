             PGM        PARM(&FILNV &LIB)
             DCL        VAR(&FILNV) TYPE(*CHAR) LEN(10)
             DCL        VAR(&LIB) TYPE(*CHAR) LEN(10) /* Drift +
                          bibliotek. */
             DCLF       FILE(SYV12_P21F/V12PDSPDBR)

             DSPDBR     FILE(&LIB/&FILNV) OUTPUT(*OUTFILE) +
                          OUTFILE(SYV12_P21F/V12PDSPDBR) /* Liste +
                          alle logiske filer. */
             MONMSG     MSGID(CPF0000) EXEC(GOTO CMDLBL(SLUTT))

 START:      RCVF       RCDFMT(QWHDRDBR)
             MONMSG     MSGID(CPF0000) EXEC(GOTO CMDLBL(SLUTT))

             IF         COND(&WHREFI *NE '          ') THEN(DO)
             IF         COND(&WHREFI *NE &FILNV) THEN(DLTF +
                          FILE(&WHRELI/&WHREFI)) /* Slett alle +
                          logiske filer i driftbibliotek. */
             ENDDO
             GOTO       CMDLBL(START)

 SLUTT:      ENDPGM
