/* RSTTAX SKAL KOPIERES TIL QGPL OG KJØRES DERFRA */
             PGM        PARM(&LIB)
             DCL        VAR(&LIB) TYPE(*CHAR) LEN(10)
/* TØM BIBLIOTEK SVTTAX */
             DLTOBJ     OBJ(SVTTAX/*ALL) OBJTYPE(*FILE)
/* RESTORE SVTTAX  */
             RSTOBJ     OBJ(*ALL) SAVLIB(SVTTAX) DEV(*SAVF) +
                          SAVF(QGPL/SVTTAX) MBROPT(*ALL) +
                          ALWOBJDIF(*ALL)
/* KOPIER FRA SVTTAX TIL &LIB */
