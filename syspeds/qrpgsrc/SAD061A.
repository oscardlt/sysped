     C*
     C*** AVGIFT KAN OVERSTYRES BÅDE SATS OG BELØP
     C     STTST1        TAG
     C     W2KDAÆ        IFNE      *BLANKS                                      1<-B
     C     W2KDAÆ        ANDNE     '99'
     C     W2KDAÆ        IFEQ      'TL'
     C     W2KDAÆ        OREQ      'TA'
S04  C*    W2KDAÆ        OREQ      'MV'
     C                   MOVE      '1  '         W2KDSÆ
     C                   END
     C     KODS8K        CHAIN     KOS8R                              79
     C   79W2KDAÆ        CHAIN     KODTS8L2                           79
     C  N79W2KDAÆ        IFNE      'TL'
     C     STTSTSKV      TAG
     C                   IF        W2KDSÆ = *BLANKS
     C                   MOVE      *BLANKS       XSDKDSÆ           3
     C     SADS5K        SETLL     SADSD
     C     SADS5K        READE     SADSD                                  79
     C                   ELSE
     C     SADS3K        SETLL     SADSD
     C     SADS3K        READE     SADSD                                  79
     C                   END
     C     *IN79         DOWEQ     '0'
     C     SDDTF         IFLE      USIDT
     C     SDDTT         ANDGE     USIDT
     C                   IF        W2KDSÆ = *BLANKS
     C                   IF        XSDKDSÆ = *BLANKS
     C                   MOVE      SDKDSÆ        XSDKDSÆ           3
     C                   ELSE
     C                   MOVE      'S'           UMOD              1            AVG.SEKVENS
     C                   CALL      'SAD061O'
     C                   PARM                    W2KDAÆ
     C                   PARM                    W2KDSÆ
     C                   PARM                    UMOD
     C                   GOTO      STTSTSKV
     C                   END
     C                   ELSE
     C                   LEAVE
     C                   END
     C                   END
     C                   IF        W2KDSÆ = *BLANKS
     C     SADS5K        READE     SADSD                                  79
     C                   ELSE
     C     SADS3K        READE     SADSD                                  79
     C                   END
     C  N79              ENDDO
     C                   END
     C   79              IF        W2KDSÆ = *BLANKS
     C                   IF        XSDKDSÆ <> *BLANKS
     C                   MOVE      XSDKDSÆ       W2KDSÆ
     C                   MOVE      *OFF          *IN79
     C                   END
     C                   END
     C   79              SETON                                        30
     C*** NÅR GRUNNLAG IKKE OPPGIS HENTER SYSTEMET GRUNNLAG AVHENGIG AV
     C*** AVGIFT TYPE
     C*** AVGIFT MED '%' I SATSTYPE - STAT. VERDI PÅ LINJE SETTES INN
     C*** AVGIFT MED 'P' I SATSTYPE - NETTO VEKT  PÅ LINJE SETTES INN
     C*** AVGIFT MED 'H' I SATSTYPE - NETTO VEKT  PÅ LINJE SETTES INN
     C*** AVGIFT MED 'K' I SATSTYPE - NETTO VEKT  PÅ LINJE SETTES INN
     C*** AVGIFT MED ' ' I SATSTYPE - ANTALL STK. PÅ LINJE SETTES INN
     C*** AVGIFT MED '*' I SATSTYPE - ANTALL STK. PÅ LINJE SETTES INN
      *** AVGIFT MED 'G' I SATSTYPE - SATS PR GRAM (NETTO)
      *** AVGIFT MED 'M' I SATSTYPE - MÅ ANGIS MANUELT
     C*** AVGIFT MED 'D' I SATSTYPE - STYKK/12 SETTES INN
     C*** AVGIFT MED 'T' I SATSTYPE - NETTO VEKT/1000 SETTES INN
      *** AVGIFT MED 'L' I SATSTYPE - MANUELL SETT PÅ AVGIFTGRUNNLAG
N14   *** AVGIFT MED 'B' I SATSTYPE - MANUELL SETT PÅ AVGIFTGRUNNLAG, ANTALL HESTEKRAFT
     C  N79W2BLGÆ        IFEQ      0                                             2<-B
     C     KS8STY        ANDNE     'M'
     C     KS8STY        IFEQ      '%'                                            3<-B
     C  N38XTVFA2        IFEQ      0
     C     W2BELT        MULT(H)   XTVFAK        XBEL9N            9 0
     C                   ELSE
     C     W2BELT        MULT(H)   XTVFA2        XBEL9N            9 0
     C                   END
     C  N38              Z-ADD     XBEL9N        W2BLGÆ
     C   38              Z-ADD     XPROGR        W2BLGÆ
     C                   ELSE                                                     3
     C     KS8STY        IFEQ      'P'                                             4<-B
     C     KS8STY        OREQ      'H'
     C     KS8STY        OREQ      'K'
     C     KS8STY        OREQ      'G'
     C                   IF        KS8AVG <> 'FK'
     C**JOV!!    KS8STY    OREQ 'E'
     C  N38W2VKTN        IFEQ      0
     C     W2VKTB        MULT(H)   UBVNV         W2VKTN
     C                   ENDIF
     C  N38              Z-ADD     W2VKTN        W2BLGÆ
     C   38              Z-ADD     XVEKGR        W2BLGÆ
     C     KS8STY        IFEQ      'G'
     C**JOV!!    KS8STY    OREQ 'E'
     C                   MULT      1000          W2BLGÆ
     C                   END
     C                   END
     C                   ELSE                                                      4
     C     KS8STY        IFEQ      'D'
     C  N38              EVAL(H)   W2BLGÆ = W2NTM / 12 + 0.5
     C   38              EVAL(H)   W2BLGÆ = XSTKGR / 12 + 0.5
     C                   ELSE
     C     KS8STY        IFEQ      'T'
     C  N38              EVAL(H)   W2BLGÆ = W2VKTN / 1000 + 0.5
     C   38              EVAL(H)   W2BLGÆ = XVEKGR / 1000 + 0.5
     C                   ELSE
     C     KS8STY        IFNE      'M'
     C     KS8STY        ANDNE     'A'
N14  C     KS8STY        ANDNE     'B'
      * KS8STY = '*' ELLER ' '  ELLER 'L'
     C  N38              Z-ADD     W2NTM         W2BLGÆ
     C   38              Z-ADD     XSTKGR        W2BLGÆ
     C                   END
     C                   END
     C                   END
     C                   END                                                       4<-E
     C                   END                                                      3<-E
     C                   END                                                     2<-E
     C                   MOVE      KS8ØRE        XS8ØRE            1
     C  N79W2BLÆ         IFGE      999999999                                     2<-B
     C                   Z-ADD     0             W2BLÆ
     C                   MOVE      ' '           XEH2Æ             1
     C                   ELSE                                                    2
     C*** HVIS SATS=0, HENTES SATS FRA KODE-REGISTER
     C     W2BLSÆ        IFEQ      0
     C     SDBLSÆ        IFEQ      0
     C                   MOVE      KS8SAT        W2BLSÆ
     C                   ELSE
     C     SDBLSÆ        IFNE      99999,99
     C     SDBLSÆ        ANDNE     999,99
     C                   MOVE      SDBLSÆ        W2BLSÆ
     C                   END
     C                   END
     C                   END
     C                   MOVE      KS8STY        XEH2Æ
