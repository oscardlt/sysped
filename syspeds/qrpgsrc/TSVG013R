      /copy SYSPEDS/qrpgsrcpy,hspecs
      /copy SYSPEDS/qrpgsrcpy,hspecsbnd
      *********************************************************************
     FBRIDF     IF   E           K DISK    USROPN
     FEDIC      UF   E             DISK    USROPN
     FSVTFI     IF   E             DISK    USROPN
     FEDIM      O  A E             DISK    USROPN
     FSVIH      UF   E           K DISK    USROPN
     FSVEH      UF   E           K DISK    USROPN
     FSVNH      UF   E           K DISK    USROPN
     FSVXH      UF   E           K DISK    USROPN
      *--------------------------------------------------------------------
      * Variables common to all CGIs
      *--------------------------------------------------------------------
      /copy SYSPEDS/qrpgsrcpy,prototypeb
      /copy SYSPEDS/qrpgsrcpy,usec
      /copy SYSPEDS/qrpgsrcpy,variables3
      *
      * Varible for
     D WSUSER          S             10
     D AVD             S              4
     D AVDA            S              4
     D AVDN            S              4  0
     D OPD             S              7
     D OPDA            S              7
     D OPDN            S              7  0
     D URSP            S              2
     D JSONstring      s          32000
     D Error           S            150
     D AntLest         S              9  0
      *   SYEDI data area
     D EDIDTA          DS           256
     D  UELIBF               171    180

      *get input-string
      /copy syspeds/qrpgsrcpy,prolog3

      * Parse input
     C                   exsr      Parse
      * Open files etc
     C                   EXSR      INIT
      * signera respektive
     C     URSP          CASEQ     'TI'          SRTI
     C                   END
     C     URSP          CASEQ     'TE'          SRTE
     C                   END
     C     URSP          CASEQ     'NI'          SRNI
     C                   END
     C     URSP          CASEQ     'NE'          SRNE
     C                   END
      *
     c                   callp     gethtmlifs(
     C                             '/syspeddev/html/JSON.html':
     C                             '/CGITAG/')
     C                   callp     wrtsection('top')
      *
      * ............................................................................................
      * skriv JSON-Object start..(alltid '{' + x ant param. m/komma mellom (IKKE komma etter siste))
      * ............................................................................................
      *
      /free
        JSONstring='{'
        +'¬user¬ : ¬'+%trim(WSUSER)+'¬, '
        +'¬ursp¬ : ¬'+%trim(URSP)+'¬, '
        +'¬avd¬ : ¬'+%trim(%editc(AVDN:'Z'))+'¬, '
        +'¬opd¬ : ¬'+%trim(%editc(OPDN:'Z'))+'¬, '
        +'¬errMsg¬ : ¬'+%trim(Error)+'¬, ';
      /end-free
      * JSONfix escaper " i tekst,  for deretter å bytte ¬->"
     c                   call      'JSONFIX'
     c                   parm                    JSONstring
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')
      *
      * ............................................................................................
      * Skriv JSON-LISTE Start (en liste er EN parameter(fra [ til   )
      * ............................................................................................
     C                   eval      JSONstring ='"signering" : ['
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')
     c                   eval      JSONstring =']'
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')
      * ............................................................................................
      * Skriv JSON-string Avsluttning
      * ............................................................................................
     c                   eval      JSONstring ='}'
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')
      *
      * Quit
     C                   exsr      Exit


      *=====================================================================
      * Close output html and quit
      *=====================================================================
     C     Exit          begsr
      * Lukk fil
     C                   CLOSE     BRIDF
     C  N50              CLOSE     EDIC
     C  N50              CLOSE     SVTFI
     C  N50              CLOSE     EDIM
     C  N50              CLOSE     SVIH
     C  N50              CLOSE     SVEH
     C  N50              CLOSE     SVNH
     C  N50              CLOSE     SVXH

      * Do not delete the call to wrtsection with section name *fini.  It is needed
      * to ensure that all output html that has been buffered gets output.
     C                   callp     wrtsection('*fini')
      * Quit
     C                   MOVE      *ON           *INLR
     C                   return
     C                   endsr
      *
      *********************************************************
     C     Parse         Begsr
      *********************************************************
      * Get input
     C                   EVAL      WSUSER  = zhbgetvar('USER')
     C                   EVAL      AVD     = zhbgetvar('AVD')
     C                   eval      AVDN    = c2n2(AVD)
     C                   MOVE      AVDN          AVDA
     C                   EVAL      OPD     = zhbgetvar('OPD')
     C                   eval      OPDN    = c2n2(OPD)
     C                   MOVE      OPDN          OPDA
     C                   EVAL      URSP    = zhbgetvar('ursp')
     c                   endsr
      *
      *********************************************************
     C     INIT          Begsr
      *********************************************************
     C                   OPEN      BRIDF
      * Test innlogging
     C     WSUSER        CHAIN     BRIDF                              50
     C     BILIBL        ifeq      *blanks
     C                   callb     'INCGI'
     C                   parm                    BISTDL
     C                   else
     C                   call      BILIBL
     C                   end
      *
     C     SvKey         klist
     C                   kfld                    AVDN
     C                   kfld                    OPDN
      *
     C   50              eval      Error = 'Invalid userID'
     C                   IF        *IN50 = *OFF
     C                   OPEN      EDIC
     C                   OPEN      SVTFI
     C                   OPEN      EDIM
     C                   OPEN      SVIH
     C                   OPEN      SVEH
     C                   OPEN      SVNH
     C                   OPEN      SVXH
      * Hæmta firmaupplysningar
     C     1             CHAIN     SVTFI                              51
      * Hæmta næsta meddelandenummer
     C     1             CHAIN     EDIC                               51
     C                   ADD       1             CMN
     C                   UPDATE    EDICR
      * Hæmta dagens datum och klockslag
     C                   CALL      'SYGE15C'
     C                   PARM                    WDT               8
     C                   PARM                    WTM               6
      * Åpne dataarea
     C     *DTAARA       DEFINE                  EDIDTA
     C                   IN        EDIDTA
     C                   ENDIF

     c                   endsr
      *
      *___________________________________________
      * Signera TDS Import                    SRTI
      *"""""""""""""""""""""""""""""""""""""""""""
     C     SRTI          BEGSR
      *_____________
      * Ændra status
      *"""""""""""""
     C     SVKEY         CHAIN     SVIH                               52
     C                   EVAL      SVIH_SYST='A'
     C                   UPDATE    SVIHR
      *_______________
      * Skriv til EDIM
      *"""""""""""""""
     C                   CLEAR                   EDIMR
     C                   EVAL      M0004=SVTF_0004
     C                   EVAL      M0010=SVTF_0010
     C*                  EVAL      M0035=SVTF_0035
     C                   EVAL      M0035=SVIH_0035
     C                   MOVEL     SVIH_SYAV     W0062            11
     C                   MOVE      SVIH_SYOP     W0062
     C                   EVAL      M0062=W0062
     C                   EVAL      M0065='CUSDEC'
     C                   EVAL      M1001=SVIH_MTYP
     C                   EVAL      M1004=SVIH_TUID
     C                   EVAL      M1225=SVIH_MTYP
     C                   EVAL      M1N07='SVI'
     C                   EVAL      MMN=CMN
     C                   EVAL      MSR='S'
     C                   MOVEL     WDT           MDT
     C                   MOVEL     WTM           MTM
     C                   EVAL      MAVD=SVIH_SYAV
     C                   EVAL      MTDN=SVIH_SYOP

     C                   WRITE     EDIMR
     C                   ENDSR
      *___________________________________________
      * Signera TDS Export                    SRTE
      *"""""""""""""""""""""""""""""""""""""""""""
     C     SRTE          BEGSR
      *_____________
      * Ændra status
      *"""""""""""""
     C     SVKEY         CHAIN     SVEH                               52
     C                   EVAL      SVEH_SYST='A'
     C                   UPDATE    SVEHR
      *_______________
      * Skriv til EDIM
      *"""""""""""""""
     C                   CLEAR                   EDIMR
     C                   EVAL      M0004=SVTF_0004
     C                   EVAL      M0010=SVTF_0010
     C*                  EVAL      M0035=SVTF_0035
     C                   EVAL      M0035=SVEH_0035
     C                   MOVEL     SVEH_SYAV     W0062            11
     C                   MOVE      SVEH_SYOP     W0062
     C                   EVAL      M0062=W0062
     C                   EVAL      M0065='CUSDEC'
     C                   EVAL      M1001=SVEH_MTYP
     C                   EVAL      M1004=SVEH_TUID
     C                   EVAL      M1225=SVEH_MTYP
     C                   EVAL      M1N07='SVE'
     C                   EVAL      MMN=CMN
     C                   EVAL      MSR='S'
     C                   MOVEL     WDT           MDT
     C                   MOVEL     WTM           MTM
     C                   EVAL      MAVD=SVEH_SYAV
     C                   EVAL      MTDN=SVEH_SYOP

     C                   WRITE     EDIMR
     C                   ENDSR
      *___________________________________________
      * Signera NCTS Import                   SRNI
      *"""""""""""""""""""""""""""""""""""""""""""
     C     SRNI          BEGSR
      *_____________
      * Ændra status
      *"""""""""""""
     C     SVKEY         CHAIN     SVNH                               52
     C                   EVAL      TIST='A'
     C                   UPDATE    SVNHR
      *_______________
      * Skriv til EDIM
      *"""""""""""""""
     C                   CLEAR                   EDIMR
     C                   EVAL      M0004=SVTF_0004
     C                   EVAL      M0010=SVTF_0010
     C                   EVAL      M0035=SVTF_0035
     C                   MOVEL     TIAVD         W0062            11
     C                   MOVE      TITDN         W0062
     C                   EVAL      M0062=W0062
     C                   EVAL      M0065='CUSDEC'
     C                   EVAL      M1001='STI'
     C                   EVAL      M1004=TITUID
     C                   EVAL      M0068=TITUID
N02  C                   IF        NIKONF = 1 OR NIMN1 <> *BLANKS
     C                   MOVEL     '044'         M1N07
     C                   MOVEL     '044'         M1225
     C                   ELSE
     C                   MOVEL     '007'         M1N07
     C                   MOVEL     '007'         M1225
     C                   END
     C                   EVAL      MMN=CMN
     C                   EVAL      MSR='S'
     C                   MOVEL     WDT           MDT
     C                   MOVEL     WTM           MTM
     C                   EVAL      MAVD=TIAVD
     C                   EVAL      MTDN=TITDN

     C                   WRITE     EDIMR
     C                   ENDSR
      *___________________________________________
      * Signera NCTS Export                   SRNE
      *"""""""""""""""""""""""""""""""""""""""""""
     C     SRNE          BEGSR
      *_____________
      * Ændra status
      *"""""""""""""
     C     SVKEY         CHAIN     SVXH                               52
     C                   EVAL      THST='A'
     C                   UPDATE    SVXHR
      *_______________
      * Skriv til EDIM
      *"""""""""""""""
     C                   CLEAR                   EDIMR
     C                   EVAL      M0004=SVTF_0004
     C                   EVAL      M0010=SVTF_0010
     C                   EVAL      M0035=SVTF_0035
     C                   MOVEL     THAVD         W0062            11
     C                   MOVE      THTDN         W0062
     C                   EVAL      M0062=W0062
     C                   EVAL      M0065='CUSDEC'
     C                   EVAL      M1001='STE'
     C                   EVAL      M1004=THTUID
     C                   EVAL      M0068=THTUID
     C                   EVAL      M1225=THMF
     C                   EVAL      M1N07=THMF
     C                   EVAL      MMN=CMN
     C                   EVAL      MSR='S'
     C                   MOVEL     WDT           MDT
     C                   MOVEL     WTM           MTM
     C                   EVAL      MAVD=THAVD
     C                   EVAL      MTDN=THTDN

     C                   WRITE     EDIMR
     C                   ENDSR
