             CMD        PROMPT('Sett PDAst 2=FERD. Gamle turer')
             PARM       KWD(SJ) TYPE(*CHAR) LEN(8) RTNVAL(*NO) +
                          DFT('00000073') FULL(*NO) +
                          PROMPT('Sjåførnr (m/ledende nuller)')
             PARM       KWD(DT) TYPE(*CHAR) LEN(8) RTNVAL(*NO) +
                          DFT('20090925') FULL(*NO) PROMPT('Dato +
                          til og med')
