             PGM        PARM(&FRALIB &TILLIB)
             DCL        VAR(&FRALIB) TYPE(*CHAR) LEN(10) /* Fra +
                          bibliotek V11 */
             DCL        VAR(&TILLIB) TYPE(*CHAR) LEN(10) /* Til +
                          bibliotek for datafiler V12 */

             OVRDBF     FILE(PRJOIN) TOFILE(&FRALIB/PRJOIN)
             OVRDBF     FILE(PRIK) TOFILE(&TILLIB/PRIK)
             CALL       PGM(V12RE107)
             OVRDBF     FILE(PRJOIN) TOFILE(&FRALIB/PRBJOIN)
             OVRDBF     FILE(PRIK) TOFILE(&TILLIB/PRIKB)
             CALL       PGM(V12RE107)

             ENDPGM
