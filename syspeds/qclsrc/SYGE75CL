/* INPUT DIREKTORY                 */
             PGM        PARM(&UDIR)
             DCL        VAR(&UDIR) TYPE(*CHAR) LEN(100) +
/*                      */
/* HENT FILNAVN         */
/*                      */
             CALL       PGM(SYGE75) PARM(&UDIR)
/*                      */
                          ENDPGM
