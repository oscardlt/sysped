             CMD        PROMPT('Opprett ny Saas kunde')
             PARM       KWD(TIL) TYPE(*CHAR) LEN(3) RSTD(*NO) +
                          PROMPT('Prefix ny kunde (A??): ')
             PARM       KWD(FRA) TYPE(*CHAR) LEN(3) RSTD(*NO) +
                          PROMPT('Prefix fra kunde (A??): ')
             PARM       KWD(L1) TYPE(*CHAR) LEN(1) RTNVAL(*NO) +
                          RSTD(*NO) DFT(N) PROMPT('Bruk L1? ')
