/*       * RETTINGER I DETTE PROGRAM:                                      */
             PGM
             DCL        VAR(&USOK) TYPE(*CHAR) LEN(30)
             DCL        VAR(&URRN) TYPE(*DEC) LEN(10 0)
             DCL        VAR(&UPOFA) TYPE(*CHAR) LEN(1)
/*______________*/
/* LES FIRMAFIL */
/*""""""""""""""*/
             DCLF       FILE(FIRM) RCDFMT(*ALL)
             RCVF       DEV(*FILE) RCDFMT(*FILE)

             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CALL       PGM(VCUNDC3R) PARM(&FIFIRM &USOK &URRN &UPOFA)
             ENDPGM
