    /* UTSKRIFT AV RUNDTURER */
             PGM
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1)
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10) /* Printernavn */
             DCL        VAR(&PT) TYPE(*CHAR) LEN(10) /* Papirtype */
             DCL        VAR(&HE) TYPE(*CHAR) LEN(1) /* HEADING */
             DCL        VAR(&TY) TYPE(*CHAR) LEN(1) VALUE('9')
             DCL        VAR(&JOBQ) TYPE(*CHAR) LEN(1)
             DCL        VAR(&USKJER) TYPE(*CHAR) LEN(1) /* */
             DCL        VAR(&NR) TYPE(*CHAR) LEN(10) /* BILNR */
             DCL        VAR(&JOBNBR) TYPE(*CHAR) LEN(6) /* JOBBNR */
             DCL        VAR(&MEMBER) TYPE(*CHAR) LEN(7) /* MEMBER */
             DCL        VAR(&PREFIX) TYPE(*CHAR) LEN(1) VALUE('A')
             DCL        VAR(&SKJE) TYPE(*CHAR) LEN(10)
             DCL        VAR(&KOD) TYPE(*CHAR) LEN(3)
             DCL        VAR(&TRAN) TYPE(*CHAR) LEN(8)
             DCL        VAR(&AVD) TYPE(*CHAR) LEN(4)
             DCL        VAR(&FP) TYPE(*CHAR) LEN(4) /* FRA PERIODE */
             DCL        VAR(&TP) TYPE(*CHAR) LEN(4) /* TIL PERIODE */
             RTVJOBA    JOB(&SKJE) NBR(&JOBNBR)
             CHGVAR     VAR(&MEMBER) VALUE(&PREFIX *CAT &JOBNBR)
             ADDPFM     FILE(XTUR) MBR(&MEMBER)
             MONMSG     MSGID(CPF0000 CPF7306)
             CLRPFM     FILE(XTUR) MBR(&MEMBER)
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CALL       PGM(SYFA00) PARM(&IN01)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT)) /* +
                          Hvis &IN01 på, avslutter program. */
             CALL       PGM(SYTR92A) PARM(&IN01 &AVD &PRT &PT &HE +
                          &NR &KOD &TRAN &FP &TP &JOBQ)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT)) /* +
                          Hvis &IN01 på, avslutter program. */
             IF         COND(&JOBQ = 'Y') THEN(DO)
             SBMJOB     CMD(CALL PGM(SYTR92CLJ) PARM(&SKJE &TY +
                          &MEMBER &PRT &PT &IN01 &AVD &NR &KOD +
                          &TRAN &FP &TP)) JOB(RUNDTUR) +
                          JOBD(*USRPRF) JOBQ(SYJOBQPRT) +
                          LOG(4 00 *MSG) LOGCLPGM(*YES)
                        ENDDO
             IF         COND(&JOBQ *NE 'Y') THEN(DO)
             CALL       PGM(SYTR92CLJ) PARM(&SKJE &TY &MEMBER &PRT +
                          &PT &IN01 &AVD &NR &KOD &TRAN &FP &TP)
             ENDDO
    /* */
 SLUTT:      ENDPGM
