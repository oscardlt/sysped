     A            TLIB          10          COLHDG('Filbibliotek')
     A            TFILE         10          COLHDG('Filnavn')
     A            TMBR          10          COLHDG('Komponentnavn')
     A            TDEV          10          COLHDG('Arbetsstasjon')
     A            TUSER         10          COLHDG('Brukerid')
     A            TEVENT         1          COLHDG('Postaktivitet')
      *                                     1=Add
      *                                     2=Delete
      *                                     3=Update
     A            TBEFAFT        1          COLHDG('Før/etter update')
      *     1=Etter Add/Delete/Update
      *     2=Før Add/Delete/Update
     A            TDATE          8          COLHDG('Dato')
     A            TTIME          6          COLHDG('Tid')
      * NOEN FELTER FRA KUNDREGISTRET
     A            FIRMA          2A         COLHDG('FIRMCODE')
     A            KUNDNR         8S 0       COLHDG('CUST.NO')
     A            KNAVN         30A         COLHDG('NAME')
      * FRITEKST VEDR ENDREDE FELTER
     A            TFTX        1024A         COLHDG('FREETXT')
