N09  C                   Z-ADD     HEFBV         XXXXFB            9 0
N09  C     HEAKEY        CHAIN     HEADF                              90
N09  C  N90              Z-ADD     XXXXFB        HEFBV
N09  C  N90              EXCEPT    UPDHE
N09  C                   END
N08  C                   EXSR      setFBV
s08  C*    AFTYPE        IFEQ      'M'
s08  C*                  MOVEL     HEM3          XXXM3             6 0            *HEM3=7,3
s08  C*                  Z-ADD     XXXM3         HEFBV
s08  C*                  ELSE
s08  C*    AFTYPE        IFEQ      'L'
s08  C*                  MOVE      *ZEROS        HEFBV
s08  C*                  MOVE      HELM          HEFBV
s08  C*                  END
s08  C*                  END
s08  C*    AFTYPE        IFEQ      ' '
