             PGM        PARM(&ZTRAN)
             DCL        VAR(&ZTRAN) TYPE(*CHAR) LEN(8)
             DCL        VAR(&PNR) TYPE(*DEC) LEN(3 0) VALUE(013)
             DCL        VAR(&AVD) TYPE(*CHAR) LEN(4)
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10)
             DCL        VAR(&FORM) TYPE(*CHAR) LEN(10)
             RTVDTAARA  DTAARA(*LDA (15 4)) RTNVAR(&AVD)
             CALL       PGM(GETPRT) PARM(&AVD &PNR &PRT &FORM)
             OVRPRTF    FILE(E308P) PAGESIZE(70 80) CPI(10) +
                          OVRFLW(60) OUTQ(&PRT) FORMTYPE(&FORM) +
                          COPIES(2) HOLD(*YES)
             CALL       PGM(E308R) PARM(&ZTRAN)
             CALL       PGM(E309R) PARM(&ZTRAN)
             SNDPGMMSG  MSG('Liste (i 2 eksemplar) er sendt til +
                          printer ' *BCAT &PRT *BCAT '.') +
                          MSGTYPE(*INFO)
 SLUTT:      ENDPGM
