/* *********************************************************************   */
/* ********************************************************************    */
/* SENDE  PROFORMA     PÅ EMAIL       */
             PGM        PARM(&AVDOPD &STMF &BANE &VISE &INFOP)
             DCL        VAR(&INFOP) TYPE(*CHAR) LEN(1)
             DCL        VAR(&TYPE) TYPE(*CHAR) LEN(3) VALUE('CPY')
             DCL        VAR(&COMM) TYPE(*CHAR) LEN(1) VALUE('N')
             DCL        VAR(&KNC) TYPE(*CHAR) LEN(8) VALUE('00000000')
             DCL        VAR(&UTFAX) TYPE(*CHAR) LEN(1)
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10) VALUE('OUTMAIL')
             DCL        VAR(&CPY) TYPE(*CHAR) LEN(1) VALUE('1')
             DCL        VAR(&UTFAX) TYPE(*CHAR) LEN(1) VALUE('A')
             DCL        VAR(&LOC1) TYPE(*CHAR) LEN(5)
             DCL        VAR(&LOC2) TYPE(*CHAR) LEN(5)
             DCL        VAR(&LOC3) TYPE(*CHAR) LEN(5)
             DCL        VAR(&SPLF) TYPE(*CHAR) LEN(10) +
                          VALUE('GSS022P   ')
             DCL        VAR(&STMF) TYPE(*CHAR) LEN(152)
             DCL        VAR(&VISE) TYPE(*CHAR) LEN(1)
             DCL        VAR(&BANE) TYPE(*CHAR) LEN(50)
             DCL        VAR(&QUSER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QJOB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QNBR) TYPE(*CHAR) LEN(6)
             DCL        VAR(&AVDOPD) TYPE(*CHAR) LEN(11)
             DCL        VAR(&UNIK) TYPE(*CHAR) LEN(7)
    /* SKRIVER UT FRAKTBREVKOPI           */
    /*                                    */
             RTVJOBA    JOB(&QJOB) USER(&QUSER) NBR(&QNBR)
    /* ORDNER PRINTER FOR FRAKTBREV */
             IF         COND(&INFOP = 'D') THEN(DO)
             OVRPRTF    FILE(&SPLF) DEVTYPE(*AFPDS) PAGESIZE(70 120) +
                          LPI(6) CPI(15) FRONTMGN(0.1 0.25) +
                          OVRFLW(63) OUTQ(OUTMAIL) FORMTYPE(*STD) +
                          TOSTMF(&BANE) WSCST(*PDF)
                          ENDDO
             IF         COND(&INFOP *NE 'D') THEN(DO)
             OVRPRTF    FILE(&SPLF) DEVTYPE(*AFPDS) PAGESIZE(70 120) +
                          LPI(6) CPI(15) FRONTMGN(0.1 0.25) +
                          OVRFLW(63) OUTQ(OUTMAIL) FORMTYPE(*STD)
                          ENDDO
             CHGVAR     VAR(&UNIK) VALUE('W' *CAT &QNBR)
             RMVM       FILE(GSS022W) MBR(&UNIK)
             MONMSG     MSGID(CPF0000)
             ADDPFM     FILE(GSS022W) MBR(&UNIK)
             OVRDBF     FILE(GSS022W) TOFILE(GSS022W) MBR(&UNIK)
             RMVM       FILE(GSS031W) MBR(&QJOB)
             MONMSG     MSGID(CPF0000)
             ADDPFM     FILE(GSS031W) MBR(&QJOB)
             OVRDBF     FILE(GSS031W) TOFILE(GSS031W) MBR(&QJOB)
             CHGVAR     VAR(&UTFAX) VALUE('A')
/*N01*/      CALL       PGM(GSS022R) PARM(&AVDOPD &TYPE &CPY &PRT +
                          &UTFAX &QJOB &QUSER +
                          &QNBR)
             IF         COND(&INFOP = 'D') THEN(RETURN)
/*                                       */
             ADDLIBLE   LIB(MMAIL)
             MONMSG     MSGID(CPF0000)
/*                                       */
/* LAGER PDF OG LEGGER I MAPPE          */
/* UTEN INFOPRINT                        */
/*                                       */
             IF         COND(&INFOP = 'N') THEN(DO)
             CVTSPLFPDF SPLF(&SPLF) JOB(&QNBR/&QUSER/&QJOB) +
                          TOPDF(&BANE) ORIENTAT(*V)
             DLTSPLF    FILE(&SPLF) JOB(&QNBR/&QUSER/&QJOB) +
                          SPLNBR(*LAST)
             MONMSG     MSGID(CPF0000)
                          ENDDO
/*                                       */
/*                                       */
/* LAGER PDF OG LEGGER I MAPPE           */
/* MED  INFOPRINT                        */
/*                                       */
             IF         COND(&INFOP = 'J') THEN(DO)
             CALL       PGM(INF001CL) PARM(&SPLF &QNBR &QUSER &QJOB +
                          &BANE)
                          ENDDO
/*                                       */
             RMVLIBLE   LIB(MMAIL)
             MONMSG     MSGID(CPF0000)
    /* */

/*                                       */
    /* */
 SLUTT:      ENDPGM
