N06   *
N06   * HVIS POSTAL FORSØK Å HENTE POSTNR FRA HENTE/LEVADR.
N06   * START POSTNUMMERLOGIKK
N06   *
N06  C     KOAPOS        IFEQ      'J'                                          B1
N06  C* FORFRAKT FRA
N06  C     WSSDFF        IFEQ      *BLANKS                                       B2
N06  C     WSADS3        ANDNE     *BLANKS                                       B2
N06  C                   MOVEL     WSADS3        TESTN5            5
N06  C                   TESTN                   TESTN5               33
N06  C     *IN33         IFEQ      '1'                                            B3
N06  C                   MOVEL     TESTN5        WSSDFF
N06  C                   ELSE                                                     X3
N06  C                   MOVEL     WSADS3        TESTN4            4
N06  C                   TESTN                   TESTN4               33
N06  C     *IN33         IFEQ      '1'                                             B4
N06  C                   MOVEL     TESTN4        WSSDFF
N06  C                   END                                                       E4
N06  C                   END                                                      E3
N06  C                   END                                                     E2
N06  C* VIDEREFRAKT TIL
N06  C     WSSDVT        IFEQ      *BLANKS                                       B2
N06  C     WSADK3        ANDNE     *BLANKS                                       B2
N06  C                   MOVEL     WSADK3        TESTN5            5
N06  C                   TESTN                   TESTN5               33
N06  C     *IN33         IFEQ      '1'                                            B3
N06  C                   MOVEL     TESTN5        WSSDVT
N06  C                   ELSE                                                     X3
N06  C                   MOVEL     WSADK3        TESTN4            4
N06  C                   TESTN                   TESTN4               33
N06  C     *IN33         IFEQ      '1'                                             B4
N06  C                   MOVEL     TESTN4        WSSDVT
N06  C                   END                                                       E4
N06  C                   END                                                      E3
N06  C                   END                                                     E2
N06   * HVIS REN IMP/EKS (ELLER INNLAND) KAN LANDKODE SETTES???
N06  C                   SELECT
N06  C     KOAIE         WHENEQ    'I'
N06  C* IMPORT
N06  C     WSLKS         IFEQ      *BLANKS
N06  C                   MOVE      KOALK         WSLKS
N06  C                   END
N06  C     WSLKK         IFEQ      *BLANKS
N06  C                   MOVE      FILAND        WSLKK
N06  C                   END
N06   *EKSPORT
N06  C     KOAIE         WHENEQ    'E'
N06  C     WSLKS         IFEQ      *BLANKS
N06  C                   MOVE      FILAND        WSLKS
N06  C                   END
N06  C     WSLKK         IFEQ      *BLANKS
N06  C                   MOVE      KOALK         WSLKK
N06  C                   END
N06   *INNLAND
N06  C     KOAIE         WHENEQ    'D'
N06  C     WSLKS         IFEQ      *BLANKS
N06  C                   MOVE      FILAND        WSLKS
N06  C                   END
N06  C     WSLKK         IFEQ      *BLANKS
N06  C                   MOVE      FILAND        WSLKK
N06  C                   END
N06  C                   ENDSL
N06  C*
N06  C                   MOVE      '0'           *IN30
N06  C*
N06  C                   END
