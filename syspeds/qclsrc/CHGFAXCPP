             PGM        PARM(&USERID)
             DCL        VAR(&USERID) TYPE(*CHAR) LEN(8)
             DCL        VAR(&FAXNBR) TYPE(*CHAR) LEN(32)
             DCL        VAR(&TO) TYPE(*CHAR) LEN(40)
             DCL        VAR(&TITLE) TYPE(*CHAR) LEN(40)
             DCL        VAR(&COMMENT) TYPE(*CHAR) LEN(40)
             DCL        VAR(&TO1) TYPE(*CHAR) LEN(40)
             DCL        VAR(&TO2) TYPE(*CHAR) LEN(40)
             DCL        VAR(&TO3) TYPE(*CHAR) LEN(40)
             DCL        VAR(&FI) TYPE(*CHAR) LEN(2)
             DCL        VAR(&KNU) TYPE(*CHAR) LEN(8)
             DCL        VAR(&ALFA) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TELE) TYPE(*CHAR) LEN(32)
             DCL        VAR(&IN) TYPE(*CHAR) LEN(1)
             DCL        VAR(&MSG) TYPE(*CHAR) LEN(150)
             DCL        VAR(&DESC) TYPE(*CHAR) LEN(50)
             DCL        VAR(&DESC1) TYPE(*CHAR) LEN(50)
             CALL       PGM(HNTREC) PARM(&IN &USERID &FAXNBR &TO &TITLE +
                          &COMMENT &TO1 &TO2 &TO3 &FI &KNU &ALFA &TELE)
             IF         COND(&IN *EQ '1') THEN(DO)
             GOTO       CMDLBL(SLUTT)
             ENDDO
             CHGVAR VAR(&DESC1) VALUE(&KNU *CAT ' ' *CAT &TO)
             CHGUSRPRF USRPRF(&USERID) PASSWORD(*NONE) +
                       TEXT(&TO)
             MONMSG MSGID(CPF0000) EXEC(GOTO CMDLBL(END))
             CHGVAR VAR(&DESC) VALUE('FAX USER ' *CAT &USERID)
             CHGDIRE    USRID(&USERID FAXUSR) USRD(&DESC &DESC) +
                          USER(&USERID) LSTNAM(&TO) FSTNAM(&ALFA) +
                          FULNAM(&TO) DEPT(&KNU) CMPNY(&FI) +
                          TELNBR1(&TELE) TELNBR2(&FAXNBR) +
                          ADDR1(&TO1) ADDR2(&TO2) ADDR3(&TO3) +
                          INDUSR(*YES) PRTPERS(*YES) PRTCOVER(*NO) +
                          TEXT(&DESC)
             MONMSG MSGID(CPF0000)
             CALL       PGM(CHGREC) PARM(&USERID &faxnbr &to &title +
                          &comment &TO1 &TO2 &TO3 &FI &KNU &ALFA &TELE)
             SNDPGMMSG  MSG('Brukerprofilen er endret')
 SLUTT:      SNDPGMMSG  MSG('Brukerprofilen ble ikke endret')
 END:        RETURN
             ENDPGM
