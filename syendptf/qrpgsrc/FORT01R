12XXX * 01 YBC PTF12 TILPASSE ENDRING I SADV
********************************************************************
     FDKEH      IF   E           K DISK
     FDKEV      IF   E           K DISK
     FSADH      UF A E           K DISK
     FSADV      UF A E           K DISK
     FSTANDI    IF   E           K DISK

     C                   EXSR      INIT
     C                   EXSR      HODE
     C     *IN51         CABEQ     *ON           SLUTT
     C                   EXSR      LINJE

     C     SLUTT         TAG

     C                   EVAL      *INLR=*ON
      *___________________________________________________
      * Init                                          INIT
      *"""""""""""""""""""""""""""""""""""""""""""""""""""
     C     INIT          BEGSR

     C     *ENTRY        PLIST
     C                   PARM                    UAVDFRA           4
     C                   PARM                    UAVDTIL           4
     C                   PARM                    UOPDFRA           7
     C                   PARM                    UOPDTIL           7
     C                   PARM                    USISG             3

     C     *LIKE         DEFINE    SIAVD         WSIAVD
     C     *LIKE         DEFINE    SITDN         WSITDN
     C     *LIKE         DEFINE    SISG          WSISG
     C     *LIKE         DEFINE    SVLI          WSVLI
     C     *LIKE         DEFINE    DKEH_SYAV     WSYAV
     C     *LIKE         DEFINE    DKEH_SYOP     WSYOP

     C     KEYDKEH       KLIST
     C                   KFLD                    WSYAV
     C                   KFLD                    WSYOP

     C                   MOVE      UAVDFRA       WSYAV
     C                   MOVE      UOPDFRA       WSYOP
     C                   MOVE      UAVDTIL       WSIAVD
     C                   MOVE      UOPDTIL       WSITDN
     C                   MOVE      USISG         WSISG

     C                   CALL      'SYGE15C'
     C                   PARM                    WDT               8
     C                   PARM                    WTM               6

     C                   ENDSR
      *___________________________________________________
      * Hode                                          HODE
      *"""""""""""""""""""""""""""""""""""""""""""""""""""
     C     HODE          BEGSR

     C                   CLEAR                   SADHR
     C     WSIAVD        CHAIN     STANDI                             51
     C     *IN51         CABEQ     *ON           HODE9

     C     KEYDKEH       CHAIN     DKEH                               51
     C     *IN51         CABEQ     *ON           HODE9

     C                   EVAL      SIST=*BLANKS
     C                   EVAL      SIAVD=WSIAVD
     C                   EVAL      SITDN=WSITDN
     C                   EVAL      SIDTY='EU'
     C                   MOVEL     DKEH_02B      SINAS
     C                   MOVEL     DKEH_02C      SIADS1
     C                   EVAL      SIADS2=%trim(DKEH_02D)+' '+%trim(DKEH_02E)
     C                   MOVEL     DKEH_02F      SIADS3
     C                   EVAL      SINTK=DKEH_06
     C                   EVAL      SIVKB=DKEH_BRUT
     C                   EVAL      SISG=WSISG
     C                   MOVEL     DKEH_08B      SINAK
     C                   MOVEL     DKEH_08C      SIADK1
     C                   EVAL      SIADK2=%trim(DKEH_08D)+' '+%trim(DKEH_08E)
     C                   MOVEL     DKEH_08F      SIADK3
     C                   EVAL      SILKA='DK'
     C                   MOVEL     DKEH_201      SILV
     C                   MOVEL     DKEH_202      SILVT
     C                   MOVEL     DKEH_181      SITRID
     C                   MOVEL     DKEH_21       SILKT
     C                   EVAL      SIVAL3=DKEH_221
     C                   EVAL      SIBEL3=DKEH_222
     C                   MOVEL     DKEH_25       SITRM
     C                   MOVEL     DKEH_28B      SIFIF
     C                   EVAL      SIFID=DKEH_28C
     C                   MOVEL     WDT           SIDT
     C                   WRITE     SADHR
     C

     C     HODE9         TAG
     C                   ENDSR
      *___________________________________________________
      * Linje                                        LINJE
      *"""""""""""""""""""""""""""""""""""""""""""""""""""
     C     LINJE         BEGSR

     C     KEYDKEH       SETLL     DKEV
     C     KEYDKEH       READE     DKEV                                   51
     C     *IN51         DOWEQ     *OFF

     C                   CLEAR                   SADVR
     C                   EVAL      SVAVD=WSIAVD
     C                   EVAL      SVTDN=WSITDN
     C                   ADD       1             WSVLI
     C                   EVAL      SVLI=WSVLI
     C                   EVAL      SVLK=DKEV_34A
     C                   MOVE      DKEV_331      SVVNT
     C                   EVAL      SVVKTB=DKEV_35
     C     DKEV_411      IFNE      *BLANKS
     C                   EVAL      SVNTM=DKEV_412
     C                   ENDIF
     C                   EVAL      SVBELT=DKEV_42
     C                   EVAL      SVVKTN=DKEV_38
S01  C*                  MOVEL     DKEV_311      SVFT
S01  C*                  EVAL      SVNT=DKEV_313
S01  C*                  MOVEL     DKEV_314      SVEH
S01  C*                  MOVEL     DKEV_315      SVVT
N01  C                   MOVEL     DKEV_311      SVFT01
N01  C                   EVAL      SVNT01=DKEV_313
N01  C                   MOVEL     DKEV_314      SVEH01
N01  C                   MOVEL     DKEV_315      SVVT01
     C                   WRITE     SADVR

     C     KEYDKEH       READE     DKEV                                   51
     C                   ENDDO

     C                   ENDSR
