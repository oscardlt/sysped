     C                   MOVE      XXBLSÆ        W2BLSÆ
     C     KODS8K        CHAIN     KOS8R                              33
     C   33KOS82K        CHAIN     KOS8R                              33
     C  N33              IF        KS8AVG <> 'FK'
     C  N33              SELECT
     C     KS8STY        WHENEQ    '%'
     C                   Z-ADD     XPROGR        W2BLGÆ
     C     KS8STY        WHENEQ    'P'
     C     KS8STY        OREQ      'H'
     C     KS8STY        OREQ      'K'
     C                   Z-ADD     XVEKGR        W2BLGÆ
     C     KS8STY        WHENEQ    'G'
     C     XVEKGR        MULT      1000          W2BLGÆ
     C     KS8STY        WHENEQ    'D'
     C                   EVAL(H)   W2BLGÆ = XSTKGR / 12 + 0.5
     C     KS8STY        WHENEQ    'T'
     C                   EVAL(H)   W2BLGÆ = XVEKGR / 1000 + 0.5
     C                   OTHER
     C     KS8STY        IFNE      'M'
     C     KS8STY        ANDNE     'A'
N14  C     KS8STY        ANDNE     'B'
     C                   Z-ADD     XSTKGR        W2BLGÆ
     C                   END
     C                   ENDSL
     C                   END
     C     W2BLSÆ        IFEQ      0
     C                   Z-ADD     KS8SAT        W2BLSÆ
     C                   END
     C                   MOVE      KS8STY        XEH2Æ
     C                   MOVE      KS8ØRE        XS8ØRE
