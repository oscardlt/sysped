             PGM        PARM(&FRALIB &TILLIB &TOMME)
             DCL        VAR(&FRALIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TILLIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TOMME)  TYPE(*CHAR) LEN(1)

             CLRPFM     FILE(&TILLIB/BLLOKSTD)
             OVRDBF     FILE(BLSTDNY) TOFILE(&TILLIB/BLLOKSTD)
             OVRDBF     FILE(BLSTDGM) TOFILE(&FRALIB/BLLOKSTD)
             CALL       PGM(V5FKR05)
             IF         COND(&TOMME = 'J') THEN(CLRPFM +
                          FILE(&FRALIB/BLLOKSTD))

             ENDPGM
