CMD  PROMPT('EXTRACT PAGES FROM PDF')

PARM FROMPDF    TYPE(*PNAME) LEN(63) MIN(1) +
                  PROMPT('INPUT PDF')

PARM TOPDF      TYPE(*PNAME) LEN(63) MIN(1) +
                  PROMPT('OUTPUT PDF')

PARM SELECTION  TYPE(*CHAR) LEN(20) MIN(1) +
                PROMPT('SELECTION')

PARM JDEBUG     TYPE(*CHAR) LEN(4) RSTD(*YES) +
                DFT(*NO) VALUES(*YES *NO) +
                PROMPT('DEBUG JAVA')
