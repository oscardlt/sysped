     C*
     C* FINNER FRAKTBEREGNINGSVEKT
     C*
e01  C     HELM          MULT(H)   AFOMRL        XFBV1             9 0
     C*
     C* TAR EVENTUELLT OMREGNINGSFAKTOR NUMMER 2
     C     HEVKT         IFGT      AFOMR1
     C     AFOMR2        ANDNE     0
     C     AFOMR1        ANDNE     0
     C                   Z-ADD     AFOMR2        AFOMRM
     C                   END
e01  C     HEM3          MULT(H)   AFOMRM        XFBV2             9 0
     C     HEVKT         IFGE      XFBV1
     C                   Z-ADD     HEVKT         HEFBV             9 0
     C                   ELSE
     C                   Z-ADD     XFBV1         HEFBV
     C                   END
     C     HEFBV         IFLT      XFBV2
     C                   Z-ADD     XFBV2         HEFBV
     C                   END
S09  C*    USK           IFNE      'A'
S09  C*                  Z-ADD     HEFBV         XXXXFB            9 0
S09  C*    HEAKEY        CHAIN     HEADF                              90
S09  C* N90              Z-ADD     XXXXFB        HEFBV
S09  C* N90              EXCEPT    UPDHE
S09  C*                  END
     C*
     C                   ENDSR
     C***************************************************
     C     RFRAKT        BEGSR
     C***************************************************
