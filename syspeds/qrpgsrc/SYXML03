      ***********************************************
     C     SRSTART       BEGSR
      ***********************************************
     C                   EVAL      SDATA = TXT(001)
     C                   EXSR      TILFIL
     C                   EVAL      SDATA = TXT(002)
     C                   EXSR      TILFIL
     C                   EVAL      SDATA = TXT(003)
     C                   EXSR      TILFIL
     C                   EVAL      SDATA = %trim(TXT(004))
     C                             + %trim(XMBR)
     C                             + '"'
     C                   IF        XMST = ' '
     C                   CAT       TXT(141):1    SDATA
     C                   ELSE
     C                   CAT       TXT(142):1    SDATA
     C                   END
     C                   EXSR      TILFIL
     C                   EVAL      SDATA = TXT(005)
     C                   EXSR      TILFIL
