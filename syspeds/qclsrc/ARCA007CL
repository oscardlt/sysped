/* ********************************************************************    */
             PGM
             DCL        VAR(&DIR) TYPE(*CHAR) LEN(100) +
                          VALUE('/sysca/SS')
             DCL        VAR(&PGM) TYPE(*CHAR) LEN(10) VALUE('ARCA007R')
             CALL       PGM(ARCA001R) PARM(&DIR &PGM)
             ENDPGM
