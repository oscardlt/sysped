             PGM
             DCL        VAR(&UR) TYPE(*CHAR) LEN(1) VALUE('D') /* +
                          Oppdragstype */
             DCL        VAR(&STACK1) TYPE(*CHAR) LEN(2) VALUE('OP')
             DCL        VAR(&STACK2) TYPE(*CHAR) LEN(2)
             DCL        VAR(&STACK3) TYPE(*CHAR) LEN(2)
             DCL        VAR(&TILMOD) TYPE(*CHAR) LEN(2)
             DCL        VAR(&AVD) TYPE(*CHAR) LEN(4)
             DCL        VAR(&NR67) TYPE(*DEC) LEN(3 0) VALUE(067)
             DCL        VAR(&PRT67) TYPE(*CHAR) LEN(10) /* Printernavn+
                          til ZEBRA/ELTRON-strekkodemerkelapper */
             DCL        VAR(&PT67) TYPE(*CHAR) LEN(10) /* Papirtype +
                          til ZEBRA/ELTRON strekkodemerkelapper */
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1)
             DCL        VAR(&KOPI) TYPE(*LGL) LEN(1) VALUE('0')
             DCL        VAR(&USR) TYPE(*CHAR) LEN(10)
             DCL        VAR(&PGM) TYPE(*CHAR) LEN(10) VALUE('EXP +
                          FLY   ')
             CALL       PGM(LOGUSR) PARM(&PGM)
                                                          /**/
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CALL       PGM(SYFA00) PARM(&IN01)
             IF         COND(&IN01 = '0') THEN(DO)
             OVRDBF     FILE(HEADFOPD) TOFILE(HEADF)
             OVRDBF     FILE(HEDUMMY) TOFILE(HEDUMMY) MBR(URD)
             RTVDTAARA  DTAARA(*LDA (15 4)) RTNVAR(&AVD)
             CALL       PGM(GETPRT) PARM(&AVD &NR67 &PRT67 &PT67)
             OVRPRTF    FILE(ZBARC) OUTQ(&PRT67) FORMTYPE(&PT67)
             OVRPRTF    FILE(SYLA03P) OUTQ(&PRT67) FORMTYPE(&PT67)
             RTVUSRPRF  RTNUSRPRF(&USR)
             CHGDTAARA  DTAARA(*LDA (301 10)) VALUE(&USR)
             STRPCO
             MONMSG     MSGID(CPF0000)
             CALL       PGM(SYOP01R) PARM(&UR &STACK1 &STACK2 +
                          &STACK3 &TILMOD)
             RCLRSC
             CALL       PGM(SYEFFRCL) PARM(&KOPI)
             ENDDO
                                                          /**/
             ENDPGM
