     Ffirmgoog  IF   E           K DISK
     c     *entry        plist
     c                   parm                    googjn            1
     c                   parm                    tmpban          120
     c                   move      'N'           googjn
     c                   read      firmgooG                               33
     c     *in33         ifeq      '0'
     C                   MOVE      'J'           GOOGJN
     c
     c                   z-add     1             pos               3 0
     c                   z-add     0             str               3 0
     c                   move      *blanks       NYBANE          120
     c                   move      *blanks       NYDOK           120
      * finn dokumentnavn i fraban
     c                   dow       Pos<> 0
     c                   eval      Pos= %SCAN ('/' : tmpban: Str+1)
     c                   if        Pos<> 0
     c                   eval      Str= Pos
     c                   endif
     c                   enddo
     c                   eval      NYDOK     = %subst(tmpBAN:Str+1)
     c                   eval      NYBANE    = %subst(tmpBAN:1:Str)
     c                   eval      tmpban=%trim(gotmpf)+%trim(nydok)
     c                   end
     C                   SETON                                        LR
     C                   RETURN
