             PGM
             DCL        VAR(&LIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&UNIK)   TYPE(*CHAR) LEN(7) /* Unikkode */
             DCL        VAR(&NBR) TYPE(*CHAR) LEN(6)
             RTVDTAARA  DTAARA(FIRMDT (141 10)) RTNVAR(&LIB)
             RTVJOBA    NBR(&NBR)
             CHGVAR     VAR(&UNIK) VALUE('W' *CAT &NBR)

             ADDPFM     FILE(ARKSRC160) MBR(&UNIK)
             MONMSG     MSGID(CPF0000)
             CLRPFM     FILE(ARKSRC160) MBR(&UNIK)
             OVRDBF     FILE(ARKSRC160) TOFILE(&LIB/ARKSRC160) MBR(&UNIK)
  /* send meil med link til bookinger for esped oversjø-system */
             CALL       PGM(ESOVERM) PARM(&UNIK)

             RMVM       FILE(ARKSRC160) MBR(&UNIK)
             MONMSG     MSGID(CPF0000)
 SLUTT:      ENDPGM
