     C                   MOVE      '0'           UFLAGG            1
     C                   CALL      'IE001C1'
     C                   PARM                    WSLIB
     C                   PARM                    UFLAGG
     C     UFLAGG        IFEQ      '1'
     C  N30              MOVE      'YBC0620'     MSGNR
     C  N30              Z-ADD     22            LINNBR
     C  N30              Z-ADD     45            POSNBR
     C                   SETON                                        3089
     C                   END
N01  C     UFLAGG        IFEQ      '2'
N01  C                   MOVE      'J'           EGETBI            1
N01  C                   ELSE
N01  C                   MOVE      'N'           EGETBI            1
N01  C                   END
