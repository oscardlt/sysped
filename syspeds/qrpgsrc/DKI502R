      ***********************************************
     C     SROPDMR       BEGSR
      ***********************************************
      *
      * OPPDATER EKSP.NR OG PROSEDYRE PÅ LAGER INNLEGGINGSPOSTER
     C     LLMRFK        CHAIN     LLMRF                              33
     C                   IF        *IN33
     C                   EVAL      *INLR = *ON
     C                   RETURN
     C                   END
     C                   EVAL      MRST = 'P'
     C                   MOVEL     DKIH_ANDT     MRDT
     C                   MOVE      DKIH_ANDT     MRTM
     C                   MOVEL     DKIH_07       MRGN
     C                   MOVEL     DKIV_37       MRTRNR
     C                   UPDATE    LLMRFR
      *
      * FRIGJØR LAGERBEHOLDNING
     C     LLMRFK        SETLL     LLAGJL07
     C     LLMRFK        READE     LLAGJL07                               33
      *
     C                   DOW       *IN33 = *OFF
     C                   IF        VADATO = 99999999
     C                   MOVEL     DKIH_ANDT     VADATO
     C                   EVAL      VAAVD  = DKIH_SYAV
     C                   EVAL      VAOPD  = DKIH_SYOP
     C                   EVAL      VABUSE = 'SKAT'
     C                   MOVE      WDT           VABDAT
     C                   MOVE      WTM           VABKL
     C     LLVARE01K     CHAIN     LLVARE01                           33
     C                   ADD       VASALD        LVSALD
     C                   UPDATE    LLVARER
     C                   END
     C                   UPDATE    LLAGJR
      *
     C     LLMRFK        READE     LLAGJL07                               33
     C                   ENDDO
      *
     C                   ENDSR
      *
      ***********************************************
     C     SROPDJD       BEGSR
      ***********************************************
      *
      * OPPDATER EKSP.NR OG PROSEDYRE PÅ LAGER UTTAKSPOSTER
     C     LLAGJL01K     CHAIN     LLAGJL01                           33
     C     DKHKEY        SETLL     LLAGJD06
     C     DKHKEY        READE     LLAGJD06                               33
     C                   DOW       *IN33 = *OFF
     C                   EVAL      VDTEXT = DKIH_07 + ' P:' + DKIV_37
     C                   UPDATE    LLAGJDR
     C     DKHKEY        READE     LLAGJD06                               33
     C                   ENDDO
