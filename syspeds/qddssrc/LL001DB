     A          R BILDE01
     A                                      WINDOW(WINDOWSZ)
     A                                      OVERLAY
     A                                      CF04(04 'SØK')
     A                                      CA12(12 'Forrige')
     A                                      CSRLOC(LINNBR     POSNBR)
     A            LINNBR         3S 0H
     A            POSNBR         3S 0H
     A                                  1  2'LL001RA'
     A                                      COLOR(BLU)
     A                                  1 10'Vedlikehold tariffnr'
     A                                      COLOR(WHT)
     A                                  2  2'Art:'
     A            UANR          15A  O  2  7DSPATR(UL)
     A                                  4  2'Språkkod:'
     A            WSSK           2A  B  4 13DSPATR(UL)
     A  40                                  DSPATR(PR)
     A N40N30
     AO 90                                  DSPATR(PC)
     A  40                                  DSPATR(HI)
     A  90                                  DSPATR(RI)
