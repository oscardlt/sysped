     C                   SELECT
     C                   WHEN      XTKTXTE = 'PIECES'
     C                   ADD       1             XTOTL
     C                   WHEN      TKTXTE <> '.'
     C                   ADD       1             XTOTL
     C                   ENDSL
     C                   END
