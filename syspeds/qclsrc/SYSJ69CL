             PGM        PARM(&OPDF &OPDT &PRT29 &PT29 &JOBK)
             DCL        VAR(&OPDF) TYPE(*DEC) LEN(7 0)
             DCL        VAR(&OPDT) TYPE(*DEC) LEN(7 0)
             DCL        VAR(&OPDF2) TYPE(*CHAR) LEN(7)
             DCL        VAR(&OPDT2) TYPE(*CHAR) LEN(7)
             DCL        VAR(&PRT29) TYPE(*CHAR) LEN(10)
             DCL        VAR(&PT29) TYPE(*CHAR) LEN(10)
             DCL        VAR(&JOBK) TYPE(*CHAR) LEN(1)
             DCL        VAR(&USRPRF) TYPE(*CHAR) LEN(10)
             DCL        VAR(&CVP6) TYPE(*CHAR) LEN(50) /* Brukernavn */
       /* */
             CHGVAR     VAR(&OPDF2) VALUE(&OPDF)
             CHGVAR     VAR(&OPDT2) VALUE(&OPDT)
             RTVJOBA    USER(&USRPRF)
             RTVUSRPRF  USRPRF(&USRPRF) TEXT(&CVP6) /* Kun faximile +
                          support 400 bruke coverpage. */
             CALL       PGM(SYSJ69) PARM(&OPDF2 &OPDT2 &PRT29 &PT29 +
                          &JOBK &CVP6)
       /* */
             ENDPGM
