     C* SR for å behandle MOA-segmentet                   SRMOAH
      *""""""""""""""""""""""""""""""""""""""""""""""""""""""""""
     C     SRMOAH        BEGSR
      * LES FORBI ':'
     C                   MOVE      *BLANKS       TEGN
     C                   DOW       TEGN <> LOGKOL
     C                   EXSR      GETCH
     C   LR              GOTO      ENDMOAH
     C                   ENDDO
      * LES FAKTURASUM
     C                   MOVE      *ZEROS        SEBEL1
     C     1             DO        20            NR
     C                   EXSR      GETCH
     C   LR              GOTO      ENDMOAH
     C                   IF        ((((TEGN = LOGKOL) OR
     C                                (TEGN = LOGPLU)) OR
     C                                (TEGN = LOGAPP)) OR
     C                                (TEGN = ','))
     C                   LEAVE
     C                   END
     C                   MULT      10            SEBEL1H
     C                   MOVE      TEGN          SEBEL1H
     C                   ENDDO
      *
     C                   IF        TEGN = ','
     C                   EXSR      GETCH
     C                   MOVE      TEGN          SEBEL1D1
     C                   EXSR      GETCH
     C                   IF        TEGN <> LOGKOL
     C                   MOVE      TEGN          SEBEL1D2
     C                   EXSR      GETCH
     C                   END
     C                   END
      * LES VALUTAKODE
     C                   MOVE      *BLANKS       FE
     C     1             DO        5             NR
     C                   EXSR      GETCH
     C   LR              GOTO      ENDMOAH
     C                   IF        ((TEGN = LOGPLU) OR (TEGN = LOGAPP))
     C                   LEAVE
     C                   END
     C                   MOVE      TEGN          FE(NR)
     C                   END
     C                   MOVEL     FELT          SEVAL1
      *
     C                   EXSR      GETSEG
     C     SEGNAM        CABNE     'MOA'         ENDMOAH
      *
      * FRAKT
      * LES FORBI ':'
     C                   MOVE      *BLANKS       TEGN
     C                   DOW       TEGN <> LOGKOL
     C                   EXSR      GETCH
     C   LR              GOTO      ENDMOAH
     C                   ENDDO
      * LES FRAKT
     C                   MOVE      *ZEROS        SEBEL2
     C     1             DO        20            NR
     C                   EXSR      GETCH
     C   LR              GOTO      ENDMOAH
     C                   IF        ((((TEGN = LOGKOL) OR
     C                                (TEGN = LOGPLU)) OR
     C                                (TEGN = LOGAPP)) OR
     C                                (TEGN = ','))
     C                   LEAVE
     C                   END
     C                   MULT      10            SEBEL2H
     C                   MOVE      TEGN          SEBEL2H
     C                   ENDDO
      *
     C                   IF        TEGN = ','
     C                   EXSR      GETCH
     C                   MOVE      TEGN          SEBEL2D1
     C                   EXSR      GETCH
     C                   IF        TEGN <> LOGKOL
     C                   MOVE      TEGN          SEBEL2D2
     C                   EXSR      GETCH
     C                   END
     C                   END
      *
     C                   EXSR      GETSEG
      *
     C     ENDMOAH       ENDSR
