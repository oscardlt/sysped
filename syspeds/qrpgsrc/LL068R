     C     1             DO        900           XN
     C                   IF        AN(XN) = *BLANKS
     C                   LEAVE
     C                   END
     C                   ADD       1             ZRECNO
     C                   MOVE      AN(XN)        LVANR
     C                   MOVE      BS(XN)        LVVB
     C                   MOVE      NT(XN)        XANT1
     C                   MOVE      NS(XN)        XANT2
     C                   MOVE      LC(XN)        LVPLOK
     C                   WRITE     SUBFIL
     C                   ENDDO
      *
     C                   Z-ADD     ZRECNO        YRECNO            4 0
     C                   Z-ADD     5             LINNBR
     C                   Z-ADD     2             POSNBR
     C                   IF        ZRECNO > 1
     C                   Z-ADD     1             ZRECNO
     C                   END
      *
     C     START3        TAG
     C                   SETOFF                                       9998
     C                   MOVEA     '11'          *IN(91)
     C     ZRECNO        IFEQ      0
     C                   EXFMT     NYVARE
     C                   ELSE
     C                   WRITE     BUNN
     C                   EXFMT     SUBCTL
     C                   END
      *
     C     WSCLOC        DIV       256           LINNBR
     C                   MVR                     POSNBR
     C                   Z-ADD     WSSUBN        XRECNO            4 0
      *
     C                   SETOFF                                       9830
      *** AVSLUTT
     C   03              GOTO      SLUTT
      *** PRINT
     C   07              DO
     C                   EXSR      SRPRINT
     C                   GOTO      SLUTT
     C                   END
     C*** AVSLUTT
     C   12              DO
     C                   Z-ADD     5             LINNBR
     C                   Z-ADD     30            POSNBR
     C                   MOVEA     '00'          *IN(98)
     C                   MOVE      '0'           *IN30
     C                   GOTO      BG1
     C                   END
