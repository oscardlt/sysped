      *** SLETT VARE
     C     WSNR          IFEQ      '4'
     C                   MOVE      ' '           WSNR
     C                   EXFMT     VAREL3
     C  N12              DO
     C     SADLK2        CHAIN     SADVARE                            33
     C  N33              DELETE    SADVARER
     C                   EXSR      MOVB1
     C                   EVAL      VARENR = '*DLT'
     C                   END
     C                   UPDATE    SUBFIL
     C                   GOTO      START3
     C                   END
