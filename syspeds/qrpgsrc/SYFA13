     C                   MOVE      '1'           *IN50
     C*
     C                   ENDSR
     C*
     C***********************************************
     C     SOKKUN        BEGSR
     C*
     C     SOK           CHAIN     KOAR                               10
     C     *IN10         IFEQ      '0'
     C                   Z-ADD     KOAKNR        NAKUND
     C                   Z-ADD     10            LINNBR
     C                   Z-ADD     29            POSNBR
     C                   ELSE
     C                   Z-ADD     0             NAKUND
     C                   MOVE      'TBR0004'     MSGNR
     C                   Z-ADD     8             LINNBR
     C                   Z-ADD     29            POSNBR
     C                   END
     C                   EXSR      TSTSCR
     C                   EXFMT     B10
     C*
     C                   ENDSR
