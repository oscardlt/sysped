             PGM        PARM(&UNIK &DEV &HLD)
             DCL        VAR(&QNBR) TYPE(*CHAR) LEN(6) +
                          VALUE('000000')
             DCL        VAR(&DEV) TYPE(*CHAR) LEN(10)
             DCL        VAR(&UNIK) TYPE(*CHAR) LEN(9)
             DCL        VAR(&HLD)  TYPE(*CHAR) LEN(1)
             DCL        VAR(&UPR) TYPE(*CHAR) LEN(1) VALUE('J')
             DCL        VAR(&HLD2) TYPE(*CHAR) LEN(10) +
                          VALUE('*NO       ')
             IF         COND(&HLD *EQ 'J') THEN(CHGVAR VAR(&HLD2) +
                          VALUE('*YES       '))
             OVRPRTF    FILE(FAX022C) DEV(&DEV) PAGESIZE(72 90) +
                          LPI(6) CPI(10) OVRFLW(72) PAGRTT(0) +
                          OUTQ(&DEV) FORMTYPE(*STD) HOLD(&HLD2)
    /* */
             CALL       PGM(FAX022) PARM(&UNIK &QNBR &UPR)
 SLUTT:      ENDPGM
