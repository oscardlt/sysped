     FTRUV      UF A E           K DISK
     FTRUVAM    O    E             DISK
     FSAEH      IF   E           K DISK
     FSAEV      IF   E           K DISK
     FFIRM      IF   E           K DISK
     FCUNDL01   IF   E           K DISK
     D                 DS
     D  SYRG                   1     14
     D  SYFOR9                 1      9
     D  SYFORE                 1     11
     C                   EXSR      SRINIT
     C                   IF        *IN51=*OFF
     C                   EXSR      READSAEV
     C                   ENDIF
     C                   EVAL      *INLR=*ON
      *_______________________________________________
      * Les SAEV                              READSAEV
      *"""""""""""""""""""""""""""""""""""""""""""""""
     C     READSAEV      BEGSR
     C     SAEHKEY       SETLL     SAEV
     C     SAEHKEY       READE     SAEV                                   53
     C     *IN53         DOWEQ     *OFF

     C                   CLEAR                   TRUVR
     C                   CLEAR                   TRUVAMR
     C                   EVAL      TVAVD=UTVAVD
     C                   EVAL      TVTDN=UTVTDN
     C                   ADD       1             ULI2
     C                   EVAL      TVLI=ULI2
     C                   MOVE      SVVNT         TVVNT
     C                   MOVEL     SVVT01        TVVT
     C                   Z-ADD     SVVKTB        TVVKTB
     C                   Z-ADD     SVVKTN        TVVKTN
     C                   EVAL      TVALK=SELKA
     C                   EVAL      TVBLK=SELKB
     C                   EVAL      TVDTY = '830'
     C                   MOVEL     SETLE         TVDREF
     C                   CAT       SETLL:0       TVDREF
     C                   EVAL      TVAVD2=UTVAVD2
     C                   EVAL      TVTDN2=UTVTDN2
     C                   MOVEL     SVFT01        TVMN
     C                   MOVEL     SVEH01        TVEH
     C                   IF        TVEH=*BLANKS
     C                   EVAL      TVEH='CT'
     C                   ENDIF
     C                   MOVE      SVNT01        TVNT
     C                   MOVE      SVNTM         TVNTEH
     C                   WRITE     TRUVR
     C                   EXSR      HADR2

     C     SAEHKEY       READE     SAEV                                   53
     C                   ENDDO
     C                   ENDSR
     C***********************************************
     C     HADR2         BEGSR
     C***********************************************
     C                   MOVE      SEKNK         TVKNS
     C                   MOVE      SENAK         TVNAS
     C                   MOVE      SEADK1        TVADS1
     C     TVADS1        IFEQ      *BLANKS
     C                   MOVEL     '.'           TVADS1
     C                   END
     C                   MOVEL     SEADK3        TVPNS
     C                   MOVEL     SEADK3        TVPSS
     C                   MOVEL     'NO'          TVSKS
     C                   MOVEL     'NO'          TVLKS
     C     SEKNK         IFEQ      0
     C                   MOVE      *BLANKS       TVTINS
     C                   ELSE
     C                   MOVE      SEKNK         YKN
     C     KUND1K        CHAIN     CUNDL01                            34
     C     SYFOR9        IFEQ      *ZEROS
     C                   MOVE      *BLANKS       TVTINS
     C                   ELSE
     C                   MOVEL     SYFORE        TVTINS
     C                   END
     C                   END
     C                   MOVE      SEKNS         TVKNK
     C                   MOVE      SENAS         TVNAK
     C                   MOVE      SEADS1        TVADK1
     C     TVADK1        IFEQ      *BLANKS
     C                   MOVEL     '.'           TVADK1
     C                   END
     C                   MOVEL     SEADS3        TVPNK
     C                   MOVEL     SEADS3        TVPSK
     C                   MOVEL     'NO'          TVSKK
     C                   MOVEL     SELKA         TVLKK
     C     SEKNS         IFEQ      0
     C                   MOVE      *BLANKS       TVTINK
     C                   ELSE
     C                   MOVE      SEKNS         YKN
     C     KUND1K        CHAIN     CUNDL01                            34
     C     SYFOR9        IFEQ      *ZEROS
     C                   MOVE      *BLANKS       TVTINK
     C                   ELSE
     C                   MOVEL     SYFORE        TVTINK
     C                   END
     C                   END

     C                   WRITE     TRUVAMR
     C                   ENDSR
      *________________________________________________
      * INIT                                     SRINIT
      *""""""""""""""""""""""""""""""""""""""""""""""""
     C     SRINIT        BEGSR
     C     *ENTRY        PLIST
     C                   PARM                    UTVAVD
     C                   PARM                    UTVTDN
     C                   PARM                    UTVAVD2
     C                   PARM                    UTVTDN2
     C                   PARM                    ULI2              5 0

     C     *LIKE         DEFINE    TVAVD         UTVAVD
     C     *LIKE         DEFINE    TVTDN         UTVTDN
     C     *LIKE         DEFINE    TVAVD2        UTVAVD2
     C     *LIKE         DEFINE    TVTDN2        UTVTDN2
     C                   Z-ADD     0             YKN               8 0

     C     SAEHKEY       KLIST
     C                   KFLD                    UTVAVD2
     C                   KFLD                    UTVTDN2
     C     TRUHKEY       KLIST
     C                   KFLD                    UTVAVD
     C                   KFLD                    UTVTDN
     C     KUND1K        KLIST
     C                   KFLD                    FIFIRM
     C                   KFLD                    YKN
     C     FIFIRM        SETLL     FIRM
     C                   READ      FIRM
      * Finnes eksportdeklarasjonen?
     C     SAEHKEY       CHAIN     SAEH                               51
     C                   ENDSR
