     C     SR0001K       BEGSR
      *
     C     EPOLK         READE     EPOK                                   33
     C                   IF        *IN33 = *OFF
     C                   MOVE      'P'           EKST
     C                   MOVE      WDT           EKDTPR
     C                   MOVE      WUSR          EKUSR
     C                   UPDATE    EPOKR
     C                   END
      *
     C                   ENDSR
      *_____________________________________________________
      * Initialise subroutine, set character set
      *"""""""""""""""""""""""""""""""""""""""""""""""""""""
     C     SRINIT        BEGSR
      *
      * Input Parameter
     C     *ENTRY        PLIST
     C                   PARM                    WUSR             10
     C                   PARM                    WFIRM             2
     C                   PARM                    WMLAYO            2
     C                   PARM                    WKN               8
     C                   PARM                    WBES             10
     C                   PARM                    WLIN              7
     C                   PARM                    WULIN             5
     C                   PARM                    uukopi            1
      * Define workfields
     C     EPOHK         KLIST
     C                   KFLD                    EHKUND
     C                   KFLD                    EHBES
     C     EPOLK         KLIST
     C                   KFLD                    EHKUND
     C                   KFLD                    EHBES
     C                   KFLD                    ELLIN
     C                   KFLD                    ELULIN
     C     CUNDL01K      KLIST
     C                   KFLD                    WFIRM
     C                   KFLD                    EHKUND
     C     VADR3K        KLIST
     C                   KFLD                    WFIRM
     C                   KFLD                    EHKUND
     C                   KFLD                    VAKURE
      *
N01  C     *LIKE         DEFINE    VADRNA        X_VADRNA
N01  C     *LIKE         DEFINE    VADRN1        X_VADRN1
N01  C     *LIKE         DEFINE    EHDPNA        X_EHDPNA
N01  C     *LIKE         DEFINE    EHDPA1        X_EHDPA1
     C                   MOVE      WKN           EHKUND
     C                   MOVE      WBES          EHBES
     C     EPOHK         CHAIN     EPOH                               33
     C     CUNDL01K      CHAIN     CUNDL01                            33
     C                   EVAL      VAKURE = EHINV
     C     VADR3K        CHAIN     VADR3                              33
N01  C     EPOHK         CHAIN     EPOHDP                             33
N01  C                   IF        *IN33 = *OFF
N01  C     LO:UPC        XLATE     VADRNA        X_VADRNA
N01  C     LO:UPC        XLATE     VADRN1        X_VADRNA
N01  C     LO:UPC        XLATE     EHDPNA        X_EHDPNA
N01  C     LO:UPC        XLATE     EHDPA1        X_EHDPA1
N01  C                   IF        VADRNA <> EHDPNA OR VADRN1 <> EHDPA1
N01  C                   EVAL      VADRNA = EHDPNA
N01  C                   EVAL      VADRN1 = EHDPA1
N01  C                   EVAL      VADRN2 = EHDPA2
N01  C                   EVAL      VADRN3 = EHDPA3
N01  C                   END
N01  C                   END
     C                   MOVE      WLIN          ELLIN
     C                   MOVE      WULIN         ELULIN
     C     EPOLK         CHAIN     EPOL                               33
     C                   IF        ELNTKL = 0
     C                   EVAL      ELNTKL = 1
     C                   END
     C     EPOLK         CHAIN     EPOLT                              33
     C*                  EVAL(H)   XVEKT = ELVKTL * ELNTSL / ELNTKL
     C*                  EVAL(H)   XM3   = ELM3L  * ELNTSL / ELNTKL
     C                   EVAL(H)   XVEKT = ELVKTL / ELNTKL
     C                   EVAL(H)   XVEKTn = ELVKNL / ELNTKL
     C                   EVAL(H)   XM3   = ELM3L / ELNTKL
      *
     c     SÆRT:OKT      XLATE     KNAVN         KNAVN
     c     SÆRT:OKT      XLATE     ADR1          ADR1
     c     SÆRT:OKT      XLATE     EHLNAV        EHLNAV
     c     SÆRT:OKT      XLATE     EHLAD1        EHLAD1
     c     SÆRT:OKT      XLATE     EHLAD2        EHLAD2
     c     SÆRT:OKT      XLATE     EHLAD3        EHLAD3
     c     SÆRT:OKT      XLATE     EHLAD4        EHLAD4
     c     SÆRT:OKT      XLATE     VADRNA        VADRNA
     c     SÆRT:OKT      XLATE     VADRN1        VADRN1
     c     SÆRT:OKT      XLATE     VADRN2        VADRN2
     c     SÆRT:OKT      XLATE     VADRN3        VADRN3
     c     SÆRT:OKT      XLATE     ELART         ELART
     c     SÆRT:OKT      XLATE     ELARTL        ELARTL
     c     SÆRT:OKT      XLATE     ETBES1        ETBES1
     c     SÆRT:OKT      XLATE     ETBES2        ETBES2
     c     SÆRT:OKT      XLATE     ETREF1        ETREF1
     c     SÆRT:OKT      XLATE     ETREF2        ETREF2
