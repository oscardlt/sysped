      *
     FDOKUI     IF   E           K DISK
     FSYFM21FD  CF   E             WORKSTN
      *
     C     DOKUIK        KLIST
     C                   KFLD                    XDIRI
     C                   KFLD                    UAVD
     C                   KFLD                    UOPD
     C                   KFLD                    XFBNR
     C*
     C     *ENTRY        PLIST
     C                   PARM                    UFIRM             2
     C                   PARM                    UAVD              4 0
     C                   PARM                    UOPD              7 0
     C                   PARM                    UFLAGG            1 0
     C*
     C                   MOVE      0             UFLAGG
     C                   Z-ADD     1             XFBNR             3 0
     C                   MOVE      'I'           XDIRI             1
     C*
     C     DOKUIK        CHAIN     DOKUI                              30
     C   30              SETON                                        LR
     C   30              RETURN
     C*
     C                   MOVE      'DIV0067'     MSG01
     C                   MOVE      'DIV0113'     MSG02
     C                   MOVE      'DIV0142'     MSG03
     C                   MOVE      'DIV0029'     MSG04
     C                   MOVE      'DIV0178'     MSGNR
     C*
     C*
     C     START         TAG
     C                   EXFMT     BILDE01
     C                   SETOFF                                       30
     C*
     C   24              DO
     C     CMDT          IFLT      1
     C                   ADD       1             CMDT              1 0
     C                   MOVE      'DIV0020'     MSG01
     C                   MOVE      'DIV0177'     MSG02
     C                   MOVE      'DIV0114'     MSG03
     C                   ELSE
     C                   MOVE      'DIV0067'     MSG01
     C                   MOVE      'DIV0113'     MSG02
     C                   MOVE      'DIV0142'     MSG03
     C                   Z-ADD     0             CMDT
     C                   END
     C                   GOTO      START
     C                   END
     C*
     C                   Z-ADD     0             UFLAGG
     C   01              ADD       1             UFLAGG
     C   02              ADD       2             UFLAGG
     C   03              ADD       3             UFLAGG
     C   06              GOTO      SLUTT
     C   12              ADD       9             UFLAGG
     C*
     C     UFLAGG        IFEQ      0
     C                   ADD       1             XFBNR
     C     DOKUIK        CHAIN     DOKUI                              30
     C   30              SUB       1             XFBNR
     C                   GOTO      START
     C                   END
     C*
     C     SLUTT         TAG
     C                   SETON                                        LR
     C                   RETURN
     C*
