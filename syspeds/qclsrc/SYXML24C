             PGM        PARM(&SN)
             DCL        VAR(&SN) TYPE(*CHAR) LEN(9)

             OVRDBF     FILE(XMLRCVF01) TOFILE(XMLRCVF)
             OVRDBF     FILE(XMLRCVF02) TOFILE(XMLRCVF)
             OVRDBF     FILE(XMLRCVF03) TOFILE(XMLRCVF)
             OVRDBF     FILE(XMLRCVF04) TOFILE(XMLRCVF)
             OVRDBF     FILE(XMLRCVF05) TOFILE(XMLRCVF)
             CALL       PGM(SYXML24) PARM(&SN)

             ENDPGM
