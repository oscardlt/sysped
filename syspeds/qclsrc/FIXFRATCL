             PGM
             /* */
             DLTF       FILE(FRATFRA)
             DLTF       FILE(FRATTIL)
             /* */
             RNMOBJ     OBJ(FRAT) OBJTYPE(*FILE) NEWOBJ(FRATGM)
             CRTPF      FILE(SYSPEDF/FRAT) SRCFILE(SYSPEDS/QDDSSRC) +
                          SRCMBR(FRAT) SIZE(10000 1000 10) LVLCHK(*NO)
             CPYF       FROMFILE(SYSPEDF/FRATGM) +
                          TOFILE(SYSPEDF/FRAT) MBROPT(*ADD) +
                          FMTOPT(*MAP *DROP)
             DLTF       FILE(SYSPEDF/FRATGM)
             /* */
             CRTLF      FILE(SYSPEDF/FRATFRA) +
                          SRCFILE(SYSPEDS/QDDSSRC) LVLCHK(*NO)
             CRTLF      FILE(SYSPEDF/FRATTIL) +
                          SRCFILE(SYSPEDS/QDDSSRC) LVLCHK(*NO)
             /* */
             CALL       PGM(FIXFRAT)
             /* */
             ENDPGM
