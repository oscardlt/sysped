/* HENTE EN BRUKERS EPOSTADRESSE   */
             PGM        PARM(&EPOSTT &EMNE &CVP1)
             DCL        VAR(&EPOSTT) TYPE(*CHAR) LEN(70)
             DCL        VAR(&EMNE) TYPE(*CHAR) LEN(40)
             DCL        VAR(&CVP1) TYPE(*CHAR) LEN(40)
/*                      */
/* HENT REPLYADRESSE    */
/*                      */
/*                      */
             CALL       PGM(SYGE73B) PARM(&EPOSTT &EMNE &CVP1)
/*                      */
                          ENDPGM
