/*S01        PGM        PARM(&KJED &PNR &MRNR &FLAG) */
/*N01*/      PGM        PARM(&KJED &PNR &MRNR &AVD &OPD &FLAG)
             DCL        VAR(&KJED) TYPE(*CHAR) LEN(8)
             DCL        VAR(&PNR) TYPE(*CHAR) LEN(8)
             DCL        VAR(&MRNR) TYPE(*CHAR) LEN(5)
/*N01*/      DCL        VAR(&AVD) TYPE(*CHAR) LEN(4)
/*N01*/      DCL        VAR(&OPD) TYPE(*CHAR) LEN(7)
             DCL        VAR(&FLAG) TYPE(*CHAR) LEN(1)

/*S01        CALL       PGM(DKI501R) PARM(&KJED &PNR &MRNR &FLAG) */
/*N01*/      CALL       PGM(DKI501R) PARM(&KJED &PNR &MRNR &AVD &OPD +
                          &FLAG)

             ENDPGM
