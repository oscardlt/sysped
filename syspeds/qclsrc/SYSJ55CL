             PGM        PARM(&OPDF &OPDT &PRT26 &PT26 &PRT28 &PT28 +
                          &JOBK)
             DCL        VAR(&OPDF) TYPE(*DEC) LEN(7 0)
             DCL        VAR(&OPDT) TYPE(*DEC) LEN(7 0)
             DCL        VAR(&OPDF2) TYPE(*CHAR) LEN(7)
             DCL        VAR(&OPDT2) TYPE(*CHAR) LEN(7)
             DCL        VAR(&PRT26) TYPE(*CHAR) LEN(10)
             DCL        VAR(&PT26) TYPE(*CHAR) LEN(10)
             DCL        VAR(&PRT28) TYPE(*CHAR) LEN(10)
             DCL        VAR(&PT28) TYPE(*CHAR) LEN(10)
             DCL        VAR(&JOBK) TYPE(*CHAR) LEN(1)
       /* */
             CHGVAR     VAR(&OPDF2) VALUE(&OPDF)
             CHGVAR     VAR(&OPDT2) VALUE(&OPDT)
             IF         COND(&JOBK = 'J') THEN(DO)
             SBMJOB     CMD(CALL PGM(SYSJ55CLJ) PARM(&OPDF2 &OPDT2 +
                          &PRT26 &PT26 &PRT28 &PT28)) +
                          JOB(BILLOFLAD) JOBD(*USRPRF) +
                          JOBQ(SYJOBQNMAX)
                        ENDDO
             ELSE       CMD(DO)
             CALL       PGM(SYSJ55CLJ) PARM(&OPDF2 &OPDT2 &PRT26 +
                          &PT26 &PRT28 &PT28)
                        ENDDO
       /* */
             ENDPGM
