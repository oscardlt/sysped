         dow not %eof;
           write sadfkor;
           reade (uavd: utdn) saef;
         enddo;
       endif;

       chain (uavd: utdn) saevko;
       if not %found;
         setll (uavd: utdn) saev;
         reade (uavd: utdn) saev;
         dow not %eof;
           write saevkor;
           reade (uavd: utdn) saev;
         enddo;
       endif;

