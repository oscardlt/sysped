             PGM
             DCL        VAR(&LIBRARY) TYPE(*CHAR) LEN(10)
             DCL        VAR(&FILE) TYPE(*CHAR) LEN(10)
             DCL        VAR(&MEMBER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&WAIT) TYPE(*DEC) LEN(5 0) VALUE(-99999)
             DCL        VAR(&DATA) TYPE(*CHAR) LEN(30)
             DCL        VAR(&NBRCURRCD) TYPE(*DEC) LEN(10 0)
 START:      CALL       PGM(QRCVDTAQ) PARM(E081Q *LIBL 30 &DATA &WAIT)
             CHGVAR     VAR(&LIBRARY) VALUE(%SST(&DATA 1 10))
             CHGVAR     VAR(&FILE) VALUE(%SST(&DATA 11 10))
             CHGVAR     VAR(&MEMBER) VALUE(%SST(&DATA 21 10))
             OVRDBF     FILE(EDIRE) SHARE(*YES) OPNSCOPE(*JOB)
             CALL       PGM(E081R) PARM(&LIBRARY &FILE &MEMBER +
                          &NBRCURRCD)
             OVRDBF     FILE(EDIRE) SHARE(*NO) OPNSCOPE(*JOB)
             DLTOVR     FILE(EDIRE) LVL(*JOB)
             MONMSG     MSGID(CPF0000)
             RCLRSC
             MONMSG     MSGID(CPF0000)
             GOTO       CMDLBL(START)
             ENDPGM
