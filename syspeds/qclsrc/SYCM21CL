             PGM
             DCL        VAR(&FILIBO) TYPE(*CHAR) LEN(10)
             RTVDTAARA  DTAARA(FIRMDT (151 10)) RTNVAR(&FILIBO)
             ADDLIBLE   LIB(&FILIBO)
             MONMSG     MSGID(CPF2103)
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CALL       PGM(SYCM21)
             RMVLIBLE   LIB(&FILIBO)
             MONMSG     MSGID(CPF2104)
             ENDPGM
