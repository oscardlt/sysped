      /free
       if AntLest=1;
          JSONstring=*blanks;
          else;
          JSONstring=', ';
          endif;
       JSONstring=%trim(JSONstring)+'{'
          +'¬dkse_knr¬ : ¬'+%trim(%editc(DKSE_KNR:'Z'))+'¬, '
          +'¬dkse_331¬ : ¬'+%trim(DKSE_331)+'¬, '
