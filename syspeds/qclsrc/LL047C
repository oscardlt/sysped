             PGM
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1)
             DCL        VAR(&OK) TYPE(*CHAR) LEN(1)

             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CALL       PGM(SYFA00) PARM(&IN01)
             IF         COND(&IN01 = '0') THEN(DO)
             CALL       PGM(LL047R) PARM(&OK)
             IF         COND(&OK = 'J') THEN(DO)
             RGZPFM     FILE(LLLTLF)
             MONMSG     MSGID(CPF0000)
             ENDDO
             ENDDO

             ENDPGM
