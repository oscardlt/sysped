     C*
     C                   EXSR      INIT
     C*
     C     UPRO          IFNE      0
     C     HEAD8K        SETLL     HEAD8
     C     START         TAG
     C     UAVD          READE     HEAD8                                  33
     C  N33UPRO          COMP      HEPRO                              3333
     C  N33HEST          IFNE      'S'
     C     HEKDKK        ANDNE     'K'
     C     KODO2K        CHAIN     KODTOT2                            51
     C  N51KO2PST        IFNE      'N'
     C                   EXSR      PRINT
     C                   END
     C                   END
     C  N33              GOTO      START
     C*
     C     XNTOPD        IFGT      0
     C                   Z-ADD     1             XPRANT
     C     UIN03         IFEQ      '1'
     C                   EXCEPT    PTOT
     C                   ELSE
     C                   EXCEPT    ÆPTOT
     C                   END
     C                   END
     C*
     C                   ELSE
     C*
     C     UGN           IFNE      *BLANKS
     C     HEAD3K        SETLL     HEAD3
     C     START2        TAG
     C     HEAD3K        READE     HEAD3                                  33
     C  N33UDT           IFNE      0
     C     UDT8          ANDGT     HEDTOP
     C                   GOTO      START2
     C                   END
     C  N33HEST          IFNE      'S'
     C     HEKDKK        ANDNE     'K'
     C     KODO2K        CHAIN     KODTOT2                            51
     C  N51KO2PST        IFNE      'N'
     C                   EXSR      PRINT
     C                   END
     C                   END
     C  N33              GOTO      START2
     C*
     C     XNTOPD        IFGT      0
     C                   Z-ADD     1             XPRANT
     C     UIN03         IFEQ      '1'
     C                   EXCEPT    PTOT
     C                   ELSE
     C                   EXCEPT    ÆPTOT
     C                   END
     C                   END
     C                   END
     C                   END
     C*
     C                   MOVE      XPRANT        UPRANT
     C                   SETON                                        LR
     C*
     C******************************************
     C     PRINT         BEGSR
     C******************************************
     C   OF              EXCEPT    HEAD
     C   OA              EXCEPT    ÆHEAD
     C     XN            IFEQ      0
     C     UIN03         IFEQ      '1'
     C                   EXCEPT    HEAD
     C                   ELSE
     C                   EXCEPT    ÆHEAD
     C                   END
     C                   END
     C                   ADD       1             XN                5 0
     C     KOVAVR        IFNE      'J'
     C                   EXSR      OMREGN
     C                   ELSE
     C                   Z-ADD     HEFBV         XBVKT             5 0
     C                   END
     C     UIN03         IFEQ      '1'
     C                   EXCEPT    DET
     C                   ELSE
     C                   EXCEPT    ÆDET
     C                   END
     C                   ADD       1             XNTOPD            3 0
     C                   ADD       HEVKT         XTVKT             7 0
     C                   ADD       XBVKT         XTBVKT            7 0
     C                   ADD       HENT          XTNT              7 0
     C*
     C                   ENDSR
     C*
     C******************************************
     C     OMREGN        BEGSR
     C******************************************
     C*
     C* FORHØYER VEKTBEREGNING - UNDER 1000 KG TIL NÆRMESTE  10 KG
     C* ELLERS TIL NÆRM. 100 KG
     C*
     C     HEFBV         IFLT      1000                                         *VEKT UNDER 1000 KG
     C     HEFBV         ADD       9             XBVKT             5 0           -rund opp til nærm-
     C                   MOVE      '0'           XBVKT                            este 10.
     C                   ELSE                                                   *VEKT LIK/OVER 1000.
     C     HEFBV         ADD       99            XBVKT                            10 eller mer: rund
     C                   MOVE      '00'          XBVKT                            til nærmeste 100kg
     C                   END                                                     ..?) ER LIK 1
     C*
     C                   ENDSR
     C*
     C***********************************************
     C     INIT          BEGSR
     C***********************************************
     C     HEAD3K        KLIST
     C                   KFLD                    UAVD
     C                   KFLD                    UGN
     C     HEAD8K        KLIST
     C                   KFLD                    UAVD
     C                   KFLD                    UPRO
     C                   KFLD                    YPOS1
     C     HODEK         KLIST
     C                   KFLD                    YNET
     C                   KFLD                    UAVD
     C     KODTHK        KLIST
     C                   KFLD                    YHUNI
     C                   KFLD                    UAVD
     C     KODTVK        KLIST
     C                   KFLD                    YVUNI
     C                   KFLD                    UAVD
     C     KODO2K        KLIST
     C                   KFLD                    YO2UNI
     C                   KFLD                    HEOT
     C*
     C     *ENTRY        PLIST
     C                   PARM                    UPRANT            2
     C                   PARM                    UIN03             1
     C                   PARM                    UIN04             1
     C                   MOVE      *ZEROS        XPRANT            2 0
     C*
     C                   MOVE      *BLANKS       YPOS1             7
     C                   MOVE      ' '           YNET              1
     C                   MOVE      'H'           YHUNI             1
     C                   MOVE      'V'           YVUNI             1
     C                   MOVE      'O2Y'         YO2UNI            3
     C     UDT           IFGT      501231
     C     UDT           ADD       19000000      UDT8              8 0
     C                   ELSE
     C     UDT           ADD       20000000      UDT8
     C                   END
     C                   MOVE      UDAY          UDT1              2 0
     C                   MOVE      UMONTH        UDT2              2 0
     C                   MOVE      UYEAR         UDT3              2 0
     C                   CALL      'DATOFMT'
     C                   PARM                    UDT1
     C                   PARM                    UDT2
     C                   PARM                    UDT3
     C                   MOVE      UDT1          IDAG1
     C                   MOVE      UDT2          IDAG2
     C                   MOVE      UDT3          IDAG3
     C                   MOVE      *ZEROS        XNTOPD            3 0
     C*
     C     KODTHK        CHAIN     KODTH                              33
     C     KODTVK        CHAIN     KODTV                              33
     C     KOHLOS        COMP      'J'                                    40
     C   40HODEK         CHAIN     HODE                               33
     C*
     C                   ENDSR
     C*
