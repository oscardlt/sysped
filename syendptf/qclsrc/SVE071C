             PGM        PARM(&BANE)
             /*_____________________*/
             /* Deklarere Variabler */
             /*"""""""""""""""""""""*/
             DCL        VAR(&BANE) TYPE(*CHAR) LEN(50)
             DLYJOB     DLY(15)
             /*_____________________________________________*/
             /* Generere fil for tcpip kommandoer og output */
             /*"""""""""""""""""""""""""""""""""""""""""""""*/
             CLRPFM     FILE(SVE071FR)
             MONMSG     MSGID(CPF0000)
             CLRPFM     FILE(SVE071FS)
             MONMSG     MSGID(CPF0000)
             CALL       PGM(SVE071R) PARM(&BANE)
             /*_______________*/
             /* Send over fil */
             /*"""""""""""""""*/
             OVRDBF     FILE(INPUT) TOFILE(SVE071FS)
             OVRDBF     FILE(OUTPUT) TOFILE(SVE071FR)
             STRTCPFTP  RMTSYS('10.11.47.61')
             ENDPGM
