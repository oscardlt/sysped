     C******************************************
     C     PRINT         BEGSR
     C******************************************
     C                   Z-ADD     1024          UCMDL            15 5
     C                   MOVE      *BLANKS       UCMD
     C     XCMD11        CAT       WSPRT:0       UCMD
     C                   CAT       ')':0         UCMD
     C                   CAT       XCMD21:1      UCMD
     C                   CALL      'QCMDEXC'                            33
     C                   PARM                    UCMD
     C                   PARM                    UCMDL
      *
     C     *LOVAL        SETLL     SYSJ61WF
     C                   READ      SYSJ61WF                               33
      *
     C     *IN33         DOWEQ     '0'
     C     BOOKK         CHAIN     BOOKING                            34
     C     BOLSH         IFEQ      *BLANKS
     C                   MOVE      '.'           BOLSH
     C                   END
     C     BOVHL         IFEQ      *BLANKS
     C                   MOVE      '.'           BOVHL
     C                   END
     C     BOLSH         IFNE      XLSH
     C     BOVHL         ORNE      XVHL
     C     W61TYP        ORNE      XTYP
     C     XLSH          IFNE      *BLANKS
     C                   WRITE     SYSJ61PC
     C     1             DO        10            NR
     C     VV(NR)        CABEQ     *BLANKS       UTARR1
     C                   MOVE      VV(NR)        VVNR
     C                   MOVE      VP(NR)        VPNR
     C                   MOVE      VC(NR)        VCNR
     C                   MOVE      VG(NR)        VGNR
     C                   WRITE     SYSJ61PD
     C                   END
     C     UTARR1        TAG
     C                   CLOSE     SYSJ61P
     C                   END
     C                   EXSR      HHEAD
     C                   END
      *
     C                   EXSR      HDETJ
      *
     C     BLV1K         SETLL     BLV1LF
     C     BLV1K         READE     BLV1LF                                 34
     C     *IN34         IFEQ      '1'
     C                   MOVE      *BLANKS       BVMN
     C                   MOVE      *BLANKS       BVVS
     C                   MOVE      *ZEROS        BVVKT
     C                   MOVE      *ZEROS        BVM3
     C                   MOVE      *BLANKS       BVKDC
     C                   END
      *
     C                   ADD       1             XNR
     C                   MOVE      CO(1)         XSCN
     C                   MOVE      AD(1)         XADR
      *
     C     *IN27         IFEQ      '1'
     C                   WRITE     SYSJ61PB
     C                   MOVE      '0'           *IN27
     C                   WRITE     SYSJ61P1
     C                   WRITE     SYSJ61PB
     C                   END
     C                   WRITE     SYSJ61P2
     C                   ADD       BVVKT         XTVKT
     C                   ADD       BVM3          XTM3
     C                   ADD       BVNT          XTNT
      *
     C     BOVALL        IFNE      *BLANKS
      * FØRSTE ELLER LIK FØRSTE, AKKUMULER I BUNN-TOT.VARIABLER
     C     XTVAL         IFEQ      *BLANKS
     C     XTVAL         OREQ      BOVALL
     C                   MOVE      BOVALL        XTVAL
     C                   SELECT
     C     BOCPL         WHENEQ    'C'
     C                   ADD       BOBLCP        XTLC
     C     BOCPL         WHENEQ    'P'
     C                   ADD       BOBLCP        XTLP
     C                   ENDSL
     C                   ADD       BOBLG         XTBLG
     C                   ELSE
      * FINN FØRSTE LIKE/LEDIGE ARRAY-ELEMENT, ADD DER
     C     1             DO        10            NR                2 0
     C     VV(NR)        IFEQ      BOVALL
     C     VV(NR)        OREQ      *BLANKS
     C                   GOTO      UTLOOP
     C                   END
     C                   END
     C     UTLOOP        TAG
      *
     C                   MOVE      BOVALL        VV(NR)
     C                   SELECT
     C     BOCPL         WHENEQ    'C'
     C                   ADD       BOBLCP        VC(NR)
     C     BOCPL         WHENEQ    'P'
     C                   ADD       BOBLCP        VP(NR)
     C                   ENDSL
     C                   ADD       BOBLG         VG(NR)
     C                   END
     C                   END
      *
     C                   Z-ADD     0             XBLLP
     C                   Z-ADD     0             XBLLC
      *
     C     *IN34         DOWEQ     '0'
     C     BLV1K         READE     BLV1LF                                 34
     C     *IN34         IFEQ      '0'
     C     XN            IFLE      12
     C                   ADD       1             XN
     C                   MOVE      CO(XN)        XSCN
     C                   MOVE      AD(XN)        XADR
     C                   END
     C     *IN27         IFEQ      '1'
     C                   WRITE     SYSJ61PB
     C                   MOVE      '0'           *IN27
     C                   WRITE     SYSJ61P1
     C                   WRITE     SYSJ61PB
     C                   END
     C                   WRITE     SYSJ61P3
     C                   ADD       BVVKT         XTVKT
     C                   ADD       BVM3          XTM3
     C                   ADD       BVNT          XTNT
     C                   END
     C                   ENDDO
      *
     C     XN            IFLE      12
     C                   ADD       1             XN
     C                   MOVE      CO(XN)        XSCN
     C                   MOVE      AD(XN)        XADR
     C                   END
     C     *IN27         IFEQ      '1'
     C                   WRITE     SYSJ61PB
     C                   MOVE      '0'           *IN27
     C                   WRITE     SYSJ61P1
     C                   WRITE     SYSJ61PB
     C                   END
     C                   WRITE     SYSJ61P4
      *
     C     BOOB          IFNE      *BLANKS
     C     XN            IFLE      12
     C                   ADD       1             XN
     C                   MOVE      CO(XN)        XSCN
     C                   MOVE      AD(XN)        XADR
     C                   END
     C     *IN27         IFEQ      '1'
     C                   WRITE     SYSJ61PB
     C                   MOVE      '0'           *IN27
     C                   WRITE     SYSJ61P1
     C                   WRITE     SYSJ61PB
     C                   END
     C                   WRITE     SYSJ61P5
     C                   END
      *
     C     BODT3         IFNE      0
     C     XN            IFLE      12
     C                   ADD       1             XN
     C                   MOVE      CO(XN)        XSCN
     C                   MOVE      AD(XN)        XADR
     C                   END
     C     *IN27         IFEQ      '1'
     C                   WRITE     SYSJ61PB
     C                   MOVE      '0'           *IN27
