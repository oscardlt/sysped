     C*
     C                   EXSR      INIT
     C* KJØREORDRE DIREKTE FRA ET ENKELTOPPDRAG?
     C     *IN28         IFEQ      '1'
     C     HEADFK        CHAIN(N)  HEADF                              33
     C                   GOTO      OPPD
     C                   END
     C*
     C* ELLERS LESELOOP F.O.M DATO eller PÅ TURNUMMER
     C*
     C*
     C     UTUR          IFEQ      *ZEROS
     C     HEAD2K        SETLL     HEADD
     C*
     C                   ELSE
     C     UTUR          SETLL     HEAD11
     C                   END
     C*
     C     START         TAG
     C*
     C     UTUR          IFEQ      *ZEROS
     C                   READ      HEADD                                  32
     C                   ELSE
     C     UTUR          READE     HEAD11                                 32
     C                   END
     C*
     C   32              GOTO      SLUTT
     C*
     C     HEPRO         IFEQ      *ZEROS
     C     HEST          IFNE      'S'
     C                   GOTO      START
     C                   END
     C                   END
     C*
     C     USG           IFNE      *BLANKS
     C     HESG          ANDNE     USG
     C                   GOTO      START
     C                   END
     C*
     C*
     C     UTUR          IFEQ      *ZEROS
     C     UAVD          ANDNE     HEAVD
     C                   GOTO      START
     C                   END
     C*
     C     UTYPE         IFEQ      'I'
     C     HEPK3         ANDNE     'J'
     C                   GOTO      START
     C                   END
     C*
     C     UTYPE         IFEQ      'E'
     C     HEPK5         ANDNE     'J'
     C                   GOTO      START
     C                   END
     C*
     C     UTYPE         IFEQ      'B'                                          *BEGGE:
     C     HEPK3         ANDNE     'J'                                           MINST EN=J
N01  C     UTYPE         OREQ      'B'
     C     HEPK5         ANDNE     'J'                                           -GÅR VIDERE
     C                   GOTO      START
     C                   END
     C*
     C     HEUR          IFNE      'H'
     C                   GOTO      START
     C                   END
      *
N04  C     UOPD          IFNE      *ZEROS
N04  C                   If        HEAVD<>Uavd or HEOPD<>Uopd
N04  C                   GOTO      START
N04  C                   endif
N04  C                   END
     C*
     C     OPPD          TAG
     C*
     C     HEST          IFNE      'S'
     C     HEADFK        CHAIN     DISP                               33
     C     *IN28         IFEQ      '0'
     C  N33KEYTUR        CHAIN     TURER                              33
     C                   ELSE
     C     HEPRO         IFNE      *ZEROS
     C  N33KEYTUR        CHAIN     TURER                              33
     C                   END
     C                   END
     C                   END
     C* KJØREORDRE DIREKTE FRA ET ENKELTOPPDRAG?  UT OM IKKE OK!!
     C     *IN28         IFEQ      '1'
     C     *IN33         ANDEQ     '1'
     C                   GOTO      SLUTT
     C                   END
     C   33              GOTO      START
     C*
     C                   MOVE      ' '           INT               1
     C                   MOVE      ' '           EKS               1
      *
     C                   MOVE      TSDFDÅ        XSDFDÅ
     C                   MOVE      TSDFDM        XSDFDM
     C                   MOVE      TSDFDD        XSDFDD
     C                   MOVE      TSDTDÅ        XSDTDÅ
     C                   MOVE      TSDTDM        XSDTDM
     C                   MOVE      TSDTDD        XSDTDD
     C                   MOVE      TSFFDÅ        XSFFDÅ
     C                   MOVE      TSFFDM        XSFFDM
     C                   MOVE      TSFFDD        XSFFDD
     C                   MOVE      TSVTDÅ        XSVTDÅ
     C                   MOVE      TSVTDM        XSVTDM
     C                   MOVE      TSVTDD        XSVTDD
     C* SNUR DATOER
     C     FIDTFM        IFNE      3
     C                   CALL      'DATFM4'
     C                   PARM                    FIDTFM
     C     XRSDFD        PARM      XRSDFD        URSDFD            6 0
     C     XRSDTD        PARM      XRSDTD        URSDTD            6 0
     C     XRSFFD        PARM      XRSFFD        URSFFD            6 0
     C     XRSVTD        PARM      XRSVTD        URSVTD            6 0
     C                   END
     C     FIDTFM        IFNE      3
     C                   MOVE      HEDTÅ         UDT1              2 0
     C                   MOVE      HEDTM         UDT2              2 0
     C                   MOVE      HEDTD         UDT3              2 0
     C                   CALL      'DATFM3'
     C                   PARM                    FIDTFM
     C     PRDATO        PARM      UDATO         XXDATO            6 0
     C                   END
     C* PRINT EKSTERN
     C     UTYPE         IFEQ      'B'
     C     UTYPE         OREQ      'E'
     C     HEPK5         IFEQ      'J'
     C     *IN28         OREQ      '1'
     C                   MOVE      'J'           EKS
     C                   EXSR      PRINT
     C                   END
     C                   END
     C* PRINT INTERN
     C     UTYPE         IFEQ      'B'
     C     UTYPE         OREQ      'I'
     C     HEPK3         IFEQ      'J'
     C                   MOVE      'J'           INT
     C                   SETON                                        90
     C* INTERN ORDREUTSKRIFT
     C                   SETON                                        91
     C                   EXSR      PRINT
     C* INTERN FAKTURAORDRE
     C                   SETOFF                                       91
     C                   EXSR      PRINT
     C                   SETOFF                                       90
     C                   END
     C                   END
     C*
     C* KJØREORDRE DIREKTE FRA ET ENKELTOPPDRAG?  IKKE OPPDATER STATUS
     C     *IN28         IFEQ      '1'
     C                   GOTO      SLUTT
     C                   END
     C     HEADFK        CHAIN     HEADF                              33
     C  N33INT           IFEQ      'J'
     C                   MOVE      'P'           HEPK3
     C                   END
     C  N33EKS           IFEQ      'J'
     C                   MOVE      'P'           HEPK5
     C                   END
     C  N33              UPDATE    HEADFR
     C                   ADD       1             UANT
     C                   GOTO      START
     C     SLUTT         TAG
     C                   SETON                                        LR
     C                   RETURN
     C*
     C*****************************************************************
     C     PRINT         BEGSR
     C*****************************************************************
