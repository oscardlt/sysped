             PGM
             DCL        VAR(&DT) TYPE(*DEC) LEN(6 0)
             DCL        VAR(&VH) TYPE(*CHAR) LEN(5)
             DCL        VAR(&KNR) TYPE(*DEC) LEN(8 0)
             DCL        VAR(&TR) TYPE(*CHAR) LEN(4)
             DCL        VAR(&PRT30) TYPE(*CHAR) LEN(10)
             DCL        VAR(&PT30) TYPE(*CHAR) LEN(10)
             DCL        VAR(&JOBK) TYPE(*CHAR) LEN(1)
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1) VALUE('0') /* +
                          Hvis &in01 på, betyr det avslutt program +
                          fra valgbilde. */
       /* */
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
       /* */
             CALL       PGM(SYFA00) PARM(&IN01)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT)) /* +
                          Hvis &IN01 på, avslutter program. */
       /* */
             CALL       PGM(SYSJBE) PARM(&IN01 &DT &VH &KNR &TR +
                          &PRT30 &PT30 &JOBK)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT)) /* +
                          Hvis &IN01 på, avslutter program. */
       /* */
             CALL       PGM(SYSJ68CL) PARM(&DT &VH &KNR &TR &PRT30 +
                          &PT30 &JOBK)
       /* */
 SLUTT:      ENDPGM
