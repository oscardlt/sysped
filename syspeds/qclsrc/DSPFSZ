             PGM
           /*___________________*/
           /* Declare Variables */
           /*"""""""""""""""""""*/
             DCL        VAR(&MSG) TYPE(*CHAR) LEN(30)
             DCL        VAR(&MBSIZ) TYPE(*DEC) LEN(10 0)
             DCL        VAR(&MBSIZI) TYPE(*DEC) LEN(5 0)
             DCL        VAR(&MBSIZM) TYPE(*DEC) LEN(5 0)
           /*______________*/
           /* Declare File */
           /*""""""""""""""*/
             DCLF       FILE(DSPFDYCB) RCDFMT(QWHFDMBR)
           /*____________*/
           /* Clear File */
           /*""""""""""""*/
             CLRPFM     FILE(DSPFDYCB) MBR(*FIRST)
             MONMSG     MSGID(CPF0000)
           /*____________________________*/
           /* Display File Description */
           /*""""""""""""""""""""""""""""*/
             DSPFD      FILE(SAEV) TYPE(*MBR) +
                          OUTPUT(*OUTFILE) OUTFILE(DSPFDYCB)
           /*___________*/
           /* Read File */
           /*"""""""""""*/
 READ:       RCVF
             MONMSG     MSGID(CPF0000) EXEC(GOTO CMDLBL(END))
           /*__________*/
           /* Send MSG */
           /*""""""""""*/
             CHGVAR     VAR(&MSG) VALUE(&MBSIZ)
             SNDPGMMSG  MSG(&MSG)
           /*____________*/
           /* Clear File */
           /*""""""""""""*/
 END:        CLRPFM     FILE(DSPFDYCB) MBR(*FIRST)
             MONMSG     MSGID(CPF0000)
             ENDPGM
