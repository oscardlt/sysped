             PGM
             DCL        VAR(&FLAGG) TYPE(*DEC) LEN(1 0) VALUE(0)
             DCL        VAR(&KN) TYPE(*DEC) LEN(8 0) VALUE(0)
             DCL        VAR(&KD) TYPE(*CHAR) LEN(4)
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CALL       PGM(SYSJ17) PARM(&FLAGG &KN &KD)
             ENDPGM
