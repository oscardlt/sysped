     C                   clear                   HEREFF
     C                   CALL      'SYGE15C'
     C                   PARM                    WDT               8
     C                   PARM                    WTM               6
     C                   move      WDT           TRSDFD
      * Write HEADF
     C                   WRITE     HEADFR
      * Write TRACKX
     C                   eval      TXAVD  = HEAVD
     C                   eval      TXOPD  = HEOPD
     C                   eval      TXETDD = TRSDFD
     C                   WRITE     TRACKTR
      * Write DISP
     C                   eval      DIAVD  = HEAVD
     C                   eval      DIOPD  = HEOPD
     C                   eval      DISTEF = HESDF
     C                   eval      DISTET = HESDT
     C                   eval      DISONF = HELKA
     C                   eval      DISONT = HETRI
     C                   eval      DIDATF = TRSDFD
     C                   eval      DITIMF = TRSDFK
     C                   eval      DIDATT = TRSDTD
     C                   eval      DITIMT = TRSDTK
     C                   eval      DISIGN = HESG
     C                   eval      DIOT   = HEOT
     C                   WRITE     DISPR
     C                   ENDSR
