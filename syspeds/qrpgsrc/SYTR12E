     C                   EXSR      INIT
     C     UTNR          IFEQ      *ZEROS
     C                   EXSR      ALLE
     C                   ELSE
     C                   EXSR      ENTRAN
     C                   END
     C*
     C     SLUTT         TAG
     C                   SETON                                        LR
     C                   RETURN
     C*
     C***********************************************
     C     ALLE          BEGSR
     C***********************************************
     C*
     C*------------------------------------------------------*
     C*                                                      *
     C*     FINN ALLE TRANSPORTØRER MED ' '-RECORD I TRAVH.  *
     C*     (" " = ÅPEN           )                          *
     C*""""""""""""""""""""""""""""""""""""""""""""""""""""""*
     C                   MOVE      ' '           YYST              1
     C*
     C     TAG1          TAG
     C                   SETOFF                                       32
     C     TRAHK1        SETLL     TRAVH
     C                   READ      TRAVH                                  32
     C  N32THST          COMP      YYST                               32
     C  N32              EXSR      ENTRAN
     C* FLERE TRANSPORTØRER TIL AVREGNING???
N01  c     THTNR         ifne      99999999
     C  N32THTNR         ADD       1             YTNR
     C  N32              GOTO      TAG1
N01  c                   endif
     C                   ENDSR
     C*
     C*
     C***********************************************
     C     ENTRAN        BEGSR
     C***********************************************
     C                   EXSR      GETTRA
     C* VED "ALLE" KUN DE MED KREDITNOTAKODE PÅ UTSKRIFT
     C     UTNR          IFEQ      *ZEROS
     C     VMINCR        ANDNE     0
     C     VMINCR        ANDNE     3
     C                   GOTO      UTTRAN
     C                   END
     C* TVUNGEN OVERFLOW GIR HEADING
     C                   SETON                                        88
     C                   MOVE      *ZEROS        XSIDE             2 0
     C                   MOVE      *ZEROS        XTOTAL           13 2
     C                   MOVE      *ZEROS        XMVAGR           13 2
     C                   EXSR      NEWPAG
     C*
     C                   SETOFF                                       94
     C     TRAHK2        SETLL     TRAVH
     C*
     C     TRAHK2        READE     TRAVH                                  94
     C     *IN94         DOWEQ     '0'
     C                   EXSR      DECI
     C                   ADD       THTOT         XTOTAL
     C     THKDM         COMP      'J'                                    34
     C   34              ADD       THTOT         XMVAGR
     C*
     C* 98 ON - GODSKRIV
     C     THRTNR        COMP      99999998                               98
     C  N98              DO
     C*
     C     TURERK        CHAIN     TURER                              33
     C* KONVERTER TURDATO TIL LOKALT FORMAT
     C   33              MOVE      *ZEROS        TUDT
     C                   MOVE      TUDT          TUDATO
     C     FIDTFM        IFNE      3                                            SNU DATO TIL
     C                   CALL      'DATFM3'                                     LOKALT FORMA
     C                   PARM                    FIDTFM
     C                   PARM                    TUDATO
     C                   END
     C     TUTLM         COMP      *ZEROS                                 96
     C     TUTM3         COMP      *ZEROS                                 97
     C     THSTAR        COMP      ' '                                    99    *UBESKYTTET
     C                   EXSR      NEWPAG
     C                   WRITE     DET
     C                   WRITE     DETST
     C                   EXSR      VARELI
     C                   END
     C* SKRIV DETALJLINJE
     C   98THTOT         IFNE      *ZEROS
     C     THBUDA        ORNE      *ZEROS
     C                   EXSR      GODSKR
     C                   END
     C     TRAHK2        READE     TRAVH                                  94
     C  N94              END
     C*
     C* SKRIV BUNNTOTAL FOR DENNE TRANSPORTØR
     C                   EXSR      TOTAL
     C*
     C     UTTRAN        ENDSR
