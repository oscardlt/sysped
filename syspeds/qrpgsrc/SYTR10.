N77  C                   move      TROPD2        WSOPD2
N77  c                   IF        WSOPD2<>0
N77  c                   exsr      OPDLOGUtk2
N77  C                   ENDIF
N77   * Utkjøringsdup via fraktbrev ?
N77  C     DokufKeyA     setll     DOKUF
N77  C     DokufKeyA     reade(N)  DOKUF                                  33
N77  C     *in33         doweq     '0'
N77  c                   If        %subst(DFBREF:1:5)='U.op:'
N77  c                             and %subst(DFBREF:10:1)='/'
N77  c                   eval      WSAVD2 = %int(%subst(DFBREF:6:4))
N77  c                   eval      WSOPD2 = %int(%subst(DFBREF:11:7))
N77  c                   IF        WSOPD2<>0
N77  c                   exsr      OPDLOGUtk2
N77  C                   ENDIF
N77  C                   endif
N77  C     DokufKeyA     reade(N)  DOKUF                                  33
N77  C                   enddo
