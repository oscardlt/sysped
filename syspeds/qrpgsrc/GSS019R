     C                   SELECT
     C                   WHEN      XSLT = 'J'
     C                   EVAL      LGAGM = RFREF
     C                   EVAL      LGANY = *BLANKS
     C                   EVAL      WSKD  = RFKD
     C     KUFASK        CHAIN     KUFAST                             33
     C                   EVAL      LGTXT = KFTXT
     C                   WRITE     GSSLGFR
     C                   WHEN      *IN33
     C                   EVAL      LGAGM = *BLANKS
     C                   EVAL      LGANY = WSREF
     C                   EVAL      LGTXT = KFTXT
     C                   WRITE     GSSLGFR
     C                   WHEN      WSREF <> RFREF
     C                   EVAL      LGAGM = RFREF
     C                   EVAL      LGANY = WSREF
     C                   EVAL      LGTXT = KFTXT
