             PGM
             DCL        VAR(&WSAVNR) TYPE(*DEC) LEN(9 0) VALUE(0)
             DCL        VAR(&UKUNNR) TYPE(*DEC) LEN(8 0) VALUE(0)
             DCL        VAR(&FLAGG) TYPE(*DEC) LEN(1 0) VALUE(0)
             OVRDBF     FILE(AVFRHCOP) TOFILE(AVFRH) OVRSCOPE(*JOB)
             OVRDBF     FILE(AVFRDL01C) TOFILE(AVFRDL01) OVRSCOPE(*JOB)
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             OVRDBF     FILE(AVFRH) TOFILE(AVFRHN)
             OVRDBF     FILE(AVFRHL01) TOFILE(AVFRHN01)
             OVRDBF     FILE(AVFRD) TOFILE(AVFRDN)
             OVRDBF     FILE(AVFRDL01) TOFILE(AVFRDN01)
             OVRDBF     FILE(AVFRDL02) TOFILE(AVFRDN02)
             OVRDBF     FILE(AVFRDL02) TOFILE(AVFRDN03)
             CALL       PGM(SYPO09) PARM(&WSAVNR &UKUNNR &FLAGG)
             DLTOVR     FILE(*ALL)
             ENDPGM
