    /* SPØRRING PÅ RUNDTUR */
             PGM
             DCL        VAR(&RUND) TYPE(*DEC) LEN(8 0)
             DCL        VAR(&SKJE) TYPE(*CHAR) LEN(10)
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             RTVJOBA    JOB(&SKJE)
             CALL       PGM(SYTR90) PARM(&RUND &SKJE)
             ENDPGM
