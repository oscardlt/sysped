      *
      ***********************************************************
      *** PROGRAM skjekker om prestartjobber er aktive        ***
      ***********************************************************
      *
     Fsydrstat  IF   E             DISK
     F                                     USROPN
     Fsydrsbsj  UF   E           K DISK
     FSYdr01fm  CF   E             WORKSTN
     F                                     SFILE(SUBFIL:ZRECNO)
     F                                     INFDS(FBAREA)
     D                 DS
     D  TEXTDS                 1    132
     D  JOBNVN                 4     13
     D  BRUKER                17     26
     D  JOBB                  30     35
     D  JOBTYP                39     47
     D  AKTIVE                49     58
     D FBAREA          DS
     D  WSCLOC               370    371B 0
     D  WSSUBN               378    379B 0
     D                 DS
     D  SYTXT                  1     30
     D  XDLT                   1      4
     C*
     c     start0        tag
     C                   MOVE      '1'           *IN93
     C                   WRITE     SUBCTL
     C                   EXSR      INIT
     C*
     C     START         TAG
     C*                  MOVE      '1'           *IN93
     C*                  WRITE     SUBCTL
     C                   MOVEA     '00'          *IN(93)
     C                   Z-ADD     *ZEROS        ZRECNO
     C                   MOVE      sysbs         Ysbs
     C                   MOVE      syjob         Yjob
      *_____________
      * Fyll Subfile
      *"""""""""""""
     C     keyst0        SETLL     sydrsbsj
     C     START1        TAG
     C                   READ(n)   sydrsbsj                               94
     C   94              MOVE      *BLANKS       WSNR
      *
     C     START2        TAG
      *
     C  N94              DO
     C                   DO        15
     C                   ADD       1             ZRECNO
     c                   setoff                                       7071
     c     sybru         ifeq      'J'
     c     syakt         andeq     'N'
     c                   seton                                        70
     c                   end
     c     syakt         ifeq      'M'
     c     sybru         andeq     'J'
     c                   seton                                        71
     c                   end
     C                   write     SUBFIL
     C                   READ(n)   sydrsbsj                               94
     C  N94              MOVE      sysbs         Ysbs
     C  N94              MOVE      syjob         Yjob
     C  N94              END
     C                   END
     C                   Z-ADD     ZRECNO        YRECNO            5 0
     C                   Z-ADD     6             LINNBR
     C                   Z-ADD     2             POSNBR
     C*
     C     START3        TAG
     C                   SETOFF                                       9998
     C                   MOVEA     '11'          *IN(91)
     C     ZRECNO        IFEQ      0
     C                   EXFMT     NYVARE
     C                   ELSE
     C                   WRITE     SUBCTL
     C                   EXFMT     BUNN
     C                   END
     C*
     C***
     C     WSCLOC        DIV       256           LINNBR
     C                   MVR                     POSNBR
     C                   Z-ADD     WSSUBN        XRECNO            4 0
     C     XRECNO        IFEQ      0
     C                   Z-ADD     ZRECNO        XRECNO            4 0
     C                   END
     C***
     C*
     C                   SETOFF                                       9830
     C*** AVSLUTT
     C   03              GOTO      SLUTT
     C*** FRISK OPP
     C   05              GOTO      START0
     C*** LAGE NY
     C   06              DO
     C                   MOVE      *blanks       wsSBS
     C                   MOVE      *blanks       WSjob
     C                   GOTO      START5
     C                   END
     C*** SØK FRA AVDELING
     C   08              GOTO      START6
     C*
     C*** BLA VIDERE
     C   25              DO
     C                   Z-ADD     YRECNO        ZRECNO
     C                   SETOFF                                       25
     C  N94keyrol        SETLL     sydrsbsj
     C  N94              READ(n)   sydrsbsj                               94
     C                   GOTO      START2
     C                   END
     C*
     C     ZRECNO        IFNE      0
     C                   EXSR      HENT
     C  N98              GOTO      START0
     C                   END
     C   98XRECNO        IFNE      0
     C                   Z-ADD     XRECNO        ZRECNO
     C                   END
     C                   GOTO      START3
     C*
     C*** LAGE
     C     START5        TAG
     C                   GOTO      START3
     C*
     C*** SØK FRA AVDELING
     C     START6        TAG
     C                   EXFMT     VAREL2
     C                   Z-ADD     6             LINNBR
     C                   Z-ADD     2             POSNBR
     C   12              GOTO      START3
     C                   GOTO      START
     C*
     C     SLUTT         TAG
     C                   SETON                                        LR
     C                   RETURN
     C*
     C***********************************************
     C     HENT          BEGSR
     C***********************************************
     C                   Z-ADD     0             LINNBR
     C                   SETOFF                                       919299
     C     *IN98         DOWEQ     '0'
     C                   READC     SUBFIL                                 98
     C   98              DO
     C   99              SETOFF                                       98
     C                   Z-ADD     1             LINNBR
     C                   GOTO      SLHENT
     C                   END
     C     ZRECNO        DIV       7             LINNBR
     C                   MVR                     XLIN              2 0
     C     XLIN          MULT      2             XLIN
     C     XLIN          IFEQ      0
     C                   Z-ADD     14            LINNBR
     C                   ELSE
     C                   Z-ADD     XLIN          LINNBR
     C                   END
      *
      * 02 = endre
      *
     C     WSNR          IFEQ      '2'
     c                   exsr      endre
     c                   seton                                        99
     c                   setoff                                       7071
     c     sybru         ifeq      'J'
     c     syakt         andeq     'N'
     c                   seton                                        70
     c                   end
     c     syakt         ifeq      'M'
     c     sybru         andeq     'J'
     c                   seton                                        71
     c                   end
     C                   MOVE      ' '           WSNR
     c                   update    subfil
     C                   GOTO      SLHENT
     C                   END
      *
      * start jobb WSNR= S
      *
     C     WSNR          IFEQ      'S'
     C     syakt         andeq     'N'
     c     syjobt        ifeq      'PJ'
     c                   exsr      startpj
     c                   end
     c     syjobt        ifeq      'BATCH'
     c                   exsr      startbj
     c                   end
     c                   seton                                        99
     c                   setoff                                       7071
     c     sybru         ifeq      'J'
     c     syakt         andeq     'N'
     c                   seton                                        70
     c                   end
     c     syakt         ifeq      'M'
     c     sybru         andeq     'J'
     c                   seton                                        71
     c                   end
     C                   MOVE      ' '           WSNR
     c                   update    subfil
     C                   GOTO      SLHENT
     C                   END
      *
      *
      * avslutt jobb WSNR = A
      *
     C     WSNR          IFEQ      'A'
     C     syakt         andeq     'J'
     c                   exsr      avslupj
     c                   seton                                        99
     c                   setoff                                       7071
     c     sybru         ifeq      'J'
     c     syakt         andeq     'N'
     c                   seton                                        70
     c                   end
     c     syakt         ifeq      'M'
     c     sybru         andeq     'J'
     c                   seton                                        71
     c                   end
     C                   MOVE      ' '           WSNR
     c                   update    subfil
     C                   GOTO      SLHENT
     C                   END
      *
      * vise jobb med melding   V = Vis jobb
      *
     C     WSNR          IFEQ      'V'
     C     syakt         andeq     'M'
     c                   exsr      visjobb
     c                   seton                                        99
     c                   setoff                                       7071
     c     sybru         ifeq      'J'
     c     syakt         andeq     'N'
     c                   seton                                        70
     c                   end
     c     syakt         ifeq      'M'
     c     sybru         andeq     'J'
     c                   seton                                        71
     c                   end
     C                   MOVE      ' '           WSNR
     c                   update    subfil
     C                   GOTO      SLHENT
     C                   END
      *
      * arbeide med jobb = W
      *
     C     WSNR          IFEQ      'W'
     c                   exsr      wrkjobb
     c                   seton                                        99
     c                   setoff                                       7071
     c     sybru         ifeq      'J'
     c     syakt         andeq     'N'
     c                   seton                                        70
     c                   end
     c     syakt         ifeq      'M'
     c     sybru         andeq     'J'
     c                   seton                                        71
     c                   end
     C                   MOVE      ' '           WSNR
     c                   update    subfil
     C                   GOTO      SLHENT
     C                   END
      *
     C                   ENDDO
      *
     C*
     C     SLHENT        TAG
     C   99              SETOFF                                       98
     C                   Z-add     6             LINNBR
     C                   Z-ADD     2             POSNBR
     C                   SETOFF                                       99
     C   98              DO
     C                   MOVE      'DIV0012'     MSGNR
     C                   SETON                                            30
     C                   END
     C*
     C                   ENDSR
     C*
     C***********************************************
     C     INIT          BEGSR
     C***********************************************
     c     *entry        plist
     c                   parm                    usbs             10
     C     keye          KLIST
     C                   KFLD                    SYSBS            10
     C                   KFLD                    SYJOB            10
     C     keyst         KLIST
     C                   KFLD                    WSSBS            10
     C                   KFLD                    WSJOB            10
     C     keyst0        KLIST
     C                   KFLD                    WSSBS            10
     C     keyrol        KLIST
     C                   KFLD                    ysbs             10
     C                   KFLD                    yjob             10
     C*
     c                   exsr      tomm
     c                   exsr      test
     C                   ENDSR
     C*
     C***********************************************
     C     test          BEGSR
     C***********************************************
     c                   call      'SYDR01CL0'
     c                   parm                    usbs
     c                   open      sydrstat
     c                   setoff                                       33
     c                   move      'N'           startl            1
     c     *in33         doweq     '0'
     c                   read      sydrstat                               33
     c     *in33         ifeq      '0'
     c                   move      TEXT          TEXTDS
     c     startl        ifeq      'J'
     c     jobnvn        andne     *blanks
     c                   eval      wsjob=jobnvn
     c                   eval      wssbs=usbs
     c     keyst         chain     sydrsbsj                           34
     c     *in34         ifeq      '0'
     c     aktive        ifeq      'ACTIVE'
     c                   MOVE      'J'           SYAKT
     c                   eval      syjobt=jobtyp
     c                   eval      syjobn=jobb
     c                   eval      syuser=bruker
     c                   update    sydrsbsjr
     c                   end
     c     aktive        ifeq      'MSGW'
     c                   MOVE      'M'           SYAKT
     c                   eval      syjobt=jobtyp
     c                   eval      syjobn=jobb
     c                   eval      syuser=bruker
     c                   update    sydrsbsjr
     c                   end
     c                   end
     c                   end
     c     jobnvn        ifeq      'Jobbnavn'
     c                   move      'J'           startl
     c                   end
     c                   end
     c                   enddo
     c                   close     sydrstat
     c                   endsr
      ***********************************************
     C     tomm          BEGSR
     C***********************************************
     c                   setoff                                       33
     c     keyst0        setll     sydrsbsj
     c     *in(33)       doweq     '0'
     c                   read      sydrsbsj                               33
     c     *in(33)       ifeq      '0'
     c                   move      'N'           syakt
     c                   update    sydrsbsjr
     c                   end
     c                   enddo
     c                   endsr
     C***********************************************
     C     endre         BEGSR
     C***********************************************
     c                   move      sybru         wsbru
     c                   exfmt     vareli
     c  N12              do
     c     keye          chain     sydrsbsj                           33
     c                   move      wsbru         sybru
     c                   update    sydrsbsjr
     C                   END
     c                   endsr
     C***********************************************
     C     Startpj       BEGSR
     C***********************************************
     c                   call      'SYDR01STPJ'
     c                   parm                    usbs
     c                   parm                    sypgm
     c                   parm                    sylib
     c                   parm                    syjob
     c                   endsr
     C***********************************************
