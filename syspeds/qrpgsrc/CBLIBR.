     C                   END
     C*
     C* INVOICES CONSIGNEE
     C*
     C  N33              DO
     C     UIN07         IFEQ      '1'
     C                   MOVE      HEFNGK        XKREDA
     C     FACURI        IFNE      *BLANKS
     C                   MOVE      FACURI        HEVALK
     C                   END
     C                   END
