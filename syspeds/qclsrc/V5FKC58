             PGM        PARM(&FRALIB &TILLIB &TOMME)
             DCL        VAR(&FRALIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TILLIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TOMME)  TYPE(*CHAR) LEN(1)

             CPYF       FROMFILE(&FRALIB/STATLOGF) +
                          TOFILE(&TILLIB/STATLOGF) MBROPT(*REPLACE) +
                          INCCHAR(*RCD 103 *NE '  ') FMTOPT(*MAP *DROP)
             MONMSG     MSGID(CPF0000) CMPDTA(*NONE) EXEC(GOTO +
                          CMDLBL(SLUTT))
             OVRDBF     FILE(STATLOGF) TOFILE(&TILLIB/STATLOGF)
             CALL       PGM(V5FKR58)
 SLUTT:      IF         COND(&TOMME = 'J') THEN(CLRPFM +
                          FILE(&FRALIB/STATLOGF))

             ENDPGM
