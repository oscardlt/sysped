      *  Initiering                                           SRA /
      *////////////////////////////////////////////////////////////
     C     SRA           BEGSR
      *__________________________
      * Definiere arbetsvariablar
      *""""""""""""""""""""""""""
     C     *LIKE         DEFINE    SVIH_MOKN     UVIH_MOKN
      *__________________
      * Definiere Nøkkler
      *""""""""""""""""""
     C     KEYA          KLIST
     C                   KFLD                    SVIH_SYAV
     C                   KFLD                    SVIH_SYOP
      *_________________
      * Input parametrer
      *"""""""""""""""""
     C     *ENTRY        PLIST
     C                   PARM                    UDATO             8
     C                   MOVEL     UDATO         DATON
     C                   ENDSR
      *////////////////////////////////////////////////////////////
      *  Start av file                                        SRB /
      *////////////////////////////////////////////////////////////
     C     SRB           BEGSR
     C                   READ      SVIH                                   51
     C     *IN51         DOWEQ     *OFF
     C     SVIH_SYDT     IFLE      DATON
     C     SVIH_EUP1     IFEQ      '4200'
     C     SVIH_SYST     IFEQ      'G'
     C     KEYA          CHAIN(N)  SVIHI                              53
     C   53              EXSR      SRBA
     C                   ENDIF
     C                   ENDIF
     C                   ENDIF
      * Læs næsta varulinje
     C                   READ      SVIH                                   51
     C  N51              ENDDO
     C                   ENDSR
      *////////////////////////////////////////////////////////////
      *  Les varelinjer                                      SRBA /
      *////////////////////////////////////////////////////////////
     C     SRBA          BEGSR
     C                   EVAL      *IN52=*OFF
     C     KEYA          SETLL     SVIV
     C     KEYA          READE     SVIV                                   52
     C     *IN52         DOWEQ     *OFF
     C                   IF        SVIH_VAKD='EUC'
     C                   EVAL      SVIH_VAKD='EUR'
     C                   ENDIF
N01  C                   EVAL      SVIH_VAKD='SEK'
N01  C                   EVAL      SVIV_FABL=SVIV_STVA
     C                   EXCEPT    UT
     C                   CLEAR                   SVIHIR
     C                   EVAL      SVIHI_SAV=SVIH_SYAV
     C                   EVAL      SVIHI_SOP=SVIH_SYOP
N02  C                   EVAL      ZVIV_NETO=SVIV_NETO
N02  C                   IF        ZVIV_NETO=*ZEROS
N02  C                   EVAL      ZVIV_NETO=1
N02  C                   ENDIF
     C                   WRITE     SVIHIR
     C     KEYA          READE     SVIV                                   52
     C  N52              ENDDO
     C                   ENDSR
     OQ         EADD         UT
