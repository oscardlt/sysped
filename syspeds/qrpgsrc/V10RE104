     C                   IF        XINIT <> 'J'
     C                   EVAL      FNFN = 'GSSOLF'
     C     FNFN          CHAIN(N)  V10FNF                             33
     C                   IF        UDRIFT = 'J'
     C                   IF        FNST = 'J'
     C                   EVAL      *INLR = *ON
     C                   RETURN
     C                   END
     C                   ELSE
     C                   IF        FNHST = 'J'
     C                   EVAL      *INLR = *ON
     C                   RETURN
     C                   END
     C                   END
     C                   MOVE      'J'           XINIT             1
     C                   END
      *
     C                   MOVEL     OLTNO         XAN06             6 0
     C                   MOVE      XAN06         XAN04             4 0
     C                   MOVEL     XAN04         XAN062            6 0
