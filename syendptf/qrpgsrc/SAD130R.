     C***********************************************
     C     TESTB1        BEGSR
     C***********************************************
     C                   MOVE      *BLANKS       KOANVN
     C     WSAVD         IFNE      0
     C     KODTAK        CHAIN     KODTA                              99
     C   99              Z-ADD     4             LINNBR
     C   99              Z-ADD     26            POSNBR
     C   99              MOVE      'SPA0001'     MSGNR
     C   99              SETON                                        30
     C   99              GOTO      SLTB1
     C                   MOVE      WSAVD         UAVD
     C     CUNDL01K      CHAIN     CUNDL01                            33
     C                   END
      *
     C                   MOVEA     '00000'       *IN(41)
      *
     C     WSPRO         COMP      *ZEROS                             4141
     C   41              GOTO      STTB1
     C     WSUUID        COMP      *BLANKS                            4242
     C   42              GOTO      STTB1
     C     WSETA         COMP      *ZEROS                             4343
     C   43              GOTO      STTB1
     C     WSDTR         COMP      *ZEROS                             4444
     C   44              GOTO      STTB1
     C     WSST          COMP      *BLANKS                            4545
     C   45              GOTO      STTB1
      *
     C                   MOVE      'DIV0064'     MSGNR
     C                   SETON                                        3098
     C                   Z-ADD     8             LINNBR
     C                   Z-ADD     35            POSNBR
     C                   GOTO      SLTB1
      *
     C     STTB1         TAG
      *
     C     SLTB1         ENDSR
      *
     C***********************************************
     C     SETKEY        BEGSR
     C***********************************************
     C   41              DO
     C                   END
     C   42              DO
     C                   END
     C   43              DO
     C                   IF        WSETAD = 999999
     C                   EVAL      XSETAD = 99999999
     C                   ELSE
     C                   MOVE      WSETAD        XSETAD
     C                   MOVE      WSETAM        XSETAM
     C     WSETAA        ADD       2000          XSETAA
     C                   END
     C                   END
     C   44              DO
     C                   IF        WSDTR = 999999
     C                   EVAL      XSDTR = 99999999
     C                   ELSE
     C                   MOVE      WSDTRD        XSDTRD
     C                   MOVE      WSDTRM        XSDTRM
     C     WSDTRA        ADD       2000          XSDTRA
     C                   END
     C                   END
     C   45              DO
     C                   END
      *
     C                   EXSR      MINTUR
      *
     C                   ENDSR
      *
     C***********************************************
     C     MINTUR        BEGSR
     C***********************************************
      * SET KEY NÅR SØK KUN I EN !!!!  AVDELING
     C   41              IF        WSAVD = 0
     C     WSPRO         SETLL     SADEFL02
     C                   ELSE
     C     SADEFL01K2    SETLL     SADEFL01
     C                   END
     C   42WSUUID        SETLL     SADEFL03
     C   43XSETA         SETLL     SADEFL04
     C   44XSDTR         SETLL     SADEFL05
     C*  45SADEFL0XK     SETLL     SADEFL0X
      *
     C                   ENDSR
      *
     C***********************************************
     C     LESREC        BEGSR
     C***********************************************
     C   41              IF        WSAVD = 0
     C                   READ      SADEFL02                               94
     C                   ELSE
     C     WSAVD         READE     SADEFL01                               94
     C                   END
     C   42              READ      SADEFL03                               94
     C   43              IF        XSETA = 99999999
     C                   READP     SADEFL04                               94
     C                   ELSE
     C                   READ      SADEFL04                               94
     C                   END
     C   44              IF        XSDTR = 99999999
     C                   READP     SADEFL05                               94
     C                   ELSE
     C                   READ      SADEFL05                               94
     C                   END
     C*  45              READ      SADEFL0X                               94
      *
     C                   ENDSR
      *
     C***********************************************
     C     INIT          BEGSR
     C***********************************************
     C     SADEFL01K1    KLIST
     C                   KFLD                    WSAVD
