             PGM        PARM(&FRALIB &TILLIB &TOMME)
             DCL        VAR(&FRALIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TILLIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TOMME)  TYPE(*CHAR) LEN(1)

             CLRPFM     FILE(&TILLIB/BOOKING)
             OVRDBF     FILE(BOOKNY) TOFILE(&TILLIB/BOOKING)
             OVRDBF     FILE(BOOKGM) TOFILE(&FRALIB/BOOKING)
             CALL       PGM(V5FKR06)
             IF         COND(&TOMME = 'J') THEN(CLRPFM +
                          FILE(&FRALIB/BOOKING))

             ENDPGM
