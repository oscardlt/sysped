/* KJØRER FACSIMILESUPPORT */
/*N01 * FRERE PARAM */
/*S01        PGM        PARM(&FAXSYS &AVD &OPD &ACT &DATE &TIME &DATL &TIML +
                          &TEXT &USR &CONT &FAX)  */
/*N01 */     PGM        PARM(&FAXSYS &AVD &OPD &ACT &DATE &TIME &DATL &TIML +
                          &EDEV &TEXL &TEXT &USR &CONT &FAX)
             DCL        VAR(&FAXSYS) TYPE(*CHAR) LEN(1) /*F=FAXIMILE B=BLUEFAX*/
             DCL        VAR(&AVD)  TYPE(*CHAR) LEN(4)
             DCL        VAR(&OPD)  TYPE(*CHAR) LEN(7)
             DCL        VAR(&ACT)  TYPE(*CHAR) LEN(3)
             DCL        VAR(&DATE) TYPE(*CHAR) LEN(8)
             DCL        VAR(&TIME) TYPE(*CHAR) LEN(6)
             DCL        VAR(&DATL) TYPE(*CHAR) LEN(8)
             DCL        VAR(&TIML) TYPE(*CHAR) LEN(6)
/*N01 */     DCL        VAR(&EDEV) TYPE(*CHAR) LEN(3)
/*N01 */     DCL        VAR(&TEXL) TYPE(*CHAR) LEN(70)
             DCL        VAR(&TEXT) TYPE(*CHAR) LEN(70)
             DCL        VAR(&USR)  TYPE(*CHAR) LEN(10)
             DCL        VAR(&CONT) TYPE(*CHAR) LEN(30)
             DCL        VAR(&FAX)  TYPE(*CHAR) LEN(21)

/* FAXIMILSEUPPORT  */
             IF         COND(&FAXSYS = 'F') THEN(DO)
/*E01*/      SBMJOB     CMD(CALL PGM(ESTSTTFCF) PARM(&AVD &OPD &ACT +
                          &DATE &TIME &DATL &TIML &EDEV &TEXL &TEXT +
                          &USR &CONT &FAX)) JOB(SNDFAXADVI) +
                          JOBD(*USRPRF) JOBQ(SYJOBQNMAX)
             ENDDO

/* BLUEFAX  */
             IF         COND(&FAXSYS = 'B') THEN(DO)
/*E01*/      SBMJOB     CMD(CALL PGM(ESTSTTFCB) PARM(&AVD &OPD &ACT +
                          &DATE &TIME &DATL &TIML &EDEV &TEXL &TEXT +
                          &USR &CONT &FAX)) JOB(SNDFAXADVI) +
                          JOBD(*USRPRF) JOBQ(SYJOBQNMAX)
             ENDDO

 SLUTT:      ENDPGM
