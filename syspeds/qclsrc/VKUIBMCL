             PGM
             DCL        VAR(&SPEDDTAARA) TYPE(*CHAR) LEN(2000) /* +
                          Denne variabelen skal bevare *LDA. */
             RTVDTAARA  DTAARA(*LDA *ALL) RTNVAR(&SPEDDTAARA) /* +
                          Bevare *LDA. */
             CHGDTAARA  DTAARA(*LDA *ALL) +
                          VALUE('                              ') +
                          /* Tøm LDA */
 /* */
             CALL       PGM(SYSPED/PVKUNDE)
 /* */
 /* */
             CALL       INLIBL
 /* */
 /* LEGGE &SPEDDTAARA TILBAKE TIL *LDA  */
             CHGDTAARA  DTAARA(*LDA *ALL) VALUE(&SPEDDTAARA)
 /*  */
             ENDPGM
