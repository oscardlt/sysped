             PGM        PARM(&USER &NAME &FINS)
             DCL        VAR(&USER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&NAME) TYPE(*CHAR) LEN(50)
             DCL        VAR(&FINS) TYPE(*CHAR) LEN(1)
             DLTF       FILE(QTEMP/TESTUSR)
             MONMSG     MSGID(CPF0000)
             DSPUSRPRF  USRPRF(&USER) OUTPUT(*OUTFILE) +
                          OUTFILE(QTEMP/TESTUSR)
             CALL       PGM(SYGE139) PARM(&USER &NAME &FINS)
             DMPCLPGM
             ENDPGM
