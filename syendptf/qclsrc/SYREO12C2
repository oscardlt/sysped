             PGM        PARM(&AVD &OPD &PRO)
             DCL        VAR(&AVD) TYPE(*CHAR) LEN(4)
             DCL        VAR(&OPD) TYPE(*CHAR) LEN(7)
             DCL        VAR(&PRO) TYPE(*CHAR) LEN(8)

             OVRDBF     FILE(DOKEB) TOFILE(DOKEB) FRCRATIO(*NONE)
             OVRDBF     FILE(DOKEC) TOFILE(DOKEC) FRCRATIO(*NONE)
             OVRDBF     FILE(DOKECE) TOFILE(DOKECE) FRCRATIO(*NONE)
             OVRDBF     FILE(DOKECEL) TOFILE(DOKECEL) FRCRATIO(*NONE)
