             PGM
             RUNQRY     QRY(NCTSGBL1_4) QRYFILE((TRUH)) +
                          OUTTYPE(*OUTFILE) RCDSLT(*NO)
             RUNQRY     QRY(NCTSGBL1_5) QRYFILE((NCTSEH)) +
                          OUTTYPE(*OUTFILE) RCDSLT(*NO)
             RUNQRY     QRY(NCTSGBL1_9) OUTTYPE(*DISPLAY) +
                          RCDSLT(*NO)
             GOTO       CMDLBL(SLUTT)
             DLTF       FILE(QTEMP/Q)
             MONMSG CPF0000
             RUNQRY     QRY(NCTSGBL1_9) OUTTYPE(*OUTFILE) +
                          RCDSLT(*NO) OUTFILE(QTEMP/Q)
             MONMSG CPF0000
             CPYTOIMPF  FROMFILE(QTEMP/Q) +
                          TOSTMF('/HOME/CB/GARANTI.CSV') +
                          RCDDLM(*CRLF) STRDLM(*NONE) FLDDLM(';') +
                          ADDCOLNAM(*SQL)
             MONMSG CPF0000
             SNDSMTPEMM RCP(('CHRISTER.BECH@WISETECHGLOBAL.COM')) +
                          SUBJECT('GARANTIBELØP') +
                          ATTACH(('/home/cb/garanti.csv' *PLAIN *TXT))
             MONMSG CPF0000
 SLUTT:      ENDPGM
