             PGM        PARM(&FILNAV &MAPPE &FLYTT)
             DCL        VAR(&FILNAV) TYPE(*CHAR) LEN(256)
             DCL        VAR(&MAPPE) TYPE(*CHAR) LEN(256)
             DCL        VAR(&FLYTT) TYPE(*CHAR) LEN(1)

             CALL       PGM(EDI10R6T) PARM(&FILNAV &FLYTT)
             IF         COND(&FLYTT = 'J') THEN(DO)
             MOV        OBJ(&FILNAV) TODIR(&MAPPE)
             ENDDO

             ENDPGM
