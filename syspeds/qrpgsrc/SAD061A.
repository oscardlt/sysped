     C     W2BLÆ         IFEQ      0                                              3<-B
     C                   SELECT
     C     XEH2Æ         WHENEQ    ' '
     C     XEH2Æ         OREQ      'K'
     C     XEH2Æ         OREQ      'G'
     C     XEH2Æ         OREQ      'M'
     C     XEH2Æ         OREQ      'D'
     C     XEH2Æ         OREQ      'T'
N13  C     XEH2Æ         OREQ      'L'
N14  C     XEH2Æ         OREQ      'B'
     C                   EVAL      W2BLÆ = W2BLGÆ * W2BLSÆ
     C     XEH2Æ         WHENEQ    '%'
     C     XEH2Æ         OREQ      'H'
     C     XEH2Æ         OREQ      '*'
     C                   EVAL      W2BLÆ = W2BLGÆ * W2BLSÆ / 100
     C     XEH2Æ         WHENEQ    'P'
     C                   EVAL      W2BLÆ = W2BLGÆ * W2BLSÆ / 1000
     C                   ENDSL
     C     XS8ØRE        IFEQ      '*'
S07  C*                  DIV       100           W2BLÆ
N07  C                   DIV(H)    100           W2BLÆ
     C                   END                                                      3<-E
     C                   END                                                      3<-E
     C                   END                                                     2<-E
     C                   ELSE                                                   1
     C     W2BLÆ         COMP      0                                  7878
     C   78              SETON                                        30
     C                   END                                                    1<-E
