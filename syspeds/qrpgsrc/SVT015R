      *""""
     C     *ENTRY        PLIST
     C                   PARM                    UVTA_IE           1
     C                   PARM                    UVTA_VATA        10
     C                   PARM                    ULK               2
     C                   PARM                    UYKOD1            4
     C                   PARM                    UYKOD2            4
     C                   PARM                    UYKOD3            4
     C                   PARM                    UYKOD4            4
     C                   PARM                    UYKOD5            4
     C                   PARM                    UYKOD6            4
     C                   PARM                    UYKOD7            4
     C                   PARM                    UYKOD8            4
     C                   PARM                    UYKOD9            4
      *________________________________
      * Hæmta alla val från TARIC-filen
      *""""""""""""""""""""""""""""""""
     C                   CALL      'SVT016R'
     C                   PARM                    UVTA_IE
     C                   PARM                    UVTA_VATA
     C                   PARM                    ULK
     C                   PARM                    UYKOD1
     C                   PARM                    UYTXT1          100
     C                   PARM                    UYKOD2
     C                   PARM                    UYTXT2          100
     C                   PARM                    UYKOD3
     C                   PARM                    UYTXT3          100
     C                   PARM                    UYKOD4
     C                   PARM                    UYTXT4          100
     C                   PARM                    UYKOD5
     C                   PARM                    UYTXT5          100
     C                   PARM                    UYKOD6
     C                   PARM                    UYTXT6          100
     C                   PARM                    UYKOD7
     C                   PARM                    UYTXT7          100
     C                   PARM                    UYKOD8
     C                   PARM                    UYTXT8          100
     C                   PARM                    UYKOD9
     C                   PARM                    UYTXT9          100
      *___________________
      * Finnes noe å velge
      *"""""""""""""""""""
     C     UYKOD1        CABEQ     *BLANKS       SLUTT
      *______________
      * Clear Subfile
      *""""""""""""""
     C                   OPEN      SVT015D

     C                   MOVE      '1'           *IN93
     C                   MOVE      *BLANKS       SFNR
     C                   WRITE     SUBCTL
     C                   MOVEA     '00'          *IN(93)
     C                   Z-ADD     *ZEROS        ZRECNO
      *_____________
      * Fyll Subfile
      *"""""""""""""
     C     UYKOD1        IFNE      *BLANKS
     C                   EVAL      XYKOD=UYKOD1
     C                   EVAL      XYTXT=UYTXT1
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
     C                   ENDIF

     C     UYKOD2        IFNE      *BLANKS
     C                   EVAL      XYKOD=UYKOD2
     C                   EVAL      XYTXT=UYTXT2
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
     C                   ENDIF

     C     UYKOD3        IFNE      *BLANKS
     C                   EVAL      XYKOD=UYKOD3
     C                   EVAL      XYTXT=UYTXT3
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
     C                   ENDIF

     C     UYKOD4        IFNE      *BLANKS
     C                   EVAL      XYKOD=UYKOD4
     C                   EVAL      XYTXT=UYTXT4
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
     C                   ENDIF

     C     UYKOD5        IFNE      *BLANKS
     C                   EVAL      XYKOD=UYKOD5
     C                   EVAL      XYTXT=UYTXT5
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
     C                   ENDIF

     C     UYKOD6        IFNE      *BLANKS
     C                   EVAL      XYKOD=UYKOD6
     C                   EVAL      XYTXT=UYTXT6
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
     C                   ENDIF

     C     UYKOD7        IFNE      *BLANKS
     C                   EVAL      XYKOD=UYKOD7
     C                   EVAL      XYTXT=UYTXT7
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
     C                   ENDIF

     C     UYKOD8        IFNE      *BLANKS
     C                   EVAL      XYKOD=UYKOD8
     C                   EVAL      XYTXT=UYTXT8
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
     C                   ENDIF

     C     UYKOD9        IFNE      *BLANKS
     C                   EVAL      XYKOD=UYKOD9
     C                   EVAL      XYTXT=UYTXT9
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
     C                   ENDIF
     C                   EVAL      UYKOD1=*BLANKS
     C                   EVAL      UYKOD2=*BLANKS
     C                   EVAL      UYKOD3=*BLANKS
     C                   EVAL      UYKOD4=*BLANKS
     C                   EVAL      UYKOD5=*BLANKS
     C                   EVAL      UYKOD6=*BLANKS
     C                   EVAL      UYKOD7=*BLANKS
     C                   EVAL      UYKOD8=*BLANKS
     C                   EVAL      UYKOD9=*BLANKS
      *________________
      * Display Subfile
      *""""""""""""""""
     C                   Z-ADD     1             ZRECNO

     C                   MOVE      *OFF          *IN51
     C                   MOVEA     '11'          *IN(91)
     C* KOMMER HIT VED TOM FIL
     C     ZRECNO        IFEQ      0
     C                   MOVE      *OFF          *IN94
     C                   ADD       1             ZRECNO
     C                   MOVE      *ON           *IN51
     C                   WRITE     SUBFIL
     C                   SETON                                        94
     C                   ENDIF
     C                   WRITE     WIND01
     C                   WRITE     BUNN
     C     STBUNN        TAG

     C                   EXFMT     SUBCTL

     C                   SETOFF                                       999830
      * FORRIGE
     C   12              GOTO      SLUTT

     C     ZRECNO        IFNE      0

     C                   MOVE      *OFF          *IN98
     C                   EVAL      UYKOD1=*BLANKS
     C                   EVAL      UYKOD2=*BLANKS
     C                   EVAL      UYKOD3=*BLANKS
     C                   EVAL      UYKOD4=*BLANKS
     C                   EVAL      UYKOD5=*BLANKS
     C                   EVAL      UYKOD6=*BLANKS
     C                   EVAL      UYKOD7=*BLANKS
     C                   EVAL      UYKOD8=*BLANKS
     C                   EVAL      UYKOD9=*BLANKS
     C     *IN98         DOWEQ     '0'
     C                   READC     SUBFIL                                 98
     C  N98SFNR          IFEQ      '1'
     C                   SELECT
     C     UYKOD1        WHENEQ    *BLANKS
     C                   EVAL      UYKOD1=XYKOD
     C     UYKOD2        WHENEQ    *BLANKS
     C                   EVAL      UYKOD2=XYKOD
     C     UYKOD3        WHENEQ    *BLANKS
     C                   EVAL      UYKOD3=XYKOD
     C     UYKOD4        WHENEQ    *BLANKS
     C                   EVAL      UYKOD4=XYKOD
     C     UYKOD5        WHENEQ    *BLANKS
     C                   EVAL      UYKOD5=XYKOD
     C     UYKOD6        WHENEQ    *BLANKS
     C                   EVAL      UYKOD6=XYKOD
     C     UYKOD7        WHENEQ    *BLANKS
     C                   EVAL      UYKOD7=XYKOD
     C     UYKOD8        WHENEQ    *BLANKS
     C                   EVAL      UYKOD8=XYKOD
     C     UYKOD9        WHENEQ    *BLANKS
     C                   EVAL      UYKOD9=XYKOD
     C                   ENDSL
     C                   ENDIF
     C                   ENDDO

     C                   ENDIF

     C     SLUTT         TAG

     C                   EVAL      *INLR=*ON
