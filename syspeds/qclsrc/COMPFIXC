/*   BYTTER INNHOLD I COMPCGI2 BASERT PÅ INNHOLD I QTEMP/XX          */
             PGM
             DLTF       FILE(QTEMP/XXX)
             MONMSG     MSGID(CPF0000)
             CRTPF      FILE(QTEMP/XXX) RCDLEN(100)
             CALL COMPFIXR
             CPYF       FROMFILE(XXX) TOFILE(SYENDPTF/QCLSRC) +
                          TOMBR(COMPCGI2) MBROPT(*REPLACE) +
                          FMTOPT(*CVTSRC)
             CRTCLPGM   PGM(SYENDPTF/COMPCGI2) SRCFILE(SYENDPTF/QCLSRC)
             SNDPGMMSG  MSG('CL COMPCGI2 ER SKAPT, Batchkjøring: +
                          CALL COMPCGI (Online: CALL COMPCGI2)')
             ENDPGM
