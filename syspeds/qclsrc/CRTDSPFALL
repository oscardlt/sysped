             PGM

             DCLF       FILE(SYSPEDF/CRTDSPFALL)
             DCL        VAR(&MBNAME) TYPE(*CHAR) LEN(10)
             DCL        VAR(&MBSEU2) TYPE(*CHAR) LEN(10)
             CLRPFM     FILE(SYSPEDF/CRTDSPFALL)
             MONMSG     MSGID(CPF0000)
             DSPFD      FILE(SYSPEDS/QDDSSRC) TYPE(*MBR) +
                          OUTPUT(*OUTFILE) OUTFILE(SYSPEDF/CRTDSPFALL)

 READ:       RCVF       RCDFMT(QWHFDMBR)
             MONMSG     MSGID(CPF0000) EXEC(GOTO CMDLBL(EXIT))
             IF         COND(&MBSEU2 *NE DSPF) THEN(GOTO +
                          CMDLBL(READ))
             DLTF       FILE(SYSPED/&MBNAME)
             MONMSG     MSGID(CPF0000)
             SBMJOB     CMD(CRTDSPF FILE(SYSPED/&MBNAME) +
                          SRCFILE(SYSPEDS/QDDSSRC) RSTDSP(*YES) +
                          LVLCHK(*NO)) JOB(&MBNAME) +
                          JOBQ(SYSPED/SYJOBQNMAX)
             MONMSG     MSGID(CPF0000)
             GOTO READ

 EXIT:       CLRPFM     FILE(SYSPEDF/CRTDSPFALL)
             ENDPGM
