             CMD        PROMPT('BEVARE EDI-HISTORIKK')
             PARM       KWD(TAP) TYPE(*CHAR) LEN(10) DFT(*DISK) +
                          CHOICE(NAVN) PROMPT('Enhet eller Disk')
             PARM       KWD(JOBPR) TYPE(*CHAR) LEN(2) DFT(51) +
                          CHOICE('20, 30, 50, ...') +
                          PROMPT('Jobbprioritet')
             PARM       KWD(TODATE) TYPE(*CHAR) LEN(8) DFT(00000000) +
                          RANGE(00000000 99991231) +
                          CHOICE('≈≈≈≈MMDD') PROMPT('T.o.m. Dato')
