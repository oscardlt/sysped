             PGM        PARM(&FILNV &FRALIB &TILLIB &TOMME)
             DCL        VAR(&FILNV) TYPE(*CHAR) LEN(10)
             DCL        VAR(&FRALIB) TYPE(*CHAR) LEN(10) /* +
                          Driftbibliotek */
             DCL        VAR(&TILLIB) TYPE(*CHAR) LEN(10) /* +
                          Oppbevareringsbibliotek for datafiler fra +
                          V6 */
             DCL        VAR(&TOMME) TYPE(*CHAR) LEN(1) /* Om datafil +
                          fra V6 skal tømmes etter konvertering. */

             CLRPFM     FILE(&FILNV)
             MONMSG     MSGID(CPF0000)
             OVRDBF     FILE(STANDIG) TOFILE(&FRALIB/&FILNV)
             CALL       PGM(V7FKR09)
             MOVOBJ     OBJ(&FRALIB/&FILNV) OBJTYPE(*FILE) +
                          TOLIB(&TILLIB)
             MONMSG     MSGID(CPF0000)
             CPYF       FROMFILE(&FILNV) TOFILE(&FRALIB/&FILNV) +
                          FROMMBR(*ALL) TOMBR(*FROMMBR) +
                          MBROPT(*ADD) CRTFILE(*YES)
             MONMSG     MSGID(CPF0000)
             IF         COND(&TOMME = 'J') THEN(DO)
             CLRPFM     FILE(&FRALIB/&FILNV)
             MONMSG     MSGID(CPF0000)
             ENDDO
             ENDPGM
