/* PTFnr:*Sek Sig Vers  Beskrivelse..........................              */
/* """"""*"""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""    */
/* 10XXX * 01 YBC PTF10 MONMSG PÅ SLETTING AV OVERSTYRING                  */
/* ********************************************************************    */
             PGM        PARM(&WSDABA &FEIL)
             DCL        VAR(&WSDABA) TYPE(*CHAR) LEN(10)
             DCL        VAR(&FEIL) TYPE(*CHAR) LEN(1)
             DCL        VAR(&FLAGG) TYPE(*DEC) LEN(1 0) VALUE(0)
             CHKOBJ     OBJ(&WSDABA/AVFRH) OBJTYPE(*FILE)
             MONMSG     MSGID(CPF9801 CPF9810) EXEC(CHGVAR +
                          VAR(&FEIL) VALUE('J'))
             IF         COND(&FEIL = 'J') THEN(GOTO CMDLBL(SLUTT))
             DLTOVR     FILE(AVFRHCOP) LVL(*JOB)
/*N01*/      MONMSG     MSGID(CPF0000)
             OVRDBF     FILE(AVFRHCOP) TOFILE(&WSDABA/AVFRH) +
                          OVRSCOPE(*JOB)
             MONMSG     MSGID(CPF0000) EXEC(CHGVAR VAR(&FEIL) +
                          VALUE('J'))
             DLTOVR     FILE(AVFRDL01C) LVL(*JOB)
/*N01*/      MONMSG     MSGID(CPF0000)
             OVRDBF     FILE(AVFRDL01C) TOFILE(&WSDABA/AVFRDL01) +
                          OVRSCOPE(*JOB)
             MONMSG     MSGID(CPF0000) EXEC(CHGVAR VAR(&FEIL) +
                          VALUE('J'))
SLUTT:       ENDPGM
