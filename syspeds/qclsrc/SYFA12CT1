/* MIDLERTIDIG FRAKTBREV FRA "E-SENDING", TIL HJELP MED INNGÅENDE SJEKK  */
/* SIMULERER(DELVIS) AT DEN FAKSES. KODE M I &PO (-> F)                  */
             PGM        PARM(&AVD &OPD &TERPRT &TERPT)
             DCL        VAR(&AVD) TYPE(*CHAR) LEN(4)
             DCL        VAR(&OPD) TYPE(*CHAR) LEN(7)
             DCL        VAR(&TERPRT) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TERPT) TYPE(*CHAR) LEN(10)
             DCL        VAR(&GMAVD) TYPE(*CHAR) LEN(4)
             DCL        VAR(&KODE) TYPE(*CHAR) LEN(1) VALUE('F')
             DCL        VAR(&VB) TYPE(*CHAR) LEN(1)
             DCL        VAR(&EKSP) TYPE(*CHAR) LEN(1)
             DCL        VAR(&LG) TYPE(*CHAR) LEN(1)
             DCL        VAR(&PO) TYPE(*CHAR) LEN(1) VALUE('M')
     /*HUSK GML UDSAVD*/
             RTVDTAARA  DTAARA(*LDA (15 4)) RTNVAR(&GMAVD)
    /* ----------------------------------------------------------- */
    /* SKRIVER MIDLERTIDIG FR.BREV PÅ ETT OPPDRAG                  */
    /* ----------------------------------------------------------- */
             CHGDTAARA  DTAARA(*LDA (15 4)) VALUE(&AVD)
/*V10        CHGDTAARA  DTAARA(*LDA (22 5)) VALUE(&OPD) */
/*V10*/      CHGDTAARA  DTAARA(*LDA (22 8)) VALUE('00000000')
/*V10*/      CHGDTAARA  DTAARA(*LDA (82 7)) VALUE(&OPD)
             CHGDTAARA  DTAARA(*LDA (27 3)) VALUE('001')
             CHGDTAARA  DTAARA(*LDA (721 10)) VALUE(&TERPRT)  /*FR.BR.*/
             CHGDTAARA  DTAARA(*LDA (731 10)) VALUE(&TERPT)
             CHGDTAARA  DTAARA(*LDA (761 10)) VALUE('QPRINT')
             CHGDTAARA  DTAARA(*LDA (781 10)) VALUE('QPRINT')
             CHGDTAARA  DTAARA(*LDA (801 10)) VALUE('QPRINT')
             CHGDTAARA  DTAARA(*LDA (801 10)) VALUE('QPRINT')
             CHGDTAARA  DTAARA(*LDA (881 10)) VALUE('QPRINT')
             CHGDTAARA  DTAARA(*LDA (891 10)) VALUE('QPRINT')
             CHGDTAARA  DTAARA(*LDA (921 10)) VALUE('QPRINT')
             CHGDTAARA  DTAARA(*LDA (941 10)) VALUE('QPRINT')

             CALL       PGM(SYFA12CLJ) PARM(&KODE &VB &EKSP &LG &PO)

     /*SETT GML UDSAVD*/
             CHGDTAARA  DTAARA(*LDA (15 4)) VALUE(&GMAVD)
             CHGDTAARA  DTAARA(*LDA (22 8)) VALUE('00000000')
/*V10*/      CHGDTAARA  DTAARA(*LDA (82 7)) VALUE('0000000')

 SLUTT:      ENDPGM
