             PGM        PARM(&PNR &USN &FIRM &ALNG &SORT &TYPE)
             DCL        VAR(&PNR) TYPE(*CHAR) LEN(8)
             DCL        VAR(&USN) TYPE(*CHAR) LEN(10)
             DCL        VAR(&FIRM) TYPE(*CHAR) LEN(2)
             DCL        VAR(&ALNG) TYPE(*CHAR) LEN(7)
             DCL        VAR(&ALCPY) TYPE(*CHAR) LEN(7)
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10) VALUE('*JOB')
             DCL        VAR(&SORT) TYPE(*CHAR) LEN(1)
             DCL        VAR(&TYPE) TYPE(*CHAR) LEN(1)
             DCL        VAR(&LAS) TYPE(*CHAR) LEN(1)
             DCL        VAR(&STR) TYPE(*CHAR) LEN(5)
             DCL        VAR(&QJOB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QUSER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QNBR) TYPE(*CHAR) LEN(6)
             DCL        VAR(&STMF) TYPE(*CHAR) LEN(50)

             RTVJOBA    JOB(&QJOB) USER(&QUSER) NBR(&QNBR)
             OVRPRTF    FILE(LL013P) OUTQ(SYCGIOUTQ) FORMTYPE(*STD) +
                          HOLD(*YES) SPLFOWN(*JOB)
             CALL       PGM(LL013C) PARM(&FIRM &ALNG &ALCPY &PRT +
                          &SORT &TYPE &LAS &STR)

             ADDLIBLE   LIB(MMAIL)
             MONMSG     MSGID(CPF0000)
             CHGVAR     VAR(&STMF) VALUE('/syspeddev/temp/PLL' *CAT +
                          &PNR *CAT &ALNG *CAT &USN *CAT '.PDF')
             CVTSPLFPDF SPLF(LL013P) JOB(&QNBR/&QUSER/&QJOB) +
                          SPLNBR(*LAST) TOPDF(&STMF)
             DLTSPLF    FILE(ll013P) JOB(&QNBR/&QUSER/&QJOB) +
                          SPLNBR(*LAST)
             MONMSG     MSGID(CPF0000)
             RMVLIBLE   LIB(MMAIL)
             MONMSG     MSGID(CPF0000)
             ENDPGM
