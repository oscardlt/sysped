     C                   CAT       ORLC:1        LGTXT
     C                   EVAL      LGANY = WSAREA
     C                   EVAL      LGAGM = ORAREA
     C                   WRITE     GSSLGFR
     C                   END
      *
     C                   MOVE      W2TYM≈        WSTYM1            4
     C                   MOVE      W2TYMU        WSTYM2            2
     C                   IF        ((ORAAR <> WSTYM1) OR (ORUKE <> WSTYM2))
     C                   EVAL      LGTXT = XXTXT07
     C                   CAT       ORLC:1        LGTXT
