     D I               S              2    DIM(16)
     D O               S              1    DIM(16)

     C     *ENTRY        PLIST
     C                   PARM                    X25ADR           16
     C                   PARM                    X25LEN            1

     C                   MOVEA     X25ADR        I
      * Oversett X25 lengde
     C     ' '           SCAN      X25ADR        X                 2 0
     C     X             SUB       1             X
     C                   SELECT
     C     X             WHENEQ    1
     C                   MOVE      X'01'         X25LEN
     C     X             WHENEQ    2
     C                   MOVE      X'02'         X25LEN
     C     X             WHENEQ    3
     C                   MOVE      X'03'         X25LEN
     C     X             WHENEQ    4
     C                   MOVE      X'04'         X25LEN
     C     X             WHENEQ    5
     C                   MOVE      X'05'         X25LEN
     C     X             WHENEQ    6
     C                   MOVE      X'06'         X25LEN
     C     X             WHENEQ    7
     C                   MOVE      X'07'         X25LEN
     C     X             WHENEQ    8
     C                   MOVE      X'08'         X25LEN
     C     X             WHENEQ    9
     C                   MOVE      X'09'         X25LEN
     C     X             WHENEQ    10
     C                   MOVE      X'0A'         X25LEN
     C     X             WHENEQ    11
     C                   MOVE      X'0B'         X25LEN
     C     X             WHENEQ    12
     C                   MOVE      X'0C'         X25LEN
     C     X             WHENEQ    13
     C                   MOVE      X'0D'         X25LEN
     C     X             WHENEQ    14
     C                   MOVE      X'0E'         X25LEN
     C     X             WHENEQ    15
     C                   MOVE      X'0F'         X25LEN
     C     X             WHENEQ    16
     C                   MOVE      X'10'         X25LEN
     C                   ENDSL
     C                   Z-ADD     0             X
      * Oversett X25 nummer
     C     1             DO        16            X

     C                   BITOFF    '01234567'    O(X)
     C                   MOVEL     I(X)          A1                1
     C                   MOVE      I(X)          A2                1

     C                   SELECT
     C     A1            WHENEQ    '1'
     C                   BITON     '3'           O(X)
     C     A1            WHENEQ    '2'
     C                   BITON     '2'           O(X)
     C     A1            WHENEQ    '3'
     C                   BITON     '23'          O(X)
     C     A1            WHENEQ    '4'
     C                   BITON     '1'           O(X)
     C     A1            WHENEQ    '5'
     C                   BITON     '13'          O(X)
     C     A1            WHENEQ    '6'
     C                   BITON     '12'          O(X)
     C     A1            WHENEQ    '7'
     C                   BITON     '123'         O(X)
     C     A1            WHENEQ    '8'
     C                   BITON     '0'           O(X)
     C     A1            WHENEQ    '9'
     C                   BITON     '03'          O(X)
     C                   ENDSL
     C                   SELECT
     C     A2            WHENEQ    '1'
     C                   BITON     '7'           O(X)
     C     A2            WHENEQ    '2'
     C                   BITON     '6'           O(X)
     C     A2            WHENEQ    '3'
     C                   BITON     '67'          O(X)
     C     A2            WHENEQ    '4'
     C                   BITON     '5'           O(X)
     C     A2            WHENEQ    '5'
