             PGM        PARM(&FRALIB &TILLIB &TOMME)
             DCL        VAR(&FRALIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TILLIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TOMME)  TYPE(*CHAR) LEN(1)

             CLRPFM     FILE(&TILLIB/KODTVA)
             OVRDBF     FILE(KODTVANY) TOFILE(&TILLIB/KODTVA)
             OVRDBF     FILE(KODTVAGM) TOFILE(&FRALIB/KODTVA)
             CALL       PGM(V5FKR37)
             IF         COND(&TOMME = 'J') THEN(CLRPFM +
                          FILE(&FRALIB/KODTVA))

             ENDPGM
