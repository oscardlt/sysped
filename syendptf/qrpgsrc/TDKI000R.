      *********************************************************
     C     Parse         Begsr
      *********************************************************
      * Get input
     C                   EVAL      WSUSER  = zhbgetvar('USER')
     C                   EVAL      AVD     = zhbgetvar('AVD')
     C                   eval      AVDN    = c2n2(AVD)
     C                   EVAL      SIGN    = zhbgetvar('SIGN')
     C                   EVAL      OPD     = zhbgetvar('OPD')
     C                   EVAL      OPDN    = c2n2(OPD)
     C                   EVAL      REFNR   = zhbgetvar('REFNR')
     C                   EVAL      XREF    = zhbgetvar('XREF')
     C                   EVAL      DATUM   = zhbgetvar('DATUM')
     C                   EVAL      DATUMN  = c2n2(DATUM)
     C                   EVAL      DATUMT  = zhbgetvar('DATUMT')
     C                   EVAL      DATUMTN = c2n2(DATUMT)
     C                   EVAL      STATUS  = zhbgetvar('STATUS')
     C                   EVAL      AVSNAVN = zhbgetvar('AVSNAVN')
     C                   EVAL      MOTNAVN = zhbgetvar('MOTNAVN')
     C                   EVAL      AART    = zhbgetvar('AART')
     C                   EVAL      FAKT    = zhbgetvar('FAKT')
      * Er dato= blank eller 0, sett  da dato 14 dager tilbake
S01  C*                  OPEN      BRPARF
S01  C*                  IF        OPDN = *ZEROS
S01  C*                  IF        REFNR = *BLANKS
S01  C*                  IF        FAKT  = *BLANKS
S01  C*                  IF        DATUMN = *ZEROS
S01
S01   /free
S01
S01   //   wpabrid=wsuser;
S01   //   wpakunr=0;
S01   //   wpasort=0;
S01   //   chain (WPABRID: WPAKUNR: 'DFTDG': WPASORT) BRPARF;
S01   //   if not %found or pavrdn = 0;
S01   //      pavrdn=7;
S01   //   endif;
S01
S01   /end-free
S01
S01  C*                  CALL      'SYGE20'
S01  C*                  PARM      'SUB'         UFUNK
S01  C*                  PARM      DATUMN        UDAT1
S01  C*                  PARM                    UDAT2
S01  C*                  PARM      PAVRDN        UDGR
S01  C*                  EVAL      DATUMN=UDAT2
S01  C*                  ENDIF
S01  C*                  ENDIF
S01  C*                  ENDIF
S01  C*                  ENDIF
      * Sett stop søking alfa
     C                   MOVEA     AVSNAVN       ZB
     C     ZB(1)         IFNE      ' '
     C                   Z-ADD     0             ZAX               2 0
     C     1             DO        35            ZAX               2 0
     C     36            SUB       ZAX           WAX               2 0
     C     ZB(WAX)       CABNE     *BLANKS       SRB3
     C                   MOVE      '9'           ZB(WAX)
     C                   ENDDO
     C                   ENDIF
     C     SRB3          TAG
     C                   MOVEA     ZB            STOPP1
      * Sett stop søking alfa
     C                   MOVEA     *BLANKS       ZB
     C                   MOVEA     MOTNAVN       ZB
     C     ZB(1)         IFNE      ' '
     C                   Z-ADD     0             ZAX               2 0
     C     1             DO        35            ZAX               2 0
     C     36            SUB       ZAX           WAX               2 0
     C     ZB(WAX)       CABNE     *BLANKS       SRB4
     C                   MOVE      '9'           ZB(WAX)
     C                   ENDDO
     C                   ENDIF
     C     SRB4          TAG
     C                   MOVEA     ZB            STOPP2
      * Sett stop søking alfa
     C                   MOVEA     *BLANKS       ZB
     C                   MOVEA     XREF          ZB
     C     ZB(1)         IFNE      ' '
     C                   Z-ADD     0             ZAX               2 0
     C     1             DO        35            ZAX               2 0
     C     36            SUB       ZAX           WAX               2 0
     C     ZB(WAX)       CABNE     *BLANKS       SRB5
     C                   MOVE      '9'           ZB(WAX)
     C                   ENDDO
     C                   ENDIF
     C     SRB5          TAG
     C                   MOVEA     ZB            STOPP3
      * Sett stop søking alfa
     C                   MOVEA     *BLANKS       ZC
     C                   MOVEA     FAKT          ZC
     C     ZC(1)         IFNE      ' '
     C                   Z-ADD     0             ZAX               2 0
     C     1             DO        17            ZAX               2 0
     C     18            SUB       ZAX           WAX               2 0
     C     ZC(WAX)       CABNE     *BLANKS       SRB6
     C                   MOVE      '9'           ZC(WAX)
     C                   ENDDO
     C                   ENDIF
     C     SRB6          TAG
     C                   MOVEA     ZC            STOPP4
     c                   endsr
