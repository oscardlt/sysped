      /copy syspeds/qrpgsrcpy,hspecs
      /copy syspeds/qrpgsrcpy,hspecsbnd
     FBRIDF     UF   E           K DISK    USROPN
     FGSSORF    UF   E           k DISK    USROPN
     FGSSPOF    IF   E           k DISK    USROPN
     FGSSLGF    O    E             DISK    USROPN
     FTRACKF    O    E             DISK    USROPN
     FVADR      IF   E           k DISK    USROPN
      *--------------------------------------------------------------------
      * Prototype defintions and standard system API error structure
      *--------------------------------------------------------------------
      /copy syspeds/qrpgsrcpy,prototypeb
      /copy syspeds/qrpgsrcpy,usec
      *--------------------------------------------------------------------
      * Variables common to CGI programs
      *--------------------------------------------------------------------
      /copy syspeds/qrpgsrcpy,variables3
      *--------------------------------------------------------------------
      * Variables received from the input string
     D lng             s              2
     D UsrSpcName      s             10
     D WSUSER          S             10
     D WSONO           S              7
     D XXONO           S              7  0
     D DRIVER          S             15
     D SIGN            S             30
     D DATO            S              8
     D KLOKKE          S              4
     D XXKLOKKE        S              4  0
     D LAGREPOD        S              1
     D FMELDNR         S              2
     D FEILMELDING     S             60
     D FEIL01          C                   CONST('Driver can not be blank.')
     D FEIL02          C                   CONST('Signatur of receiver has to -
     D                                     given.')
     D FEIL03          C                   CONST('Receiver date is not valid.')
     D FEIL04          C                   CONST('Receiver time is not valid.')
     D*XXTXT01         C                   CONST('Put POD on order.')
     D XXTXT01         C                   CONST('Delivered to end customer.')
     D XXTXT02         C                   CONST('Driver:')
     D XXTXT03         C                   CONST('Sign:')
     D XXTXT01L        C                   CONST('Levert endelig mottakerr.')
     D XXTXT02L        C                   CONST('Sjåfør:')
     D XXTXT03L        C                   CONST('Sign:')
     D PGM             C                   CONST('SYSPED/CHECKUSR')
      *
     D Lib             s             10
     D Fn              s             10
     D Mbr             s             10
      *
     D Spaces          s             12a   inz('&nbsp;&nbsp;')
     D                 DS
     D XXDATO                  1      8  0
     D XXDATOÅÅ                1      4  0
     D XXDATOMM                5      6  0
     D XXDATODD                7      8  0
     D                 DS
     D ORBCD                   1      5
     D ORBCD2                  2      5
