********************************************************************
      * RETTINGER I DETTE PROGRAM:
PTFnr:*Sek Sig Vers  Beskrivelse...........................
""""""*"""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""
12XXX * 01 YBC PTF12 Sjekk om alle linjer er slettet.
12248 * 02 YBC PTF12 VEKT, KOLLI ER UTVIDET.
********************************************************************
     H DFTACTGRP(*NO)
     fsadh      if   e           k disk
N01  fsadv      if   e           k disk
     fsaeh      if   e           k disk
S02  f*saev      if   e           k disk
N02  fsaev      if   e           k disk    PREFIX(E_)
     Ç*  Prototype for syim03ra
     d syim03ra        pr
     d uavd_                               like(uavd)
     d uopd_                               like(uopd)
     d unt_                                like(unt)
     d uvkt_                               like(uvkt)
     d utll_                               like(utll)
     d utle_                               like(utle)
     d udtg_                               like(udtg)
     d uflag_                              like(uflag)
     Ç*  *ENTRY Interface for Main Procedure
     d syim03ra        pi
     d uavd                           4  0
     d uopd                           7  0
S02  d*unt                            7  0
N02  d unt                           10  0
S02  d*uvkt                           9  0
N02  d uvkt                          11  0
     d utll                          10
     d utle                           6
     d udtg                          10
     d uflag                          1  0
N01   *
N01  d xsltalle        s              1
     Ç*_____
     Ç* Init
     Ç*"""""
      /free
       exsr srinit;

       chain (uavd: sitdn) sadh;
       if %found;
N01      xsltalle  = 'J';
N01      setll (uavd: sitdn) sadv;
N01      reade (uavd: sitdn) sadv;
N01      dow not %eof;
N01        if svpre∆ <> 'S' and svpre∆ <> 'D';
N01          xsltalle  = 'N';
N01          leave;
N01        endif;
N01        reade (uavd: sitdn) sadv;
N01      enddo;
N01      if xsltalle = 'J';
N01        unt    = 0;
N01        uvkt   = 0;
N01      else;
           unt    = sintk;
           uvkt   = sivkb;
N01      endif;
           utll   = sitll;
           utle   = sitle;
           udtg   = sidtg;
           uflag  = 1;
       else;
           chain (uavd: sitdn) saeh;
           if %found;
N01          xsltalle  = 'J';
N01          setll (uavd: sitdn) saev;
N01          reade (uavd: sitdn) saev;
N01          dow not %eof;
S02          //if svnyl <> 'S' and svnyl <> 'D';
N02            if E_svnyl <> 'S' and E_svnyl <> 'D';
N01              xsltalle  = 'N';
N01              leave;
N01            endif;
N01            reade (uavd: sitdn) saev;
N01          enddo;
N01          if xsltalle = 'J';
N01            unt    = 0;
N01            uvkt   = 0;
N01          else;
               unt    = sentk;
               uvkt   = sevkb;
N01          endif;
               utll   = setll;
               utle   = setle;
               udtg   = sedtg;
               uflag  = 1;
           endif;
       endif;

       *in33 = *on;
       return;

       //___________________________________________________
       // Initiering                                  SRINIT
       //"""""""""""""""""""""""""""""""""""""""""""""""""""
       begsr srinit;

           sitdn = 0 - uopd;
           uflag  = 0;

       endsr;

      /end-free
