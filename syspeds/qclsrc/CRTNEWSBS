             MONMSG     MSGID(CPF0000)
             ADDJOBQE   SBSD(&LIB/&DELSYS) JOBQ(&LIB/SY400QER) +
                          MAXACT(1) SEQNBR(50)
             MONMSG     MSGID(CPF0000)
             ADDJOBQE   SBSD(&LIB/&DELSYS) JOBQ(&LIB/SYJOBQFAX) +
                          MAXACT(2) SEQNBR(60)
             MONMSG     MSGID(CPF0000)
             ADDJOBQE   SBSD(&LIB/&DELSYS) JOBQ(&LIB/SYJOBQARK) +
                          MAXACT(1) SEQNBR(70)
             MONMSG     MSGID(CPF0000)

/* JOBBESKRIVELSE */
             CRTJOBD    JOBD(&LIB/SY400JOBD) JOBQ(&LIB/SY400JOBQ) +
                          TEXT('SYSPED Generel JOBD')
             MONMSG     MSGID(CPF0000)
             ?          CHGJOBD JOBD(&LIB/SY400JOBD)
             CRTJOBD    JOBD(&LIB/SY400AUTJD) JOBQ(&LIB/SY400JOBQ) +
