     H DFTACTGRP(*NO)
     FMAILSP    UF   E           K DISK
     FOMDI      UF   E           K DISK
     FPALLD     UF   E           K DISK
     FRUCKP     UF   E           K DISK
     FRUCKPU    UF   E           K DISK
     FSTSCLI4   UF   E           K DISK
     FDOKUFL    UF   E           K DISK
     FSTSFBR    UF   E           K DISK
     FSTSOPD1   UF   E           K DISK
     FSTSTUR    UF   E           K DISK
     FUTLLOGF   UF   E           K DISK
     FVARELI    UF   E           K DISK
      *

     d                 ds
     d  scclid                 1     20A
     d  scclid_2               1      2A
     d  scclid_18              3     18A

     d xavd            S              4  0
     d xopd            S              7  0
     d xpro            S              8  0
     D xclid           S             20A
      *  Prototype for syreo12r4
     d syreo12r4       pr
     d uavd_                               like(uavd)
     d uopd_                               like(uopd)
     d upro_                               like(upro)
      *  *ENTRY Interface for Main Procedure
     d syreo12r4       pi
     d uavd                           4
     d uopd                           7
     d upro                           7
      *
      /free

         exsr init;

         exec sql
         delete from omdi
         where omavd = :xavd and omopd = :xopd
         with NC;

         exec sql
         delete from palld
         where paavd = :xavd and paopd = :xopd
         with NC;

         exec sql
         delete from ruckpu
         where ruavd = :xavd and ruopd = :xopd
         with NC;

         exec sql
         delete from ruckp
         where ruavd = :xavd and ruopd = :xopd
         with NC;

         chain (xavd: xopd) stsopd1;
         if %found;
           setll (xavd: xopd) stscli4;
           reade (xavd: xopd) stscli4;
           dow not %eof;
             if scclid <> *blanks;
               exec sql
               delete from dokufl
               where fnclid = :scclid
               with NC;
               if scclid = '00';
                 xclid = scclid_18;
                 exec sql
                 delete from dokufl
                 where fnclid = :xclid
                 with NC;
               endif;
             endif;
             delete stsclir;
             reade (xavd: xopd) stscli4;
           enddo;

           exec sql
           delete from stsfbr
           where sfavd = :xavd and sfopd = :xopd
           with NC;

           if xpro > 0;
             exec sql
             delete from ststur
             where stpro = :xpro
             with NC;
           endif;

           exec sql
           delete from trackk
           where tkavd = :xavd and tkopd = :xopd
           with NC;

           delete stsopdr;
         endif;

         exec sql
         delete from utllogf
         where ulavd = :xavd and ulopd = :xopd
         with NC;

         exec sql
         delete from vareli
         where vlavd = :xavd and vlopd = :xopd
         with NC;

         *inlr = *on;
         return;

      //***********************************************
       begsr init;
      //***********************************************
       xavd = %float(uavd);
       xopd = %float(uopd);
       xpro = %float(upro);

       endsr;

      /end-free
