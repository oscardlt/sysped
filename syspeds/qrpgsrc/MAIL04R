     C*
     C     SLUTT         TAG
     C                   SETON                                        LR
     C                   RETURN
     C*
     C***********************************************
     C     HENT          BEGSR
     C***********************************************
     C                   Z-ADD     0             LINNBR
     C                   SETOFF                                       919299
     C     *IN98         DOWEQ     '0'
     C                   READC     SUBFIL                                 98
     C   98              DO
     C   99              SETOFF                                       98
     C                   Z-ADD     1             LINNBR
     C                   GOTO      SLHENT
     C                   END
     C     ZRECNO        DIV       14            LINNBR
     C                   MVR                     XLIN              2 0
     C     XLIN          IFEQ      0
     C                   Z-ADD     14            LINNBR
     C                   ELSE
     C                   Z-ADD     XLIN          LINNBR
     C                   END
      *
      * 8=VEDLIKEHOLD EPOSTMOTTAGERE TIL DOKUMENT
      *
     C     WSNR          IFEQ      '8'                                          ENDRE
     C                   MOVE      ' '           WSNR
     C                   EXSR      EPOMOT
     C                   END
      *
      * 1=prukk
      *
     C     WSNR          IFEQ      '1'                                          ENDRE
     c                   move      artxt         udok11           11
     c                   movel     '*'           udok11
     C                   MOVEL     udok11        udok
     C                   seton                                        lr
     c                   return
     C                   END
      *
      * ENDRE
      *
     C     WSNR          IFEQ      '2'                                          ENDRE
     C                   MOVE      ' '           WSNR
     C                   EXSR      CHGSF
     C   12              GOTO      SLHENT
     C                   END
      *
      * SLETTE
      *
     C     WSNR          IFEQ      '4'                                          SLETTE
     C                   MOVE      ' '           WSNR
     C                   EXSR      DLTSF
     C   12              GOTO      SLHENT
     C                   END
     C                   END
     C     SLHENT        TAG
     C                   ADD       5             LINNBR
     C                   Z-ADD     2             POSNBR
     C*
     C                   ENDSR
     C*
     C*
     C***********************************************
     C     EPOMOT        BEGSR
     C***********************************************
     C                   MOVE      ARTXT         U11              11
     C                   MOVEL     '*'           U11
     C                   MOVEL     U11           UDOC             30
     C                   CALL      'VCUNDC3R'
     C                   PARM                    FIFIRM
     C                   PARM                    UDOC
     C                   PARM                    USOK80           10 0
     C                   PARM                    EPOFAX            1
     C*
     C                   UPDATE    SUBFIL
     C                   ENDSR
     C***********************************************
     C     INIT          BEGSR
     C***********************************************
     c     *entry        plist
