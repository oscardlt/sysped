 /*           SENDE FAKTURA PÅ FAX    */
             PGM        PARM(&AVNR &KODE)
             DCL        VAR(&AVNR) TYPE(*DEC) LEN(9 0)
             DCL        VAR(&KODE) TYPE(*CHAR) LEN(1)
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             OVRDBF     FILE(AVFRH) TOFILE(AVFRHN)
             OVRDBF     FILE(AVFRHL01) TOFILE(AVFRHN01)
             OVRDBF     FILE(AVFRD) TOFILE(AVFRDN)
             OVRDBF     FILE(AVFRDL01) TOFILE(AVFRDN01)
             OVRDBF     FILE(AVFRDL02) TOFILE(AVFRDN02)
             OVRDBF     FILE(AVFRDL02) TOFILE(AVFRDN03)
             CALL       PGM(SYPO12) PARM(&AVNR &KODE)
             DLTOVR     FILE(*ALL)
    /* */
 SLUTT:      ENDPGM
