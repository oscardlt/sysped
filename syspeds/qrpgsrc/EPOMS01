          +'window.open('+Ap+'/sycgip/EPOMS03.pgm?user=/%USER%/'
          +'&UsrSpcName=/%UsrSpcName%/&WSKN=/%WSKN%/&ADDRMV=ADD'
          +'&POKUND=/%EHKUND%/&POBES=/%EHBES%/'+Ap
          +','+Ap+'TstSubWindow'+Ap+','+Ap+'width=350,height=200,'
          +'left=450,top=200,'+Ap+'scrollbars,resizable'+Ap+')">'
          +'<img src="/syspeddev/Checked2.jpg" width=13 height=13 border=0'
          +' alt="Pick to shp order"></A></td>';
N01     ColDta02=' <td>'+%trim(EHBES)+' </td>                             ';
