N03   * SKJEKKER OM AVTALTYPE
N03  C  N33AFTYPE        IFEQ      'M'
N03  C     HEM3          ANDEQ     0
N03  C     AFTYPE        OREQ      'L'
N03  C     HELM          ANDEQ     0
N03  C     AFTYPE        OREQ      'V'
N03  C     TRVERB        ANDEQ     0
N03  C                   GOTO      STFAVT
N03  C                   END
n10   * UTGÅTT AVTALE
N10  C  N33AFGJDT        IFNE      0
n10  C     HEDTOP        ANDGT     AFGJDT
n10  C                   eval      umsg=har
N10  C                   GOTO      stfavt
n10  C                   END
n10   * fra dato
N10  C  N33AFAKDT        IFNE      0
n10  C     HEDTOP        ANDLT     AFAKDT
n10  C                   eval      umsg=har
N10  C                   GOTO      stfavt
n10  C                   END
      *
      * SKJEKKER OM AVTALEN TILFREDSTILLER KRITERIER
      *
     C  N33HESDFF        IFGE      AFPNF1
     C     HESDFF        ANDLE     AFPNF2
     C     HESDVT        ANDGE     AFPNT1
     C     HESDVT        ANDLE     AFPNT2
S09  c*    afbfra        ifeq      ' '
N09  c     afbfra        ifeq      'T'
n08  c     aftype        ifeq      'B'
n08  c     aftype        oreq      'M'
n08  c     aftype        oreq      'L'
n08  c     aftype        oreq      'X'
N09  C                   EXSR      FINNVK
n08  c                   exsr      setfbv
n08  C     AVFRDKSH      SETLL     AVFRD
n08  C     AVFDXKSH      READE     AVFRD                                  33
n08  c   33              goto      stfavt
n08  c                   end
n08  c                   end
     C                   GOTO      SLFAVT
     C                   ELSE
     C                   GOTO      STFAVT
     C                   END
