             PGM        PARM(&TUR)
             DCL        VAR(&POST) TYPE(*CHAR) LEN(1)
             DCL        VAR(&TUR) TYPE(*CHAR) LEN(11)
             DCL        VAR(&FILE) TYPE(*CHAR) LEN(10) VALUE('HLBFILT')
             DCL        VAR(&MBR) TYPE(*CHAR) LEN(10) VALUE('HLBFILT')
             CRTDUPOBJ  OBJ(HLBFILT) FROMLIB(*LIBL) OBJTYPE(*FILE) +
                          TOLIB(QTEMP)
             MONMSG     MSGID(CPF0000)
             CLRPFM     FILE(QTEMP/HLBFILT)
             CALL       PGM(GSS082RT) PARM(&TUR &POST)
             RUNQRY     QRY(QGSS082T) QRYFILE((QTEMP/HLBFILT))
             IF         COND(&POST = 'J') THEN(DO)
             CALL       PGM(SYGE55CL) PARM(&FILE &MBR)
             ENDDO
             ENDPGM
