             PGM
             DCL        VAR(&WS) TYPE(*CHAR) LEN(10)
             DCL        VAR(&USR) TYPE(*CHAR) LEN(10)
             DCL        VAR(&NBR) TYPE(*CHAR) LEN(10)
             DCL        VAR(&MSGTXT) TYPE(*CHAR) LEN(70)
             DCL        VAR(&IN03) TYPE(*LGL) LEN(1)
             DCL        VAR(&FAP) TYPE(*CHAR) LEN(12)
             DCL        VAR(&ØAP) TYPE(*CHAR) LEN(12)
             DCL        VAR(&MAØ) TYPE(*CHAR) LEN(12)
             DCLF       FILE(SYSPED/FILERFM) RCDFMT(BILDE01 BILDE02)
             /**/
             MONMSG     MSGID(CPF1907) EXEC(GOTO CMDLBL(EXIT))
             MONMSG     MSGID(CPF6801) EXEC(GOTO CMDLBL(EXIT))
             MONMSG     MSGID(CPF0000)
             /**/
             CHGCURLIB  CURLIB(SYSPEDF)
             RTVJOBA    JOB(&WS) USER(&USR) NBR(&NBR)
 START:      SNDRCVF    DEV(&WS) RCDFMT(BILDE01)
             IF         COND(&IN03 = '1') THEN(GOTO CMDLBL(EXIT))
             CHGVAR     VAR(&AUTO) VALUE(&AUTO)
             IF         COND(&AUTO = 'A') THEN(GOTO CMDLBL(BILDE02))
             IF         COND(&AUTO = 'a') THEN(GOTO CMDLBL(BILDE02))
             IF         COND(&AUTO = 'M') THEN(GOTO CMDLBL(MANUELL))
             IF         COND(&AUTO = 'm') THEN(GOTO CMDLBL(MANUELL))
             ELSE       CMD(GOTO CMDLBL(MSG1))
 BILDE02:    DO
             SNDRCVF    DEV(&WS) RCDFMT(BILDE02) WAIT(*YES)

             IF         COND(&IN03 = '1') THEN(GOTO CMDLBL(EXIT))
             GOTO AUTO
             ENDDO
