     C                   eval      Error = *blanks
      *
     c     HeadfKey      chain     HEADF                              33
     C   33              eval      Error = 'Unknown order'
      * ant/vekt
OX   c                   if        FVANT =0 or FVVKT=0
OX   C                   eval      Error = 'No of pieces/collies and weight '
OX   c                             +'is mandatory'
OX   c                   endif
      *
OX    * beregning av LM ut fra Formel er ren Bring / olgeexpress-spesial
OX   c                   if        HEOT='OX' or HEOT='LO' or HEOT='OB'
OX   c                   if        FVLEN=0 or FVBRD=0 or FVHOY=0
OX   C                   eval      Error = 'L x B x H is mandatory on '
OX   c                             +'ordertype '+HEOT
OX   c                   else
OX    * Hovedreget : LM=LenxBe/2,4    MEN  - div unntak
OX   c                   if        FVLM =0
OX   C                   Z-ADD     0             XWORK            15 4
OX   c                   eval(h)   XWORK = FVLEN*FVBRD/2.4/10000                Hoved = Len*Bre/2.4
OX   c                   eval(h)   FVLM  = XWORK                                Hoved = Len*Bre/2.4
OX   c                   if        FVBRD > 125
OX   c                   eval(h)   FVLM =FVLEN/100                              Unnt 1= Len
OX   c                   if        FVLEN <= 125
OX   c                   eval(h)   FVLM =FVBRD/2/100                            Unnt 2= Bre/2
OX   c                   endif
OX   c                   else
OX   c                   if        FVBRD <= 120 and FVLEN > 130
OX   c                   eval(h)   FVLM=FVLEN/2/100                             Unnt 3= Len/2
OX   c                   endif
OX   c                   endif
OX   c     FVLM          ifne      0
OX   c                   mult      FVANT         FVLM
OX   c                   end
OX   c                   endif
OX   c                   endif
OX   c                   endif
      *
     c     FVLM2         ifeq      0
     c                   eval      FVLM2 = FVLM
     c                   endif
