     C                   PARM                    ZKTH_USID
     C     *IN30         PARM                    WIN30
     C                   PARM                    WOBJTYP
     C                   IF        *IN30
     C                   EVAL      *IN30=*ON
     C                   EVAL      *IN33=*ON
     C                   MOVE      'DKT0007'     ZMSGNO
     C                   GOTO      SRBB1
     C                   ENDIF
      *______________
      * Oppdatere FIL
      *""""""""""""""
     C     ZKTH_SYSG     CHAIN     DKTHA                              53
     C                   EVAL      DKTH_NAMN=ZKTH_NAMN
     C                   EVAL      DKTH_USID=ZKTH_USID
     C                   UPDATE    DKTHAR

     C                   ENDIF

     C                   ENDSR
      *////////////////////////////////////////////////////////////
      *  Slette                                              SRBD /
      *////////////////////////////////////////////////////////////
     C     SRBD          BEGSR
