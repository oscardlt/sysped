             CMD        PROMPT('Recreate modules and programs')
             PARM       KWD(TGTRLS) TYPE(*CHAR) LEN(8) DFT(*CURRENT) +
                          PROMPT('Target release')
             PARM       KWD(SRCLIB) TYPE(*NAME) LEN(10) +
                          CONSTANT(SYSPEDS)
             PARM       KWD(OBJLIB) TYPE(*NAME) LEN(10) +
                          CONSTANT(SYSPED)
