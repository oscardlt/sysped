     FEDIRE1K   IF   E           K DISK
     FSVTX31F   UF A E             DISK
     FSVTX03F   O  A E             DISK

     D  A              C                   '<taxCode '
     D  B              C                   '<taxCodeDescription '
     D  C              C                   '</taxCode>'

     D  A02            C                   'SID="'
     D  A03            C                   'dateEnd="'
     D  A04            C                   'dateStart="'
     D  A05            C                   'taxCode="'

     D                 DS
     D  WDT10                  1     10
     D  WDT10Y                 1      4
     D  WDT10M                 6      7
     D  WDT10D                 9     10
     D                 DS
     D  WDT08                  1      8
     D  WDT08Y                 1      4
     D  WDT08M                 5      6
     D  WDT08D                 7      8

     C     *ENTRY        PLIST
     C                   PARM                    ULIB             10
     C                   PARM                    UFILE            10
     C                   PARM                    UMBR             10
     C

     C                   EXSR      SLETT

     C                   CALL      'SYGE15C'
     C                   PARM                    WDT               8
     C                   PARM                    WTM               6

     C     UMBR          SETLL     EDIRE1K
     C     UMBR          READE     EDIRE1K                                51
     C     *IN51         DOWEQ     *OFF

     C     A             SCAN      RDATA                                  41
     C     B             SCAN      RDATA                                  42
     C     C             SCAN      RDATA                                  43
     C
     C                   SELECT
     C                   WHEN      *IN41
     C                   CLEAR                   SVTX31R
     C                   EXSR      XML2
     C                   WHEN      *IN42
     C                   EXSR      XML3
     C
     C                   WHEN      *IN43
     C

     C                   IF        SVTX31_03=*BLANKS
     C                   EVAL      SVTX31_03='9999-12-31'
     C                   ENDIF
     C
     C                   EVAL      WDT10=SVTX31_03
     C                   EVAL      WDT08Y=WDT10Y
     C                   EVAL      WDT08M=WDT10M
     C                   EVAL      WDT08D=WDT10D

     C     WDT08         IFGE      WDT
     C                   WRITE     SVTX31R

     C                   CLEAR                   SVTX03R
     C                   EVAL      SVTX03_01='A'
     C                   EVAL      SVTX03_02='CHA'
     C                   EVAL      SVTX03_03=SVTX31_01
     C                   EVAL      SVTX03_04=SVTX31_02
     C                   EVAL      SVTX03_05=SVTX31_03
     C                   EVAL      SVTX03_06=SVTX31_04
     C                   WRITE     SVTX03R

     C                   ENDIF
     C                   ENDSL

     C     UMBR          READE     EDIRE1K                                51
     C                   ENDDO
     C                   EVAL      *INLR=*ON
      *______________________________________________________________
      * Les <measurementUnitQualifier                            XML2
      *""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""
     C     XML2          BEGSR

     C     A02           SCAN      RDATA         X                 3 0    20
     C                   IF        *IN20
     C                   ADD       5             X
     C     '"'           SCAN      RDATA :X      Y                 3 0    21
     C                   IF        *IN20
     C     Y             SUB       X             Z                 3 0
     C                   EVAL      SVTX31_05=%SUBST(RDATA :X:Z)
     C                   ENDIF
     C                   ENDIF

     C     A03           SCAN      RDATA         X                 3 0    20
     C                   IF        *IN20
     C                   ADD       9             X
     C     '"'           SCAN      RDATA :X      Y                 3 0    21
     C                   IF        *IN20
     C     Y             SUB       X             Z                 3 0
     C                   EVAL      SVTX31_03=%SUBST(RDATA :X:Z)
     C                   ENDIF
     C                   ENDIF

     C     A04           SCAN      RDATA         X                        20
     C                   IF        *IN20
     C                   ADD       11            X
     C     '"'           SCAN      RDATA :X      Y                        21
     C                   IF        *IN20
     C     Y             SUB       X             Z
     C                   EVAL      SVTX31_04=%SUBST(RDATA :X:Z)
     C                   ENDIF
     C                   ENDIF

     C     A05           SCAN      RDATA         X                        20
     C                   IF        *IN20
     C                   ADD       9             X
     C     '"'           SCAN      RDATA :X      Y                        21
     C                   IF        *IN20
     C     Y             SUB       X             Z
     C                   EVAL      SVTX31_01=%SUBST(RDATA :X:Z)
     C                   ENDIF
     C                   ENDIF

     C                   ENDSR
