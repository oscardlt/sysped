             PGM        PARM(&MRNNR &MEMBER)
             DCL        VAR(&MRNNR) TYPE(*CHAR) LEN(18)
             DCL        VAR(&MEMBER) TYPE(*CHAR) LEN(10) /* Member  */
             DCL        VAR(&USER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&MSGTXT) TYPE(*CHAR) LEN(256)
             DCL        VAR(&EPOST) TYPE(*CHAR) LEN(70)

             CHGVAR     VAR(&MSGTXT) VALUE('FEIL VED INNGÅENDE TET: +
                          MRN-NR=' *BCAT &MRNNR *BCAT ', +
                          FILEKOMPONENT=' *BCAT &MEMBER *BCAT +
                          '. KONTAKT SYSTEMA.')

             RTVDTAARA  DTAARA(EDIDTA (1 10)) RTNVAR(&USER)
             SNDMSG     MSG(&MSGTXT) TOUSR(&USER)
             MONMSG     MSGID(CPF0000)
             GOTO       CMDLBL(SLUTT)

             SNDDST     TYPE(*LMSG) TOINTNET((&EPOST)) DSTD('Feil +
                          ved TET-import') LONGMSG(&MSGTXT) +
                          SNDFMT(*NOCHG) DOCD('Feil ved TET-import')
             MONMSG     MSGID(CPF0000)

 SLUTT:      ENDPGM
