     C                   WRITE     GSSLGFR
     C                   END
     C                   EVAL      LGANY = *BLANKS
     C                   EVAL      LGAGM = *BLANKS
      *
     C                   IF        ORCLI <> W2CLI
     C                   EVAL      LGTXT = XXTXT03
     C                   CAT       ORLC:1        LGTXT
     C                   EVAL      LGNNY = W2CLI
     C                   EVAL      LGNGM = ORCLI
