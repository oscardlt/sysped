     C                   Z-ADD     AAVD          XVTEXT            4 0
     C*
     C     STCHG         TAG
     C                   EXFMT     B30
     C                   MOVE      '0'           *IN30
     C                   MOVEA     '00000'       *IN(61)
     C                   MOVE      '0'           *IN(67)
     C     *IN12         IFEQ      '1'
     C     *IN03         OREQ      '1'
     C                   MOVE      *BLANKS       SVKODE
     C                   MOVEL     AVKODE        SVKODE
     C                   Z-ADD     AVTEXT        SVTEXT
     C                   CLEAR                   AVKODE
     C                   CLEAR                   AVTEXT
     C                   UPDATE    H50
     C                   GOTO      CHGEND
     C                   END                                                    <*IN12
     C                   Z-ADD     AVTEXT        AAVD
     C                   UPDATE    AVDGRR
     C                   MOVE      *BLANKS       SVKODE
     C                   MOVEL     AVKODE        SVKODE
     C                   Z-ADD     AVTEXT        SVTEXT
     C     AVDKY1        CHAIN     KOAR                               90
     C     *IN90         IFEQ      '0'
     C                   MOVE      *BLANKS       BESKR
     C                   MOVEL     KOANVN        BESKR
     C                   ELSE
     C                   MOVE      *BLANKS       BESKR
     C                   END
     C                   UPDATE    H50
     C     *IN30         IFEQ      '1'
     C                   GOTO      STCHG
     C                   END                                                    <*IN30
     C*
     C     CHGEND        TAG
     C                   ENDSR
     C*
