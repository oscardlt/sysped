      ***********
      * KODTS8
      ***********
     C                   MOVE      AVGKOD        XS8AVG            2
     C     AVGKOD        IFEQ      'MV'
     c     hoyven        andne     ' '
     C                   MOVE      *BLANKS       XS8SKV
     C                   MOVE      AVGSKV        XXXSKV            1
     C                   MOVEL     XXXSKV        XS8SKV
     C                   ELSE
     C                   MOVE      AVGSKV        XS8SKV            3
     C                   END
     C                   SELECT                                                   3<-B
     C     ENHET         WHENEQ    'L'
     C                   MOVE      'L'           XS8STY
     C     ENHET         WHENEQ    'S'
     C                   MOVE      *BLANKS       XS8STY
     C     ENHET         WHENEQ    'B'
     C                   MOVE      'B'           XS8STY
     C     ENHET         WHENEQ    'A'
     C                   MOVE      'A'           XS8STY
     C     ENHET         WHENEQ    'M'
     C                   MOVE      *BLANKS       XS8STY            1
     C     ENHET         WHENEQ    'P'
     C                   MOVE      '%'           XS8STY
     C     ENHET         WHENEQ    'R'
     C                   MOVE      'P'           XS8STY
     C     ENHET         WHENEQ    'H'
     C                   MOVE      'H'           XS8STY
     C     ENHET         WHENEQ    'K'
     C                   MOVE      'K'           XS8STY
     C     ENHET         WHENEQ    'D'
     C                   MOVE      'D'           XS8STY
     C     ENHET         WHENEQ    'T'
     C                   MOVE      'T'           XS8STY
     C     ENHET         WHENEQ    'G'
     C                   MOVE      'G'           XS8STY
     C                   OTHER
     C                   MOVE      ENHET         XS8STY
     C                   ENDSL                                                    3<-E
     C                   MOVE      ENHET2        XS8ØRE            1
     c     enhet3        ifeq      '*'
     c                   move      enhet3        XS8ØRE
     c                   end
     C     XS8AVG        CHAIN     KODTS8                             33
     C   33              EVAL      GM_KS8FTX = *BLANKS
      *
      * HEADER/GRUNN-DEFINISJON MED BLANK I SEKVENS...
     C     KODS8H        CHAIN     KODTS8NY                           33
     C     *IN33         IFEQ      '1'                                            3<-B
     C                   MOVE      XS8AVG        KS8AVG
     C                   MOVE      *BLANKS       KS8SKV
     C                   MOVE      XS8STY        KS8STY
     C                   MOVE      *ZEROS        KS8SAT
S01  C*                  MOVE      XS8ØRE        KS8ØRE
     C                   MOVE      GM_KS8FTX     KS8FTX
N01  C                   MOVE      XS8ØRE        KS8ØRE
     C                   WRITE     KOS8R
     C                   END                                                      3<-E
      * AVGIFT/SEKVENS (MED SATS.. FRA FØRSTE FOREKOMST.)
     C     KODS8K        CHAIN     KODTS8NY                           33
     C     *IN33         IFEQ      '1'                                            3<-B
     C                   MOVE      XS8SKV        KS8SKV
     C                   MOVE      XS8STY        KS8STY
     C                   Z-ADD     SATS          KS8SAT
S01  C*                  MOVE      XS8ØRE        KS8ØRE
     C                   MOVE      GM_KS8FTX     KS8FTX
N01  C                   MOVE      XS8ØRE        KS8ØRE
     C                   WRITE     KOS8R
     C                   END                                                      3<-E
