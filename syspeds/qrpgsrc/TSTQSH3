     d  wcmd           s           2048
     d  wcmd1          s            256
     d  wcmd2          s            256
     d  wcmd3          s            256
     d  wcmd4          s            256
     d  wpgp           s            128
     d  wnopgp         s            128
     d  x7D            s              1
     d runcmd          pr                  extpgm('QCMDEXC')
     d  Cmd                         200a   const
     d  len                          15p 5 const

      /free

       x7D=x'7D';
       wpgp='stor.xml.gz.pgp';
       wnopgp='stor.xml.gz';
       wcmd1='cd /home/tktesting';
       wcmd2='gpg -o /home/tktesting/' + %trim(wnopgp) + ' --decrypt ' +
             ' /home/tktesting/' + %trim(wpgp);
       wcmd3='export -s CLASSPATH=/home/tktesting';
       wcmd4='java GZipFile' + ' ' +  %trim(wnopgp);

       wcmd = 'QSH CMD(' + x7D +
              wcmd1 + ';' +
              ' '   + wcmd2 + ';' +
              ' '   + wcmd3 + ';' +
              ' '   + wcmd4 + ';' +
              x7D + ')';

       RunCmd(Wcmd :%len(%trim(Wcmd)));

       *inlr=*on;
      /end-free
