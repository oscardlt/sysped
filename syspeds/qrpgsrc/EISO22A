     FWRKCGISS  IF   E           K DISK
     D                 DS
     D  WKEY2                  1    256
     D  XXMOD                  1      1
     D  XXLK                   2      3
     D  XXUNIK                 1      3
     D                 DS
     D  XXKEY                  1    256
     D  X1MOD                  1      1
     D  X1LK                   2      3
     D  X1DT                   4     11  0
     D  X1BILN                12     20
     D  X2BILN                 2     10
     D  X2DT                  11     18  0
     D  X2LK                  19     20
     D  X2MOD                 21     21
     D                 DS
     D  WDATA                  1   1024
     D  WXPRO                 18     25  0
      *
     C                   EXSR      INIT
      *
     C     WRKCGISSK     SETLL     WRKCGISS
     C     WRKCGISSK     READE     WRKCGISS                               33
     C  N33              IF        UALL = 'J'
     C     XXMOD         COMP      '*'                                3333
     C                   ELSE
     C     XXUNIK        COMP      XXUNIK                             3333
     C                   END
     C                   DOW       *IN33 = *OFF
     C                   IF        WXPRO = UPRO
     C                   EVAL      UJN = 'J'
     C                   LEAVE
     C                   END
     C     WRKCGISSK     READE     WRKCGISS                               33
     C  N33              IF        UALL = 'J'
     C     XXMOD         COMP      '*'                                3333
     C                   ELSE
     C     XXUNIK        COMP      XXUNIK                             3333
     C                   END
     C                   ENDDO
      *
     C                   EVAL      *INLR = *ON
     C                   RETURN
      *
      *=====================================================================******
      *  INITIER
      *=====================================================================******
     C     INIT          BEGSR
     C     WRKCGISSK     klist
     C                   kfld                    UINTEG
     C                   kfld                    XXKEY
      *
     C     *ENTRY        PLIST
     C                   PARM                    UINTEG            6 0
     C                   PARM                    UPRO              8 0
     C                   PARM                    UALL              1
     C                   PARM                    UMOD              1
     C                   PARM                    ULK               2
     C                   PARM                    UDT               8 0
     C                   PARM                    UBILN             9
     C                   PARM                    UJN               1
      *
     C                   EVAL      UJN = 'N'
     C                   IF        UALL = 'J'
     C                   EVAL      X2MOD  = '*'
     C                   EVAL      X2BILN = UBILN
     C                   EVAL      X2DT   = UDT - 1
     C                   EVAL      X2LK   = ULK
     C                   EVAL      X2MOD  = UMOD
     C                   ELSE
     C                   EVAL      X1MOD  = UMOD
     C                   EVAL      X1LK   = ULK
     C                   EVAL      X1DT   = UDT - 1
     C                   EVAL      X1BILN = UBILN
     C                   END
      *                                               G
     C                   ENDSR
      *
