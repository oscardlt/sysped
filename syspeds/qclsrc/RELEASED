/* - Variant der alle CGIDEV2 er gjort om til SYSPED                    */
/*============================================================================*/
/*                                                                            */
/* CRTPGM SYSPED/RELEASED MODULE(*LIBL/RELEASED)                              */
/*        ACTGRP(*CALLER) AUT(*USE)                                           */
/*                                                                            */
/*============================================================================*/
             PGM
             DCL        VAR(&RLSSTAMP) TYPE(*CHAR) LEN(12)
             DCL        VAR(&DATE) TYPE(*CHAR) LEN(6)
             DCL        VAR(&TIME) TYPE(*CHAR) LEN(6)
             DCL        VAR(&MTHNBR) TYPE(*CHAR) LEN(2)
             DCL        VAR(&MONTH) TYPE(*CHAR) LEN(30)
             DCL        VAR(&DAY) TYPE(*CHAR) LEN(2)
             DCL        VAR(&YEAR) TYPE(*CHAR) LEN(4)
             DCLF       FILE(SYSPED/RELEASED)
              /*========*/
             RTVDTAARA  DTAARA(SYSPED/RELEASE (1 12)) +
                          RTNVAR(&RLSSTAMP)
             CHGVAR     VAR(&DATE) VALUE(%SST(&RLSSTAMP 1 6))
             CHGVAR     VAR(&MTHNBR) VALUE(%SST(&DATE 3 2))
             CHGVAR     VAR(&YEAR) VALUE('20' *CAT %SST(&DATE 1 2))
             CHGVAR     VAR(&DAY) VALUE(%SST(&DATE 5 2))
             IF         COND(&MTHNBR = '01') THEN(CHGVAR VAR(&MONTH) +
                          VALUE('January'))
             IF         COND(&MTHNBR = '02') THEN(CHGVAR VAR(&MONTH) +
                          VALUE('February'))
             IF         COND(&MTHNBR = '03') THEN(CHGVAR VAR(&MONTH) +
                          VALUE('March'))
             IF         COND(&MTHNBR = '04') THEN(CHGVAR VAR(&MONTH) +
                          VALUE('April'))
             IF         COND(&MTHNBR = '05') THEN(CHGVAR VAR(&MONTH) +
                          VALUE('May'))
             IF         COND(&MTHNBR = '06') THEN(CHGVAR VAR(&MONTH) +
                          VALUE('June'))
             IF         COND(&MTHNBR = '07') THEN(CHGVAR VAR(&MONTH) +
                          VALUE('July'))
             IF         COND(&MTHNBR = '08') THEN(CHGVAR VAR(&MONTH) +
                          VALUE('August'))
             IF         COND(&MTHNBR = '09') THEN(CHGVAR VAR(&MONTH) +
                          VALUE('September'))
             IF         COND(&MTHNBR = '10') THEN(CHGVAR VAR(&MONTH) +
                          VALUE('October'))
             IF         COND(&MTHNBR = '11') THEN(CHGVAR VAR(&MONTH) +
                          VALUE('November'))
             IF         COND(&MTHNBR = '12') THEN(CHGVAR VAR(&MONTH) +
                          VALUE('December'))
             CHGVAR     VAR(&TIME) VALUE(%SST(&RLSSTAMP 7 6))
             OVRDBF     FILE(RELEASED) TOFILE(SYSPED/RELEASED) +
                          SECURE(*YES) OVRSCOPE(*JOB) OPNSCOPE(*JOB)
             CHGVAR     VAR(&RLSDAT) VALUE(&MONTH *BCAT &DAY *TCAT +
                          ',' *BCAT &YEAR)
             CHGVAR     VAR(&RLSTME) VALUE(&TIME)
             SNDRCVF    RCDFMT(RCD)
             DLTOVR     FILE(RELEASED) LVL(*JOB)
             RETURN
             ENDPGM
