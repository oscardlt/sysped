     C                   EXSR      INIT
      *
     C     START         TAG
     C                   EXFMT     TR015D01
     C                   SETOFF                                       999830
      *
     C   03              GOTO      SLUTT
     C     TRHK          CHAIN     TRUH                               99
S02  C* N99              IF        ((UIE = 'I') AND
S02  C*                            ((THDK <> 'SS') OR (THBLK <> 'NO')))
N02  C  N99              IF        UIE = 'I' AND
N02  C                             THDK<>'SS' AND THDK<>'ENTRY' AND THDK<>'EXIT'
N02  C                             OR UIE='I' AND THDK='SS' AND THBLK<>'NO'
N02  C                             OR UIE='I' AND THDK='ENTRY' AND THBLK<>'NO'
N02  C                             OR UIE='I' AND THDK='EXIT' AND THALK<>'NO'
N01  C                   UPDATE    TRUHR
N01  C                   EVAL      *IN99 = *ON
N01  C                   END
     C   99              DO
     C                   MOVE      'SPT0035'     MSGNR
     C                   SETON                                        30
     C                   GOTO      START
     C                   END
     C     KODSIK        CHAIN     KODTSI                             98
     C   98              DO
     C                   UPDATE    TRUHR
     C                   MOVE      'SPS0001'     MSGNR
     C                   SETON                                        30
     C                   GOTO      START
     C                   END
