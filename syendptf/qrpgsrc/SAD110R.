     C***********************************************
     C     NYTUR         BEGSR
     C***********************************************
     C     W2AVD         IFEQ      0
     C                   MOVE      UAVD          W2AVD
     C                   END
     C     SADKML01K1    CHAIN     SADKML01                           98
     C   98              DO
     C                   MOVE      'YBC2035'     MSGNR
     C                   GOTO      SLTUR
     C                   END
     C                   MOVE      '1'           *IN90
      *
     C                   SELECT
     C     W2NR          WHENEQ    2
      * kan ikke gå inn på normale oppdrag fra dummy
     C                   IF        KMST <> ' ' AND KMST <> 'C' AND KMST <> 'M'
     C                   MOVE      '1'           *IN30
     C                   MOVE      'YBC2036'     MSGNR
     C                   GOTO      SLTUR
     C                   END
     C                   EXSR      CHGTUR
     C     W2NR          WHENEQ    3
     C                   EXSR      CPYTUR
     C     W2NR          WHENEQ    5
     C                   EXSR      DSPTUR
     C     W2NR          WHENEQ    9
     C                   EXSR      DSPLOG
     C                   ENDSL
      *
     C     SLTUR         TAG
     C   98              MOVE      '1'           *IN30
     C                   MOVE      '0'           *IN90
     C                   MOVE      *ZEROS        W2NR
     C  N98              MOVE      *ZEROS        W2AVD
     C  N98              MOVE      *ZEROS        W2PRO
      *
     C                   ENDSR
      *
     C***********************************************
     C     HENT          BEGSR
     C***********************************************
     C                   MOVE      *BLANKS       MSGNR
     C                   MOVEA     '000'         *IN(90)
     C                   MOVEA     '00'          *IN(98)
     C     *IN98         DOWEQ     '0'                                          1<-B
     C                   READC     SUBFIL                                 98
     C     *IN98         IFEQ      '0'                                           2<-B
     C                   SELECT                                                   3<-B
     C     WSNR          WHENEQ    2                                              3
     C                   MOVE      '1'           *IN99
     C                   IF        KMST <> ' ' AND KMST <> 'C' AND KMST <> 'M'
     C                   MOVE      '1'           *IN30
     C                   MOVE      'YBC2036'     MSGNR
     C                   MOVE      *ZEROS        WSNR
     C                   UPDATE    SUBFIL
     C                   GOTO      SLHENT
     C                   END
     C                   EXSR      CHGTUR
     C     WSNR          WHENEQ    3                                              3
     C                   MOVE      '1'           *IN99
     C                   EXSR      CPYTUR
     C     WSNR          WHENEQ    5                                              3
     C                   MOVE      '1'           *IN99
     C                   EXSR      DSPTUR
     C     WSNR          WHENEQ    9                                              3
     C                   MOVE      '1'           *IN99
     C                   EXSR      DSPLOG
     C     WSNR          WHENEQ    90                                             3
     C                   IF        KMST <> ' ' AND KMST <> 'C' AND KMST <> 'M'
     C                   MOVE      '1'           *IN30
     C                   MOVE      'YBC2036'     MSGNR
     C                   GOTO      SLHENT
     C                   END
     C                   MOVE      '1'           *IN99
     C                   EXSR      SNDTUR
     C                   ENDSL                                                    3<-E
     C                   END                                                     2<-E
     C                   ENDDO                                                  1<-E
      *
     C     SLHENT        TAG
     C   98
     CANN99              DO
     C                   MOVE      'DIV0012'     MSGNR
     C                   MOVE      '1'           *IN30
     C                   END
     C                   MOVE      '0'           *IN99
      *
     C                   ENDSR
