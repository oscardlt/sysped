             PGM        PARM(&TYPE &MN &AVD &OPD &STMF &BANE &VISE &INFOP)
             DCL        VAR(&TYPE) TYPE(*CHAR) LEN(4)
             DCL        VAR(&MN) TYPE(*DEC) LEN(9 0)
             DCL        VAR(&FILEN) TYPE(*CHAR) LEN(10) VALUE('TVINMEAR')
             DCL        VAR(&STMF) TYPE(*CHAR) LEN(152)
             DCL        VAR(&VISE) TYPE(*CHAR) LEN(1)
             DCL        VAR(&INFOP) TYPE(*CHAR) LEN(1)
             DCL        VAR(&BANE) TYPE(*CHAR) LEN(50)
             DCL        VAR(&QUSER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QJOB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QNBR) TYPE(*CHAR) LEN(6)
             DCL        VAR(&AVD) TYPE(*DEC) LEN(4 0)
             DCL        VAR(&OPD) TYPE(*DEC) LEN(7 0)
    /* */
    /* */
             RTVJOBA    JOB(&QJOB) USER(&QUSER) NBR(&QNBR)
    /* */
    /*                                    */
    /*                                    */
    /* START FOR PDF DOKUMENTER           */
    /*                                    */
    /* */
             IF         COND(&INFOP = 'D') THEN(DO)
             OVRPRTF    FILE(&FILEN) DEVTYPE(*AFPDS) PAGESIZE(70 80) +
                          CPI(10) OUTQ(OUTMAIL) FORMTYPE(*STD) +
                          TOSTMF(&BANE) WSCST(*PDF)
             ENDDO
             IF         COND(&INFOP *NE 'D') THEN(DO)
             OVRPRTF    FILE(&FILEN) DEVTYPE(*AFPDS) PAGESIZE(70 80) +
                          CPI(10) OUTQ(OUTMAIL) FORMTYPE(*STD)
             ENDDO
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
/*                                                              */
/*    KALLER PROGRAM FOR PRINT DOKUMENTKONTROLL                */
/*                                                              */
             CALL       PGM(TVI28AR) PARM(&TYPE &MN &AVD &OPD)
/*                                                              */
             IF         COND(&INFOP = 'D') THEN(RETURN)
    /* */
             ADDLIBLE   LIB(MMAIL)
             MONMSG     MSGID(CPF0000)
/*                                       */
/* LAGER PDF OG LEGGER I MAPPE          */
/* UTEN INFOPRINT                        */
/*                                       */
             IF         COND(&INFOP = 'N') THEN(DO)
             CVTSPLFPDF SPLF(&FILEN) JOB(&QNBR/&QUSER/&QJOB) +
                          TOPDF(&BANE) ORIENTAT(*V)
                          ENDDO
/*                                       */
/* LAGER PDF OG LEGGER I MAPPE           */
/* MED  INFOPRINT                        */
/*                                       */
             IF         COND(&INFOP = 'J') THEN(DO)
             CALL       PGM(INF001CL) PARM(&FILEN &QNBR &QUSER &QJOB +
                          &BANE)
                          ENDDO
             RMVLIBLE   LIB(MMAIL)
    /* */

 SLUTT:      ENDPGM
