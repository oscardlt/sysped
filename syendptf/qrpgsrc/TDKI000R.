      *********************************************************
     C     INIT          Begsr
      *********************************************************
     C                   OPEN      BRIDF
      * Test innlogging
     C     WSUSER        CHAIN     BRIDF                              50
     C     BILIBL        ifeq      *blanks
     C                   callb     'INCGI'
     C                   parm                    BISTDL
     C                   else
     C                   call      BILIBL
     C                   end

     C     FRIKEY        klist
     C                   kfld                    DKIH_SYAV
     C                   kfld                    DKIH_SYOP
     C                   kfld                    WFSKODE           3
     C                   kfld                    XREF
     C     FRIKEY2       klist
     C                   kfld                    DKIH_SYAV
     C                   kfld                    DKIH_SYOP
     C                   kfld                    WFSKODE
     C     DKIHKEY       klist
     C                   kfld                    DKIV_SYAV
     C                   kfld                    DKIV_SYOP
     C                   EVAL      WFSKODE='*CR'

     C     *LIKE         DEFINE    DKIH_SYDT     WSETGT
     C                   EVAL      WSETGT = 20991231

     C   50              eval      Error = 'Invalid userID'
     C  N50              OPEN      DKIH
     C  N50              OPEN      DKIH10
     C  N50              OPEN      FRISOK
     C  N50              OPEN      DKIV
N01  C  N50              OPEN      BRPARF
N01   * Er dato= blank eller 0, sett  da dato 14 dager tilbake
N01  C  N50              IF        OPDN = *ZEROS
N01  C                   IF        REFNR = *BLANKS
N01  C                   IF        FAKT  = *BLANKS
N01  C                   IF        DATUMN = *ZEROS
N01
N01   /free
N01
N01        wpabrid=wsuser;
N01        wpakunr=0;
N01        wpasort=0;
N01        chain (WPABRID: WPAKUNR: 'DFTDG': WPASORT) BRPARF;
N01        if not %found or pavrdn = 0;
N01           pavrdn=7;
N01        endif;
N01
N01   /end-free
N01
N01  C                   CALL      'SYGE20'
N01  C                   PARM      'SUB'         UFUNK
N01  C                   PARM      DATUMN        UDAT1
N01  C                   PARM                    UDAT2
N01  C                   PARM      PAVRDN        UDGR
N01  C                   EVAL      DATUMN=UDAT2
N01  C                   ENDIF
N01  C                   ENDIF
N01  C                   ENDIF
N01  C                   ENDIF

     c                   endsr
      *********************************************************
     C     LESVARE       BEGSR
      *********************************************************
     C     *LIKE         DEFINE    DKIV_SYOP     WSYOP
     C                   EVAL      WSYOP=*ZEROS
     C                   READ      DKIV                                   56
     C     *IN56         DOWEQ     '0'

     C     DKIV_SYOP     CABEQ     WSYOP         HOPP2

     C     DKIHKEY       CHAIN     DKIH                               57
     C                   IF        *IN57=*OFF
     C                   IF        AVDN > 0
     C     DKIH_SYAV     CABNE     AVDN          HOPP2
     C                   ENDIF
     C                   IF        SIGN <> *BLANKS
     C     DKIH_SYSG     CABNE     SIGN          HOPP2
     C                   ENDIF
     C                   IF        OPDN > 0
     C     DKIH_SYOP     CABNE     OPDN          HOPP2
     C                   ENDIF
     C                   IF        REFNR <> *BLANKS
     C     DKIH_07       CABNE     REFNR         HOPP2
     C                   ENDIF
     C                   IF        DATUMN > 0
     C     DKIH_SYDT     CABLT     DATUMN        HOPP2
     C                   ENDIF
     C                   IF        STATUS <> *BLANKS
     C     STATUS        IFNE      'b '
     C     DKIH_SYST     CABNE     STATUS        HOPP2
     C                   ENDIF
     C     STATUS        IFEQ      'b '
     C     DKIH_SYST     CABNE     *BLANKS       HOPP2
     C                   ENDIF
     C                   ENDIF
     C     AVSNAVN       IFNE      *BLANKS
N01  C     DKIH_02B      CABLT     AVSNAVN       HOPP2
N01  C     DKIH_02B      CABGT     STOPP1        HOPP2
     C                   ENDIF
     C     MOTNAVN       IFNE      *BLANKS
N01  C     DKIH_08B      CABLT     MOTNAVN       HOPP2
N01  C     DKIH_08B      CABGT     STOPP2        HOPP2
     C                   ENDIF
N01  C     DKIV_28B      CABLT     FAKT          HOPP3
N01  C     DKIV_28B      CABGT     STOPP4        HOPP3
     C                   GOTO      HOPP4
     C     HOPP3         TAG
     C                   MOVEL     DKIV_446TA    W17              17
N01  C     W17           CABLT     FAKT          HOPP2
N01  C     W17           CABGT     STOPP4        HOPP2
     C     HOPP4         TAG
     C     AART          IFNE      *BLANKS
N01  C     DKIH_AART     CABNE     AART          HOPP2
     C                   ENDIF
     C     FRIKEY2       CHAIN     FRISOK                             21
     C   21              EVAL      FSSOK=*BLANKS
     C     XREF          IFNE      *BLANKS
N01  C     FSSOK         CABLT     XREF          HOPP2
N01  C     FSSOK         CABGT     STOPP3        HOPP2
     C                   ENDIF

     C                   EVAL      WSYOP=DKIV_SYOP
     C                   Add       1             AntLest
      * ............................................................................................
      * Skriv en JSON-Array-linje pr menypunkt - komma før hvert nytt element
      * ............................................................................................
      /free
       if AntLest=1;
          JSONstring=*blanks;
          else;
          JSONstring=', ';
          endif;
       JSONstring=%trim(JSONstring)+'{'
          +'¬avd¬ : ¬'+%trim(%editc(DKIH_SYAV:'Z'))+'¬, '
          +'¬opd¬ : ¬'+%trim(%editc(DKIH_SYOP:'Z'))+'¬, '
          +'¬tst¬ : ¬'+%trim(DKIH_0035)+'¬, '
          +'¬refnr¬ : ¬'+%trim(DKIH_07)+'¬, '
