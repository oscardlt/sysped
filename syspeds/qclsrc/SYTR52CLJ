    /* UTSKRIFT AV FERJEFAKTURA */
             PGM        PARM(&TNR &AAR &PER &PRT &PT &FHE &FBBS &AWB +
                          &AVD &SG &PRT2 &PT2 &TYP &VAL)
             DCL        VAR(&VAL) TYPE(*CHAR) LEN(3) /* VALUTAKODE */
             DCL        VAR(&AUSER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&ATYPE) TYPE(*CHAR) LEN(2) VALUE('ff')
             DCL        VAR(&FINS) TYPE(*CHAR) LEN(1) VALUE('N')
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10) /* Printernavn */
             DCL        VAR(&PT) TYPE(*CHAR) LEN(10)  /* Papirtype */
             DCL        VAR(&FHE) TYPE(*CHAR) LEN(1)  /* HEADING */
             DCL        VAR(&FBBS) TYPE(*CHAR) LEN(1) /* BBS     */
             DCL        VAR(&AWB) TYPE(*CHAR) LEN(1)  /* SEPARAT GIRO*/
             DCL        VAR(&SG) TYPE(*CHAR) LEN(3)   /* SIGN    */
             DCL        VAR(&AVD) TYPE(*CHAR) LEN(4)  /* AVD     */
             DCL        VAR(&TNR) TYPE(*CHAR) LEN(8)  /*TRANSP   */
             DCL        VAR(&PER) TYPE(*CHAR) LEN(2)  /*PERMND   */
             DCL        VAR(&AAR) TYPE(*CHAR) LEN(2)  /*PERÅR    */
             DCL        VAR(&PRT2) TYPE(*CHAR) LEN(10)/* Prt  GIRO*/
             DCL        VAR(&PT2) TYPE(*CHAR) LEN(10) /* P.Type GIRO*/
             DCL        VAR(&TYP) TYPE(*CHAR) LEN(1)  /*T=TEST K=KOPI*/
             DCL        VAR(&TYPE) TYPE(*CHAR) LEN(1)
             RTVJOBA    USER(&AUSER)
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
    /* */
    /* SKRIV FERJEFAKTURA       */
    /* */
   /* FAKTURA BLANKETT */
             RTVDTAARA  DTAARA(*LDA (48 1)) RTNVAR(&TYPE)
             IF         COND(&TYPE *EQ B) THEN(OVRPRTF +
                          FILE(SYTR52PF) OUTQ(&PRT) DEVTYPE(*IPDS) +
                          PAGESIZE(72 80) CPI(10) OVRFLW(72) +
                          FONT(011) CHRID(590 340) +
                          FORMTYPE(&PT))
             ELSE       CMD(OVRPRTF FILE(SYTR52PF) DEVTYPE(*AFPDS) +
                          PAGESIZE(72 80) CPI(10) OVRFLW(72) +
                          FRONTOVL(LOGOFAKT 0.1) OUTQ(&PRT) +
                          FORMTYPE(&PT))
   /* SEPARAT GIRO */
             IF         COND(&TYPE *EQ B) THEN(OVRPRTF +
                          FILE(SYTR52PG) OUTQ(&PRT2) DEVTYPE(*IPDS) +
                          PAGESIZE(24 80) CPI(10) OVRFLW(24) +
                          FONT(011) CHRID(590 340) +
                          FORMTYPE(&PT2))
             ELSE       CMD(OVRPRTF FILE(SYTR52PG) OUTQ(&PRT2) +
                          PAGESIZE(24 80) CPI(10) OVRFLW(24) +
                          FORMTYPE(&PT2))
    /* */
             CALL       PGM(SYTR52) PARM(&AVD &SG &FHE &FBBS &AWB +
                          &TNR &AAR &PER &TYP &VAL)
             SBMJOB     CMD(CALL PGM(ARC002CL) PARM(&ATYPE &AUSER +
                          &FINS)) JOB(ARKIV) JOBD(*USRPRF) +
                          JOBQ(SYJOBQARK)
             ENDPGM
