/**************************************************************************/
             PGM   PARM(&DATA &STOPP)
             DCL        VAR(&DATA) TYPE(*CHAR) LEN(32000)
             DCL        VAR(&STOPP) TYPE(*CHAR) LEN(1)
             DMPCLPGM
             CRTPF      FILE(A38SYEND/WMLTEST) RCDLEN(32000)
             MONMSG     MSGID(CPF0000)
             CLRPFM     FILE(A38SYEND/WMLTEST)
             MONMSG     MSGID(CPF0000)
             OVRDBF     FILE(WMLTEST) TOFILE(A38SYEND/WMLTEST)
             CALL       PGM(WSEDIINNR) PARM(&DATA &STOPP)
             CPYTOSTMF  +
                          FROMMBR('/qsys.lib/a38syend.lib/wmltest.f+
                          ile/wmltest.mbr') +
                          TOSTMF('/home/svein/wmltest.txt') +
                          STMFOPT(*REPLACE)
             ENDPGM
