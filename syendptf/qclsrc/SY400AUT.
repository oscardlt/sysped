             MONMSG     MSGID(CPF0000)
/*N06*/      CRTDTAQ    DTAQ(&FILIBF/DKDTAQ) MAXLEN(1) FORCE(*YES) +
                          TEXT('SKAT  Dataq for SND/RCV SKAT/TOLD IMP/EKS') +
                          AUT(*ALL)
/*N06*/      MONMSG     MSGID(CPF0000)
