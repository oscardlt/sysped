      * OPPDATERER KUNDER SOM "PART"
     C     FISTJ         IFEQ      'X'
     C                   MOVE      'J'           SFAST
     C                   MOVE      *BLANKS       SBLANK
     C     HEKNS         IFNE      0
     C                   MOVE      HEKNS         KUNDE
     C                   EXSR      OPPDAT
     C                   END
     C     HEKNSF        IFNE      0
     C     HEKNSF        ANDNE     HEKNS
     C                   MOVE      HEKNSF        KUNDE
     C                   EXSR      OPPDAT
     C                   END
     C     HEKNK         IFNE      0
     C     HEKNK         ANDNE     HEKNS
     C     HEKNK         ANDNE     HEKNSF
     C                   MOVE      HEKNK         KUNDE
     C                   EXSR      OPPDAT
     C                   END
     C     HEKNKF        IFNE      0
     C     HEKNKF        ANDNE     HEKNS
     C     HEKNKF        ANDNE     HEKNSF
     C     HEKNKF        ANDNE     HEKNK
     C                   MOVE      HEKNKF        KUNDE
     C                   EXSR      OPPDAT
     C                   END
     C                   END                                                    *FIST? IF...
     C*
     C* OPPDATERER UNDERKUNDE (VAREMOTTAKER NÅR FORSKJELLIG FRA F.MOT)
     C     FISTW         IFEQ      'X'
     C                   MOVE      'W'           SFAST
     C                   MOVE      *BLANKS       SBLANK
     C     HEKNS         IFNE      0
     C     HEKNS         ANDNE     HEKNSF
     C                   MOVE      HEKNS         KUNDE
     C                   EXSR      OPPDAT
     C                   END
     C     HEKNK         IFNE      0
     C     HEKNK         ANDNE     HEKNKF
     C     HEKNK         ANDNE     HEKNS
     C                   MOVE      HEKNK         KUNDE
     C                   EXSR      OPPDAT
     C                   END
     C                   END                                                    *FIST? IF...
      *
      * OPPDATERER AGENT/FRANKATUR
     C     FISTE         IFEQ      'X'
     C     HEKNA         ANDNE     0
     C                   MOVE      'E'           SFAST
     C                   MOVE      HEKNA         KUNDE
     C                   MOVE      *BLANKS       SBLANK
     C                   MOVE      HEFR          SBLANK
     C                   EXSR      OPPDAT
     C                   END                                                    *FIST? IF...
     C*
     C                   ENDSR
     C*
     C******************************************
     C     OPPDAT        BEGSR
     C******************************************
     C     KOVPRO        IFEQ      'F'
     C     KOVPRO        OREQ      'B'
     C                   MOVE      FACC          YDTC
     C                   MOVE      FAÅR          YDTÅ
     C     FAMND         IFEQ      13
     C                   Z-ADD     12            PMN
     C                   ELSE
     C                   MOVE      FAMND         PMN               2 0
     C                   END
     C     STAKEY        CHAIN     STATHFL1                           40
     C                   EXSR      OPDXRE                                       *XREF OPPDRA
     C   40              EXSR      MOVEZ
     C                   EXSR      ADDVER
     C   40              EXCEPT    NY
     C  N40              EXCEPT    GML
     C     FAGGRP        IFNE      *BLANKS
      * OPPDATER BELØP
     C     STAD1K        CHAIN     STATDFL1                           40
     C   40              MOVEA     *ZEROS        BL
     C                   ADD(H)    FABELN        BL(PMN)
     C                   ADD(H)    FABELN        BL(13)
     C   40              MOVE      FAGGRP        STGR
     C   40              WRITE     STATDFR
     C  N40              UPDATE    STATDFR
      * OPPDATER BELØP PÅ TOTALL AV INNTEKT/KOSTNAD
     C     STADXK        CHAIN     STATDFL1                           40
     C   40              MOVEA     *ZEROS        BL
     C                   ADD(H)    FABELN        BL(PMN)
     C                   ADD(H)    FABELN        BL(13)
     C   40              MOVE      XGRTOT        STGR
     C   40              WRITE     STATDFR
     C  N40              UPDATE    STATDFR
     C                   END
     C                   END
      *
     C     KOVPRO        IFEQ      'O'
     C     KOVPRO        OREQ      'B'
     C     HEDTM         IFEQ      0
     C                   MOVEL     HEDTRÅ        YDTC
     C                   MOVE      HEDTRÅ        YDTÅ
     C                   MOVE      HEDTRM        PMN
     C                   ELSE
     C                   MOVEL     HEDTÅ         YDTC
     C                   MOVE      HEDTÅ         YDTÅ
     C                   MOVE      HEDTM         PMN               2 0
     C                   END
     C     STAKEY        CHAIN     STATOHL1                           40
     C                   EXSR      OPDXRE                                       *XREF OPPDRA
     C   40              EXSR      MOVEZ
     C                   EXSR      ADDVER
     C   40              EXCEPT    NYO
     C  N40              EXCEPT    GMLO
     C     FAGGRP        IFNE      *BLANKS
      * OPPDATER BELØP
     C     STAD1K        CHAIN     STATODL1                           40
     C   40              MOVEA     *ZEROS        BL
     C                   ADD(H)    FABELN        BL(PMN)
     C                   ADD(H)    FABELN        BL(13)
     C   40              MOVE      FAGGRP        STGR
     C   40              WRITE     STATODFR
     C  N40              UPDATE    STATODFR
      * OPPDATER BELØP PÅ TOTALL AV INNTEKT/KOSTNAD
     C     STADXK        CHAIN     STATODL1                           40
     C   40              MOVEA     *ZEROS        BL
     C                   ADD(H)    FABELN        BL(PMN)
     C                   ADD(H)    FABELN        BL(13)
     C   40              MOVE      XGRTOT        STGR
     C   40              WRITE     STATODFR
     C  N40              UPDATE    STATODFR
     C                   END
