             PGM        PARM(&KOPI &ANT2)
             DCL        VAR(&KOPI) TYPE(*CHAR) LEN(1) /* KOPI-  +
                          utskrifter. */
             DCL        VAR(&ANT2) TYPE(*CHAR) LEN(3) /* Antall kopi +
                          av utskrifter. */
             DCL        VAR(&ANT) TYPE(*DEC) LEN(3 0) VALUE(0) /* +
                          Antall kopi av utskrifter. */
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10) /* Printernavn +
                          til fakturaer. */
             DCL        VAR(&PT) TYPE(*CHAR) LEN(10) /* Papirtype til +
                          fakturaer. */
             DCL        VAR(&PRANT) TYPE(*CHAR) LEN(2) /* Antall +
                          utskrifter. */
             DCL        VAR(&MSGTXT) TYPE(*CHAR) LEN(12) /* +
                          Meldingtekst */
    /* */
             CHGVAR     VAR(&ANT) VALUE(&ANT2)
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             RTVDTAARA  DTAARA(*LDA (581 10)) RTNVAR(&PRT)
             RTVDTAARA  DTAARA(*LDA (591 10)) RTNVAR(&PT)
             OVRPRTF    FILE(SYEX41PF) PAGESIZE(72 80) CPI(10) +
                          OVRFLW(72) PAGRTT(0) OUTQ(&PRT) +
                          FORMTYPE(&PT) COPIES(&ANT)
             CALL       PGM(SYEX41) PARM(&PRANT &KOPI)
             CHGVAR     VAR(&MSGTXT) VALUE(&PRANT *CAT &PRT)
             SNDPGMMSG  MSGID(DIV0721) MSGF(SYMSGF) MSGDTA(&MSGTXT)
    /* */
             ENDPGM
