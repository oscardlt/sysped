     h DftActGrp( *No )
     fsadh      if   e           k disk
     fheadf     if a e           k disk

     d sadhhead1r      pr
     d zsiavd_                             like(zsiavd)
     d zsitdn_                             like(zsitdn)

     d sadhhead1r      pi
     d zsiavd                         4  0
     d zsitdn                         7  0

      /free
N04    chain (zsiavd:zsitdn) sadh;
         if %found;
N04        chain (zsiavd:zsitdn) headf;
            if not %found;

           clear headfr;

           heur='A';
           heavd=siavd;
           heopd=sitdn;
           hesg=sisg;
           hedtop=sidt;
           hedtr=sidt;
           hegn=sign;
           hehawb=sitrid;
           hepos1=sipos;
           hekns=sikns;
           henas=sinas;
           heads1=siads1;
           heads2=siads2;
           heads3=siads3;
           heknk=siknk;
           henak=sinas;
           headk1=siadk1;
           headk2=siadk2;
           headk3=siadk3;
           heot='?';
           hefr=silv;
           hent=sintk;
           hevkt=sivkb;
           hetrm=sitrm;
           trfrav=sival1;
           trfrab=sibel1;

           write headfr;

       endif;                                                   //not %found HEADF
           endif;                                               // %found SADH
       *inlr=*on;
      /end-free
