     C*
     C                   EXSR      INIT
     C*
     C     ' '           SETLL     DOKUT1
     C     START         TAG
     C     ' '           READE     DOKUT1                                 33
     C   33              GOTO      SLUTT
     C     DTSG          IFNE      USG
     C                   GOTO      START
     C                   END
     C     HEADFK        CHAIN     HEADF                              36
     C   36              GOTO      START
     C                   UPDATE    HEADFR
     C     USG           IFNE      'ALL'
     C     USG           ANDNE     HESG
     C                   GOTO      START
     C                   END
     C     UAVD          IFNE      HEAVD
     C                   GOTO      START
     C                   END
     C*
     C* SKJEKKER OM TURNR ER TASTET
     C*
     C     UPRO          IFNE      0
     C     UPRO          ANDNE     HEPRO
V10  C*    UPRO          ANDNE     HEOPD
V10  C     UOPD          ANDEQ     0
V10  C     UOPD          ORNE      0
V10  C     UOPD          ANDNE     HEOPD
V10  C     UPRO          ANDEQ     0
     C                   GOTO      START
     C                   END
     C*
     C* SKJEKKER OM GODSNR ER TASTET
     C*
     C     UGN           IFNE      *BLANKS
     C     UGN           IFNE      HEGN
     C                   GOTO      START
     C                   END
     C     UDT           IFNE      0
     C     UDT8          ANDGT     HEDTR
     C                   GOTO      START
     C                   END
     C                   END
     C*
     C                   EXSR      TOLL
     C                   GOTO      START
     C*
     C     SLUTT         TAG
     C                   MOVE      XPRANT        UPRANT
     C                   SETON                                        LR
     C                   RETURN
     C*
     C******************************************
     C     TOLL          BEGSR
     C******************************************
     C     DOKUTK        CHAIN     DOKUT                              41
     C  N41DTTOLL        IFEQ      *BLANKS
     C                   SETON                                        41
     C                   ELSE
     C                   MOVE      DTTOLL        YTOLL
     C     KODTSK        CHAIN     KODTTST                            41
     C                   END
     C  N41USG           IFNE      'ALL'
     C                   MOVE      USG           HESG
     C                   END
     C  N41KODSFK        CHAIN     KODTSF                             41
     C   41              GOTO      SLTOLL
     C*
     C     HEKNS         IFNE      0
     C                   MOVE      HEKNS         YKN
     C     KUND1K        CHAIN     CUNDL01                            55
     C  N55DKUND         IFEQ      ' '
     C                   MOVEL     KNAVN         HENAS
     C                   MOVEL     ADR1          HEADS1
     C                   MOVEL     ADR2          HEADS2
     C                   MOVE      *BLANKS       HEADS3
     C                   MOVEL     POSTNR        HEADS3
     C                   MOVE      ADR3          HEADS3
     C                   END
     C                   END
     C*
     C                   MOVE      *BLANKS       MN
     C                   MOVE      *BLANKS       VS
     C     HEUR          IFEQ      'F'
     C     HEUR          OREQ      'G'
     C                   CALL      'SAD61PBL'
     C                   PARM      HEAVD         UAVD2             4 0
     C                   PARM      HEOPD         UOPD2             7 0
     C                   PARM                    UNR               2 0
     C                   PARM                    MN
     C                   PARM                    UNR2              2 0
     C                   PARM                    VS
     C                   END
     C     MN(1)         IFEQ      *BLANKS
     C                   MOVE      HEGM1         MN(1)
     C                   MOVE      HEGM2         MN(2)
     C                   END
     C     VS(1)         IFEQ      *BLANKS
     C                   MOVE      HEVS1         VS(1)
     C                   MOVE      HEVS2         VS(2)
     C                   END
     C*
     C*** SKRIVE UT HOVEDARK
     C                   MOVE      *BLANKS       KFMT1
     C                   MOVE      *BLANKS       KFMT2
     C     F1(1)         IFEQ      *BLANKS                                      1<-B
     C                   EXCEPT    DEL1
     C                   ELSE                                                   1
     C                   MOVE      *ZEROS        YN                3 0
     C     YN            DOWLT     9                                             2<-B
     C                   ADD       1             YN
     C     F1(YN)        IFEQ      *BLANKS                                        3<-B
     C                   ADD       10            YN
     C                   ELSE                                                     3
     C                   MOVE      F1(YN)        KFMT1
     C                   MOVE      F2(YN)        KFMT2
     C                   EXCEPT    DEL1
     C                   END                                                      3<-E
     C                   END                                                     2<-E
     C                   END                                                    1<-E
     C                   ADD       1             XPRANT
     C*
