             IF         COND(&INIFSR *NE ' ') THEN(CHGVAR +
                          VAR(&TODIR) VALUE(&INIFSR))
/*_______________*/
/* KOPIER LENKE  */
/*"""""""""""""""*/
             CPY        OBJ(&OBJ) TODIR(&TODIR)
ENDPGM
