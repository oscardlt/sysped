             PGM        PARM(&BILNR &TURNR &SCTXT &AVD +
                  &SNDNR &FRA &TIL &TIL1 &TIL3 &USRNVN &AVID &AVADR +
                          &INTAD1 &INTAD2 &INTAD3 &INTAD4 &INTAD5)
             DCL        VAR(&BILNR) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TURNR) TYPE(*CHAR) LEN(8)
             DCL        VAR(&SCTXT) TYPE(*CHAR) LEN(80)
             DCL        VAR(&AVD) TYPE(*CHAR) LEN(4)
             DCL        VAR(&SNDNR) TYPE(*CHAR) LEN(20)
             DCL        VAR(&FRA) TYPE(*CHAR) LEN(30)
             DCL        VAR(&TIL) TYPE(*CHAR) LEN(30)
             DCL        VAR(&TIL1) TYPE(*CHAR) LEN(30)
             DCL        VAR(&TIL3) TYPE(*CHAR) LEN(30)
             DCL        VAR(&USRNVN) TYPE(*CHAR) LEN(30)
             DCL        VAR(&AVID) TYPE(*CHAR) LEN(10)
             DCL        VAR(&AVADR) TYPE(*CHAR) LEN(10)
             DCL        VAR(&INTAD1) TYPE(*CHAR) LEN(70)
             DCL        VAR(&INTAD2) TYPE(*CHAR) LEN(70)
             DCL        VAR(&INTAD3) TYPE(*CHAR) LEN(70)
             DCL        VAR(&INTAD4) TYPE(*CHAR) LEN(70)
             DCL        VAR(&INTAD5) TYPE(*CHAR) LEN(70)
             DCL        VAR(&MELD) TYPE(*CHAR) LEN(2000)
             DCL        VAR(&EMNE) TYPE(*CHAR) LEN(44)
             DCL        VAR(&SIGTXT) TYPE(*CHAR) LEN(50)

             RTVUSRPRF  USRPRF(&AVID) TEXT(&SIGTXT)
             CHGVAR     VAR(&EMNE) VALUE('Mld. om feilplasserte +
                          varer tilh. avd ' *CAT &AVD)

             CHGVAR     VAR(&MELD) VALUE('Ved utlasting av Bil/tur ' +
                          *CAT &BILNR *BCAT ' / ' *CAT &TURNR *CAT +
                          ':/N' *CAT &SCTXT *CAT ':/Ntilhørende avd +
                          ' *CAT &AVD *CAT ' påtruffet!!!:/PSending +
                          ' *CAT &SNDNR *CAT ' er: :/NFra: ' *CAT +
                          &FRA *CAT ':/NTil: ' *CAT &TIL *BCAT +
                          &TIL1 *BCAT &TIL3 *CAT ':/PSkutt av: ' +
                          *CAT &USRNVN *CAT ' ')

             SNDDST     TYPE(*LMSG) TOINTNET((&INTAD1) (&INTAD2) +
                          (&INTAD3) (&INTAD4) (&INTAD5)) +
                          DSTD(&EMNE) LONGMSG(&MELD) USRID(&AVID +
                          &AVADR) SNDFMT(*NOCHG)

             ENDPGM
