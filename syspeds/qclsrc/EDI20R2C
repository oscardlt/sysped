             PGM PARM(&VALUE &TCP)
/*____________________*/
/* Definier variabler */
/* MIDLERTIDIG HEADF I SYENDPTF */
/*""""""""""""""""""""*/
             DCL        VAR(&LIB3) TYPE(*CHAR) LEN(10) +
                          VALUE('SMALIBF   ')
             DCL        VAR(&VALUE) TYPE(*CHAR) LEN(8)
             DCL        VAR(&TCP) TYPE(*CHAR) LEN(1)
             OVRDBF     FILE(LOGL05) TOFILE(&LIB3/LOGL05)
             OVRDBF     FILE(CPFL01) TOFILE(&LIB3/CPFL01)
             OVRDBF     FILE(LOCL01) TOFILE(&LIB3/LOCL01)
             OVRDBF     FILE(PARL01) TOFILE(&LIB3/PARL01)
             OVRDBF     FILE(ISUL01) TOFILE(&LIB3/ISUL01)
             OVRDBF     FILE(CNFF) TOFILE(&LIB3/CNFF)
             OVRDBF     FILE(LOGF) TOFILE(&LIB3/LOGF)
             OVRDBF     FILE(FSR00F) TOFILE(&LIB3/FSR00F)
             CALL       PGM(EDI20R2) PARM(&VALUE &TCP)
             ENDPGM
