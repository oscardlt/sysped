     C     VESSE2        BEGSR
     C                   MOVE      *BLANKS       DATA
     C                   Z-ADD     1             NM                2 0
     C     1             DO        30            NM
     C     A(AXV)        IFNE      X'15'
     C                   MOVE      A(AXV)        D(NM)
     C                   ADD       1             AXV
     C                   ELSE
     C                   GOTO      UTV
     C                   END
     C                   END
     C     UTV           ENDSR
      *////////////////////////////////////////////////////////////
      *  NUMKON                                             NUMKON/
      *////////////////////////////////////////////////////////////
     C     NUMKON        BEGSR
     C                   MOVE      *ZEROS        NUMVAL
     C                   MOVE      *ZEROS        XDESIMAL          1 0
     C                   Z-ADD     30            NM                2 0
     C                   Z-ADD     30            NN                2 0
     C                   DO        30
     C     D(NM)         IFGE      '0'
     C     D(NM)         ANDLE     '9'
     C                   MOVE      D(NM)         N(NN)
     C                   SUB       1             NN
     C                   ELSE
     C     D(NM)         IFEQ      '.'
     C     D(NM)         OREQ      ','
     C                   EVAL      XDESIMAL = 30 - NN
     C                   END
     C                   END
     C                   SUB       1             NM
     C                   END
     C                   ENDSR
