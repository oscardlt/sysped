     C                   MOVEL     DF1004        X1004
     C                   MOVEL     DFNAVM        XMOTT
     C                   MOVEL     DFPOUL        XPOST
     C                   MOVEL     DFADUL        XSTED
     C                   MOVEL     *BLANKS       XADR
     C                   Z-ADD     DFNT          XANT
     C                   Z-ADD     DFVKT         XVEKT
     C                   Z-ADD     DFM3          XVOL
     C                   WRITE     E308PB
     C                   ADD       1             YSND
     C                   ADD       DFNT          YANT
     C                   ADD       DFVKT         YVEKT
