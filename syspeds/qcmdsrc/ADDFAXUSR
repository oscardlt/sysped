             CMD        PROMPT('Add Fax User')
             PARM       KWD(USER) TYPE(*CHAR) LEN(8) MIN(1) +
                          PROMPT('User id:')
             PARM       KWD(FAXNBR) TYPE(*CHAR) LEN(32) MIN(1) +
                          PROMPT('FAX phone number')
             PARM       KWD(TO) TYPE(*CHAR) LEN(40) DFT(*NONE) +
                          SPCVAL((*NONE)) PROMPT('To line text')
             PARM       KWD(TO1) TYPE(*CHAR) LEN(40) DFT(*NONE) +
                          SPCVAL((*NONE)) PROMPT('Street address')
             PARM       KWD(TO2) TYPE(*CHAR) LEN(40) DFT(*NONE) +
                          SPCVAL((*NONE)) PROMPT('Postal address')
             PARM       KWD(TO3) TYPE(*CHAR) LEN(40) DFT(*NONE) +
                          SPCVAL((*NONE)) PROMPT('ZIP-code & city')
             PARM       KWD(FI) TYPE(*CHAR) LEN(2) DFT(SS) +
                          SPCVAL((' ')) PROMPT('Company ID')
             PARM       KWD(KNU) TYPE(*CHAR) LEN(8) DFT(*NONE) +
                          SPCVAL((*NONE)) PROMPT('Customer number')
             PARM       KWD(ALFA) TYPE(*CHAR) LEN(10) DFT(*NONE) +
                          SPCVAL((*NONE)) PROMPT('Search name')
             PARM       KWD(TELE) TYPE(*CHAR) LEN(32) DFT(*NONE) +
                          SPCVAL((*NONE)) PROMPT('Telephone number')
             PARM       KWD(TITLE) TYPE(*CHAR) LEN(40) DFT('Fax fra +
                          office mot facsimile/400') +
