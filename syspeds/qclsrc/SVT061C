             PGM        PARM(&OK)
             DCL        VAR(&OK) TYPE(*CHAR) LEN(1) VALUE('1')
             DLTDTAQ    DTAQ(QTEMP/SVT061DQ)
             MONMSG     MSGID(CPF0000)
             DLTDTAQ    DTAQ(QTEMP/SVX040DQ)
             MONMSG     MSGID(CPF0000)
             CALL       PGM(SVT061R) PARM(&OK)
             ENDPGM
