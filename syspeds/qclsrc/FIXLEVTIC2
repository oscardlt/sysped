/* BEHANDLE EN CSV-LEVTID-FIL                                               */
             PGM  PARM(&DIR &ENTRY &ENTPATH)
             DCL        VAR(&DIR) TYPE(*CHAR) LEN(100)
             DCL        VAR(&ENTRY) TYPE(*CHAR) LEN(120)
             DCL        VAR(&ENTPATH) TYPE(*CHAR) LEN(256)
             DCL        VAR(&WS) TYPE(*CHAR) LEN(10)
             RTVJOBA    JOB(&WS)
/*BACKUP AV GAMMEL */
             DLTF       FILE(SYENDRE/LEVTID.G)
             MONMSG     MSGID(CPF0000)
             CPYF       FROMFILE(LEVTID) TOFILE(SYENDRE/LEVTID.G) +
                          CRTFILE(*YES)
             MONMSG     MSGID(CPF0000)
/*CSV-FIL TIL FYSISK  */
             DLTF       FILE(SYENDRE/LEVTICSV)
             MONMSG     MSGID(CPF0000)
             CRTPF      FILE(SYENDRE/LEVTICSV) RCDLEN(1024)
             CPYFRMSTMF FROMSTMF(&ENTPATH) +
                          TOMBR('/QSYS.LIB/SYENDRE.LIB/LEVTICSV.FILE/+
                          LEVTICSV.MBR') MBROPT(*REPLACE) +
                          CVTDTA(*AUTO) STMFCODPAG(*STMF)

             CALL       PGM(FIXLEVTI)

 /*          RMVLNK     OBJLNK(&ENTPATH)   */

             SNDBRKMSG  MSG('FIL ' *BCAT &ENTPATH *BCAT 'er +
                          konvertert til LEVTID. Gammel LEVTID er +
                          bevart som SYENDRE/LEVTID.G.') TOMSGQ(&WS)

 SLUTT:      ENDPGM
