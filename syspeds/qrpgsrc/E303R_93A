     C                   MOVEL     '5'           T4055
     C                   SELECT
     C     HEFR          WHENEQ    'M  '
     C                   MOVEL     'CC '         T4053
     C     HEFR          WHENEQ    'A  '
     C                   MOVEL     'TP '         T4053
     C                   OTHER
     C                   MOVEL     'PP '         T4053
