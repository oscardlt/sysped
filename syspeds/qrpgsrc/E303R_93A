      *
      * Notisblokk kode G  "SIC" (Senders Instr. to Carrier)
     C                   MOVE      'G'           FRTKOD
     C                   MOVEL     'SIC'         F4451
     C                   Z-ADD     0             X                 3 0
     C                   CLEAR                   TX
     C     Free2Key      SETLL     Freetxt2
     C     Free2Key      READE     Freetxt2                               90
     C     *IN90         DOWEQ     *OFF
     C                   ADD       1             X
     C                   MOVEL     FRTTXT        TX(X)
     C     Free2Key      READE     Freetxt2                               90
     C                   ENDDO
      * Legg ut 5 og 5 array-element i ett TXT_segment
     C     X             IFGT      0
     C                   EXSR      FTX15B
     C                   END
      *
      * Notisblokk kode R  "ICN" (melding til mottaker)
     C                   MOVE      'R'           FRTKOD
     C                   MOVEL     'ICN'         F4451
     C                   Z-ADD     0             X
     C                   CLEAR                   TX
     C     Free2Key      SETLL     Freetxt2
     C     Free2Key      READE     Freetxt2                               90
     C     *IN90         DOWEQ     *OFF
     C                   ADD       1             X
     C                   MOVEL     FRTTXT        TX(X)
     C     Free2Key      READE     Freetxt2                               90
     C                   ENDDO
      * linjer fra R38 i evt fraktbrev..
     C     R29KEY        SETLL     DOK29
     C     R29KEY        READE     DOK29                                  90
     C     *IN90         DOWEQ     *OFF
     C                   IF        X >= 95
     C                   LEAVE
     C                   END
     C                   ADD       1             X
     C                   MOVEL     D29TXT        TX(X)
     C     R29KEY        READE     DOK29                                  90
     C                   ENDDO
      * Legg ut 5 og 5 array-element i ett TXT_segment
     C     X             IFGT      0
     C                   EXSR      FTX15B
     C                   END
     C                   ENDSR
      *////////////////////////////////////////////////////////////
      *  Skriv fritekst fra array til segment (5 og 5)      FTX00B/
      *////////////////////////////////////////////////////////////
     C     FTX15B        BEGSR
      * Når SR er kalt - Alltid minst ett segment a 5. linjer,
     C                   Z-ADD     1             XA                3 0
     C     STRFTX        TAG
     C     XA            ADD       1             XB                3 0
     C     XA            ADD       2             XC                3 0
     C     XA            ADD       3             XD                3 0
     C     XA            ADD       4             XE                3 0
     C*
     C                   ADD       1             WREP1
     C                   Z-ADD     WREP1         FREP1
     C     TX(XA)        IFEQ      *BLANKS
     C                   EVAL      TX(XA) = '.'
     C                   ENDIF
     C                   MOVEL     TX(XA)        F4440A
     C                   MOVEL     TX(XB)        F4440B
     C                   MOVEL     TX(XC)        F4440C
     C                   MOVEL     TX(XD)        F4440D
     C                   MOVEL     TX(XE)        F4440E
     C                   WRITE     FTXR
      * Flere??
     C     XE            IFLT      X
     C                   ADD       5             XA
     C                   GOTO      STRFTX
     C                   END
     C                   ENDSR
