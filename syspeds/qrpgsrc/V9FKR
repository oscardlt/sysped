     C                   SETOFF                                       919299
     C     *IN98         DOWEQ     '0'
     C                   READC     SUBFIL                                 98
     C   98              DO
     C   99              SETOFF                                       98
     C                   GOTO      SLHENT
     C                   END
     C     FNFN          CHAIN     V9FNF                              33
     C     UHIST         IFEQ      'J'
     C     FNHST         IFEQ      ' '
     C                   MOVE      WSKNV         FNHKNV
     C                   END
     C                   ELSE
     C     FNST          IFEQ      ' '
     C                   MOVE      WSKNV         FNKNV
     C                   END
     C                   END
     C                   UPDATE    V9FNFR
     C                   UPDATE    SUBFIL
     C                   MOVE      '1'           *IN99
     C                   END
