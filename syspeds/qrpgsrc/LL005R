     C     *IN04         IFEQ      '1'
     C                   CALL      'LL008R'
     C                   PARM                    WSLLT
     C                   GOTO      START5
     C                   END
     C     *IN12         IFEQ      '1'
     C                   EXSR      MOVB2
     C                   ELSE
     C                   EXSR      TESTB
     C     *IN30         CABEQ     '1'           START5
     C                   END
     C                   Z-ADD     5             LINNBR
     C                   Z-ADD     2             POSNBR
     C                   GOTO      START3
