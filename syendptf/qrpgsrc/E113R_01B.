      *////////////////////////////////////////////////////////////
     C     RTVDB         BEGSR
      *__________________
      * INIT. DATAFELTER
      *""""""""""""""""""
     C                   CLEAR                   HEADFR
     C                   MOVE      'H'           HEUR
     C                   Z-ADD     WEKNA         HEKNA
     C                   Z-ADD     WEKNA         KUNDNR
     C     CUNDL01K      CHAIN     CUNDL01                            33
     C  N33              EVAL      HENAA = KNAVN
      *
     C                   MOVEL     's'           HESDLK
     C                   MOVEL     's'           HESDL
     C                   MOVEL     C1004         HERFA
     C                   ADD       1             WPOS
     C                   MOVEL     WEHAWB        HEHAWB
     C                   MOVEL     WPOS          HEPOS1
     C                   MOVEL     SLIB          HELIB
     C                   MOVEL     SFILE         HEFILE
     C                   MOVEL     SMBR          HEMBR
     C                   MOVE      'IFCSUM'      HETYPE
     C                   MOVE      '01B'         HELVL
     C                   Z-ADD     ZMN           HESNN
     C                   IF        WEHAWB = *BLANKS
     C                   EVAL      HEREFF = M1004
     C                   ELSE
     C                   EVAL      HEREFF = %TRIM(WEHAWB) + ' ' + %TRIM(M1004)
     C                   END
     C     FIFIRM        CHAIN     ZTELL                              33
     C                   IF        *IN33
     C                   EVAL      FIRMA = FIFIRM
     C                   Z-ADD     1000001       HEUNIK
     C                   Z-ADD     1000002       ZUNIK
     C                   WRITE     ZTELLR
     C                   ELSE
     C                   Z-ADD     ZUNIK         HEUNIK
     C                   ADD       1             ZUNIK
     c                   UPDATE    ZTELLR
     c                   END
      *__________________
      * READ ALL SEGMENTS
      *""""""""""""""""""
     C                   Z-ADD     CREP0         ZREPB0
