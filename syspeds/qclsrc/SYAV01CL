             PGM
             DCL        VAR(&PRINTE) TYPE(*CHAR) LEN(10)
             DCL        VAR(&BEVAR) TYPE(*CHAR) LEN(4)
             DCL        VAR(&HOLD) TYPE(*CHAR) LEN(4)
             DCL        VAR(&ANT) TYPE(*DEC) LEN(1 0)
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1)
             DCL        VAR(&FORFAL) TYPE(*CHAR) LEN(6)
             DCL        VAR(&JOBNM) TYPE(*CHAR) LEN(10)
             CALL       PGM(SYFA00) PARM(&IN01)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT)) /* +
                          Hvis &IN01 på, avslutter program. */
                                                          /**/
             RTVJOBA    JOB(&JOBNM)
             RMVM       FILE(SYAV06W) MBR(&JOBNM)
             MONMSG     MSGID(CPF0000)
             ADDPFM     FILE(SYAV06W) MBR(&JOBNM)
             OVRDBF     FILE(SYAV06W) TOFILE(SYAV06W) MBR(&JOBNM)
             CALL       PGM(SYAV01) PARM(&PRINTE &HOLD &BEVAR &ANT +
                          &FORFAL)
SLUTT:       ENDPGM
