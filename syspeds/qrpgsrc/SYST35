     C                   ENDSR
     C*
     C******************************************
     C     OPDXRE        BEGSR
     C******************************************
     C* NY STATPOST - HENT UNIKT LØPENR.
     C   40              DO
     C     FIFIRM        CHAIN     FIRSTA                             41
     C  N41              Z-ADD     FISTNR        STSTNR
     C  N41              ADD       1             FISTNR
     C  N41              UPDATE    FIRSTAR
     C                   END
     C* LEGG UT X-REF MELLOM UNIKT NR OG OPPDRAGET -
     C* NY POST MED HE01=J:DETTE BEGREPET ER KUN VIA TILLEGSLINJER
      *
      *
      * FORELØPIG KUTTE !!!!!  OPPDATERING AV STATXR
      *
     C                   GOTO      SLOPDX
      *
     C     STAXRK        CHAIN     STATXR                             41
     C     *IN41         IFEQ      '1'
     C                   MOVE      STSTNR        STXNR
     C                   MOVE      HEAVD         STXAVD
     C                   MOVE      HEOPD         STXOPD
     C                   MOVE      HE01          STXTIL                          *TILLEGSLIN
     C                   WRITE     STATXRR
     C                   END
     C     SLOPDX        TAG
     C*
     C                   ENDSR
     C******************************************
     C     ADDVER        BEGSR
     C******************************************
     C*Ant/vekt: - sett av parametre sjekket i INIT  *in90 On=oppdater
      * MEN IKKE akkum. ant/vekt hvis det er kunde-stat av dup-oppdrag.
     C     KUNDUP        IFNE      'J'
     C     HE01          ANDEQ     'J'
     C     *IN90         IFEQ      '1'
     C                   EXSR      ADD2
     C                   ELSE
     C     UTREKK        IFEQ      'J'
     C                   EXSR      SUB2
     C                   END
     C                   END
     C                   END
      *
     C                   ENDSR
      *
     C******************************************
     C     ADD2          BEGSR
     C******************************************
     C* ADDERER ANTALL OPPDRAG OG VEKTER
     C* TIL STATISTIKK REGISTER
     C* UTFØRES IKKE HVIS OPPDRAGSTYPE SIER N=NEI
     C* ELLER STATUS = O - TILLEGGSFAKTURERING
     C*
     C                   EXSR      HENTCO
     C                   ADD       1             AS(PMN)
     C                   ADD       HEVKT         AV(PMN)
     C                   ADD       HEFBV         FV(PMN)
     C                   ADD       1             VG(VV)
     C                   ADD       1             AS(13)
     C                   ADD       HEVKT         AV(13)
     C                   ADD       HEFBV         FV(13)
     C                   ADD       1             VG(13)
     C     XANTCO        IFEQ      0                                             2<-B
     C                   ADD       1             SS(PMN)
     C                   ADD       1             SS(13)
     C                   ADD       HEVKT         VS(PMN)
     C                   ADD       HEVKT         VS(13)
     C                   ADD       HEM3          MS(PMN)
     C                   ADD       HEM3          MS(13)
     C                   ELSE                                                    2
     C                   ADD       XANTCO        AT(PMN)
     C                   ADD       XANTCO        AT(13)
     C                   END                                                     2<-E
      *
     C                   ENDSR
     C******************************************
     C     SUB2          BEGSR
     C******************************************
     C* TREKKER ANTALL OPPDRAG OG VEKTER
     C* FRA STATISTIKK REGISTER
     C*
     C                   EXSR      HENTCO
     C                   SUB       1             AS(PMN)
     C                   SUB       HEVKT         AV(PMN)
     C                   SUB       HEFBV         FV(PMN)
     C                   SUB       1             VG(VV)
     C                   SUB       1             AS(13)
     C                   SUB       HEVKT         AV(13)
     C                   SUB       HEFBV         FV(13)
     C                   SUB       1             VG(13)
     C     XANTCO        IFEQ      0                                             2<-B
     C                   SUB       1             SS(PMN)
     C                   SUB       1             SS(13)
     C                   SUB       HEVKT         VS(PMN)
     C                   SUB       HEVKT         VS(13)
     C                   SUB       HEM3          MS(PMN)
     C                   SUB       HEM3          MS(13)
     C                   ELSE                                                    2
     C                   SUB       XANTCO        AT(PMN)
     C                   SUB       XANTCO        AT(13)
     C                   END                                                     2<-E
      *
     C                   ENDSR
     C******************************************
     C     HENTCO        BEGSR
     C******************************************
     C     BLV1K         SETLL     BLV1                               35
     C                   MOVE      *ZEROS        XANTCO            9 0
     C     STHCO         TAG
     C     BLV1K         READE     BLV1                                   35
     C  N35              DO
     C     BVNT          MULT      BVENH         XWORK             7 0
     C                   ADD       XWORK         XANTCO
     C                   END
     C  N35              GOTO      STHCO
     C*
     C                   ENDSR
     C*
     C******************************************
     C     MOVEZ         BEGSR
     C******************************************
     C                   MOVE      *ZEROS        FV
     C                   MOVE      *ZEROS        AV
     C                   MOVE      *ZEROS        AS
     C                   MOVE      *ZEROS        VG
     C                   MOVE      *ZEROS        AT
     C                   MOVE      *ZEROS        SS
     C                   MOVE      *ZEROS        VS
     C                   MOVE      *ZEROS        MS
     C                   ENDSR
     C*
     C******************************************
     C     INIT          BEGSR
     C******************************************
     C     *ENTRY        PLIST
     C                   PARM                    UAVD              4 0
     C                   PARM                    UOPD              7 0
     C                   PARM                    ULI               5 0
     C                   PARM                    UXOPD             1
     C                   PARM                    UEND              1
     C                   PARM                    UTREKK            1
      *
     C     UEND          IFEQ      'J'
     C                   SETON                                        LR
     C                   RETURN
     C                   END
      *
     C                   MOVE      UAVD          FAAVD
     C                   MOVE      UOPD          FAOPD
     C                   MOVE      ULI           FALI
     C                   MOVE      UXOPD         XXOPD             1
      * PARAM XXOPD:HVA SKAL OPPDATERES
      * XXOPD='N' En FAKTlinje (inntekt/kostnad) er innparameter
      *           Oppdater beløpsfeltene.
      *           OM HE01 ULIK 'J' Oppdater ant/vekt OG HE01.
      * XXOPD='J' Et OPPDRAG (uavhengig av fakt.linjer) er innparameter
      *           OM HE01 ULIK 'J' Oppdater ant/vekt OG HE01.
      * XXOPD='0' En FAKTlinje (inntekt/kostnad) er innparameter
      *           Oppdater beløpsfeltene. IKKE OPPDATER ANT/VEKT - HE01
      * XXOPD='1' Et OPPDRAG (uavhengig av fakt.linjer) er innparameter
      *           Oppdater ant/vekt UAVHENGIG AV HE01 (Ikke oppd. HE01)
     C*--------------------------------------------------------------
     C* DEFINERER NØKKEL TIL KODE-TEKST-FIL TYPE V (AVD. VEKTGRUPPER)
     C*--------------------------------------------------------------
     C     KODVKE        KLIST
     C                   KFLD                    KOVUNI
     C                   KFLD                    KOVAVD
     C*--------------------------------------------------------------
     C* DEFINERER NØKKEL TIL KODE-TEKST-FIL TYPE A (AVD.OPPLYSNINER)
     C*--------------------------------------------------------------
     C     KODAKE        KLIST
     C                   KFLD                    KOAUNI
     C                   KFLD                    KOAAVD
     C*--------------------------------------------------------------
     C* DEFINERER NØKKEL TIL KODE-TEKST-FIL TYPE LK (LANDKODE)
     C*--------------------------------------------------------------
     C     KODLKK        KLIST
     C                   KFLD                    KLKUNI
     C                   KFLD                    KLKLK
     C*--------------------------------------------------------------
     C* DEFINERER NØKKEL TIL KODE-TEKST-FIL TYPE GE (GEBYRKODER)
     C*--------------------------------------------------------------
     C     GEBKEY        KLIST
     C                   KFLD                    KGEUNI
     C                   KFLD                    FAVK
     C*--------------------------------------------------------------
     C* DEFINERER NØKKEL TIL KODE-TEKST-FIL TYPE O2 (OPPD.TYPER)
     C*--------------------------------------------------------------
     C     KOO2KE        KLIST
     C                   KFLD                    KO2UNI
     C                   KFLD                    HEOT
     C*--------------------------------------------------------------
     C* DEFINERER NØKKEL TIL FAKTURAREGISTER: FAKT
     C*--------------------------------------------------------------
     C     FAKKEY        KLIST
     C                   KFLD                    FAAVD
     C                   KFLD                    FAOPD
     C                   KFLD                    FALI
     C*--------------------------------------------------------------
     C* DEFINERER NØKKEL TIL FAKTURAREGISTER: FAK10
     C*--------------------------------------------------------------
     C     FAK10K        KLIST
     C                   KFLD                    HEAVD
     C                   KFLD                    HEOPD
     C                   KFLD                    FAKUNR
     C*
     C*--------------------------------------------------------------
     C* DEFINERER NØKKEL TIL HEADINGREGISTER  HEAD
     C*--------------------------------------------------------------
     C     HEAKEY        KLIST
     C                   KFLD                    FAAVD
     C                   KFLD                    FAOPD
     C*
     C*--------------------------------------------------------------
     C* DEFINERER NØKKEL TIL X-REF REGISTER  STAT/HEAD
     C*--------------------------------------------------------------
     C     STAXRK        KLIST
     C                   KFLD                    STSTNR
     C                   KFLD                    HEAVD
     C                   KFLD                    HEOPD
     C*--------------------------------------------------------------
     C* DEFINERER NØKKEL TIL TURER
     C*--------------------------------------------------------------
     C     TURKEY        KLIST
     C                   KFLD                    DITUAV
     C                   KFLD                    DITUR
     C*--------------------------------------------------------------
     C* DEFINERER NØKKEL TIL BLV1  VARELINJER AV BILL OF LADING
     C*--------------------------------------------------------------
     C     BLV1K         KLIST
     C                   KFLD                    HEAVD
     C                   KFLD                    HEOPD
     C*--------------------------------------------------------------
     C* DEFINERER NØKKEL TIL STATDFL1/STATODL1
     C*--------------------------------------------------------------
     C     STAD1K        KLIST
     C                   KFLD                    STSTNR
     C                   KFLD                    FAGGRP
     C     STADXK        KLIST
     C                   KFLD                    STSTNR
     C                   KFLD                    XGRTOT
     C*--------------------------------------------------------------
     C* DEFINERER NØKKEL TIL STED2D
     C*--------------------------------------------------------------
     C     STE2DK        KLIST
     C                   KFLD                    ST2LK
     C                   KFLD                    ST2KOD
     C*
     C                   MOVE      'V'           KOVUNI
     C                   MOVE      'A'           KOAUNI
     C                   MOVE      'GE'          KGEUNI
     C                   MOVE      'O2Y'         KO2UNI
     C                   MOVE      'LK '         KLKUNI
     C*
     C* OPPDRAGSDATA SKAL HENTES UANSETT INNPARAMETRE
     C                   MOVE      *BLANKS       TUBILK
     C     HEAKEY        CHAIN     HEADF                              33
     C     *IN33         IFEQ      '0'                                          1<-B
     C     HEUR          IFEQ      'H'                                           2<-B
     C     HEAKEY        CHAIN     DISP                               34
     C  N34TURKEY        CHAIN     TURER                              34
     C     *IN34         IFEQ      '1'                                            3<-B
     C                   MOVE      HELKA         TULANF
     C                   MOVE      HETRI         TULANT
     C                   MOVE      *BLANKS       TUBILN
     C                   MOVE      *BLANKS       TUHENG
     C                   MOVE      *BLANKS       TUBILK
     C                   MOVE      *ZEROS        TUKNT2
     C                   MOVE      *ZEROS        TURUND
     C                   END                                                      3<-E
     C     HETRI         IFNE      '  '                                           3<-B
     C                   MOVE      HELKA         TULANF
     C                   MOVE      HETRI         TULANT
     C                   END                                                      3<-E
     C*
     C                   END                                                     2<-E
     C*Sett indikator for hva som skal oppdateres avhengig av parametre
     C*  *IN90 ON = Oppdater antall og vekt
     C*
     C                   MOVEA     '00'          *IN(90)
     C                   MOVE      ' '           XSTFA             1
     C*ANT/VEKT:Oppdragstype på IKKE ha "N" for oppdater antall/vekt
     C                   MOVE      HEOT          KO2KOD
     C     KOO2KE        CHAIN     KODTOT2                            34
     C     KO2ST         IFNE      'N'                                           2<-B
      * XXOPD N/J (faktlinje/oppdrag-ordinær) når HE01<>J
      * VED STATISTIKK PÅ KUNDE, SKAL TESTE OM STATISTIKK PÅ DENNE
      * KUNDEN ER OPPDATERT FRA FØR.
     C     XXOPD         IFEQ      'J'                                            3<-B
     C     XXOPD         OREQ      'N'
     C     HE01          IFEQ      'J'                                             4<-B
     C     FAKKEY        CHAIN(N)  FAKT                               33
     C     *IN33         IFEQ      '0'                                              5<-B
     C     FAKDA         ANDNE     'K'
     C     FAK10K        CHAIN     FAK10                              33
     C     *IN33         DOWEQ     '0'                                               6<-B
     C     F0KDA         IFNE      'K'                                                7<-B
     C                   LEAVE
     C                   END                                                          7<-E
     C     FAK10K        READE     FAK10                                  33
     C  N33              ENDDO                                                       6<-E
     C   33              MOVE      'J'           XSTFA
     C                   END                                                        5<-E
     C                   ELSE                                                      4
     C     FAKKEY        CHAIN(N)  FAKT                               33
     C                   IF        *IN33                                            5<-B
     C                   MOVE      '1'           *IN90
     C                   ELSE                                                       5
     C     FAKDA         IFNE      'K'                                               6<-B
     C                   MOVE      '1'           *IN90
     C                   END                                                         6<-E
     C                   END                                                        5<-E
     C                   END                                                       4<-E
     C                   END                                                      3<-E
     C*    XXOPD 1 (oppdrag-Omkjøring m.m.) UAVHENGIG AV HE01=J
     C     XXOPD         IFEQ      '1'                                            3<-B
     C                   MOVE      '1'           *IN90
     C                   END                                                      3<-E
     C*    XXOPD 0 (Fakturalinjer - Omkjøring m.m.) ALDRI
     C                   END                                                     2<-E
     C*
     C* Oppdater HE01 - dersom original kjøring (oppdrag / linje)
     C     XXOPD         IFEQ      'N'                                           2<-B
     C     FAKDA         ANDNE     'K'
     C     FAKDA         ANDNE     'I'
     C     XXOPD         OREQ      'J'
     C                   MOVE      'J'           HE01
     C                   END                                                     2<-E
     C*
     C* FAKTURALINJE SOM INNPARAMETER :
     C     XXOPD         IFEQ      'N'                                           2<-B
     C     XXOPD         OREQ      '0'
     C     FAKKEY        CHAIN     FAKT                               33
     C*Beløp adderes kun ved XXOPD N/O (faktlinje i innparam.)
     C     GEBKEY        CHAIN     KODTGE                             33
      * VED TR.OPPDRAG BRUK BILKODEN FRA TUREN.
     C     *IN33         IFEQ      '0'
     C     KGEAVR        ANDEQ     'T'
     C     TUBILK        ANDNE     *BLANKS
     C                   MOVE      FAVK          GMVK              3
     C                   MOVE      TUBILK        FAVK
     C     GEBKEY        CHAIN     KODTGE                             33
     C   33              MOVE      GMVK          FAVK
     C                   SETOFF                                       33
     C                   END
      *
     C     *IN33         IFEQ      '1'                                            3<-B
     C                   MOVE      *BLANKS       FAGGRP
     C                   ELSE                                                     3
     C                   SELECT                                                    4<-B
     C     FASK          WHENEQ    'I'                                          INTERNT
     C     FABELN        IFGE      0                                                5<-B
     C                   MOVE      KGEII         FAGGRP                         I.INNTEKT
     C                   ELSE                                                       5
     C                   MOVE      KGEKI         FAGGRP                         I.KOSTNAD
     C                   END                                                        5<-E
     C     FAVK          WHENEQ    'AME'                                        EKSTERNT
     C     KGETYP        OREQ      'AME'
     C     FABELN        IFGE      0                                                5<-B
     C                   MOVE      KGEIE         FAGGRP                         E.INNTEKT
     C                   ELSE                                                       5
     C                   MOVE      KGEKE         FAGGRP                         E.KOSTNAD
     C                   END                                                        5<-E
     C     FAKDA         WHENEQ    'K'
     C                   MOVE      KGEKE         FAGGRP                         E.KOSTNAD
     C                   OTHER
     C                   MOVE      KGEIE         FAGGRP                         E.INNTEKT
     C                   ENDSL                                                     4<-E
     C                   MOVE      FAGGRP        XGRIK             1
     C     XGRIK         IFEQ      'I'
     C                   MOVE      'XI'          XGRTOT            2
     C                   ELSE
     C                   MOVE      'XK'          XGRTOT
     C                   END
     C                   END                                                      3<-E
     C                   EXCEPT    NYGRP
     C* UDEFINERT FAKTURAÅR/MND NÅR BEHANDLER EN VALELINJE
     C     FAMND         IFEQ      *ZEROS                                         3<-B
     C                   MOVE      FADATM        FAMND
     C                   MOVE      FADATÅ        FAÅR
     C                   MOVEL     FADATÅ        FACC
     C                   END                                                      3<-E
     C                   ELSE                                                    2
     C* OPPDRAG SOM   INNPARAMETER :
     C                   CLEAR                   FAKTR
     C                   MOVE      HEDTMP        FAMND
     C                   MOVE      HEDTÅP        FAÅR
     C                   MOVE      HEDTCP        FACC
     C* UDEFINERT FAKTURAÅR/MND NÅR BEHANDLER ET OPPDRAG
     C     FAMND         IFEQ      *ZEROS                                         3<-B
     C                   MOVE      HEDTM         FAMND
     C                   MOVE      HEDTÅ         FAÅR
     C                   MOVEL     HEDTÅ         FACC
     C                   END                                                      3<-E
     C                   END                                                     2<-E
      *
     C     FAMND         IFEQ      0
     C                   RETURN
     C                   END
      *
      * IKKE OPPDATER STATUS PÅ HEADF DERSOM DET ER EN KOSTNADLINJE
     C     FAKDA         IFNE      'K'
     C     FASK          IFNE      'I'
     C                   MOVE      'O'           HEST
     C                   ELSE
     C     TRSLAG        IFNE      'HO'
     C     TRSLAG        ANDNE     '  '                                         SPED.-HO
     C                   MOVE      'O'           HEST
     C                   END
     C                   END
     C                   END
     C                   EXCEPT    HOVUPD
      *
     C                   MOVE      *ZEROS        KOVAVD
     C     KODVKE        CHAIN     KODTV                              33
     C                   MOVE      99999         FF(12)
     C                   MOVE      99999         FF(13)
     C                   Z-ADD     1             VV                2 0
      *
     C     HEFBV         IFGT      99999                                         2<-B
     C                   MOVE      99999         XFBV              5 0
     C                   ELSE                                                    2
     C                   MOVE      HEFBV         XFBV
     C                   END                                                     2<-E
     C     XFBV          LOOKUP    FF(VV)                             33  33
     C                   MOVE      HEAVD         KOAAVD
     C     KODAKE        CHAIN     KODTA                              33
     C* VV INNEHOLDER NÅ STATISTISK VEKTGRUPPE
     C*
     C* HENT FIRMAETS STATISTIKKPROFIL / FIRMAETS EGEN LANDKODE
     C     FIFIRM        IFEQ      *BLANKS                                       2<-B
     C                   READ(N)   FIRSTA                                 33
     C                   READ      FIRM                                   33
     C                   END                                                     2<-E
     C*
     C                   END                                                    1<-E
