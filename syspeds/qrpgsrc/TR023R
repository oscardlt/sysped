     C                   EXSR      INIT
      *
     C     START         TAG
     C                   MOVE      '1'           *IN93
     C                   WRITE     SUBCTL
     C                   MOVEA     '00'          *IN(93)
     C                   Z-ADD     *ZEROS        ZRECNO
     C     UTDT          IFNE      *BLANKS
     C                   MOVE      UTDT          TVTDT
     C                   MOVE      UTDR          TVTDR
     C                   MOVE      UTDSK         TVTDSK
     C                   MOVE      UTDO          TVTDO
     C                   MOVE      UTDOS         TVTDOS
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
     C                   END
      *_____________
      * Fyll Subfile
      *"""""""""""""
     C     TRVTDK        SETLL     TRUVTD
     C     TRVTDK        READE(N)  TRUVTD                                 94
      *
     C     *IN94         DOWEQ     '0'
     C                   ADD       1             ZRECNO
     C                   WRITE     SUBFIL
     C     TRVTDK        READE(N)  TRUVTD                                 94
     C  N94              END
      *
     C                   Z-ADD     ZRECNO        YRECNO            5 0
     C     ZRECNO        IFGT      0
     C                   Z-ADD     1             ZRECNO
     C                   END
      *
     C     START3        TAG
     C  N30              Z-ADD     4             LINNBR
     C  N30              Z-ADD     2             POSNBR
     C                   MOVEA     '11'          *IN(91)
     C     ZRECNO        IFEQ      0
     C                   EXFMT     NYWRK
     C                   ELSE
     C                   WRITE     BUNN
     C                   EXFMT     SUBCTL
     C                   END
      *
     C                   SETOFF                                       9830
     C   03
     COR 12              GOTO      SLUTT
      *** FRISK OPP
     C   05              DO
     C  N27              EXSR      OPDFIL
     C                   GOTO      START
     C                   END
      *
     C   06              DO
     C                   EXSR      CRTTD
     C                   GOTO      START3
     C                   END
      *
     C     ZRECNO        IFNE      0
     C                   EXSR      HENT
     C                   END
     C                   GOTO      START3
      *
     C     SLUTT         TAG
     C  N27              EXSR      OPDFIL
     C                   SETON                                        LR
     C                   RETURN
      *
     C***********************************************
     C     OPDFIL        BEGSR
     C***********************************************
     C     TRVTDK        SETLL     TRUVTD
     C     TRVTDK        READE     TRUVTD                                 33
     C     *IN33         DOWEQ     '0'
     C                   DELETE    TRUVTDR
     C     TRVTDK        READE     TRUVTD                                 33
     C  N33              ENDDO
      *
     C                   MOVE      *BLANKS       UTDT
     C                   MOVE      *BLANKS       UTDR
     C                   MOVE      *BLANKS       UTDSK
     C                   MOVE      *BLANKS       UTDO
     C                   MOVE      *BLANKS       UTDOS
     C     YRECNO        IFGT      0
     C     1             CHAIN     SUBFIL                             33
     C     TVTDT         IFNE      '*DLT'
     C                   MOVE      TVTDT         UTDT
     C                   MOVE      TVTDR         UTDR
     C                   MOVE      TVTDSK        UTDSK
     C                   MOVE      TVTDO         UTDO
     C                   MOVE      TVTDOS        UTDOS
     C                   END
     C     2             DO        YRECNO        ZRECNO
     C     ZRECNO        CHAIN     SUBFIL                             33
     C     TVTDT         IFNE      '*DLT'
     C     UTDT          IFEQ      *BLANKS
     C                   MOVE      TVTDT         UTDT
     C                   MOVE      TVTDR         UTDR
     C                   MOVE      TVTDSK        UTDSK
     C                   MOVE      TVTDO         UTDO
     C                   MOVE      TVTDOS        UTDOS
     C                   ELSE
     C                   WRITE     TRUVTDR
     C                   END
     C                   END
     C                   ENDDO
     C                   END
      *
     C                   ENDSR
      *
     C***********************************************
     C     HENT          BEGSR
     C***********************************************
     C                   MOVEA     '00'          *IN(91)
     C                   MOVEA     '00'          *IN(98)
     C     *IN98         DOWEQ     '0'
     C                   READC     SUBFIL                                 98
     C   98              DO
     C   99              MOVE      '0'           *IN98
     C                   GOTO      SLHENT
     C                   END
     C  N98WSNR          IFEQ      '2'
     C     TVTDT         COMP      '*DLT'                                 98
     C   98              GOTO      SLHENT
     C                   EXSR      CHGTD
     C                   END
     C  N98WSNR          IFEQ      '4'
     C     TVTDT         COMP      '*DLT'                                 98
     C   98              GOTO      SLHENT
     C                   EXSR      DLTTD
     C                   END
     C                   END
      *
     C     SLHENT        TAG
     C   98              DO
     C                   MOVE      'DIV0012'     MSGNR
     C                   SETON                                            30
     C                   END
