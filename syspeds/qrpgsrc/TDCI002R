     C                   OPEN      BRIDF
      * Test innlogging
     C     WSUSER        CHAIN     BRIDF                              50
     C     BILIBL        ifeq      *blanks
     C                   callb     'INCGI'
     C                   parm                    BISTDL
     C                   else
     C                   call      BILIBL
     C                   end
      *
     C     DKNHKey       klist
     C                   kfld                    TIAVD
     C                   kfld                    TITDN
     C     DKNHKeyG      klist
     C                   kfld                    GM_TIAVD
     C                   kfld                    GM_TITDN
     C     *LIKE         DEFINE    X_TIAVD       GM_TIAVD
     C     *LIKE         DEFINE    X_TITDN       GM_TITDN
      *
     C   50              eval      Error = 'Invalid userID'
     C  N50              OPEN      DKNH
     C  N50              OPEN      DKNH05
     C  N50              OPEN      DKNH10
     C  N50              OPEN      DKNSTD
     C  N50              OPEN      DKTHA
     C  N50              OPEN      DKTHA2
     C  N50              OPEN      GODSFI
     C  N50              OPEN      GODSGFSE
     C  N50              OPEN      GODSJF
      *_________________________________
      * Hæmta dagens datum och klockslag
      *"""""""""""""""""""""""""""""""""
     C                   CALL      'SYGE15C'
     C                   PARM                    WDT               8
     C                   PARM                    WTM               6
     C     TISG          CHAIN     DKTHA                              33
     C                   MOVEL     DKTH_NAMN     TITARF

     c                   endsr
      *
      *______________________________________________________
      * TEST                                             TEST
      *""""""""""""""""""""""""""""""""""""""""""""""""""""""
     C     TEST          BEGSR
      * Hæmta userid
     C     WSUSER        CHAIN     DKTHA2
      * Hæmta ærende
     C     WMODE         IFNE      'A'
     C     TIAVD         CHAIN(N)  DKNSTD
     C     DKNHKey       CHAIN     DKNH                               55
     C                   ENDIF
      * MODE A=Add  D=Delete  U=Update
     C                   SELECT
     C     WMODE         WHENEQ    'A'
     C                   eval      Error = *blanks
     C                   EXSR      SRA
     C     WMODE         WHENEQ    'C'
     C                   IF        *IN55=*ON
     C                   eval      Error = 'Order not found'
     C                   ELSE
     C                   UPDATE    DKNHR
     C                   eval      Error = *blanks
     C                   EXSR      SRC
     C                   ENDIF
     C     WMODE         WHENEQ    'D'
     C                   IF        *IN55=*ON
     C                   eval      Error = 'Order not found'
     C                   ELSE
     C                   eval      Error = *blanks
     C                   EXSR      SRD
     C                   ENDIF
     C     WMODE         WHENEQ    'S'
     C                   IF        *IN55=*ON
     C                   eval      Error = 'Order not found'
     C                   ELSE
     C                   eval      Error = *blanks
     C                   EXSR      SRS
     C                   ENDIF
     C     WMODE         WHENEQ    'U'
     C                   IF        *IN55=*ON
     C                   eval      Error = 'Order not found'
     C                   ELSE
     C                   eval      Error = *blanks
      * Test om godsnr finnes fra før
     C     TIGN          CHAIN     DKNH10                             33
     C                   SELECT
     C                   WHEN      *IN33
     C                   EVAL      *IN33 = *OFF
     C                   WHEN      X10_TIAVD <> TIAVD OR X10_TITDN <> TITDN
     C                   eval      Error = 'Goods number used by another order '
     C                             + '(' + %trim(%editc(X10_TIAVD:'Z'))
     C                             + '/' + %trim(%editc(X10_TITDN:'Z')) + ')'
     C                   ENDSL
      * Test om MRN-nr finnes fra før
     C                   IF        *IN33 = *OFF
     C     TITRNR        CHAIN     DKNH05                             33
     C                   SELECT
     C                   WHEN      *IN33
     C                   EVAL      *IN33 = *OFF
     C                   WHEN      X05_TIAVD <> TIAVD OR X05_TITDN <> TITDN
     C                   eval      Error = 'MRN number used by another order '
     C                             + '(' + %trim(%editc(X05_TIAVD:'Z'))
     C                             + '/' + %trim(%editc(X05_TITDN:'Z')) + ')'
     C                   ENDSL
     C                   END
     C                   IF        *IN33 = *OFF
     C                   EXSR      SRU
     C                   END
     C                   ENDIF
     C                   OTHER
     C                   eval      Error = 'Wrong mode'
     C                   ENDSL
     C                   ENDSR
      *______________________________________________________
      * Add                                               SRA
      *""""""""""""""""""""""""""""""""""""""""""""""""""""""
     C     SRA           BEGSR
     C                   CLEAR                   DKNHR
      * Hæmta uppdragsnummer
     C     TIAVD         CHAIN     DKNSTD                             52
     C                   ADD       1             X_TITDN
     C                   UPDATE    DKNSTDR
     C                   EVAL      X_TIAVD    = TIAVD
     C                   EVAL      TIST     = ' '
     C                   EVAL      X_TIST   = ' '
     C                   EVAL      X_TISG   = WSIGN
     C                   MOVEL     WDT           X_TIDT
     C                   MOVEL     WDT           TIDT
      *
      * Hæmta tullid
     C                   CALL      'SVG001R'
     C                   PARM                    WTUID            10
     C                   MOVE      UYEAR         XÅR               2
     C                   EVAL      X_TITUID = 'DK' + XÅR + WTUID
      * Write
     C                   WRITE     DKNHR
      * Oppdater WEB-variabler
     C                   EVAL      TITDN  = X_TITDN
     C                   EVAL      TITUID = X_TITUID
      *
     C                   ENDSR
      *______________________________________________________
      * Copy                                              SRC
      *""""""""""""""""""""""""""""""""""""""""""""""""""""""
     C     SRC           BEGSR
     C                   EVAL      WSIGN = X_TISG
      * Hæmta uppdragsnummer
     C     NEWAVD        CHAIN     DKNSTD                             52
     C                   ADD       1             X_TITDN
     C                   UPDATE    DKNSTDR
     C                   EVAL      NEWOPD   = X_TITDN
      *
     C                   EVAL      GM_TIAVD = TIAVD
     C                   EVAL      GM_TITDN = TITDN
     C     DKNHKeyG      CHAIN(N)  DKNH                               55
     C                   EVAL      X_TIAVD  = NEWAVD
     C                   EVAL      X_TITDN  = NEWOPD
     C                   EVAL      TIAVD    = NEWAVD
     C                   EVAL      TITDN    = NEWOPD
     C                   EVAL      X_TIST   = ' '
     C                   EVAL      X_TISG   = NEWSIGN
     C                   MOVEL     WDT           X_TIDT
     C                   MOVEL     WDT           TIDT
     C                   EVAL      X_TITRNR = *BLANKS
     C                   EVAL      X_TIGN   = *BLANKS
     C                   EVAL      X_TIGNSK = *BLANKS
     C                   EVAL      X_TIALSK = *BLANKS
     C                   EVAL      X_TIALS  = *BLANKS
     C                   EVAL      X_TIALSS = *BLANKS
     C                   EVAL      X_TIGLSK = *BLANKS
     C                   EVAL      X_TIACTS = *BLANKS
     C                   EVAL      X_TISKB  = *BLANKS
     C                   EVAL      X_TITSB  = *BLANKS
     C                   EVAL      X_TIALK  = *BLANKS
     C                   EVAL      X_TIKN   = 0
     C                   EVAL      X_TINA   = *BLANKS
     C                   EVAL      X_TIAD1  = *BLANKS
     C                   EVAL      X_TIPN   = *BLANKS
     C                   EVAL      X_TIPS   = *BLANKS
     C                   EVAL      X_TILK   = *BLANKS
     C                   EVAL      X_TISK   = *BLANKS
