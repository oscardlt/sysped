CMD             PROMPT('CONCATENATE PDF')

PARM INPDF      TYPE(*PNAME) LEN(100) MIN(2) +
                  MAX(300) PROMPT('INPUT PDF')

PARM OUTPDF     TYPE(*PNAME) LEN(100) MIN(1) +
                  PROMPT('OUTPUT PDF')

PARM JDEBUG     TYPE(*CHAR) LEN(4) RSTD(*YES) +
                  DFT(*NO) VALUES(*YES *NO) +
                  PROMPT('DEBUG JAVA')

