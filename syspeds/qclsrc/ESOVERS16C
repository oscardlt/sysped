             PGM        PARM(&AVD &OPD &STMF)
             DCL        VAR(&INFOP) TYPE(*CHAR) LEN(1)
             DCL        VAR(&FILEN) TYPE(*CHAR) LEN(10)
             DCL        VAR(&AVD) TYPE(*CHAR) LEN(4)
             DCL        VAR(&OPD) TYPE(*CHAR) LEN(7)
             DCL        VAR(&STMF) TYPE(*CHAR) LEN(50)
             DCL        VAR(&QJOB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QUSER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QNBR) TYPE(*CHAR) LEN(6)

             RTVJOBA    JOB(&QJOB) USER(&QUSER) NBR(&QNBR)

    /* OVERRIDE PRINTERFILE/SKRIV-FR.BEV  */
             OVRPRTF    FILE(ESOVERS16P) DEV(FAX3812) +
                          DEVTYPE(*AFPDS) PAGESIZE(70 85) LPI(6) +
                          CPI(10) OVRFLW(70) FRONTOVL(CCBTOS) +
                          OUTQ(SYCGIOUTQ) FORMTYPE(*STD) SPLFOWN(*JOB)
             CALL       PGM(ESOVERS16) PARM(&AVD &OPD)

             ADDLIBLE   LIB(MMAIL)
             MONMSG     MSGID(CPF0000)
             CHGVAR     VAR(&STMF) VALUE('/pdf/tmp/OS2' *TCAT &AVD +
                          *TCAT &OPD *TCAT '.PDF')
             CVTSPLFPDF SPLF(ESOVERS16P) JOB(&QNBR/&QUSER/&QJOB) +
                          SPLNBR(*LAST) TOPDF(&STMF)
             RMVLIBLE   LIB(MMAIL)
             MONMSG     MSGID(CPF0000)

             DLTSPLF    FILE(ESOVERS16P) JOB(&QNBR/&QUSER/&QJOB) +
                          SPLNBR(*LAST)
             MONMSG     MSGID(CPF0000)
    /* */
 SLUTT:      ENDPGM
