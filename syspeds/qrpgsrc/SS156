     C                   IF        ORLMA = *BLANKS
     C                   MOVE      *ZEROS        ORLMA
     C                   END
     C                   MOVE      W2LM          W2LMA             4
     C                   IF        W2LMA <> W2LMA
     C                   EVAL      LGTXT = XXTXT12
     C                   CAT       ORLC:1        LGTXT
     C                   EVAL      LGNNY = W2LM
     C                   MOVE      ORLMA         ORLM              4 2
     C                   EVAL      LGNGM = ORLM
