             PGM        PARM(&AVD &DT)
        /*___________________*/
        /* Declare Variables */
        /*"""""""""""""""""""*/
             DCL        VAR(&AVD)    TYPE(*DEC) LEN(4)
             DCL        VAR(&DT)     TYPE(*DEC) LEN(6)
             DCL        VAR(&DLT)    TYPE(*CHAR) LEN(1)  VALUE('J')
             DCL        VAR(&LIBFR)  TYPE(*CHAR) LEN(10)
             DCL        VAR(&LIBTO)  TYPE(*CHAR) LEN(10)
             DCL        VAR(&LIBEDI) TYPE(*CHAR) LEN(10)
        /*_________________________________________________*/
        /* Slette records t.o.m et dato for valgt avdeling */
        /*"""""""""""""""""""""""""""""""""""""""""""""""""*/
             RTVDTAARA  DTAARA(FIRMDT (141 10)) RTNVAR(&LIBFR)
             RTVDTAARA  DTAARA(FIRMDT (151 10)) RTNVAR(&LIBTO)
             RTVDTAARA  DTAARA(EDIDTA (171 10)) RTNVAR(&LIBEDI)
             OVRDBF     FILE(EDIM2)     TOFILE(&LIBEDI/EDIM2)
             OVRDBF     FILE(EDIS)      TOFILE(&LIBEDI/EDIS)
             OVRDBF     FILE(TRUERR)    TOFILE(&LIBFR/TRUERR)
             OVRDBF     FILE(TRUH09)    TOFILE(&LIBFR/TRUH09)
             OVRDBF     FILE(TRUHFR)    TOFILE(&LIBFR/TRUHFR)
             OVRDBF     FILE(TRUHFS)    TOFILE(&LIBFR/TRUHFS)
             OVRDBF     FILE(TRUHGB)    TOFILE(&LIBFR/TRUHGB)
             OVRDBF     FILE(TRUHGN)    TOFILE(&LIBFR/TRUHGN)
             OVRDBF     FILE(TRUHK)     TOFILE(&LIBFR/TRUHK)
             OVRDBF     FILE(TRUHL)     TOFILE(&LIBFR/TRUHL)
             OVRDBF     FILE(TRUV)      TOFILE(&LIBFR/TRUV)
             OVRDBF     FILE(TRUVAM)    TOFILE(&LIBFR/TRUVAM)
             OVRDBF     FILE(TRUVCN)    TOFILE(&LIBFR/TRUVCN)
             OVRDBF     FILE(TRUVDK)    TOFILE(&LIBFR/TRUVDK)
             OVRDBF     FILE(TRUVFS)    TOFILE(&LIBFR/TRUVFS)
             OVRDBF     FILE(TRUVMN)    TOFILE(&LIBFR/TRUVMN)
             OVRDBF     FILE(TRUVTD)    TOFILE(&LIBFR/TRUVTD)
             OVRDBF     FILE(TRUVVT)    TOFILE(&LIBFR/TRUVVT)
             OVRDBF     FILE(TVINF)     TOFILE(&LIBFR/TVINF)
             OVRDBF     FILE(TRUHCMP)   TOFILE(&LIBTO/TRUHCMP)

             CALL       PGM(TR202R) PARM(&AVD &DT &DLT)

             ENDPGM
