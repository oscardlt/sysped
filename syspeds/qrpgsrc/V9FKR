     C     *LOVAL        SETLL     V9FNF
     C     START1        TAG
     C                   READ(N)   V9FNF                                  94
     C  N94UHIST         IFEQ      'J'
     C     FNHIST        CABNE     'J'           START1
     C                   ELSE
     C     FNDRIF        CABNE     'J'           START1
     C                   END
     C  N94USAD          IFEQ      'J'
     C     FNSAD         CABNE     'J'           START1
     C                   END
      *
     C  N94              DO
     C                   DO        5000
     C                   ADD       1             ZRECNO
     C     UHIST         IFEQ      'J'
     C                   MOVE      FNHST         WSST
     C                   MOVE      FNHKNV        WSKNV
     C                   ELSE
     C                   MOVE      FNST          WSST
     C                   MOVE      FNKNV         WSKNV
     C                   END
     C     WSST          COMP      'J'                                    50
     C                   WRITE     SUBFIL
     C     START2        TAG
     C                   READ(N)   V9FNF                                  94
     C  N94UHIST         IFEQ      'J'
     C     FNHIST        CABNE     'J'           START2
     C                   ELSE
     C     FNDRIF        CABNE     'J'           START2
     C                   END
     C  N94USAD          IFEQ      'J'
     C     FNSAD         CABNE     'J'           START2
     C                   END
     C  N94              END
     C                   END
     C                   Z-ADD     ZRECNO        YRECNO            4 0
     C     ZRECNO        IFGT      0
     C                   Z-ADD     1             ZRECNO
     C                   END
     C                   Z-ADD     5             LINNBR
     C                   Z-ADD     2             POSNBR
