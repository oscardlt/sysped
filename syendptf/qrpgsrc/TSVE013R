      /copy SYSPEDS/qrpgsrcpy,hspecs
      /copy SYSPEDS/qrpgsrcpy,hspecsbnd
      *********************************************************************
     FBRIDF     UF   E           K DISK    USROPN
     FSVEH      UF   E           K DISK    USROPN
N01  FEDIC      UF   E             DISK    USROPN
N01  FSVTFI     IF   E             DISK    USROPN
N01  FEDIM      O  A E             DISK    USROPN
      *--------------------------------------------------------------------
      * Variables common to all CGIs
      *--------------------------------------------------------------------
      /copy SYSPEDS/qrpgsrcpy,prototypeb
      /copy SYSPEDS/qrpgsrcpy,usec
      /copy SYSPEDS/qrpgsrcpy,variables3
      *
      * Varible for
     D WSUSER          S             10
     D AVD             S              4
     D AVDA            S              4
     D AVDN            S              4  0
     D OPD             S              7
     D OPDA            S              7
     D OPDN            S              7  0
     D JSONstring      s          32000
     D Error           S            150
     D AntLest         S              9  0
N01   *   SYEDI data area
N01  D EDIDTA          DS           256
N01  D  UELIBF               171    180

      *get input-string
      /copy syspeds/qrpgsrcpy,prolog3

      * Parse input
     C                   exsr      Parse
      * Open files etc
     C                   EXSR      INIT
      * skicka
     C                   EXSR      SRTE
      *
     c                   callp     gethtmlifs(
     C                             '/syspeddev/html/JSON.html':
     C                             '/CGITAG/')
     C                   callp     wrtsection('top')
      *
      * ............................................................................................
      * skriv JSON-Object start..(alltid '{' + x ant param. m/komma mellom (IKKE komma etter siste))
      * ............................................................................................
      *
      /free
        JSONstring='{'
        +'¬user¬ : ¬'+%trim(WSUSER)+'¬, '
        +'¬avd¬ : ¬'+%trim(%editc(AVDN:'Z'))+'¬, '
        +'¬opd¬ : ¬'+%trim(%editc(OPDN:'Z'))+'¬, '
        +'¬errMsg¬ : ¬'+%trim(Error)+'¬, ';
      /end-free
      * JSONfix escaper " i tekst,  for deretter å bytte ¬->"
     c                   call      'JSONFIX'
     c                   parm                    JSONstring
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')
      *
      * ............................................................................................
      * Skriv JSON-LISTE Start (en liste er EN parameter(fra [ til   )
      * ............................................................................................
     C                   eval      JSONstring ='"skicka" : ['
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')
     c                   eval      JSONstring =']'
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')
      * ............................................................................................
      * Skriv JSON-string Avsluttning
      * ............................................................................................
     c                   eval      JSONstring ='}'
     C                   CALLP     updHTMLvar2('JSONstring'
     C                                         :%addr(JSONstring)
     C                                         :%len(JSONstring))
     C                   callp     wrtsection('JSONlin')
      *
      * Quit
     C                   exsr      Exit


      *=====================================================================
      * Close output html and quit
      *=====================================================================
     C     Exit          begsr
      * Lukk fil
     C                   CLOSE     BRIDF
     C  N50              CLOSE     SVEH
N01  C  N50              CLOSE     EDIC
N01  C  N50              CLOSE     SVTFI
N01  C  N50              CLOSE     EDIM

      * Do not delete the call to wrtsection with section name *fini.  It is needed
      * to ensure that all output html that has been buffered gets output.
     C                   callp     wrtsection('*fini')
      * Quit
     C                   MOVE      *ON           *INLR
     C                   return
     C                   endsr
      *
      *********************************************************
     C     Parse         Begsr
      *********************************************************
      * Get input
     C                   EVAL      WSUSER  = zhbgetvar('USER')
     C                   EVAL      AVD     = zhbgetvar('AVD')
     C                   eval      AVDN    = c2n2(AVD)
     C                   MOVE      AVDN          AVDA
     C                   EVAL      OPD     = zhbgetvar('OPD')
     C                   eval      OPDN    = c2n2(OPD)
     C                   MOVE      OPDN          OPDA
     c                   endsr
      *
      *********************************************************
     C     INIT          Begsr
      *********************************************************
     C                   OPEN      BRIDF
      * Test innlogging
     C     WSUSER        CHAIN     BRIDF                              50
     C     BILIBL        ifeq      *blanks
     C                   callb     'INCGI'
     C                   parm                    BISTDL
     C                   else
     C                   call      BILIBL
     C                   end
      *
     C     SvKey         klist
     C                   kfld                    AVDN
     C                   kfld                    OPDN
      *
     C   50              eval      Error = 'Invalid userID'
     C                   IF        *IN50 = *OFF
     C                   OPEN      SVEH
N01  C                   OPEN      EDIC
N01  C                   OPEN      SVTFI
N01  C                   OPEN      EDIM
N01   * Hæmta firmaupplysningar
N01  C     1             CHAIN     SVTFI                              51
N01   * Hæmta næsta meddelandenummer
N01  C     1             CHAIN     EDIC                               51
N01  C                   ADD       1             CMN
N01  C                   UPDATE    EDICR
N01   * Hæmta dagens datum och klockslag
N01  C                   CALL      'SYGE15C'
N01  C                   PARM                    WDT               8
N01  C                   PARM                    WTM               6
N01   * Åpne dataarea
N01  C     *DTAARA       DEFINE                  EDIDTA
N01  C                   IN        EDIDTA
     C                   ENDIF

     c                   endsr
      *___________________________________________
      * Signera TDS Export                    SRTE
      *"""""""""""""""""""""""""""""""""""""""""""
     C     SRTE          BEGSR
      *____________________
      * Skapa edifact + fil
      *""""""""""""""""""""
     C                   CALL      'SVE021R'
     C                   PARM                    AVDN
     C                   PARM                    OPDN
      *_____________
      * Ændra status
      *"""""""""""""
     C     SVKEY         CHAIN     SVEH                               52
S01  C*                  EVAL      SVEH_SYST='S'
N01  C                   EVAL      SVEH_SYST='A'
     C                   UPDATE    SVEHR
N01   *_______________
N01   * Skriv til EDIM
N01   *"""""""""""""""
N01  C                   CLEAR                   EDIMR
N01  C                   EVAL      M0004=SVTF_0004
N01  C                   EVAL      M0010=SVTF_0010
N01  C                   EVAL      M0035=SVEH_0035
N01  C                   MOVEL     SVEH_SYAV     W0062            11
N01  C                   MOVE      SVEH_SYOP     W0062
N01  C                   EVAL      M0062=W0062
N01  C                   EVAL      M0065='CUSDEC'
N01  C                   EVAL      M1001=SVEH_MTYP
N01  C                   EVAL      M1004=SVEH_TUID
N01  C                   EVAL      M1225=SVEH_MTYP
N01  C                   EVAL      M1N07='SVE'
N01  C                   EVAL      MMN=CMN
N01  C                   EVAL      MSR='S'
N01  C                   MOVEL     WDT           MDT
N01  C                   MOVEL     WTM           MTM
N01  C                   EVAL      MAVD=SVEH_SYAV
N01  C                   EVAL      MTDN=SVEH_SYOP
N01
N01  C                   WRITE     EDIMR
     C                   ENDSR
