             PGM
             DCL        VAR(&GRU) TYPE(*CHAR) LEN(8) /*GRUPPE*/
             DCL        VAR(&KJE) TYPE(*CHAR) LEN(8) /*KJEDE/KUNDE*/
             DCL        VAR(&DTF) TYPE(*CHAR) LEN(8) /*DATO FRA*/
             DCL        VAR(&DTT) TYPE(*CHAR) LEN(8) /*DATO TIL*/
             DCL        VAR(&TIDLIG) TYPE(*CHAR) LEN(1) /*Tidligere +
                          post med?*/
             DCL        VAR(&PRØV) TYPE(*CHAR) LEN(1) /*DATO TIL*/
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10) /*PRINTER*/
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1) VALUE('0') /* +
                          AVSLUTT*/
    /* */
    /* UTSKRIFT AV TELLELISTE */
    /* */
             CALL       PGM(LL056R) PARM(&GRU &KJE &DTF &DTT &TIDLIG +
                          &PRØV &PRT &IN01)
             IF         COND(&IN01 = '0') THEN(DO)
             CALL       PGM(LL057C) PARM(&GRU &KJE &DTF &DTT &TIDLIG +
                          &PRØV &PRT)
             SNDPGMMSG  MSG('Liste er sendt til printeren')
             ENDDO

             ENDPGM
