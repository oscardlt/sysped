     C                   MOVE      'DIV1037'     MSGNR
     C                   SETON                                        30
      *
     C     SLSØK2        ENDSR
      *
     C***********************************************
     C     SØKDT3        BEGSR
     C***********************************************
     C     POSNBR        IFGE      23
     C     POSNBR        ANDLE     60
     C     POSNBR        SUB       18            X2
     C                   DIV       5             X2
     C                   MVR                     XN
     C                   ELSE
     C                   Z-ADD     5             XN
     C                   END
      *
     C     LINNBR        IFEQ      3
     C     XN            ANDLT     3
     C                   MOVE      LI(X2)        ULIN              3 0
     C                   CALL      'SYFI10K'
     C                   PARM                    ULIN
     C                   MOVE      ULIN          LI(X2)
     C     X2            IFEQ      8
     C                   Z-ADD     4             LINNBR
     C                   Z-ADD     23            POSNBR
     C                   ELSE
     C     X2            ADD       1             POSNBR
     C                   MULT      5             POSNBR
     C                   ADD       18            POSNBR
     C                   END
     C                   GOTO      SLSØK3
     C                   END
      *
     C     LINNBR        IFEQ      4
     C     XN            ANDLT     3
     C                   MOVE      *BLANKS       UNVN
     C                   MOVE      *BLANKS       ULK
     C                   MOVE      *BLANKS       UFR
     C                   MOVEL     FR(X2)        UFR
     C                   CALL      'SYFI10F'
     C                   PARM                    UFR               5
     C                   PARM                    UNVN             30
     C                   PARM                    ULK               2
     C                   MOVEL     UFR           FR(X2)
     C     X2            IFEQ      8
     C                   Z-ADD     5             LINNBR
     C                   Z-ADD     23            POSNBR
     C                   ELSE
     C     X2            ADD       1             POSNBR
     C                   MULT      5             POSNBR
     C                   ADD       18            POSNBR
     C                   END
     C                   GOTO      SLSØK3
     C                   END
      *
     C     LINNBR        IFEQ      5
     C     XN            ANDLT     3
     C                   MOVE      *BLANKS       UNVN
     C                   MOVE      *BLANKS       ULK
     C                   MOVE      *BLANKS       UTO
     C                   MOVEL     TO(X2)        UTO
     C                   CALL      'SYFI10F'
     C                   PARM                    UTO               5
     C                   PARM                    UNVN             30
     C                   PARM                    ULK               2
     C                   MOVEL     UTO           TO(X2)
     C     X2            IFEQ      8
     C                   Z-ADD     23            POSNBR
     C                   ELSE
     C     X2            ADD       1             POSNBR
     C                   MULT      5             POSNBR
     C                   ADD       18            POSNBR
     C                   END
     C                   GOTO      SLSØK3
     C                   END
