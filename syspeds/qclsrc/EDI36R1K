/*N01 KOPIERER TIL FEIL MAPPE */
/*N02 KOPIERER TIL FEIL MAPPE */
             PGM        PARM(&INID &INIFSR &OBJ)

             DCL        VAR(&INID) TYPE(*CHAR) LEN(35)
             DCL        VAR(&INIFSR) TYPE(*CHAR) LEN(64)
             DCL        VAR(&OBJ) TYPE(*CHAR) LEN(256)
             DCL        VAR(&TODIR) TYPE(*CHAR) LEN(256)
             DCL        VAR(&LIB) TYPE(*CHAR) LEN(10)

             MONMSG     MSGID(CPF0000)

        /*_____________________*/
        /* HENT BIBLIOTEKSNAVN */
        /*"""""""""""""""""""""*/
             RTVDTAARA  DTAARA(EDIDTA (171 10)) RTNVAR(&LIB)
/*_________________________*/
