             PGM
             DCL        VAR(&USEPDF) TYPE(*CHAR) LEN(10) +
                          VALUE('TRANSAVR  ')
             DCL        VAR(&ATYPE) TYPE(*CHAR) LEN(2) +
                          VALUE('ta')
             DCL        VAR(&FINS) TYPE(*CHAR) LEN(2) +
                          VALUE('N')

             SBMJOB     CMD(CALL PGM(SYFA39CL)) JOB(STATOPTRA) +
                          JOBD(*USRPRF) JOBQ(SYJOBQAV)
/*  OPPDATRER SELVE AVREGNINGENE TIL PDF ARKIV  */
             SBMJOB     CMD(CALL PGM(ARC002CL) PARM(&ATYPE &USEPDF +
                          &FINS)) JOB(ARKIV) JOBD(*USRPRF) +
                          JOBQ(SYJOBQARK)

             ENDPGM
