             PGM        PARM(&KNA &TRF &KNAN1 &KNAN2 &KNAN3 &FPFO &FDAT +
                             &TDAT &SAKN &FLG &PRT)
  /* */
             DCL        VAR(&KNA) TYPE(*CHAR) LEN(8)
             DCL        VAR(&TRF) TYPE(*CHAR) LEN(8)
             DCL        VAR(&KNAN1) TYPE(*CHAR) LEN(8)
             DCL        VAR(&KNAN2) TYPE(*CHAR) LEN(8)
             DCL        VAR(&KNAN3) TYPE(*CHAR) LEN(8)
             DCL        VAR(&FPFO) TYPE(*CHAR) LEN(5)
             DCL        VAR(&FDAT) TYPE(*CHAR) LEN(6)
             DCL        VAR(&TDAT) TYPE(*CHAR) LEN(6)
             DCL        VAR(&SAKN) TYPE(*CHAR) LEN(7)
             DCL        VAR(&FLG) TYPE(*CHAR) LEN(1)
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10)
 /*  */
             CALL       PGM(SYIS42C) PARM(&KNA &TRF &KNAN1 &KNAN2 +
                          &KNAN3 &FPFO &FDAT &TDAT &SAKN &FLG &PRT)
             GOTO       CMDLBL(SLUTT)
             SBMJOB     CMD(CALL PGM(SYIS42C) PARM(&KNA &TRF &KNAN1 +
                          &KNAN2 &KNAN3 &FPFO &FDAT &TDAT &SAKN +
                          &FLG &PRT)) JOB(AVVIKRPT) JOBD(*USRPRF) +
                          JOBQ(SYJOBQPRT)
 /*  */
 SLUTT:      ENDPGM
