             PGM        PARM(&AVD &FPER &FAAR &TPER &TAAR +
                             &FORM &PRT &SPLQ &ANT &SAV &HLD)
  /* */
             DCL        VAR(&AVD) TYPE(*CHAR) LEN(4)
             DCL        VAR(&FPER) TYPE(*CHAR) LEN(2)
             DCL        VAR(&FAAR) TYPE(*CHAR) LEN(2)
             DCL        VAR(&TPER) TYPE(*CHAR) LEN(2)
             DCL        VAR(&TAAR) TYPE(*CHAR) LEN(2)
             DCL        VAR(&FRAPER) TYPE(*CHAR) LEN(4)
             DCL        VAR(&TILPER) TYPE(*CHAR) LEN(4)
             DCL        VAR(&BRUKER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&DATO) TYPE(*CHAR) LEN(6)
             DCL        VAR(&TID) TYPE(*CHAR) LEN(8)
             DCL        VAR(&FORM) TYPE(*CHAR) LEN(10)
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10)
             DCL        VAR(&SPLQ) TYPE(*CHAR) LEN(10)
             DCL        VAR(&ANT) TYPE(*CHAR) LEN(2)
             DCL        VAR(&SAV) TYPE(*CHAR) LEN(1)
             DCL        VAR(&HLD) TYPE(*CHAR) LEN(1)
             DCL        VAR(&WSAV) TYPE(*CHAR) LEN(4)
             DCL        VAR(&WHLD) TYPE(*CHAR) LEN(4)
 /*  */
             CLRPFM     G1WREG
             CLRPFM     GBWREG
 /*  */
             RTVJOBA    USER(&BRUKER) DATE(&DATO)
             RTVSYSVAL  SYSVAL(QTIME) RTNVAR(&TID)
             CHGVAR     VAR(&FRAPER) VALUE(&FAAR *CAT &FPER)
             CHGVAR     VAR(&TILPER) VALUE(&TAAR *CAT &TPER)
 /*  */
             IF         COND(&SAV *EQ 'J') THEN(CHGVAR VAR(&WSAV) +
                          VALUE('*YES'))
             IF         COND(&SAV *EQ ' ') THEN(CHGVAR VAR(&WSAV) +
                          VALUE('*NO'))
             IF         COND(&HLD *EQ 'J') THEN(CHGVAR VAR(&WHLD) +
                          VALUE('*YES'))
             IF         COND(&HLD *EQ ' ') THEN(CHGVAR VAR(&WHLD) +
                          VALUE('*NO'))
 /*  */
 /*  */
             CALL       PGM(SYGB03R) PARM(&AVD &FPER &FAAR &TPER &TAAR)
             CALL       PGM(SYGB03R1) PARM(&AVD &FPER &FAAR &TPER +
                          &TAAR)
             CALL       PGM(SYGB03R2) PARM(&AVD &FPER &FAAR &TPER +
                          &TAAR)
 /*  */
             OVRPRTF    FILE(SYGB04P) OUTQ(&PRT) FORMTYPE(&FORM) +
                          COPIES(&ANT) HOLD(&WHLD) SAVE(&WSAV)
             OVRPRTF    FILE(SYGB04P1) OUTQ(&PRT) FORMTYPE(&FORM) +
                          COPIES(&ANT) HOLD(&WHLD) SAVE(&WSAV)
 /*  */
             CALL       PGM(SYGB04R) PARM(&FRAPER &TILPER &BRUKER +
                          &DATO &TID)
 /*  */
             CLRPFM     G1WREG
             CLRPFM     GBWREG
 /*  */
 SLUTT:      ENDPGM
