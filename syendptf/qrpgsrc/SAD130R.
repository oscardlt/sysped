     C***********************************************
     C     DSPLOG        BEGSR
     C***********************************************
     C                   MOVE      '8'           WIN03             1
     C                   MOVE      *ZEROS        XAN11            11
     C                   MOVE      EFPRO         XAN11
     C                   MOVEL     XAN11         U_AVD
     C                   MOVE      XAN11         U_TDN
     C                   CALL      'SAD003'
     C                   PARM                    U_AVD             4 0
     C                   PARM                    U_TDN             7 0
     C                   PARM                    WIN03
     C     WIN03         IFEQ      '1'
     C                   MOVE      '1'           *INLR
     C                   RETURN
     C                   END
     C  N90              MOVE      *ZEROS        WSNR
     C  N90              UPDATE    SUBFIL
