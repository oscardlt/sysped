     C     UTCMDL        BEGSR
     C***********************************************
     C     XCMD          IFEQ      0
     C                   MOVE      'DIV0032'     MSGLN1
     C                   MOVE      'DIV0033'     MSGLN2
     C                   MOVE      'DIV3572'     MSGLN3
     C                   MOVE      'DIV0029'     MSGLN4
     C                   ADD       1             XCMD
     C                   ELSE
     C     XCMD          IFEQ      1
     C                   MOVE      'DIV0037'     MSGLN1
     C  N31              MOVE      'DIV0020'     MSGLN2
     C   31              MOVE      'DIV0024'     MSGLN2
     C                   MOVE      'DIV0000'     MSGLN3
     C  N31              ADD       1             XCMD
     C   31              Z-ADD     0             XCMD
     C                   ELSE
     C     XCMD          IFEQ      2
     C                   MOVE      'DIV0030'     MSGLN1
     C                   MOVE      'DIV0031'     MSGLN2
     C                   MOVE      'DIV1008'     MSGLN3
     C                   ADD       1             XCMD              1 0
     C                   ELSE
     C     XCMD          IFEQ      3
     C                   MOVE      'DIV0024'     MSGLN1
     C                   MOVE      'DIV0985'     MSGLN2
     C                   MOVE      'SHU0127'     MSGLN3
     C                   ADD       1             XCMD
     C                   ELSE
n21  C     XCMD          IFEQ      4
n21  C                   ADD       1             XCMD
     C                   MOVE      'DIV0041'     MSGLN1
     C                   MOVE      'CBA0160'     MSGLN2
     C                   MOVE      'JOV0803'     MSGLN3
n21  C                   ELSE
