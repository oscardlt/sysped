             PGM
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10) /* Printernavn +
                          til tolldeklarasjon. */
             DCL        VAR(&PT) TYPE(*CHAR) LEN(10) /* Papirtype til +
                          tolldeklarasjon. */
             DCL        VAR(&PRANT) TYPE(*CHAR) LEN(2) /* Antall +
                          utskrifter. */
    /* */
             RTVDTAARA  DTAARA(*LDA (601 10)) RTNVAR(&PRT)
             RTVDTAARA  DTAARA(*LDA (611 10)) RTNVAR(&PT)
             OVRPRTF    FILE(TOLLDEK) OUTQ(&PRT) PAGESIZE(72 85) +
                          CPI(10) OVRFLW(72) FORMTYPE(&PT)
             CALL       PGM(SYFA41) PARM(&PRANT)
             SNDPGMMSG  MSG(' ' *BCAT &PRANT *BCAT 'tolldeklarsjoner +
                          klare til utskrift.' *BCAT &PRT *BCAT ' ') +
                          MSGTYPE(*INFO)
    /* */
             ENDPGM
