     C***********************************************
     C     CHGSAD        BEGSR
     C***********************************************
     C*** FLYTTE DATA FRA SUBFIL TIL ARBEIDSVARIABLER
     C  N38              DO
     C     XTVFA2        IFEQ      0
     C     SVBELT        MULT(H)   XTVFAK        XPROGR
     C                   ELSE
     C     SVBELT        MULT(H)   XTVFA2        XPROGR
     C                   END
     C                   Z-ADD     SVVKTN        XVEKGR
     C                   Z-ADD     SVNTM         XSTKGR
     C                   END
     C                   MOVE      *BLANKS       MSGTX2
     C                   EXSR      MOVSVW
N33  C                   IF        UTDN > 0
N33  C                   EVAL      *IN28 = *OFF
N33  C                   ELSE
N33  C     SVPREÆ        COMP      'J'                                2828
N33  C                   END
N33  C                   EVAL      XSVPREÆ = SVPREÆ
     C*
     C     STCHG         TAG
     C                   EXFMT     VARELI
     C                   MOVEA     '00000000'    *IN(82)
     C                   MOVEA     '000000000'   *IN(73)
     C                   SETOFF                                       30
     C                   MOVE      *BLANKS       MSGTX2
      *
     C   11              MOVE      'J'           XF11
     C  N11              MOVE      'N'           XF11
     C   21              MOVE      'J'           XF21
     C  N21              MOVE      'N'           XF21
     C*
     C   04              DO
     C                   EXSR      SØKDTA
     C                   MOVEA     '11'          *IN(91)
     C                   WRITE     SUBCTL
     C     ZRECNO        CHAIN     SUBFIL                             33
     C                   GOTO      STCHG
     C                   END
     C*  07 = SØKE OPPDRAG
     C   07              DO
     C                   MOVE      'S'           UTYPE             1
     C                   CALL      'SYFM21'
     C                   PARM                    UTYPE
     C                   PARM                    UAVD
     C                   PARM                    UTDN
     C                   PARM                    USG               3
     C                   GOTO      STCHG
     C                   END
N44  C* F20 = Frie søkeveier
N44  C                   IF        *IN20
N44  C                   CALL      'SYFA64'
N44  C                   PARM                    UAVD
N44  C                   PARM                    UTDN
N44  C                   PARM                    NULL80            8 0
N44  C                   PARM                    NULL80            8 0
N44  C                   GOTO      STCHG
N44  C                   ENDIF
      *
     C   18              GOTO      STCHG
     C*
     C*** SJEKK OM MAN HAR TASTET <DUP>-TAST
     C  N12              EXSR      SRDUP
     C*
     C*** F12 ---> AVBRYTTE ENDRING
     C     *IN12         IFEQ      '1'
     C     SVVKTB        COMP      0                                      27
     C                   UPDATE    SUBFIL
     C                   ELSE
     C*** TESTE INNTASTEDE DATA. HVIS IKKE NOE FEIL, OPPDATERE PÅ FIL
     C                   EXSR      TESTB1
     C   30              GOTO      STCHG
      * OPPDATER JUSTERINGSBELØP
     C     SADVK         CHAIN(N)  SADVJ                              33
     C     *IN33         IFEQ      '0'
     C                   MOVE      SJVAL         W3VAL
     C                   MOVE      SJBEL         W3BEL
     C                   MOVE      SJPROS        W3PROS
     C                   Z-ADD     SJBELN        W3BELN
     C                   MOVE      '1'           *IN16
     C                   ELSE
     C                   MOVE      *ZEROS        W3BEL
     C                   MOVE      *ZEROS        W3PROS
     C                   MOVE      *ZEROS        W3BELN
     C                   END
