     C                   Z-ADD     WREP0         ØN153
     C                   EXSR      SRNE
     C                   MOVEL     ØAN           L1082
     C                   EVAL      L7140 = %TRIML(Z_VARENR)
     C                   MOVEL     'SA'          L7143
     C                   WRITE     LINR
     C                   EXSR      PIA22
     C                   EXSR      IMD22
     C                   EXSR      QTY22
     C                   EXSR      ALI22
     C                   EXSR      MOA23
     C                   EXSR      PRI25
     C                   EXSR      ALC35

     C     SSKEY         READE     SSTAL02                                53
