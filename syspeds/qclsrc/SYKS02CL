             PGM
             DCL        VAR(&BNR) TYPE(*DEC) LEN(7 0)
             DCL        VAR(&FLAGG) TYPE(*DEC) LEN(1 0)
             DCL        VAR(&PGM) TYPE(*CHAR) LEN(10) +
                          VALUE('KOSTFØRING')
             CALL       PGM(LOGUSR) PARM(&PGM)
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CHGDTAARA  DTAARA(*LDA (15 4)) VALUE('0000')
                        /*Avd. MÅ VÆRE DEFINERT I LDA FOR SYTR88 */
             OVRDBF     FILE(KOSTBVIS) TOFILE(KOSTB)
             CALL       PGM(SYKS02) PARM(&BNR &FLAGG)
             ENDPGM
