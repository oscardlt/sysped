             PGM
          /*_____________________*/
          /* Deklarere Variabler */
          /*"""""""""""""""""""""*/
             DCLF       FILE(EDI09F2)
             DCL        VAR(&DLIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&OLIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&MBNAME) TYPE(*CHAR) LEN(10)
             DCL        VAR(&MBFILE) TYPE(*CHAR) LEN(10)
             RTVDTAARA  DTAARA(EDIDTA (171 10)) RTNVAR(&DLIB)
             RTVDTAARA  DTAARA(EDIDTA (181 10)) RTNVAR(&OLIB)
          /*__________________________________________*/
          /* Endre navn på gamle filer I Historikklib */
          /*""""""""""""""""""""""""""""""""""""""""""*/
             DLTF       FILE(&OLIB/EDIS_)
             MONMSG     MSGID(CPF0000)
             DLTF       FILE(&OLIB/EDIM_)
             MONMSG     MSGID(CPF0000)
             DLTF       FILE(&OLIB/EDIRE_)
             MONMSG     MSGID(CPF0000)
             DLTF       FILE(&OLIB/EDIRE1K_)
             MONMSG     MSGID(CPF0000)
             DLTF       FILE(&OLIB/EDISE_)
             MONMSG     MSGID(CPF0000)
             DLTF       FILE(&OLIB/EDISE1K_)
             MONMSG     MSGID(CPF0000)
             RNMOBJ     OBJ(&OLIB/EDIS) OBJTYPE(*FILE) NEWOBJ(EDIS_)
             RNMOBJ     OBJ(&OLIB/EDIM) OBJTYPE(*FILE) NEWOBJ(EDIM_)
             RNMOBJ     OBJ(&OLIB/EDIRE) OBJTYPE(*FILE) NEWOBJ(EDIRE_)
             RNMOBJ     OBJ(&OLIB/EDIRE1K) OBJTYPE(*FILE) NEWOBJ(EDIRE1K_)
             RNMOBJ     OBJ(&OLIB/EDISE) OBJTYPE(*FILE) NEWOBJ(EDISE_)
             RNMOBJ     OBJ(&OLIB/EDISE1K) OBJTYPE(*FILE) NEWOBJ(EDISE1K_)
          /*___________________________*/
          /* Endre navn på gamle filer */
          /*"""""""""""""""""""""""""""*/
             DLTF       FILE(EDIRE_)
             MONMSG     MSGID(CPF0000)
             DLTF       FILE(EDIRE1K_)
             MONMSG     MSGID(CPF0000)
             DLTF       FILE(EDISE_)
             MONMSG     MSGID(CPF0000)
             DLTF       FILE(EDISE1K_)
             MONMSG     MSGID(CPF0000)
             RNMOBJ     OBJ(EDIRE) OBJTYPE(*FILE) NEWOBJ(EDIRE_)
             RNMOBJ     OBJ(EDIRE1K) OBJTYPE(*FILE) NEWOBJ(EDIRE1K_)
             RNMOBJ     OBJ(EDISE) OBJTYPE(*FILE) NEWOBJ(EDISE_)
             RNMOBJ     OBJ(EDISE1K) OBJTYPE(*FILE) NEWOBJ(EDISE1K_)
          /*____________________*/
          /* Kompiler nye filer */
          /*""""""""""""""""""""*/
             CRTPF      FILE(&DLIB/EDIRE) SRCFILE(SYEDIS/QDDSSRC) +
                          SIZE(*NOMAX) FRCRATIO(1)
             CRTPF      FILE(&DLIB/EDIRE1K) SRCFILE(SYEDIS/QDDSSRC) +
                          SIZE(*NOMAX) FRCRATIO(1)
             CRTPF      FILE(&DLIB/EDISE) SRCFILE(SYEDIS/QDDSSRC) +
                          SIZE(*NOMAX) FRCRATIO(1)
             CRTPF      FILE(&DLIB/EDISE1K) SRCFILE(SYEDIS/QDDSSRC) +
                          SIZE(*NOMAX) FRCRATIO(1)
          /*______________________*/
          /* Hente filbeskrivelse */
          /*""""""""""""""""""""""*/
             DSPFD      FILE(EDIRE_) TYPE(*MBR) OUTPUT(*OUTFILE) +
                          OUTFILE(EDI09F2)
             DSPFD      FILE(EDIRE1K_) TYPE(*MBR) OUTPUT(*OUTFILE) +
                          OUTFILE(EDI09F2) OUTMBR(*FIRST *ADD)
             DSPFD      FILE(EDISE_) TYPE(*MBR) OUTPUT(*OUTFILE) +
                          OUTFILE(EDI09F2) OUTMBR(*FIRST *ADD)
             DSPFD      FILE(EDISE1K_) TYPE(*MBR) OUTPUT(*OUTFILE) +
                          OUTFILE(EDI09F2) OUTMBR(*FIRST *ADD)
          /*____________________*/
          /* Les filbeskrivelse */
          /*""""""""""""""""""""*/
 READ2:      RCVF       RCDFMT(QWHFDMBR)
             MONMSG     MSGID(CPF0000) EXEC(GOTO CMDLBL(EXIT))
          /*_____*/
          /* CVT */
          /*"""""*/
             IF         COND(&MBNAME *NE '          ') THEN(DO)
             OVRDBF     FILE(&MBFILE) MBR(&MBNAME)
             MONMSG CPF0000
             IF         COND(&MBFILE *EQ EDIRE_) THEN(CALL +
                          PGM(CVTEDIR1) PARM(&MBNAME))
             IF         COND(&MBFILE *EQ EDIRE1K_) THEN(CALL +
                          PGM(CVTEDIR2) PARM(&MBNAME))
             IF         COND(&MBFILE *EQ EDISE_) THEN(CALL +
                          PGM(CVTEDIR3) PARM(&MBNAME))
             IF         COND(&MBFILE *EQ EDISE1K_) THEN(CALL +
                          PGM(CVTEDIR4) PARM(&MBNAME))
             ENDDO

             GOTO READ2

EXIT:        ENDPGM
