             PGM
             DSPOBJD    OBJ(SYENDRE/*ALL) OBJTYPE(*QRYDFN) +
                          DETAIL(*FULL) OUTPUT(*OUTFILE) +
                          OUTFILE(QTEMP/QRY)
             CRTDUPOBJ  OBJ(SYQRYF) FROMLIB(*LIBL) OBJTYPE(*FILE) +
                          TOLIB(QTEMP)
             MONMSG     MSGID(CPF0000)
             CPYF       FROMFILE(QTEMP/QRY) TOFILE(QTEMP/SYQRYF) +
                          MBROPT(*REPLACE) FMTOPT(*MAP *DROP)
             CALL       PGM(SYQ001)
             ENDPGM
