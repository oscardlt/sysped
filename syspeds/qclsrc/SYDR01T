/* SKJEKKER OM DOKUMENT FINNES       */
/*                         */
             PGM        PARM(&SBS &PGM &LIB &AKT)
             DCL        VAR(&SBS) TYPE(*CHAR) LEN(10)
             DCL        VAR(&PGM) TYPE(*CHAR) LEN(10)
             DCL        VAR(&LIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&AKT) TYPE(*CHAR) LEN(1)
             DCL        VAR(&QUSER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QJOB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QNBR) TYPE(*CHAR) LEN(6)
             CHGVAR     VAR(&AKT) VALUE('J')
             RTVJOBA    JOB(&QJOB) USER(&QUSER) NBR(&QNBR)
             DSPACTPJ   SBS(&SBS) PGM(&LIB/&PGM) OUTPUT(*PRINT)
/* FINNES IKKE */
             MONMSG     MSGID(CPF1834 CPF1833 CPF9811) EXEC(DO)
             CHGVAR     VAR(&AKT) VALUE('N')
             GOTO       CMDLBL(SLUTT)
             ENDDO
/* FINNES      */
             DLTSPLF    FILE(QSYSPRT) JOB(&QNBR/&QUSER/&QJOB) +
                          SPLNBR(*LAST)
             MONMSG     MSGID(CPF0000)
SLUTT:       DMPCLPGM
             ENDPGM
