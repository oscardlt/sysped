/* KONVETERER CSV TIL DB2      */
/*                         */
             PGM        PARM(&IFSINN &SKILLE &DB2UT &LIBUT)
             DCL        VAR(&IFSINN) TYPE(*CHAR) LEN(50)
             DCL        VAR(&SKILLE) TYPE(*CHAR) LEN(1)
             DCL        VAR(&DB2UT) TYPE(*CHAR) LEN(10)
             DCL        VAR(&LIBUT) TYPE(*CHAR) LEN(10)
/****************************************************************/
/*  KOPIERE UTFIL TIL DATABASEBESKRIVELSE FIL  QTEMP/DB2UTF     */
/****************************************************************/
             DSPFFD     FILE(&LIBUT/&DB2UT) OUTPUT(*OUTFILE) +
                          OUTFILE(QTEMP/DB2DEF)
/****************************************************************/
/*  LAGE NY DB2PACK                                             */
/****************************************************************/
             CRTPF      FILE(QTEMP/DB2PACK) RCDLEN(64)
             MONMSG     MSGID(CPF0000)
             CLRPFM     FILE(QTEMP/DB2PACK)
/****************************************************************/
/*  LAGE NY UTFIL                                               */
/****************************************************************/
/*           CRTPF      FILE(QTEMP/DB2NY) RCDLEN(2048)     */
             CRTPF      FILE(QTEMP/DB2NY) RCDLEN(4096)
             MONMSG     MSGID(CPF0000)
             CLRPFM     FILE(QTEMP/DB2NY)
/****************************************************************/
/*  LAGER NY IFSFIL    IFSINNF/QTEMP                            */
/****************************************************************/
/*           CRTPF      FILE(QTEMP/IFSINNF) RCDLEN(1024)        */
             CRTPF      FILE(QTEMP/IFSINNF) RCDLEN(4096)
             MONMSG     MSGID(CPF0000)
             CLRPFM     FILE(QTEMP/IFSINNF)
/****************************************************************/
/*  KOPIERER IFSFIL INN TIL    IFSINNF/QTEMP                    */
/****************************************************************/
             CPYFRMSTMF FROMSTMF(&IFSINN) +
                          TOMBR('qsys.lib/qtemp.lib/ifsinnf.file/ifsi+
                          nnf.mbr') MBROPT(*REPLACE)
/****************************************************************/
/*  KALLER BEHANDLINGSPROGRAM                                    */
/****************************************************************/
             CALL       PGM(SYGE119) PARM(&IFSINN &SKILLE &DB2UT +
                          &LIBUT)
/****************************************************************/
/*  BYTTER UT DB2FIL                                            */
/****************************************************************/
             CPYF       FROMFILE(QTEMP/DB2NY) TOFILE(&LIBUT/&DB2UT) +
                          MBROPT(*REPLACE) FMTOPT(*NOCHK)
 SLUTT:      ENDPGM
