     H DFTACTGRP(*NO)
     fsadhko    uf   e           k disk
     fsad061fy  cf   e             workstn
     d xavd            s              4  0
     d xtdn            s              7  0
     ‚*  Prototype for sad061y
     d sad061y         pr
     d uavd_                               like(uavd)
     d utdn_                               like(utdn)
     d uflagg_                             like(uflagg)
     ‚*  *ENTRY Interface for Main Procedure
     d sad061y         pi
     d uavd                           4
     d utdn                           7
     d uflagg                         1

      /free
       xavd = %float(uavd);
       xtdn = %float(utdn);
       chain (xavd: xtdn) sadhko;
       if %found;
         if sist = '9';
           select;
           when uflagg = '0';   // Gi beskjed avslutt uten oppdatering pga. melding fra TVINN
             write windowsz;
             exfmt bilde01;
             uflagg = '9';
           when uflagg = '1';   // Ny start: Endre status til 8 for å slutte test senere
             sist = '8';
           other;               // Ingen aksjon
             uflagg = '0';
           endsl;
         endif;
         update sadhkor;
       else;
         uflagg = '0';          // Ingen aksjon
       endif;

       *inlr = *on;
       return;

      /end-free
