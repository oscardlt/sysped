             MD         DIR(symail)
             MONMSG     MSGID(CPFA0A0)
             CPYTOSTMF  +
                          FROMMBR('QSYS.LIB/QTEMP.LIB/TOPCF.FILE/TOPC+
                          F.MBR') TOSTMF('/symail/test') +
                          STMFOPT(*REPLACE) CVTDTA(*AUTO) +
                          STMFCODPAG(*PCASCII)
             DEL        OBJLNK('/qdls/symail/test')
             MOV        OBJ('/symail/test') TOOBJ('qdls/symail/test')
             SNDDST     TYPE(*DOC) TOINTNET(('janottar@systema.no')) +
                          DSTD('DSTD-EMNE') MSG('MSG-EMNE') +
                          SNDFMT(*NOCHG) DOC(TEST) FLR(SYMAIL) +
                          DOCD('DOCD-EMNE')
             ENDPGM
