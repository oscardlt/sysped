                          CRTFILE(*YES)
             CPYF       FROMFILE(SYSPEDF/SVTX23F) +
                          TOFILE(SVTTAX/SVTX23F) MBROPT(*REPLACE) +
                          CRTFILE(*YES)
             CPYF       FROMFILE(SYSPEDF/SVTX23F2) +
                          TOFILE(SVTTAX/SVTX23F2) MBROPT(*REPLACE) +
                          CRTFILE(*YES)
             CPYF       FROMFILE(SYSPEDF/SVTX24F) +
                          TOFILE(SVTTAX/SVTX24F) MBROPT(*REPLACE) +
                          CRTFILE(*YES)
             CPYF       FROMFILE(SYSPEDF/SVTX31F) +
                          TOFILE(SVTTAX/SVTX31F) MBROPT(*REPLACE) +
                          CRTFILE(*YES)
             CPYF       FROMFILE(SYSPEDF/SVTVK) +
                          TOFILE(SVTTAX/SVTVK) MBROPT(*REPLACE) +
                          CRTFILE(*YES)
     /* SAVE TGTRLS 7.1 P.G.A. FORSJKELLER HOS KUNDER */
             CLRSAVF    FILE(QGPL/SVTTAX)
             SAVLIB     LIB(SVTTAX) DEV(*SAVF) SAVF(QGPL/SVTTAX) +
                          TGTRLS(*PRV)
             ENDPGM
