             PGM
        /*_________________*/
        /* Allocate Object */
        /*"""""""""""""""""*/
             ALCOBJ     OBJ((EDIBMIN/EBAACCP *FILE *EXCL))
             MONMSG     MSGID(CPF0000) EXEC(GOTO CMDLBL(EXIT))
        /*___________*/
        /* COPY File */
        /*"""""""""""*/
             CPYF       FROMFILE(EDIBMIN/EBAACCP) TOFILE(EDI79F1K) +
                          FROMMBR(*FIRST) TOMBR(*FROMMBR) +
                          MBROPT(*ADD) FMTOPT(*NOCHK)
             MONMSG     MSGID(CPF0000)
             CLRPFM     FILE(EDIBMIN/EBAACCP)
             MONMSG     MSGID(CPF0000)
        /*______*/
        /* Exit */
        /*""""""*/
 EXIT:       RCLRSC
             DLCOBJ     OBJ((EDIBMIN/EBAACCP *FILE *EXCL))
             MONMSG     MSGID(CPF0000)
             ENDPGM
