             PGM        PARM(&LIB)
             DCL        VAR(&LIB) TYPE(*CHAR) LEN(10)

             CHGCURLIB  CURLIB(&LIB)
             DLTF       FILE(&LIB/EDIIJ1)
             MONMSG     MSGID(CPF0000)
             CRTLF      FILE(&LIB/EDIIJ1) SRCFILE(SYSPEDS/QDDSSRC) +
                          SRCMBR(EDIIJ1) LVLCHK(*NO)
             DLTF       FILE(&LIB/MULTKODT)
             MONMSG     MSGID(CPF0000)
             CRTLF      FILE(&LIB/MULTKODT) SRCFILE(SYSPEDS/QDDSSRC) +
                          SRCMBR(MULTKODT) LVLCHK(*NO)
             CHGCURLIB  CURLIB(*CRTDFT)

             ENDPGM
