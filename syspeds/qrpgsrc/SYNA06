     C*
     C                   EXSR      INIT
     C*
     C     START         TAG
     C                   EXFMT     BILDE01
     C   03              GOTO      SLUTT
     c   12              goto      slutt
     c
      *
     C     WST           IFNE      *BLANKS
     C     1             DO        8             NR
     C     WS(NR)        IFEQ      '1'
     C                   MOVE      AV(NR)        UAVD
     C                   GOTO      SLUTT
     C                   ENDIF
     C                   ENDDO
     C                   ENDIF
     C                   MOVE      *BLANKS       WS
     C                   GOTO      START
     C     SLUTT         TAG
     C                   SETON                                        LR
     C                   RETURN
     C*
     C***********************************************
     C     INIT          BEGSR
     C***********************************************
     c     *entry        plist
     c                   parm                    levnr             8 0
     c                   parm                    Uavd              4 0
     C     NAVKEY        KLIST
     C                   KFLD                    FIFIRM
     C                   KFLD                    LEVNR
      *
     C     KODJKE        KLIST
     C                   KFLD                    FIFIRM
     C                   KFLD                    KSTED             4 0
      *
     C*
     C                   READ      FIRM                                   33
     C*
     C                   WRITE     WINDOWSZ
     c     navkey        chain     level01                            33
     c     navkey        chain     navlev                             33
      *
     c     1             do        8             nr                2 0
     c                   move      av(nr)        ksted
     c     kodjke        chain     kodjl01                            33
     C  N33              MOVE      KODTXT        KT(NR)
      *
     C     NAVAV1        COMP      *ZEROS                             7171
     C     NAVAV2        COMP      *ZEROS                             7272
     C     NAVAV3        COMP      *ZEROS                             7373
     C     NAVAV4        COMP      *ZEROS                             7474
