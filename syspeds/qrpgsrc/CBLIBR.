     C     FAK2KC        SETLL     FAK3
     C                   ELSE                                                   1
     C     FAK2K         SETLL     FAK4
     C                   END                                                    1<-E
     C*
     C                   MOVE      *ZEROS        XTOTAL
     C                   MOVE      *ZEROS        XMVAGR
     C*
     C     STKRE         TAG
     C                   SETOFF                                       33
     C     UKOPI         IFEQ      'J'                                          1<-B
     C     FAK2KC        READE     FAK3                                   33
     C  N33FAKDA         IFEQ      'K'
     C                   GOTO      STKRE
     C                   END
     C                   ELSE                                                   1
     C     FAK2K         READE     FAK4                                   33
     C                   END                                                    1<-E
     C   33              GOTO      SLKRE
     C     FASK          IFEQ      'X'
     C     FAKUNR        IFNE      FAKUGM
     C                   GOTO      STKRE
     C                   END
     C     FAVK          IFEQ      'AME'
     C                   GOTO      STKRE
     C                   END
     C                   END
     C*
     C* WHEN FULLCREDITING AN INVOICE - READ ONLY THE OPEN INV.LINES
     C* BELONGING TO THE CREDITNOTE. (MARKED "C" IN "FAKDA")
     C     UKOPI         IFNE      'J'                                          1<-B
     C     UIN07         ANDEQ     '1'
     C     FAKDA         ANDNE     'C'
     C                   GOTO      STKRE
     C                   END                                                    1<-E
     C     FASK          IFEQ      'I'
     C                   GOTO      STKRE
     C                   END
     C* INVOICE LINES WITH AMT. ZERO:
     C     FABELN        IFEQ      0                                            1<-B
     C*
     C* SKIPP BUT DO NOT DELETE IF OTHER UNIT=D(PR DAY)
     C     FA01B         IFEQ      'D'                                           2<-B
     C                   GOTO      STKRE
     C                   END                                                     2<-E
     C* SKIPP 0-LINE TYPE "PR LINE ON SAD" - DELETE IF SAD FINISHED
     C     FA01B         IFEQ      'L'                                           2<-B
     C     HEPK1         IFEQ      'J'                                           3<-B
     C                   GOTO      STKRE                                         SAD UFERDIG
     C                   ELSE                                                     X3
     C     FAKTK         CHAIN     FAKT                               33
     C  N33              DELETE    FAKTR
     C                   GOTO      STKRE
     C                   END                                                     3<-E
     C                   END                                                     2<-E
