             PGM
             ALCOBJ     OBJ((SYFA50 *PGM *EXCL)) WAIT(1)
             MONMSG     MSGID(CPF1002) EXEC(GOTO CMDLBL(SLUTT))
             DLCOBJ     OBJ((SYFA50 *PGM *EXCL))
             SBMJOB     CMD(CALL PGM(SYFA50CL)) JOB(SYFA50CL) +
                          JOBD(*USRPRF) JOBQ(SYJOBQNMAX)
 SLUTT:      ENDPGM
