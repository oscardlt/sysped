N39  c                   parm      XHEOPD        UOPD              7 0
N39  c                   parm      XHELKS        DUPTELK           2
N39  c                   parm      XHESDFF       DUPTERM           5
N39  c                   parm      'FF'          DUPFFVF           2
N39  c                   parm                    BIBRID
N39  c                   parm      XHESG         SIGN              3
N39  c                   parm                    DUPIND            1
     C                   ENDIF
