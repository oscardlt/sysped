             PGM        PARM(&VATNO &DATOF &DATOT)
             DCL        VAR(&VATNO) TYPE(*CHAR) LEN(17)
             DCL        VAR(&DATOF) TYPE(*CHAR) LEN(8)
             DCL        VAR(&DATOT) TYPE(*CHAR) LEN(8)
             DLTF       FILE(QTEMP/Q)
             MONMSG     MSGID(CPF0000)
             CRTPF      FILE(QTEMP/Q) RCDLEN(256)
             CALL       PGM(SVI201R) PARM(&VATNO &DATOF &DATOT)
             CPYTOSTMF  FROMMBR('QSYS.LIB/QTEMP.LIB/Q.FILE/Q.MBR') +
                          TOSTMF('SYSPEDF/TDS/SVIISE.CSV') +
                          STMFOPT(*REPLACE) STMFCODPAG(*STDASCII)
             CHGAUT     OBJ('SYSPEDF/TDS/SVIISE.CSV') USER(*PUBLIC) +
                          DTAAUT(*RWX) OBJAUT(*ALL)
             ENDPGM
