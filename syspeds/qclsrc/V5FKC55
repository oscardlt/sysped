             PGM        PARM(&FRALIB &TILLIB &TOMME)
             DCL        VAR(&FRALIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TILLIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TOMME)  TYPE(*CHAR) LEN(1)

             CLRPFM     FILE(&TILLIB/STANDT)
             OVRDBF     FILE(STANDTNY) TOFILE(&TILLIB/STANDT)
             OVRDBF     FILE(STANDTGM) TOFILE(&FRALIB/STANDT)
             CALL       PGM(V5FKR55)
             IF         COND(&TOMME = 'J') THEN(CLRPFM +
                          FILE(&FRALIB/STANDT))

             ENDPGM
