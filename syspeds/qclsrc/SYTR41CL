    /* UTSKRIFT AV ORDRE FRA TIL DATO */
             PGM
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1) /* Hvis &in01 +
                          på, betyr det avslutt program fra +
                          avdelingsvalg. */
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10) /* Printernavn */
             DCL        VAR(&PT) TYPE(*CHAR) LEN(10) /* Papirtype */
             DCL        VAR(&PT) TYPE(*CHAR) LEN(10) /* Papirtype */
             DCL        VAR(&HE) TYPE(*CHAR) LEN(1) /* HEADING */
             DCL        VAR(&TY) TYPE(*CHAR) LEN(1) /* TYPE */
             DCL        VAR(&CPI) TYPE(*CHAR) LEN(2) /* CPI  */
             DCL        VAR(&SORT) TYPE(*CHAR) LEN(1) VALUE('1') /* +
                          SORTERING*/
             DCL        VAR(&FAKT) TYPE(*CHAR) LEN(1) VALUE('N') /* +
                          SKRIV FAKSUM*/
             DCL        VAR(&REK) TYPE(*CHAR) LEN(1) VALUE('N') /* +
                          SKRIV REK/DUP*/
             DCL        VAR(&TILL) TYPE(*CHAR) LEN(1) VALUE('N') /* +
                          SKRIV TILLAT*/
             DCL        VAR(&JOBQ) TYPE(*CHAR) LEN(1) VALUE('J') /* +
                          JOBBKØ      */
             DCL        VAR(&ANTB) TYPE(*CHAR) LEN(1) VALUE('2') /* +
                          ANTALL BLENKLINJER MELLOM UDISPONERTE */
             DCL        VAR(&AVD) TYPE(*CHAR) LEN(4)
             DCL        VAR(&DATO) TYPE(*CHAR) LEN(6)
             DCL        VAR(&DATO2) TYPE(*CHAR) LEN(6)
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CALL       PGM(SYFA00) PARM(&IN01)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT)) /* +
                          Hvis &IN01 på, avslutter program. */
             CALL       PGM(SYTR41A) PARM(&IN01 &AVD &PRT &PT &HE +
                          &DATO &DATO2 &TY &SORT &TILL &FAKT &REK +
                          &JOBQ &ANTB &CPI)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(SLUTT)) /* +
                          Hvis &IN01 på, avslutter program. */
    /* */
             IF         COND(&JOBQ = 'J') THEN(DO)
             SBMJOB     CMD(CALL PGM(SYTR41CLJ) PARM(&AVD &DATO +
                          &DATO2 &TY &HE &SORT &FAKT &REK &TILL +
                          &ANTB &PRT &PT &CPI)) JOB(ORDRE) +
                          JOBD(*USRPRF) JOBQ(SYJOBQPRT)
             SNDPGMMSG  MSG('Utskrift av oppdragsliste er sendt i +
                          jobkø SYJOBQPRT')
                        ENDDO
             ELSE       CMD(DO)
             CALL       PGM(SYTR41CLJ) PARM(&AVD &DATO &DATO2 &TY +
                          &HE &SORT &FAKT &REK &TILL &ANTB &PRT &PT +
                          &CPI)
                        ENDDO
    /* */
 SLUTT:      ENDPGM
