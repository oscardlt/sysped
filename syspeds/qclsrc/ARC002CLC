             PGM        PARM(&ATYPE &AUSER &ORDTYP)
             DCL        VAR(&FINS) TYPE(*CHAR) LEN(1) VALUE('N')
             DCL        VAR(&AUSER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&ATYPE) TYPE(*CHAR) LEN(2)
             DCL        VAR(&ORDTYP) TYPE(*CHAR) LEN(1)

             CHGDTAARA  DTAARA(*LDA (1024 1)) VALUE(' ')

             SBMJOB     CMD(CALL PGM(ARC002CL) PARM(&ATYPE &AUSER +
                          &ORDTYP)) JOB(ARKIV) JOBD(*USRPRF) +
                          JOBQ(SYJOBQARK)
    /* */
             ENDPGM
