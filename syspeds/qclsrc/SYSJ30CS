             PGM        PARM(&KDB &REISE &DT)
             DCL        VAR(&FLAGG) TYPE(*DEC) LEN(1 0) VALUE(1)
             DCL        VAR(&KDB) TYPE(*CHAR) LEN(5)
             DCL        VAR(&REISE) TYPE(*CHAR) LEN(14)
             DCL        VAR(&DT) TYPE(*CHAR) LEN(6)
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             OVRDBF     FILE(CDERFOPD) TOFILE(CDERF)
             CALL       PGM(SYSJ30) PARM(&FLAGG &KDB &REISE &DT)
             SBMJOB     CMD(CALL PGM(SYSJ30A)) JOB(KURSLI) +
                          JOBQ(SYJOBQNMAX)
             ENDPGM
