             PGM        PARM(&LIB)
             DCL        VAR(&LIB) TYPE(*CHAR) LEN(10)

             CPYF       FROMFILE(SYPTF12F2/PONRNO) +
                          TOFILE(&LIB/PONRNO) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             CPYF       FROMFILE(SYPTF12F2/SYMENUF) +
                          TOFILE(&LIB/SYMENUF) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             CPYF       FROMFILE(SYPTF12F2/UTSKRS) +
                          TOFILE(&LIB/UTSKRS) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)

             ENDPGM
