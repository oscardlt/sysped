     C                   kfld                    WBAVD
     C                   kfld                    WBOPD
     C     EPOHK         klist
     C*                  kfld                    BIKUNR
     C*                  kfld                    BSKN
     C                   kfld                    XXKN
     C                   kfld                    ELBES
     C     EPOHK2        klist
     C*                  kfld                    BIKUNR
     C*                  kfld                    BSKN
     C                   kfld                    XXKN
     C                   kfld                    WSBES
     C     EPOLK         klist
     C*                  kfld                    BIKUNR
     C*                  kfld                    BSKN
     C                   kfld                    XXKN
     C                   kfld                    WSBES
     C                   kfld                    WBLIN
     C                   kfld                    WBULIN
     C     PosKey        klist
     C                   kfld                    WBAVD
     C                   kfld                    WBOPD
     C*                  kfld                    BSKN
     C                   kfld                    XXKN
     C                   kfld                    EPBES
     C                   kfld                    EPLIN
