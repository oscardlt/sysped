             CMD        PROMPT('Konverter data til PTF-bibl.')
             PARM       KWD(FRALIB) TYPE(*CHAR) LEN(10) RSTD(*NO) +
                          DFT('SYSPEDFxx') PROMPT('Prod.bibliotek +
                          .............:')
             PARM       KWD(PTFLIB) TYPE(*CHAR) LEN(10) RSTD(*NO) +
                          DFT('  PTF12') PROMPT('Temp bibl. for nye +
                          filer ...:')
             PARM       KWD(GMLLIB) TYPE(*CHAR) LEN(10) RSTD(*NO) +
                          DFT('  PTFGML') PROMPT('Bevaringsbibl. +
                          for gml filer:')
             PARM       KWD(TOMME) TYPE(*CHAR) LEN(1) RSTD(*NO) +
                          DFT('N') PROMPT('Tømmes etter konvert +
                          .......:')
