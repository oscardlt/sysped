      * Viser bilde
     C     TAG01         TAG
     C                   WRITE     SVE031DØ
     C                   EXFMT     SVE031DA
     C                   EVAL      *IN30=*OFF
     C                   EVAL      *IN32=*OFF
     C                   EVAL      *IN33=*OFF
     C                   EVAL      *IN34=*OFF
     C                   EVAL      *IN35=*OFF
     C                   EVAL      *IN36=*OFF
     C                   EVAL      *IN37=*OFF
     C                   EVAL      *IN38=*OFF
     C                   EVAL      *IN39=*OFF
      * andre funksjonstaster
     C     *IN03         CABEQ     *ON           SRB8
     C     *IN12         CABEQ     *ON           SRB9
      * F4=Søk
     C     WSCLOC        DIV       256           LINNBR
     C                   MVR                     POSNBR
     C     *IN04         IFEQ      '1'
     C     POSNBR        ANDEQ     15
     C                   CALL      'SVT003R'
     C                   PARM      'KLI'         UTYP              3
     C                   PARM      'A'           UIE               1
     C                   PARM      WDT           UDATO             8
     C                   PARM                    UKD              10
     C     UKD           IFNE      *BLANKS
     C                   SELECT
     C     LINNBR        WHENEQ    6
     C                   MOVEL     UKD           XVEX_KOS2
     C     LINNBR        WHENEQ    7
     C                   MOVEL     UKD           XVEX_KOS3
     C     LINNBR        WHENEQ    8
     C                   MOVEL     UKD           XVEX_KOS4
     C     LINNBR        WHENEQ    9
     C                   MOVEL     UKD           XVEX_KOS5
     C     LINNBR        WHENEQ    10
     C                   MOVEL     UKD           XVEX_KOS6
     C     LINNBR        WHENEQ    11
     C                   MOVEL     UKD           XVEX_KOS7
     C     LINNBR        WHENEQ    12
     C                   MOVEL     UKD           XVEX_KOS8
     C     LINNBR        WHENEQ    13
     C                   MOVEL     UKD           XVEX_KOS9
     C                   ENDSL
     C                   ENDIF
     C                   GOTO      TAG01
     C                   ENDIF
     C     SRB8          TAG
