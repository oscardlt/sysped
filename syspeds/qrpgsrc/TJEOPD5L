     c*    *in33         ifeq      '1'
     c*    trsffd        ifne      *zeros
     c*                  move      trsffd        hebodt
     c*                  move      trsffk        wsbotm
     c*                  else
     c*                  move      trsdfd        hebodt
     c*                  move      trsdfk        wsbotm
     c*                  ENDIF
     c*                  ENDIF
     c                   ENDIF
      *
     c                   move      wsbotm        WABOTM            4
     c                   if        wsbotm = *zeros
     c                   move      *blanks       WABOTM
     c                   endif

     C                   ENDSR
