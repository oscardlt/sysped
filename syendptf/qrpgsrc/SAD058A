     FSAEV4     IF   E           K DISK
     FSAEVCN    IF   E           K DISK
     D                 DS
     D  CNR50                  1    850
     D  CNR                    1    850    DIM(50)
      *
     C                   EXSR      INIT
      *
     C     SAEV4K        SETLL     SAEV4
     C     SAEV4K        READE     SAEV4                                  33
     C                   DOW       *IN33 = *OFF                                 1<-B
      *
     C     SAEVCNK       SETLL     SAEVCN
     C     SAEVCNK       READE     SAEVCN                                 33     2<-B
     C                   DOW       *IN33 = *OFF
      *
     C     1             DO        50            XN                3 0            3<-B
     C                   SELECT                                                    4<-B
     C                   WHEN      CNR(XN) = *BLANKS                               4
     C                   EVAL      CNR(XN) = SVCNR
     C                   LEAVE
     C                   WHEN      CNR(XN) = SVCNR                                 4
     C                   LEAVE
     C                   ENDSL                                                     4<-E
     C                   ENDDO                                                    3<-E
      *
     C     SAEVCNK       READE     SAEVCN                                 33
     C                   ENDDO                                                   2<-E
      *
     C     SAEV4K        READE     SAEV4                                  33
     C                   ENDDO                                                  1<-E
      *
     C                   SETON                                        LR
     C                   RETURN
      *
      ***********************************************
     C     INIT          BEGSR
      ***********************************************
     C     SAEV4K        KLIST
     C                   KFLD                    UAVD
     C                   KFLD                    UTDN
     C                   KFLD                    ULN
     C     SAEVCNK       KLIST
     C                   KFLD                    UAVD
     C                   KFLD                    UTDN
     C                   KFLD                    SVLI

     C     *ENTRY        PLIST
     C                   PARM                    UAVD              4 0
     C                   PARM                    UTDN              7 0
     C                   PARM                    ULN               5 0
     C                   PARM      CNR50         UCNR            850
      *
     C                   ENDSR
      *
