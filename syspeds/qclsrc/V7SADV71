             CPYSRCF    FROMFILE(V7SYSF/QRPGSRC.E) +
                          TOFILE(SYEDIP/QRPGSRC) FROMMBR(*ALL)
             ADDMSGD    MSGID(YBC1828) MSGF(SYSPED/SYMSGF) +
                          MSG('Organisasjonsnr eller personnr må +
                          oppgis.                                     -
   78 long SAD061/SAD052') SEV(20)
