     d  wcmd           s           2048
     d  wcmd1          s            256
     d  wcmd2          s            256
     d  wcmd3          s            256
     d  wcmd4          s            256
     d  x7D            s              1
     d runcmd          pr                  extpgm('QCMDEXC')
     d  Cmd                         200a   const
     d  len                          15p 5 const

      /free

       x7D=x'7D';
       wcmd1='cd /home/tktesting';
       wcmd2='gpg -o /home/tktesting/stor.gz --decrypt +
              /home/tktesting/stor.xml.gz.pgp';
       wcmd3='export -s CLASSPATH=/home/tktesting';
       wcmd4='java GZipFile stor.gz';

       wcmd = 'QSH CMD(' + x7D +
              wcmd1 + ';' +
              ' '   + wcmd2 + ';' +
              ' '   + wcmd3 + ';' +
              ' '   + wcmd4 + ';' +
              x7D + ')';

       RunCmd(Wcmd :%len(%trim(Wcmd)));

       *inlr=*on;
      /end-free
