      * LES REF
     C                   MOVE      *BLANKS       FE
     C     1             DO        35            NR
     C                   EXSR      GETCH
     C   LR              GOTO      ENDRFFH
     C                   IF        (((TEGN = LOGAPP) OR
     C                               (TEGN = LOGPLU)) OR
     C                               (TEGN = LOGKOL))
     C                   LEAVE
     C                   END
     C                   MOVE      TEGN          FE(NR)
     C                   END
     C                   MOVEL     FELT          XAN35            35
      *
     C                   SELECT
     C                   WHEN      XAN03 = 'VA'
     C                   MOVEL     XAN35         SERG
     C                   WHEN      XAN03 = 'CO'
     C                   MOVEL     XAN35         SEKT
     C                   WHEN      XAN03 = 'XC'
     C                   MOVEL     XAN35         SEGN
     C                   WHEN      XAN03 = 'LAR'
     C                   MOVEL     XAN35         SEPOS
