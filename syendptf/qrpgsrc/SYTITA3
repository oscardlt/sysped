S02  C*                  MOVE      NYPOST        G2POST          227
S03  C*                  MOVE      NYPOST        G2POST          297
S06  C*                  MOVE      NYPOST        G2POST          339
N06  C                   MOVE      NYPOST        G2POST          385
S01  C*                  MOVE      NYPOS2        G2POS2          123
S05  C*                  MOVE      NYPOS2        G2POS2          131
N05  C                   MOVE      NYPOS2        G2POS2          139
N04  C                   MOVE      TAORDK        G2ORDK            1
N05  C                   MOVE      TADATO        G2DATO            8 0
N05  C                   MOVE      TADTS         G2DTS             8 0
     C     TATANR        CHAIN     TARI                               33
N05  C                   DOW       *IN33 = *OFF
N05  C                   IF        TADTS = 0
N05  C                   LEAVE
N05  C                   END
N05  C     TATANR        READE     TARI                                   33
N05  C                   ENDDO
S06  C*                  IF        (TATANR = 21069060 OR TATANR = 21069093 OR
S06  C*                             TATANR = 21069098 OR TATANR = 35019010 OR
S06  C*                             TATANR = 35022090)
N06  C                   IF        (TATANR = 21069060 OR TATANR = 21069094 OR
N06  C                              TATANR = 21069095 OR TATANR = 21069098 OR
N06  C                              TATANR = 35019010)
N04  C                             AND TAORDK <> G2ORDK
N04  C     TATANR        READE     TARI                                   33
N05  C                   DOW       *IN33 = *OFF
N05  C                   IF        TADTS = 0
N05  C                   LEAVE
N05  C                   END
N05  C     TATANR        READE     TARI                                   33
N05  C                   ENDDO
N04  C                   END
N05  C  N33              IF        TADATO > G2DATO AND G2DTS = 0
N05  C                   MOVE      WDT           TADTS
N05  C                   END
N05  C  N33              UPDATE    TARIR
     C                   MOVE      G2POST        NYPOST
     C                   MOVE      G2POS2        NYPOS2
     C                   WRITE     TARIR
