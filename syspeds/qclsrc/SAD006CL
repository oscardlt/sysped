             PGM
             DCL        VAR(&IN01) TYPE(*CHAR) LEN(1) /* Hvis &in01 +
                          på, betyr det avslutt program fra +
                          avdelingsvalg. */
             DCL        VAR(&IN02) TYPE(*LGL) LEN(1) /* Hvis &in02 +
                          på, hopper til SYFTSS fra SAD061 for å +
                          bytte avdeling. */
             DCL        VAR(&IN03) TYPE(*LGL) LEN(1) /* Hvis &in03 +
                          på, betyr det at det skal kjøre utskrift +
                          etter SAD-registrering. */
             DCL        VAR(&IN04) TYPE(*LGL) LEN(1) /* Hvis &in04 +
                          på, betyr det man har hoppet fra +
                          oppdragregistrering til SAD-registrering. */
             DCL        VAR(&HOVP) TYPE(*CHAR) LEN(10) /* Printernavn +
                          til hoved ark på SAD. */
             DCL        VAR(&TILP) TYPE(*CHAR) LEN(10) /* Printernavn +
                          til tilleggsark på SAD. */
             DCL        VAR(&DLP) TYPE(*CHAR) LEN(10) /* Printernavn +
                          til dataliste på SAD. */
             DCL        VAR(&LSTP) TYPE(*CHAR) LEN(10) /* Printernavn +
                          til lasteliste på SAD. */
             DCL        VAR(&UTLP) TYPE(*CHAR) LEN(10) /* Printernavn +
                          til utleggsattest på TVINN. */
             DCL        VAR(&MRKP) TYPE(*CHAR) LEN(10) /* Printernavn +
                          til merknader på TVINN */
             DCL        VAR(&URUND) TYPE(*CHAR) LEN(1)
                                                          /**/
 AVD:        CALL       PGM(SYFTSS) PARM(&IN01 &HOVP &TILP &LSTP +
                          &UTLP &MRKP &DLP &URUND)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(AVSLUT)) +
                          /* Hvis &IN01 på, sjekker om det er noen +
                          utskrift. Og så avslutt program. */
             IF         COND(&IN01 = '3') THEN(GOTO CMDLBL(AVSLUT)) +
                                                          /**/
             CALL       PGM(SAD006) PARM(&IN02)
             IF         COND(&IN02 = '1') THEN(GOTO CMDLBL(AVD)) /* +
                          Hvis &IN02 på, går til AVD for å bytte +
                          avdelnger. */
                                                          /**/
 AVSLUT:     ENDPGM
