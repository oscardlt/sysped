             CMD        PROMPT('REORGANIZE LIBRARY')

                     PARM       KWD(LIBRARY) TYPE(*CHAR) LEN(10) +
                                  PROMPT('BIBLIOTEK TIL REORGANISERING')

                     PARM       KWD(END_DATE) TYPE(*CHAR) LEN(6) DFT(000000) +
                                  PROMPT('SLUTT DATO OBS:  YY/MM/DD')

                     PARM       KWD(END_TIME) TYPE(*CHAR) LEN(6) DFT(000000) +
                                  PROMPT('SLUTT  TID')
