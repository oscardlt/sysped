      /copy SYSPEDS/qrpgsrcpy,hspecs
      /copy SYSPEDS/qrpgsrcpy,hspecsbnd
      *********************************************************************
     FBRIDF     IF   E           K DISK    USROPN
     FSVTX03L2  IF   E           K DISK    USROPN
     FSVTVK     IF   E           K DISK    USROPN
     FSVTVKT    IF   E           K DISK    USROPN
     FSVTPRO    IF   E           K DISK    USROPN
      *--------------------------------------------------------------------
      * Variables common to all CGIs
      *--------------------------------------------------------------------
      /copy SYSPEDS/qrpgsrcpy,prototypeb
      /copy SYSPEDS/qrpgsrcpy,usec
      /copy SYSPEDS/qrpgsrcpy,variables3
      *
      * Varible for
     D WSUSER          S             10
     D JSONstring      s          32000
     D Error           S            150
     D Wtyp            S              3
     D Wie             S              1
     D AntLest         S              9  0
     D ZB              S              1    DIM(20)
     D                 DS
     D  WDT                    1      8
     D  WDTY                   1      4
     D  WDTM                   5      6
     D  WDTD                   7      8
     D                 DS
     D  WD                     1     10
     D  WDY                    1      4
     D  WD1                    5      5
     D  WDM                    6      7
     D  WD2                    8      8
     D  WDD                    9     10

      *get input-string
      /copy syspeds/qrpgsrcpy,prolog3

      * Parse input
     C                   exsr      Parse
      * Open files etc
     C                   EXSR      INIT
      *
     c                   callp     gethtmlifs(
     C                             '/syspeddev/html/JSON.html':
     C                             '/CGITAG/')
     C                   callp     wrtsection('top')
      *
      * ............................................................................................
      * skriv JSON-Object start..(alltid '{' + x ant param. m/komma mellom (IKKE komma etter siste))
      * ............................................................................................
      *
      /free
        JSONstring='{'
        +'¬user¬ : ¬'+%trim(WSUSER)+'¬, '
        +'¬ie¬ : ¬'+%trim(WIE)+'¬, '
        +'¬typ¬ : ¬'+%trim(WTYP)+'¬, '
        +'¬errMsg¬ : ¬'+%trim(Error)+'¬, ';
      /end-free
      * JSONfix escaper " i tekst,  for deretter å bytte ¬->"
     c                   call      'JSONFIX'
     c                   parm                    JSONstring
     C                   callp     updHTMLvar('JSONstring':JSONstring)
     C                   callp     wrtsection('JSONlin')
      *
      * ............................................................................................
      * Skriv JSON-LISTE Start (en liste er EN parameter(fra [ til   )
      * ............................................................................................
     c                   eval      JSONstring ='"kodlista" : ['
     C                   callp     updHTMLvar('JSONstring':JSONstring)
     C                   callp     wrtsection('JSONlin')
      *
      * Hæmta varuslag
      *
     c     Error         ifeq      *blanks
     C     WTYP          COMP      'MDX'                                  56
     C     WTYP          COMP      'FF1'                                  58
     C                   IF        *IN56=*OFF
     C                   IF        *IN58=*OFF
     C     WTYP          SETLL     SVTX03L2
     C     WTYP          READE     SVTX03L2                               55
     C                   ENDIF
     C                   ENDIF
     C   56*LOVAL        SETLL     SVTVK
     C   56              READ      SVTVK                                  55
     C   58WIE           SETLL     SVTPRO
     C   58WIE           READE     SVTPRO                                 55
     C     *IN55         DOWEQ     *OFF
     C                   IF        *IN56
     C                   EVAL      SVTX03_01='A'
     C                   EVAL      SVTX03_06='2015-01-01'
     C                   EVAL      SVTX03_05='2099-12-31'
     C                   EVAL      SVTX03_03=SVVK_KD
     C     SVVK_KD       CHAIN     SVTVKT                             57
     C  N57              MOVEL     SVVKT_TX      SVTX03_04
     C   57              MOVE      *BLANKS       SVTX03_04
     C                   ENDIF
     C                   IF        *IN58
     C                   EVAL      SVTX03_01=SVPR_IE
     C                   EVAL      SVTX03_06='2015-01-01'
     C                   EVAL      SVTX03_05='2099-12-31'
     C                   EVAL      SVTX03_03=SVPR_PR
     C                   MOVEL     SVPR_TX1      SVTX03_04
     C                   ENDIF
     C     SVTX03_01     IFEQ      WIE
     C     SVTX03_06     IFLE      WD
     C     SVTX03_05     ANDGE     WD
     C                   Add       1             AntLest
      * ............................................................................................
      * Skriv en JSON-Array-linje pr menypunkt - komma før hvert nytt element
      * ............................................................................................
      /free
       if AntLest=1;
          JSONstring=*blanks;
          else;
          JSONstring=', ';
          endif;
       JSONstring=%trim(JSONstring)+'{'
          +'¬svkd_kd¬ : ¬'+%trim(SVTX03_03)+'¬, '
          +'¬svkd_kbs¬ : ¬'+%trim(SVTX03_04)+'¬}';
      /end-free
     C                   call      'JSONFIX'
     C                   parm                    JSONstring
     C                   callp     updHTMLvar('JSONstring':JSONstring)
     C                   callp     wrtsection('JSONlin')
     C                   ENDIF
     C                   ENDIF
      *
     C  N56WTYP          READE     SVTX03L2                               55
     C   56              READ      SVTVK                                  55
     C   58WIE           READE     SVTPRO                                 55
     C                   ENDDO
     C     UTLES         TAG
     c                   endif
      * ............................................................................................
      * Skriv JSON-Liste/Array  Avslutning
      * ............................................................................................
     c                   eval      JSONstring =']'
     C                   callp     updHTMLvar('JSONstring':JSONstring)
     C                   callp     wrtsection('JSONlin')
      * ............................................................................................
      * Skriv JSON-string Avsluttning
      * ............................................................................................
     c                   eval      JSONstring ='}'
     C                   callp     updHTMLvar('JSONstring':JSONstring)
     C                   callp     wrtsection('JSONlin')
      *
      * Quit
     C                   exsr      Exit


      *=====================================================================
      * Close output html and quit
      *=====================================================================
     C     Exit          begsr
      * Lukk fil
     C                   CLOSE     BRIDF
     C  N50              CLOSE     SVTX03L2
     C  N50              CLOSE     SVTVK
     C  N50              CLOSE     SVTVKT
     C  N50              CLOSE     SVTPRO

      * Do not delete the call to wrtsection with section name *fini.  It is needed
      * to ensure that all output html that has been buffered gets output.
     C                   callp     wrtsection('*fini')
      * Quit
     C                   MOVE      *ON           *INLR
     C                   return
     C                   endsr
