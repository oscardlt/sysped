             PGM
             DCL        VAR(&IN07) TYPE(*LGL) LEN(1) /* På betyr +
                          utskrift av arb.liste */
             DCL        VAR(&TNR) TYPE(*CHAR) LEN(8) /*TRANSP*/
             DCL        VAR(&PER) TYPE(*CHAR) LEN(2) /*PERMND*/
             DCL        VAR(&AAR) TYPE(*CHAR) LEN(2) /*PERÅR*/
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10) /* Printernavn */
    /* */
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CALL       PGM(SYTR12) PARM(&IN07 &TNR &AAR &PER &PRT)
             IF         COND(&IN07 = '0') THEN(GOTO CMDLBL(SLUTT)) /* +
                          Hvis &IN07 AV, avslutter program. */
   /*  */
             SBMJOB     CMD(CALL PGM(SYTR12CLJ) PARM(&TNR &AAR &PER +
                          &PRT)) JOB(AVRLISTE) JOBD(*USRPRF) +
                          JOBQ(SYJOBQAV)
   /*  */
             SNDPGMMSG  MSGID(SPU0010) MSGF(SYMSGF)
 SLUTT:      ENDPGM
