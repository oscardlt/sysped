     d utdn                           7  0

      /free

       chain (uavd: utdn) saehko;
       if not %found;
         chain (uavd: utdn) saeh;
         if %found;
           write saehkor;
         endif;
       endif;

       chain (uavd: utdn) saefko;
       if not %found;
