             PGM
             DCL        VAR(&TIME) TYPE(*CHAR) LEN(6)
             CALL       PGM(SYSPED/INLIBL)
/* Hent filer fra mappe..., Skriv til CargoF  */
START:       CALL CARGOSCR
/* Behandle alle som mangler match (også gamle)  */
             CALL CARGOSCR3
             DLYJOB     DLY(30)
             RTVSYSVAL  SYSVAL(QTIME) RTNVAR(&TIME)
             IF         COND(&TIME *GT '235000') THEN(GOTO CMDLBL(END))
             GOTO       CMDLBL(START)

END:         ENDPGM
