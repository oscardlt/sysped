     C     START2        TAG
      *
     C  N94              DO
     C                   DO        17
     C                   ADD       1             ZRECNO
     C                   MOVE      *BLANKS       WSDLT
     C     KO≈UNI        IFEQ      '≈'
     C                   MOVE      FAKVAR        FAKKOS           15
     C                   ELSE
     C     KO≈UNI        IFEQ      'K'
     C                   MOVE      KOSVAR        FAKKOS
     C                   ELSE
     C                   move      webvar        FAKKOS
     C                   END
     C                   END
     C                   WRITE     SUBFIL
     C                   READ(N)   KODT≈                                  94
     C     *IN94         IFEQ      '0'
     C                   MOVE      KO≈UNI        Y≈UNI
     C                   MOVE      KO≈MND        YMND
     C                   MOVE      KO≈≈R         Y≈R
     C                   MOVE      KO≈CC         YCC
     C                   END
     C  N94              END
     C                   END
     C*
     C                   Z-ADD     ZRECNO        YRECNO            4 0
     C     START3        TAG
     C                   MOVEA     '11'          *IN(91)
     C     ZRECNO        IFEQ      0
     C                   EXFMT     NYVARE
     C                   ELSE
     C                   WRITE     SUBCTL
     C                   EXFMT     BUNN
     C                   END
     C                   SETOFF                                       9830
     C*
     C*** AVSLUTT
     C   03              GOTO      SLUTT
