             PGM        PARM(&USRID &PW &SMSNR &BODY)
             DCL        VAR(&USRID) TYPE(*CHAR) LEN(128)
             DCL        VAR(&PW) TYPE(*CHAR) LEN(32)
             DCL        VAR(&SMSNR) TYPE(*CHAR) LEN(16)
             DCL        VAR(&BODY) TYPE(*CHAR) LEN(32)

             RMVLIBLE   LIB(LIBHTTP)
             MONMSG     MSGID(CPF0000)
             ADDLIBLE   LIB(LIBHTTP) POSITION(*LAST)
             MONMSG     MSGID(CPF2110) EXEC(GOTO CMDLBL(SLUTT))
             CALL       PGM(SVT071R) PARM(&USRID &PW &SMSNR &BODY)
             RMVLIBLE   LIB(LIBHTTP)
             MONMSG     MSGID(CPF0000)
SLUTT:       ENDPGM
