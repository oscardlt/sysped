             PGM
             DCL        VAR(&FRALIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TILLIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&GMLLIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&HIST) TYPE(*CHAR) LEN(1)
             DCL        VAR(&SAD) TYPE(*CHAR) LEN(1)
             DCL        VAR(&TOMME) TYPE(*CHAR) LEN(1)
             DCL        VAR(&JOBQ) TYPE(*CHAR) LEN(1)
             DCL        VAR(&FLAGG) TYPE(*CHAR) LEN(1)
             DCL        VAR(&FB) TYPE(*CHAR) LEN(1)
             DCLF       FILE(V6FKD00) RCDFMT(BILDE01)
             CHGJOB     JOB(*) LOG(4 00 *SECLVL) LOGCLPGM(*YES)
             OVRDBF     FILE(V6FNF) TOFILE(V6SYSF/V6FNF)
             OVRDBF     FILE(V6DSPDBR) TOFILE(V6SYSF/V6DSPDBR)

LOOP1:
             SNDRCVF    DEV(*FILE) RCDFMT(*FILE)
             IF         COND(&IN03 = '1') THEN(GOTO CMDLBL(SLUTT))
             CHGVAR     VAR(&IN30) VALUE('0')
             CHGVAR     VAR(&IN31) VALUE('0')

             CHKOBJ     OBJ(QSYS/&FRALIB) OBJTYPE(*LIB)
             MONMSG     MSGID(CPF9801 CPF9810) EXEC(CHGVAR +
                          VAR(&IN30) VALUE('1'))

             IF         COND(&IN30 = '1' *OR &IN31 = '1') THEN(GOTO +
                          CMDLBL(LOOP1))

             IF         COND(&IN10 = '1') THEN(CHGVAR VAR(&JOBQ) +
                          VALUE('J'))

             IF         COND(&HIST = 'J') THEN(DO)
             CHGCURLIB  CURLIB(V6SYSO)
             ADDLIBLE   LIB(&FRALIB)
             CHGDTAARA  DTAARA(V6SYSF/V6FKDTA (1 10)) VALUE(&FRALIB)
             CHGDTAARA  DTAARA(V6SYSF/V6FKDTA (1 4)) VALUE('V6O.')
             ENDDO
             ELSE       CMD(DO)
             CHGCURLIB  CURLIB(V6SYSF)
             CHGDTAARA  DTAARA(V6SYSF/V6FKDTA (1 10)) VALUE(&FRALIB)
             CHGDTAARA  DTAARA(V6SYSF/V6FKDTA (1 4)) VALUE('V6F.')
             ENDDO
             RTVDTAARA  DTAARA(V6SYSF/V6FKDTA (1 10)) RTNVAR(&TILLIB)
             CHKOBJ     OBJ(QSYS/&TILLIB) OBJTYPE(*LIB)
             MONMSG     MSGID(CPF9801 CPF9810) EXEC(DO)
             CALL       PGM(V6FKRTOM)
             CRTLIB     LIB(&TILLIB) TEXT('Datafiler fra V5')
             CHGDTAARA  DTAARA(V6SYSF/V6FKDTA (11 1)) VALUE('N')
             ENDDO

             CALL       PGM(V6FKR) PARM(&HIST &SAD &FLAGG)
             IF         COND(&FLAGG = '1') THEN(GOTO CMDLBL(SLUTT))

             RTVDTAARA  DTAARA(V6SYSF/V6FKDTA (11 1)) RTNVAR(&FB)
             IF         COND(&FB = 'N') THEN(DO)
             /* BYGG NYE/ENDREDE LOGISKEFILER SOM FYSISKEFILER IKKE */
             /* ER ENDRET.                                          */
             IF         COND(&HIST *EQ 'J') THEN(CALL PGM(V6FKC91A) +
                          PARM(&FRALIB))
             ELSE       CMD(CALL PGM(V6FKC91) PARM(&FRALIB))
             CHGDTAARA  DTAARA(V6SYSF/V6FKDTA (11 1)) VALUE('J')
             ENDDO

             IF         COND(&JOBQ = 'J') THEN(DO)
             SBMJOB     CMD(CALL PGM(V6FKR2) PARM(&HIST &SAD &FRALIB +
                          &TILLIB &TOMME &JOBQ))
             ENDDO
             ELSE       CMD(DO)
             CHGDSPF    FILE(V6FKD2) WAITRCD(*IMMED)
             CALL       PGM(V6FKR2) PARM(&HIST &SAD &FRALIB &TILLIB +
                          &TOMME &JOBQ)
             ENDDO

             IF         COND(&HIST = 'J') THEN(RMVLIBLE LIB(&FRALIB))

 SLUTT:      RCLRSC
             CHGCURLIB  CURLIB(*CRTDFT)
             ENDPGM
