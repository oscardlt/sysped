             PGM        PARM(&LIB)
             DCL        VAR(&LIB) TYPE(*CHAR) LEN(10)

             CPYF       FROMFILE(SYV12XF2/PONRNO) +
                          TOFILE(&LIB/PONRNO) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             CPYF       FROMFILE(SYV12XF2/SYMENUF) +
                          TOFILE(&LIB/SYMENUF) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             CPYF       FROMFILE(SYV12XF2/UTSKRS) +
                          TOFILE(&LIB/UTSKRS) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             CPYF       FROMFILE(SYV12XF2/SYPTFF) +
                          TOFILE(&LIB/SYPTFF) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             CPYF       FROMFILE(SYV12XF2/SYREO01F2) +
                          TOFILE(&LIB/SYREO01F2) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             CPYF       FROMFILE(SYV12XF2/SYREO11F2) +
                          TOFILE(&LIB/SYREO11F2) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             CPYF       FROMFILE(SYV12XF2/SYREO31F2) +
                          TOFILE(&LIB/SYREO31F2) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             CPYF       FROMFILE(SYV12XF2/THMELDING) +
                          TOFILE(&LIB/THMELDING) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             CPYF       FROMFILE(SYV12XF2/TRREGELF) +
                          TOFILE(&LIB/TRREGELF) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             CPYF       FROMFILE(SYV12XF2/TVINE) TOFILE(&LIB/TVINE) +
                          MBROPT(*REPLACE) FMTOPT(*MAP *DROP)
             CPYF       FROMFILE(SYV12XF2/UTSKRS) +
                          TOFILE(&LIB/UTSKRS) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)

             OVRDBF     FILE(TVINENY) TOFILE(SYV12XF2/TVINE)
             CALL       PGM(V12RE210)
             OVRDBF     FILE(ARKTXTNY) TOFILE(SYV12XF2/ARKTXT)
             CALL       PGM(V12RE203)

             ENDPGM
