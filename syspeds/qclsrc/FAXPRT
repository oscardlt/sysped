/*       PRINTING AV INNGÅENDE TELEFAX      */

             PGM        PARM(&PRTDEV &POLL)
             DCL        VAR(&PRTDEV) TYPE(*CHAR) LEN(10)
             DCL        VAR(&POLL) TYPE(*CHAR) LEN(4)
             DCL        VAR(&POLLD) TYPE(*DEC) LEN(4)
             CHGVAR     VAR(&POLL) VALUE(&POLL *CAT &POLL)
             CHGVAR     VAR(&POLLD) VALUE(&POLL) /* Char to Dec */
 START:      DLYJOB     DLY(&POLLD)
             PRTFAX     DEV(&PRTDEV) MAPVAL(*SCALE) DLTFAX(*YES)
             MONMSG     MSGID(FAX0699) EXEC(GOTO CMDLBL(START))
             GOTO START
             ENDPGM
