      /free
        JSONstring='{'
        +'¨user¨ : ¨'+%trim(WSUSER)+'¨, '
          +'¨feavd¨ : ¨'+%trim(%editc(feavd:'Z'))+'¨, '
          +'¨fetur¨ : ¨'+%trim(%editc(fetur:'Z'))+'¨, '
