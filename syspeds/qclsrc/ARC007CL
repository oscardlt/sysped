/*       DOKUMENTER TIL ARKIVERING   ***********************************   */
/* ********************************************************************    */
             PGM
             DCL        VAR(&VEDL) TYPE(*CHAR) LEN(1) VALUE('V')
             DCL        VAR(&TYPE) TYPE(*CHAR) LEN(2)
             DCL        VAR(&TXT) TYPE(*CHAR) LEN(15)
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             CALL       PGM(ARC007) PARM(&VEDL &TYPE &TXT)
             ENDPGM
