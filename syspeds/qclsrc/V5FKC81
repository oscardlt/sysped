             PGM        PARM(&FRALIB &TILLIB &TOMME)
             DCL        VAR(&FRALIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TILLIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TOMME)  TYPE(*CHAR) LEN(1)

             CLRPFM     FILE(&TILLIB/FAKTCMP)
             OVRDBF     FILE(FAKTNY) TOFILE(&TILLIB/FAKTCMP)
             OVRDBF     FILE(FAKTGM) TOFILE(&FRALIB/FAKTCMP)
             CALL       PGM(V5FKR81)
             IF         COND(&TOMME = 'J') THEN(CLRPFM +
                          FILE(&FRALIB/FAKTCMP))

             ENDPGM
