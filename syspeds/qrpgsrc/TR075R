     C   04              DO
     C     WSCLOC        DIV       256           LINNBR
     C                   MVR                     POSNBR
     C                   IF        (((LINNBR = 4) AND
     C                              (POSNBR >= 5)) AND
     C                              (POSNBR <= 6))
     C                   MOVE      '017'         UUNIK
     C                   MOVEL     W2EH          UKODE
     C                   CALL      'TR002R'
     C                   PARM                    UUNIK
     C                   PARM                    UKODE
     C                   PARM                    UTYPE2            3
     C                   MOVEL     UKODE         W2EH
     C                   EVAL      POSNBR = 8
     C                   GOTO      START3
     C                   END
     C                   IF        (((LINNBR = 4) AND
     C                              (POSNBR >= 63)) AND
     C                              (POSNBR <= 64))
     C                   MOVE      '012'         UUNIK
     C                   MOVEL     W2MNSK        UKODE
     C                   CALL      'TR002R'
     C                   PARM                    UUNIK
     C                   PARM                    UKODE
     C                   PARM                    UTYPE2            3
     C                   MOVEL     UKODE         W2MNSK
     C                   GOTO      START3
     C                   END
     C                   END
