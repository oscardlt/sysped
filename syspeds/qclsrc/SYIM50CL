             PGM
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1) /* Hvis &in01 +
                          på, betyr det avslutt program fra +
                          avdelingsvalg. */
START:       CALL       PGM(SYIM50R) PARM(&IN01)
             CALL       PGM(SYIM51R) PARM(&IN01)
             IF         COND(&IN01 = '1') THEN(GOTO CMDLBL(START))

 SLUTT:      ENDPGM
