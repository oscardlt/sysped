     C     SRBH          BEGSR
     C     KEYC          CHAIN     EDIS                               20
     C     *IN20         IFEQ      '0'
      * Finnes komponenten kvar ?
     C     SMBR          IFEQ      'REMOVED'
     C                   MOVE      'SPS0007'     ZMSGNO
     C                   MOVE      '1'           *IN30
     C                   GOTO      SLSRBH
     C                   END
      * Vise komponent.
     C                   MOVEL     SLIB          XSLIB             6
     C     XSLIB         IFEQ      'SYSPEDO'
     C                   CALL      'EDI41RX'
     C                   PARM                    SLIB
     C                   PARM                    SFILE
     C                   PARM                    SMBR
N03  C                   PARM                    ZOP
     C                   ELSE
     C                   SELECT
     C     SFILE         WHENEQ    'EDIRE'
     C                   CALL      'EDI41R'
     C                   PARM                    SLIB
     C                   PARM                    SFILE
     C                   PARM                    SMBR
N03  C                   PARM                    ZOP
     C     SFILE         WHENEQ    'EDIRE1K'
     C                   CALL      'EDI41R1K'
     C                   PARM                    SLIB
     C                   PARM                    SFILE
     C                   PARM                    SMBR
N03  C                   PARM                    ZOP
     C     SFILE         WHENEQ    'EDISE'
     C                   CALL      'EDI41S'
     C                   PARM                    SLIB
     C                   PARM                    SFILE
     C                   PARM                    SMBR
N03  C                   PARM                    ZOP
     C     SFILE         WHENEQ    'EDISE1K'
     C                   CALL      'EDI41S1K'
     C                   PARM                    SLIB
     C                   PARM                    SFILE
     C                   PARM                    SMBR
N02  C     SFILE         WHENEQ    '*IFS'
N02  C     SIFS          IFNE      *BLANKS
N02  C                   MOVEL     SIFS          SXIFSOBJ
N02  C                   EVAL      *IN51=*OFF
N02  C                   ELSE
N02  C     SMBR          CHAIN     EDISX                              51
N02  C                   ENDIF
N02  C                   CALL      'EDI41SI'
N02  C                   PARM                    SXIFSOBJ
N03  C                   PARM                    ZOP
     C                   ENDSL
     C                   END
     C                   END
