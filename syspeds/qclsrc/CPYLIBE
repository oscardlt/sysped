             PGM        PARM(&FRALIB &TILLIB)
             DCL        VAR(&FRALIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TILLIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&FASLIB) TYPE(*CHAR) LEN(10) VALUE(SYFASF)
             MONMSG     MSGID(CPF0000)
             /*________________________________*/
             /* KOPIER FILER                   */
             /*""""""""""""""""""""""""""""""""*/
             CPYF       FROMFILE(&FRALIB/SAEH) +
                          TOFILE(&TILLIB/SADTRH) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             CPYF       FROMFILE(&FRALIB/SAEV) +
                          TOFILE(&TILLIB/SADTRV) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             CPYF       FROMFILE(&FRALIB/STANDE) +
                          TOFILE(&TILLIB/STANDT) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)


  /* DET FØLGENDE SKAL UTFØRES VED HVER NY VERSJ (MED TILLEGG...) */
  /* OBS!! FORUTSETNING AT DIV. KODEFILER LIGGER I LIB: "SYFASF"!!*/

  /* KUFAST - KUNDEDEFINERBARE KODETEKSTER - NYE TYPER + STD. */
             OVRDBF     FILE(KUFASTNY) TOFILE(&FASLIB/KUFAST)
             OVRDBF     FILE(KUFAST) TOFILE(&TILLIB/KUFAST)
             CALL       PGM(SYFAS01)
             DLTOVR     FILE(KUFASTNY)
             DLTOVR     FILE(KUFAST)

  /* UTSKRS - UTSKRIFTSNUMMER   */
             OVRDBF     FILE(UTSKRSNY) TOFILE(&FASLIB/UTSKRS)
             OVRDBF     FILE(UTSKRS) TOFILE(&TILLIB/UTSKRS)
             CALL       PGM(SYFAS02)
             DLTOVR     FILE(UTSKRSNY)
             DLTOVR     FILE(UTSKRS)

  /* TVINE  - TVINE ERROR.MSG-TEKST - NYE KODER LEGGES INN  */
             OVRDBF     FILE(TVINENY) TOFILE(&FASLIB/TVINE)
             OVRDBF     FILE(TVINE) TOFILE(&TILLIB/TVINE)
             CALL       PGM(SYFAS03)
             DLTOVR     FILE(TVINENY)
             DLTOVR     FILE(TVINE)

  /* KOFAST - SYSTEMA-DEFINERTE KODETEKSTER - BYTTES I SIN HELHET */
             CPYF       FROMFILE(&FASLIB/KOFAST) +
                          TOFILE(&TILLIB/KOFAST) MBROPT(*REPLACE)

  /* SYFLD  - SYSTEMA-HEADF (M.M.) FELT.DEFN- BYTTES I SIN HELHET */
             CPYF       FROMFILE(&FASLIB/SYFLD) +
                          TOFILE(&TILLIB/SYFLD) MBROPT(*REPLACE)

  /* SYPTFF - START DATO FOR PTF  */
             CPYF       FROMFILE(&FASLIB/SYPTFF) +
                          TOFILE(&TILLIB/SYPTFF) MBROPT(*REPLACE)

  /* SYPROD - PRODUKTELISTE */
             CPYF       FROMFILE(&FASLIB/SYPROD) +
                          TOFILE(&TILLIB/SYPROD) MBROPT(*REPLACE)

             ENDPGM
