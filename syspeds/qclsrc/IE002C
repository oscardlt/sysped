             PGM        PARM(&LIB &KN &FLAGG)
             DCL        VAR(&LIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&KN) TYPE(*CHAR) LEN(8)
             DCL        VAR(&FLAGG) TYPE(*CHAR) LEN(1)

             OVRDBF     FILE(KUNKO) TOFILE(&LIB/KUNKO)
             CALL       PGM(IE002R) PARM(&KN &FLAGG)

             ENDPGM
