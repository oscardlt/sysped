     FFIRM      IF   E           K DISK
     FMULTKODT  IF   E           K DISK
     FSYSJBKFM  CF   E             WORKSTN
     D                 DS
     D  WSUTO                  1      1
     D  WSUTL                  2      2
     D  WSUTT                  3      3
     D  WSUTP                  4      4
     D  WSUTE                  5      5
     D  BT                     1      5
     D                UDS
     D  UFIRM                  1      2
     D  UAVD                  15     18  0
     D  USG                   19     21
     C*
     C     KODTPK        KLIST
     C                   KFLD                    YPUNI
     C                   KFLD                    WSAVD
     C                   KFLD                    YPLNR
     C     KODTWK        KLIST
     C                   KFLD                    YWUNI
     C                   KFLD                    WSAVD
     C     KODTVK        KLIST
     C                   KFLD                    YVUNI
     C                   KFLD                    WSAVD
     C     KODSFK        KLIST
     C                   KFLD                    YFUNI
     C                   KFLD                    WSSG
     C*
     C*===============================================
     C     *ENTRY        PLIST
     C                   PARM                    UIN01             1
     C                   PARM                    UOPDF             7 0
     C                   PARM                    UOPDT             7 0
     C                   PARM                    UBT               5
     C                   PARM                    UPRT29           10
     C                   PARM                    UPT29            10
     C                   PARM                    UPRT37           10
     C                   PARM                    UPT37            10
     C                   PARM                    UUTA              1
     C                   PARM                    UUTBB             1
     C                   PARM                    UJBK              1
     C*===============================================
     C*
     C                   MOVE      'P'           YPUNI             1
     C                   MOVE      'W'           YWUNI             1
     C                   MOVE      'V'           YVUNI             1
     C                   MOVE      'SF'          YFUNI             2
     C                   MOVE      UAVD          WSAVD
     C                   MOVE      USG           WSSG
     C                   MOVE      'J'           WSJBK
     C                   MOVE      *BLANKS       BT
     C                   MOVE      *BLANKS       UUTA
     C                   MOVE      *BLANKS       UUTBB
     C*
     C     FIFIRM        SETLL     FIRM
     C                   READ      FIRM                                   99
     C*
     C     START         TAG
     C                   EXFMT     BILDE
     C   03              MOVE      '1'           UIN01
     C   03              GOTO      SLUTT
     C                   MOVEA     '000'         *IN(97)
     C                   SETOFF                                       30
     C*
     C* TESTER DATA PÅ BILDE
     C*
     C     KODTVK        CHAIN     KOVR                               99
     C  N99KODTWK        CHAIN     KOWR                               99
     C  N99              Z-ADD     29            YPLNR             3 0
     C  N99KODTPK        CHAIN     KOPR                               99
     C  N99              MOVE      KOPNVN        UPRT29
     C  N99              MOVE      KOPTY         UPT29
     C  N99              Z-ADD     37            YPLNR             3 0
     C  N99KODTPK        CHAIN     KOPR                               99
     C  N99              MOVE      KOPNVN        UPRT37
     C  N99              MOVE      KOPTY         UPT37
     C   99              MOVE      'SPA0001'     MSGNR
     C   99              SETON                                        30
     C*
     C     WSSG          IFNE      'ALL'
     C     KODSFK        CHAIN     KOSFR                              98
     C                   END
     C   98
     CANN30              MOVE      'SPS0001'     MSGNR
     C   98              SETON                                        30
     C*
     C     WSOPDF        COMP      0                                    9797
     C   97
     CANN30              MOVE      'SPO0016'     MSGNR
     C  N97WSOPDT        IFEQ      0
     C                   MOVE      WSOPDF        WSOPDT
     C                   ELSE
     C     WSOPDF        COMP      WSOPDT                             97
     C   97
     CANN30              MOVE      'SPO0102'     MSGNR
     C                   END
     C   97              SETON                                        30
     C*
     C   30              GOTO      START
     C*
     C     WSPRT         IFNE      *BLANKS
     C                   MOVE      WSPRT         UPRT29
     C                   MOVE      WSPRT         UPRT37
     C                   END
     C     UPRT29        IFEQ      *BLANKS
     C                   MOVEL     'QPRINT '     UPRT29
     C                   END
     C     UPT29         IFEQ      *BLANKS
     C                   MOVEL     '*STD'        UPT29
     C                   END
     C     UPRT37        IFEQ      *BLANKS
     C                   MOVEL     'QPRINT '     UPRT37
     C                   END
     C     UPT37         IFEQ      *BLANKS
     C                   MOVEL     '*STD'        UPT37
     C                   END
     C                   MOVE      WSOPDF        UOPDF
     C                   MOVE      WSOPDT        UOPDT
     C                   MOVE      WSJBK         UJBK
     C                   MOVE      BT            UBT
     C                   MOVE      WSUTA         UUTA
     C                   MOVE      WSUTBB        UUTBB
     C                   MOVE      WSSG          USG
     C*
     C     SLUTT         TAG
     C                   SETON                                        LR
     C                   RETURN
     C*
