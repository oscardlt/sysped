11XXX * 01 CB  PTF11 Fel søk kod særsk. uppl.
********************************************************************
     FSVEX      UF A E           K DISK
     FSVE033D   CF   E             WORKSTN
     F                                     INFDS(FBAREA)
     D FBAREA          DS
     D  WSCLOC               370    371B 0
     D  WSSUBN               378    379B 0
      *//////////////
      *  Hodelogikk /
      *//////////////
     C                   EXSR      SRA
     C                   EXSR      SRB
     C                   MOVE      '1'           *INLR
      *////////////////////////////////////////////////////////////
      *  Initiering                                           SRA /
      *////////////////////////////////////////////////////////////
     C     SRA           BEGSR
      *____________________
      * Definiere variabler
      *""""""""""""""""""""
     C     *LIKE         DEFINE    SVEX_SYAV     ZVEX_SYAV
     C     *LIKE         DEFINE    SVEX_SYOP     ZVEX_SYOP
      *_________________
      * Input parametere
      *"""""""""""""""""
     C     *ENTRY        PLIST
     C                   PARM                    ZVEX_SYAV
     C                   PARM                    ZVEX_SYOP
      *__________________
      * Definiere Nøkkler
      *""""""""""""""""""
     C     SVEX_KEY      KLIST
     C                   KFLD                    ZVEX_SYAV
     C                   KFLD                    ZVEX_SYOP
      *_________________________________
      * Hæmta dagens datum och klockslag
      *"""""""""""""""""""""""""""""""""
     C                   CALL      'SYGE15C'
     C                   PARM                    WDT               8
     C                   PARM                    WTM               6
     C                   ENDSR
      *////////////////////////////////////////////////////////////
      *  Bearbeide Formater                                   SRB /
      *////////////////////////////////////////////////////////////
     C     SRB           BEGSR
     C     SVEX_KEY      CHAIN(N)  SVEX                               90
     C                   EVAL      XVEX_SYAV=ZVEX_SYAV
     C                   EVAL      XVEX_SYOP=ZVEX_SYOP
     C                   EVAL      XVEX_SUK1=SVEX_SUK1
     C                   EVAL      XVEX_SUK6=SVEX_SUK6
     C                   EVAL      XVEX_SUK11=SVEX_SUK11
     C                   EVAL      XVEX_SUT1=SVEX_SUT1
     C                   EVAL      XVEX_SUT2=SVEX_SUT2
     C                   EVAL      XVEX_SUT3=SVEX_SUT3
     C                   EVAL      XVEX_SUT4=SVEX_SUT4
     C                   EVAL      XVEX_SUT5=SVEX_SUT5
     C                   EVAL      XVEX_SUT6=SVEX_SUT6
     C                   EVAL      XVEX_SUT7=SVEX_SUT7
     C                   EVAL      XVEX_SUT8=SVEX_SUT8
     C                   EVAL      XVEX_SUT9=SVEX_SUT9
     C                   EVAL      XVEX_SUT10=SVEX_SUT10
     C                   EVAL      XVEX_SUT11=SVEX_SUT11
     C                   EVAL      XVEX_SUT12=SVEX_SUT12
     C                   EVAL      XVEX_SUT13=SVEX_SUT13
     C                   EVAL      XVEX_SUT14=SVEX_SUT14
     C                   EVAL      XVEX_SUT15=SVEX_SUT15
      * Viser bilde
     C     TAG01         TAG
     C                   EXFMT     SVE033DA
      * andre funksjonstaster
     C     *IN03         CABEQ     *ON           SRB8
     C     *IN12         CABEQ     *ON           SRB9
      * F4=Søk
     C     WSCLOC        DIV       256           LINNBR
     C                   MVR                     POSNBR
     C     *IN04         IFEQ      '1'
     C     LINNBR        IFGE      6
     C     LINNBR        ANDLE     16
     C     POSNBR        ANDEQ     2
     C                   EXSR      SRC
     C                   GOTO      TAG01
     C                   ENDIF
     C                   ENDIF

     C     SRB8          TAG
     C     SVEX_KEY      CHAIN     SVEX                               90
     C   90              EVAL      SVEX_SYAV=ZVEX_SYAV
     C   90              EVAL      SVEX_SYOP=ZVEX_SYOP
     C                   EVAL      SVEX_SUK1=XVEX_SUK1
     C                   EVAL      SVEX_SUK6=XVEX_SUK6
     C                   EVAL      SVEX_SUK11=XVEX_SUK11
     C                   EVAL      SVEX_SUT1=XVEX_SUT1
     C                   EVAL      SVEX_SUT2=XVEX_SUT2
     C                   EVAL      SVEX_SUT3=XVEX_SUT3
     C                   EVAL      SVEX_SUT4=XVEX_SUT4
     C                   EVAL      SVEX_SUT5=XVEX_SUT5
     C                   EVAL      SVEX_SUT6=XVEX_SUT6
     C                   EVAL      SVEX_SUT7=XVEX_SUT7
     C                   EVAL      SVEX_SUT8=XVEX_SUT8
     C                   EVAL      SVEX_SUT9=XVEX_SUT9
     C                   EVAL      SVEX_SUT10=XVEX_SUT10
     C                   EVAL      SVEX_SUT11=XVEX_SUT11
     C                   EVAL      SVEX_SUT12=XVEX_SUT12
     C                   EVAL      SVEX_SUT13=XVEX_SUT13
     C                   EVAL      SVEX_SUT14=XVEX_SUT14
     C                   EVAL      SVEX_SUT15=XVEX_SUT15
     C  N90              UPDATE    SVEXR
     C   90              WRITE     SVEXR

     C     SRB9          TAG
     C                   ENDSR
      *////////////////////////////////////////////////////////////
      *  Søk                                                  SRC /
      *////////////////////////////////////////////////////////////
     C     SRC           BEGSR
      * Særskilda upplysningar, kod
     C                   CALL      'SVT003R'
     C                   PARM      'SAL'         UTYP              3
S01  C*                  PARM      'A'           UIE               1
N01  C                   PARM      'E'           UIE               1
     C                   PARM      WDT           UDATO             8
     C                   PARM                    UKD              10
     C     UKD           IFNE      *BLANKS
     C                   SELECT
     C     LINNBR        WHENEQ    6
     C                   MOVEL     UKD           XVEX_SUK1
     C     LINNBR        WHENEQ    11
     C                   MOVEL     UKD           XVEX_SUK6
     C     LINNBR        WHENEQ    16
     C                   MOVEL     UKD           XVEX_SUK11
     C                   ENDSL
     C                   ENDIF
     C                   GOTO      SRC1

     C     SRC1          ENDSR
