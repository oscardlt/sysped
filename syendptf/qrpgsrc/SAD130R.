     C***********************************************
     C     NYSØK         BEGSR
     C***********************************************
     C                   CLEAR                   BILDE01
     C                   CALL      'SYGE15C'
     C                   PARM                    WDT               8
     C                   PARM                    WTM               6
      *
     C     W2AVD         IFEQ      0
     C                   MOVE      UAVD          WSAVD
     C                   ELSE
     C                   MOVE      W2AVD         WSAVD
     C                   END
     C     W2PRO         IFNE      0                                            1<-B
     C                   MOVE      W2PRO         WSPRO
     C                   ELSE                                                   1
     C     W2ETAD        IFNE      0                                             2<-B
     C                   MOVE      W2ETA         WSETA
     C                   ELSE                                                    2
     C     W2ST          IFNE      *BLANKS                                        3<-B
     C                   MOVE      W2ST          WSST
     C                   ELSE                                                     3
     C     W2DTR         IFNE      0                                               4<-B
     C                   MOVE      W2DTR         WSDTR
     C                   END                                                       4<-E
     C                   END                                                      3<-E
     C                   END                                                     2<-E
     C                   END                                                    1<-E
      *
     C                   MOVE      *ZEROS        W2NR
     C                   MOVE      *ZEROS        W2AVD
     C                   MOVE      *ZEROS        W2PRO
     C                   MOVE      *ZEROS        W2ETA
     C                   MOVE      *BLANKS       W2ST
     C                   MOVE      *ZEROS        W2DTR
      *
     C                   MOVEA     '00'          *IN(98)
     C                   EXSR      TESTB1
      *
     C                   ENDSR
      *
     C***********************************************
     C     NYTUR         BEGSR
     C***********************************************
     C     W2AVD         IFEQ      0
     C                   MOVE      UAVD          W2AVD
     C                   END
     C     SADEFL01K1    CHAIN     SADEFL01                           98
     C   98              DO
     C                   MOVE      'YBC2035'     MSGNR
     C                   GOTO      SLTUR
     C                   END
     C                   MOVE      '1'           *IN90
      *
     C                   SELECT
     C     W2NR          WHENEQ    2
      * kan ikke gå inn på normale oppdrag fra dummy
     C                   IF        EFST <> ' ' AND EFST <> 'C' AND EFST <> 'M'
     C                   MOVE      '1'           *IN30
     C                   MOVE      'YBC2036'     MSGNR
     C                   GOTO      SLTUR
     C                   END
     C                   EXSR      CHGTUR
     C     W2NR          WHENEQ    3
     C                   EXSR      CPYTUR
     C     W2NR          WHENEQ    5
     C                   EXSR      DSPTUR
     C     W2NR          WHENEQ    9
     C                   EXSR      DSPLOG
     C                   ENDSL
      *
     C     SLTUR         TAG
     C   98              MOVE      '1'           *IN30
     C                   MOVE      '0'           *IN90
     C                   MOVE      *ZEROS        W2NR
     C  N98              MOVE      *ZEROS        W2AVD
     C  N98              MOVE      *ZEROS        W2PRO
      *
     C                   ENDSR
      *
     C***********************************************
     C     HENT          BEGSR
     C***********************************************
     C                   MOVE      *BLANKS       MSGNR
     C                   MOVEA     '000'         *IN(90)
     C                   MOVEA     '00'          *IN(98)
     C     *IN98         DOWEQ     '0'                                          1<-B
     C                   READC     SUBFIL                                 98
     C     *IN98         IFEQ      '0'                                           2<-B
     C                   SELECT                                                   3<-B
     C     WSNR          WHENEQ    2                                              3
     C                   MOVE      '1'           *IN99
     C                   IF        EFST <> ' ' AND EFST <> 'C' AND EFST <> 'M'
     C                   MOVE      '1'           *IN30
     C                   MOVE      'YBC2036'     MSGNR
     C                   MOVE      *ZEROS        WSNR
     C                   UPDATE    SUBFIL
     C                   GOTO      SLHENT
     C                   END
     C                   EXSR      CHGTUR
     C     WSNR          WHENEQ    3                                              3
     C                   MOVE      '1'           *IN99
     C                   EXSR      CPYTUR
     C     WSNR          WHENEQ    5                                              3
     C                   MOVE      '1'           *IN99
     C                   EXSR      DSPTUR
     C     WSNR          WHENEQ    9                                              3
     C                   MOVE      '1'           *IN99
     C                   EXSR      DSPLOG
     C     WSNR          WHENEQ    90                                             3
     C                   IF        EFST <> ' ' AND EFST <> 'C' AND EFST <> 'M'
     C                   MOVE      '1'           *IN30
     C                   MOVE      'YBC2036'     MSGNR
     C                   GOTO      SLHENT
     C                   END
     C                   MOVE      '1'           *IN99
     C                   EXSR      SNDTUR
     C                   ENDSL                                                    3<-E
     C                   END                                                     2<-E
     C                   ENDDO                                                  1<-E
      *
     C     SLHENT        TAG
     C   98
     CANN99              DO
     C                   MOVE      'DIV0012'     MSGNR
     C                   MOVE      '1'           *IN30
     C                   END
     C                   MOVE      '0'           *IN99
      *
     C                   ENDSR
      *
      ***********************************************
     C     SETCUR        BEGSR
      ***********************************************
     C                   Z-ADD     35            POSNBR
     C                   SELECT
     C                   WHEN      WSPRO  <> 0
     C                   Z-ADD     8             LINNBR
     C                   WHEN      WSUUID <> *BLANKS
     C                   Z-ADD     9             LINNBR
     C                   WHEN      WSETA <> 0
     C                   Z-ADD     10            LINNBR
     C                   WHEN      WSDTR <> 0
     C                   Z-ADD     11            LINNBR
     C                   WHEN      WSST   <> *BLANKS
     C                   Z-ADD     12            LINNBR
     C                   OTHER
     C                   Z-ADD     8             LINNBR
     C                   ENDSL
      *
     C                   ENDSR
      *
     C***********************************************
     C     DSPTUR        BEGSR
     C***********************************************
     C                   MOVE      'DSP'         XTYPE                          SPØRRING
     C                   CALL      'SAD131R'
     C                   PARM      EFAVD         U_AVD             4 0
     C                   PARM      EFPRO         U_PRO             8 0
     C                   PARM                    USG
     C                   PARM                    XTYPE             3
     C                   PARM                    UFLAGG            1 0
     C     UFLAGG        IFEQ      3
     C                   MOVE      '1'           *INLR
     C                   RETURN
     C                   END
     C  N90              MOVE      *ZEROS        WSNR
     C  N90              UPDATE    SUBFIL
      *
     C                   ENDSR
      *
     C***********************************************
     C     CHGTUR        BEGSR
     C***********************************************
     C                   MOVE      'CHG'         XTYPE             3
     C                   CALL      'SAD131R'
     C                   PARM      EFAVD         U_AVD             4 0
     C                   PARM      EFPRO         U_PRO             8 0
     C                   PARM                    USG
     C                   PARM                    XTYPE
     C                   PARM                    UFLAGG            1 0
