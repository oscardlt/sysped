     C                   EXSR      INIT
     C*
     C     KODTAK        CHAIN     KODTA                              99
     C     SAMLBK        CHAIN     SAMLB                              98
NV5  C                   MOVE      SMDTD         WSDTD
NV5  C                   MOVE      SMDTM         WSDTM
NV5  C                   MOVE      SMDTÅ         WSDTÅ
     C*
     C     START         TAG
     C                   MOVE      '1'           *IN93
     C                   WRITE     SUBCTL
     C                   MOVEA     '00'          *IN(93)
     C                   Z-ADD     *ZEROS        ZRECNO
     C                   Z-ADD     *ZEROS        WSVKT2
      *_____________
      * Fyll Subfile
      *"""""""""""""
     C     YRFA          SETLL     HEAD19
     C     START1        TAG
     C     YRFA          READE     HEAD19                                 94
      *
     C     START2        TAG
      *
     C  N94              DO
     C                   DO        14
     C                   ADD       1             ZRECNO
     C                   ADD       HEFBV         WSVKT2
     C                   WRITE     SUBFIL
     C     ST21          TAG
     C     YRFA          READE     HEAD19                                 94
     C  N94              END
     C                   END
     C*
     C     START3        TAG
     C                   MOVEA     '11'          *IN(91)
     C                   EXSR      KDTFMT
     C     ZRECNO        IFEQ      0
     C                   EXFMT     NYWRK
     C                   EXSR      KDTFMT
     C                   GOTO      ST41
     C                   ELSE
     C                   WRITE     BUNN
     C                   END
     C                   EXFMT     SUBCTL
     C                   EXSR      KDTFMT
     C*
     C*** AVSLUTT
     C     ST41          TAG
     C   03              GOTO      SLUTT
     C   12              GOTO      SLUTT
     C*
     C*** FRISK OPP
     C   05              GOTO      START
     C*
     C*** BLA VIDERE
     C   25              DO
     C                   SETOFF                                       25
     C                   GOTO      START2
     C                   END
     C*
     C                   GOTO      START3
     C*
     C     SLUTT         TAG
     C                   SETON                                        LR
     C                   RETURN
     C*
     C***********************************************
     C     INIT          BEGSR
     C***********************************************
     C     *ENTRY        PLIST
     C                   PARM                    USSD              3
     C                   PARM                    USLNR             3 0
     C                   PARM                    USAVD             4 0
     C                   MOVE      USSD          WSSD              3
     C                   MOVE      USLNR         WSLNR             3 0
     C                   MOVE      USAVD         WSAVD             4 0
     C     SAMLBK        KLIST
     C                   KFLD                    WSSD
     C                   KFLD                    WSLNR
     C     KODTAK        KLIST
     C                   KFLD                    YAUNI
     C                   KFLD                    WSAVD
     C*
     C                   MOVE      'A'           YAUNI             1
     C                   MOVE      *BLANKS       XBLANK
     C*
     C                   ENDSR
     C*
     C***********************************************
     C     KDTFMT        BEGSR
     C***********************************************
SV5  C*                    MOVE WSDT      XDTFMT  40
SV5  C*                    MOVELWSDT      UDT1    20
SV5  C*                    MOVELXDTFMT    UDT2    20
SV5  C*                    MOVE XDTFMT    UDT3    20
NV5  C                   MOVE      WSDTD         UDT1              2 0
NV5  C                   MOVE      WSDTM         UDT2              2 0
NV5  C                   MOVE      WSDTÅ         UDT3              2 0
     C                   CALL      'DATOFMT'
     C                   PARM                    UDT1
     C                   PARM                    UDT2
     C                   PARM                    UDT3
SV5  C*                    MOVELUDT1      WSDT
SV5  C*                    MOVELUDT2      XDTFMT
SV5  C*                    MOVE UDT3      XDTFMT
SV5  C*                    MOVE XDTFMT    WSDT
NV5  C                   MOVE      UDT1          WSDTD
