             PGM        PARM(&FRALIB &TILLIB &GMLLIB &TOMME)
             DCL        VAR(&FRALIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TILLIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&GMLLIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TOMME) TYPE(*CHAR) LEN(1)

             CRTLIB     LIB(&GMLLIB) TEXT('Gamle filer (hist.) som +
                          er byttet etter PTF5')

             CLRPFM     FILE(&TILLIB/BILLOF)
             CPYF       FROMFILE(&FRALIB/BILLOF) +
                          TOFILE(&TILLIB/BILLOF) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             MONMSG     MSGID(CPF0000)
             IF         COND(&TOMME = 'J') THEN(CLRPFM +
                          FILE(&FRALIB/BILLOF))
             MOVOBJ     OBJ(&FRALIB/BILLOF) OBJTYPE(*FILE) +
                          TOLIB(&GMLLIB)

             CLRPFM     FILE(&TILLIB/BLV1)
             CPYF       FROMFILE(&FRALIB/BLV1) TOFILE(&TILLIB/BLV1) +
                          MBROPT(*REPLACE) FMTOPT(*MAP *DROP)
             MONMSG     MSGID(CPF0000)
             IF         COND(&TOMME = 'J') THEN(CLRPFM +
                          FILE(&FRALIB/BLV1))
             MOVOBJ     OBJ(&FRALIB/BLV1) OBJTYPE(*FILE) TOLIB(&GMLLIB)

             CLRPFM     FILE(&TILLIB/BLV2)
             CPYF       FROMFILE(&FRALIB/BLV2) TOFILE(&TILLIB/BLV2) +
                          MBROPT(*REPLACE) FMTOPT(*MAP *DROP)
             MONMSG     MSGID(CPF0000)
             IF         COND(&TOMME = 'J') THEN(CLRPFM +
                          FILE(&FRALIB/BLV2))
             MOVOBJ     OBJ(&FRALIB/BLV2) OBJTYPE(*FILE) TOLIB(&GMLLIB)

             CLRPFM     FILE(&TILLIB/BOOKING)
             CPYF       FROMFILE(&FRALIB/BOOKING) +
                          TOFILE(&TILLIB/BOOKING) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             MONMSG     MSGID(CPF0000)
             IF         COND(&TOMME = 'J') THEN(CLRPFM +
                          FILE(&FRALIB/BOOKING))
             MOVOBJ     OBJ(&FRALIB/BOOKING) OBJTYPE(*FILE) +
                          TOLIB(&GMLLIB)

             CLRPFM     FILE(&TILLIB/DOKUDGS)
             CPYF       FROMFILE(&FRALIB/DOKUDGS) +
                          TOFILE(&TILLIB/DOKUDGS) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             MONMSG     MSGID(CPF0000)
             IF         COND(&TOMME = 'J') THEN(DO)
             CLRPFM     FILE(&FRALIB/DOKUDGS)
             MONMSG     MSGID(CPF0000)
             ENDDO
             MOVOBJ     OBJ(&FRALIB/DOKUDGS) OBJTYPE(*FILE) +
                          TOLIB(&GMLLIB)
             MONMSG     MSGID(CPF0000)

             CLRPFM     FILE(&TILLIB/DOKUFE)
             CPYF       FROMFILE(&FRALIB/DOKUFE) +
                          TOFILE(&TILLIB/DOKUFE) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             MONMSG     MSGID(CPF0000)
             IF         COND(&TOMME = 'J') THEN(DO)
             CLRPFM     FILE(&FRALIB/DOKUFE)
             MONMSG     MSGID(CPF0000)
             ENDDO
             MOVOBJ     OBJ(&FRALIB/DOKUFE) OBJTYPE(*FILE) +
                          TOLIB(&GMLLIB)
             MONMSG     MSGID(CPF0000)

             CLRPFM     FILE(&TILLIB/PALLD)
             CPYF       FROMFILE(&FRALIB/PALLD) +
                          TOFILE(&TILLIB/PALLD) MBROPT(*REPLACE) +
                          FMTOPT(*MAP *DROP)
             MONMSG     MSGID(CPF0000)
             IF         COND(&TOMME = 'J') THEN(DO)
             CLRPFM     FILE(&FRALIB/PALLD)
             ENDDO
             MOVOBJ     OBJ(&FRALIB/PALLD) OBJTYPE(*FILE) +
                          TOLIB(&GMLLIB)

             ENDPGM
