     C                   EXSR      INIT
      *
     C                   SELECT
     C                   WHEN      XMODUL = 'I'
     C                   EXSR      SROPDMR
     C                   WHEN      XMODUL = 'U'
     C                   EXSR      SROPDJD
     C                   ENDSL
      *
     C                   EVAL      *INLR = *ON
     C                   RETURN
