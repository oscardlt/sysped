      *
      ***********************************************************
      *** PROGRAM FOR LES/OPPDATER FAXUSER & BRUKERKATALOG    ***
      ***********************************************************
     FCUNDL01   IF   E           K DISK
     FFAXUSL1   UF   E           K DISK
     FFAXUSRS   UF A E           K DISK
     F                                     RENAME(FAXREC:FAXRC1)
     C*
     C                   EXSR      INIT
     C*
     C     KUNKEY        SETLL     KUNDR
     C                   READ      KUNDR                                  33
     C     *IN33         DOWEQ     '0'
     C     FIRMA         DOWEQ     UFIRMA
     C     AKTKOD        IFNE      'A'
     C     TFAXNR        IFEQ      *BLANKS
     C                   GOTO      NESTE
     C                   END
     C                   END
     C     AKTKOD        IFEQ      'A'
     C     TFAXNR        IFEQ      *BLANKS
     C                   GOTO      NESTE
     C                   END
     C                   END
     C                   MOVEL     TFAXNR        PFAXNB           32
     C                   MOVEL     KNAVN         PTO              40
     C                   MOVEL     ADR1          PTO1             40
     C                   MOVEL     ADR2          PTO2             40
     C     POSTNR        IFEQ      0
     C                   MOVEL     SYPOGE        PTO3             40
     C                   ELSE
     C                   MOVEL     POSTNR        PTO3
     C                   END
     C                   MOVE      ADR3          PTO3
     C                   MOVEL     KUNDNR        PKNU              8
     C                   MOVEL     SONAVN        PALFA            10
     C                   MOVEL     TLF           PTELE            32
     C                   EXSR      USER
     C     NESTE         TAG
     C                   READ      KUNDR                                  33
     C                   END
     C                   SETON                                        33
     C                   END
     C*
     C     SLUTT         TAG
     C                   SETON                                        LR
     C                   RETURN
     C*
     C***********************************************
     C     USER          BEGSR
     C*
     C     KUNKY1        CHAIN     FAXREC                             40
     C     *IN40         IFEQ      '1'
     C                   MOVEL     SONAVN        USRFIX            4
     C                   MOVEL     Z             USRFX1            5
     C                   MOVE      USRFIX        USRFX1
     C                   Z-ADD     1             TELLER            3 0
     C                   MOVEL     USRFX1        PUSERI
     C                   MOVE      TELLER        PUSERI
     C     EXIST         TAG
     C     PUSERI        CHAIN     FAXRC1                             41
     C     *IN41         IFEQ      '0'
     C                   ADD       1             TELLER
     C                   MOVE      TELLER        PUSERI
     C                   GOTO      EXIST
     C                   END
     C                   CALL      'ADDFAXCP'
     C                   PARM                    PUSERI
     C                   PARM                    PFAXNB
     C                   PARM                    PTO
     C                   PARM                    PTITLE
     C                   PARM                    PCOMME
     C                   PARM                    PTO1
     C                   PARM                    PTO2
     C                   PARM                    PTO3
     C                   PARM                    PFI
     C                   PARM                    PKNU
     C                   PARM                    PALFA
     C                   PARM                    PTELE
     C                   MOVE      *BLANKS       FUSERI
     C                   MOVE      *BLANKS       FFAXNB
     C                   MOVE      *BLANKS       FTO
     C                   MOVE      *BLANKS       FTITLE
     C                   MOVE      *BLANKS       FCOMME
     C                   MOVE      *BLANKS       FTO1
     C                   MOVE      *BLANKS       FTO2
     C                   MOVE      *BLANKS       FTO3
     C                   MOVE      *BLANKS       FFI
     C                   MOVE      *BLANKS       FKNU
     C                   MOVE      *BLANKS       FALFA
     C                   MOVE      *BLANKS       FTELE
     C                   MOVE      PUSERI        FUSERI
     C                   MOVE      PFAXNB        FFAXNB
     C                   MOVE      PTO           FTO
     C                   MOVE      PTITLE        FTITLE
     C                   MOVE      PCOMME        FCOMME
     C                   MOVE      PTO1          FTO1
     C                   MOVE      PTO2          FTO2
     C                   MOVE      PTO3          FTO3
     C                   MOVE      PFI           FFI
     C                   MOVE      PKNU          FKNU
     C                   MOVE      PALFA         FALFA
     C                   MOVE      PTELE         FTELE
     C                   WRITE     FAXRC1
     C                   ELSE
     C                   MOVEL     FUSERI        PUSERI            8
     C                   CALL      'CHGFAXCP'
     C                   PARM                    PUSERI
     C                   PARM                    PFAXNB
     C                   PARM                    PTO
     C                   PARM                    PTITLE
     C                   PARM                    PCOMME
     C                   PARM                    PTO1
     C                   PARM                    PTO2
     C                   PARM                    PTO3
     C                   PARM                    PFI
     C                   PARM                    PKNU
     C                   PARM                    PALFA
     C                   PARM                    PTELE
     C                   MOVE      *BLANKS       FUSERI
     C                   MOVE      *BLANKS       FFAXNB
     C                   MOVE      *BLANKS       FTO
     C                   MOVE      *BLANKS       FTITLE
     C                   MOVE      *BLANKS       FCOMME
     C                   MOVE      *BLANKS       FTO1
     C                   MOVE      *BLANKS       FTO2
     C                   MOVE      *BLANKS       FTO3
     C                   MOVE      *BLANKS       FFI
     C                   MOVE      *BLANKS       FKNU
     C                   MOVE      *BLANKS       FALFA
     C                   MOVE      *BLANKS       FTELE
     C                   MOVE      PUSERI        FUSERI
     C                   MOVE      PFAXNB        FFAXNB
     C                   MOVE      PTO           FTO
     C                   MOVE      PTITLE        FTITLE
     C                   MOVE      PCOMME        FCOMME
     C                   MOVE      PTO1          FTO1
     C                   MOVE      PTO2          FTO2
     C                   MOVE      PTO3          FTO3
     C                   MOVE      PFI           FFI
     C                   MOVE      PKNU          FKNU
     C                   MOVE      PALFA         FALFA
     C                   MOVE      PTELE         FTELE
     C                   UPDATE    FAXREC
     C                   END
     C*
     C                   ENDSR
     C*
     C***********************************************
     C     INIT          BEGSR
     C*
     C     *ENTRY        PLIST
     C                   PARM                    UFIRMA            2
     C*
     C     KUNKEY        KLIST
     C                   KFLD                    UFIRMA
     C                   KFLD                    KUNDNR
     C*
     C     KUNKY1        KLIST
     C                   KFLD                    UFIRMA
     C                   KFLD                    PKNU
     C*
     C                   MOVE      'Z'           Z                 1
     C                   MOVEL     'Fax fra '    TXT1              8
     C                   MOVEL     'officevi'    TXT2              8
     C                   MOVEL     'sion mot'    TXT3              8
     C                   MOVEL     ' facsimi'    TXT4              8
     C                   MOVEL     'le/400  '    TXT5              8
     C                   MOVEL     TXT1          TXT6             16
     C                   MOVE      TXT2          TXT6
     C                   MOVEL     TXT3          TXT7             16
     C                   MOVE      TXT4          TXT7
     C                   MOVEL     TXT6          TXT8             32
     C                   MOVE      TXT7          TXT8
     C                   MOVEL     TXT8          PTITLE           40
     C                   MOVE      TXT5          PTITLE
     C                   MOVE      *BLANKS       PCOMME           40
     C                   MOVE      UFIRMA        PFI               2
     C*
     C                   ENDSR
     C*
