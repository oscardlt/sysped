             PGM        PARM(&LIBFR &LIBTO)
             DCL        VAR(&LIBFR) TYPE(*CHAR) LEN(10)
             DCL        VAR(&LIBTO) TYPE(*CHAR) LEN(10)

             CHGJOB     RUNPTY(21) LOG(4 00 *SECLVL) LOGCLPGM(*YES)

             CHKOBJ     OBJ(&LIBFR) OBJTYPE(*LIB)
             MONMSG     MSGID(CPF9801) EXEC(DO)
             SNDPGMMSG  MSG('FRA BIBLIOTEK FINNES IKKE.')
             GOTO       CMDLBL(SLUTT)
             ENDDO

             CHKOBJ     OBJ(&LIBTO) OBJTYPE(*LIB)
             MONMSG     MSGID(CPF9801) EXEC(DO)
             CRTLIB     LIB(&LIBTO) TEXT('SYSPED - HISTORIKK      +
                          FOR XXX')
             CRTDUPOBJ  OBJ(*ALL) FROMLIB(&LIBFR) OBJTYPE(*ALL) +
                          TOLIB(&LIBTO)
             GOTO       CMDLBL(SLUTT)
             ENDDO

             SNDPGMMSG  MSG('TIL BIBLIOTEK FINNES ALLEREDE. SLETTS! +
                          PROGRAMMET SKAL AUTOMATISK LAGE DENNE.')

 SLUTT:      CHGJOB     RUNPTY(21) LOG(4 00 *SECLVL) LOGCLPGM(*YES)

             ENDPGM
