      * SUMMER KOLLI I LAGER
     c     BSKN          SETLL     EPOK02
     c     BSKN          READE     EPOK02                                 33
     C                   DOW       *IN33 = *OFF
     C                   SELECT
     C                   WHEN      WSPRT = 'J'
     C                   WHEN      WSCLT = 'J'
     C     EKDTHK        CABEQ     0             LESNESTE
     C                   OTHER
     C     EKDTIN        CABEQ     0             LESNESTE
     C                   ENDSL
S02  C*    EPOHK         CHAIN     EPOH                               33
N02  C     EPOHK         CHAIN(N)  EPOH                               33
     C  N33EHINV         COMP      WSKNK                              3333
     C  N33EPOLK         CHAIN     EPOL                               33
     C  N33              EVAL      VAKURE = EHLEVN
     C  N33VADR3K        CHAIN     VADR3                              33
     C  N331             DO        5000          XN                5 0
     C                   SELECT
     C                   WHEN      IO(XN) = *BLANKS
     C                   EVAL      IO(XN) = IOREF
     C                   EVAL      IN(XN) = ELART
     C                   EVAL      ST(XN) = ELSTA
     C                   EVAL      AN(XN)  = ELNTKL
     C                   EVAL      NT(XN)  = 1
     C                   EVAL(H)   VKB(XN) = ELVKTL / ELNTKL
     C                   EVAL(H)   VKN(XN) = ELVKNL / ELNTKL
     C                   EVAL(H)   M3(XN)  = ELM3L / ELNTKL
     C                   LEAVE
     C                   WHEN      IO(XN) = IOREF
     C                   IF        ST(XN) < ELSTA
     C                   EVAL      ST(XN) = ELSTA
     C                   END
     C                   EVAL      NT(XN)  = NT(XN) + 1
     C                   EVAL(H)   VKB(XN) = VKB(XN) + ELVKTL / ELNTKL
     C                   EVAL(H)   VKN(XN) = VKN(XN) + ELVKNL / ELNTKL
     C                   EVAL(H)   M3(XN)  = M3(XN) + ELM3L / ELNTKL
     C                   LEAVE
     C                   ENDSL
     C                   ENDDO
      *
N01  C  N33              IF        ELREAS = *BLANKS
     C  N33              SELECT
     C*                  WHEN      EHDATO < XDT_14
     C                   WHEN      EKDTIN < XDT_14
     C                   EVAL      D14(XN) = 'J'
     C                   EVAL      D07(XN) = ' '
     C*                  WHEN      EHDATO < XDT_07 AND D14(XN) = *BLANKS
     C                   WHEN      EKDTIN < XDT_07 AND D14(XN) = *BLANKS
     C                   EVAL      D07(XN) = 'J'
     C                   ENDSL
N01  C                   END
      *
     C  N33              SELECT
     C                   WHEN      DTIN(XN) = 0
     C                   EVAL      DTIN(XN) = EKDTIN
     C                   WHEN      DTIN(XN) > EKDTIN
     C                   EVAL      DTIN(XN) = EKDTIN
     C                   ENDSL
N01   *
N01  C                   IF        *IN33 = *OFF
N01  C                   MOVE      *BLANKS       DLY(XN)
N01  C                   MOVEL     'POMDL'       PAID
N01  C                   MOVE      BIBRID        PABRID
N01  C     BRPARFK       CHAIN     BRPARF                             33
N01  c   33              Eval      PABRID = '*KUNDFT*'+BIFIRM
N01  C   33BRPARFK       CHAIN     BRPARF                             33
N01  C                   DOW       *IN33 = *OFF
N01  C                   MOVEL     PAVRDA        XAN02             2
N01  C                   IF        XAN02 = ELREAS
N01  C                   EVAL      DLY(XN) = %SUBST(PAVRDA:4:47)
N01  C                   LEAVE
N01  C                   END
N01  C     BRPARFK       READE     BRPARF                                 33
N01  C                   ENDDO
N01  C                   EVAL      *IN33 = *OFF
N01  C                   END
