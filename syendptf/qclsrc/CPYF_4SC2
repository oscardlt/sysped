             PGM
             DCL        VAR(&OK) TYPE(*CHAR) LEN(1) VALUE('2')
             DCL        VAR(&FRA) TYPE(*CHAR) LEN(10)
             DCL        VAR(&TIL) TYPE(*CHAR) LEN(10)
             DCL        VAR(&LOG) TYPE(*CHAR) LEN(10)

             CALL       PGM(CPYF_4SIFR) PARM(&OK &FRA &TIL &LOG)
             IF         COND(&OK = 'J') THEN(CALL PGM(CPYF_4S2) +
                          PARM(&FRA &TIL &LOG))

             ENDPGM
