             PGM        PARM(&AVD &TDN &SN)
             DCL        VAR(&AVD) TYPE(*CHAR) LEN(4)
             DCL        VAR(&TDN) TYPE(*CHAR) LEN(7)
             DCL        VAR(&SN) TYPE(*CHAR) LEN(9)
             DCL        VAR(&LIB) TYPE(*CHAR) LEN(10)

             RTVDTAARA  DTAARA(FIRMDT (141 10)) RTNVAR(&LIB)
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)

             CLRPFM     FILE(QTEMP/SAEHEDI)
             MONMSG     MSGID(CPF3142) EXEC(CRTDUPOBJ OBJ(SAEHEDI) +
                          FROMLIB(&LIB) OBJTYPE(*FILE) TOLIB(QTEMP))
             CLRPFM     FILE(QTEMP/SAEVEDI)
             MONMSG     MSGID(CPF3142) EXEC(CRTDUPOBJ OBJ(SAEVEDI) +
                          FROMLIB(&LIB) OBJTYPE(*FILE) TOLIB(QTEMP))
             CLRPFM     FILE(QTEMP/SAEFEDI)
             MONMSG     MSGID(CPF3142) EXEC(CRTDUPOBJ OBJ(SAEFEDI) +
                          FROMLIB(&LIB) OBJTYPE(*FILE) TOLIB(QTEMP))

             CALL       PGM(SAD017) PARM(&AVD &TDN &SN)

             ENDPGM
