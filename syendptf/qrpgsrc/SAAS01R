     FEDIC      IF A E             DISK
     FFIRM      IF A E           K DISK
     FTELLGE    UF   E           K DISK
     FBRIDF     IF A E           K DISK
      *
     C     *ENTRY        PLIST
     C                   PARM                    UPREFIX           3
      *
     C     1             CHAIN     EDIC                               33
     C                   IF        *IN33 = *OFF
     C                   EVAL      CSN = 1001
     C                   EVAL      CMN = 1001
     C                   WRITE     EDICR
     C                   END
      *
     C                   EVAL      FIFIRM = %SUBST(UPREFIX:2:2)
     C     FIFIRM        CHAIN     FIRM                               33
     C                   IF        *IN33 = *OFF
     C                   EVAL      FILIBF = FIFIRM + 'SYSPEDF'
     C                   EVAL      FILIBO = FIFIRM + 'SYSPEDO'
     C                   WRITE     FIRMR
     C                   END
      *
     C     *LOVAL        SETLL     TELLGE
     C                   READ      TELLGE                                 33
     C                   DOW       *IN33 = *OFF
     C                   EVAL      GENO = 1001
     C                   UPDATE    TELLGER
     C                   READ      TELLGE                                 33
     C                   ENDDO
      *
     C                   EVAL      BIBRID = '*KUNDFT*' + FIFIRM
     C     BIBRID        CHAIN     BRIDF                              33
     C                   IF        *IN33 = *OFF
     C                   EVAL      BIAKT  = 'A'
     C                   EVAL      BIBESK = 'Parametre på kunde/FIRMA-NIVÅ'
     C                   EVAL      BIFIRM = FIFIRM
     C                   EVAL      BIKUNR = 1
     C                   EVAL      BIFARG = 'FFFFFF'
     C                   WRITE     BRIDFR
     C                   END
      *
     C                   EVAL      *INLR = *OFF
     C                   RETURN
