     d  wcmd           s           2048
     d  wcmd1          s            512
     d  wcmd2          s            512
     d  wcmd3          s            512
     d  x7D            s              1
     d runcmd          pr                  extpgm('QCMDEXC')
     d  Cmd                         200a   const
     d  len                          15p 5 const

      /free

       x7D=x'7D';
       wcmd1='cd /home/CB';
       wcmd2='ls';
       wcmd3='pwd';

       wcmd = 'QSH CMD(' + x7D +
              wcmd1 + ';' +
              ' '   + wcmd2 + ';' +
              ' '  + wcmd3 + ';' +
              x7D + ')';

       RunCmd(Wcmd :%len(%trim(Wcmd)));

       *inlr=*on;
      /end-free
