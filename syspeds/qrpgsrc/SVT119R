     C                   EVAL      SVTX19_12B=W12
     C                   EVAL      SVTX19_13B=W13
     C                   EVAL      SVTX19_14B=W14
     C                   EVAL      SVTX19_15B=W15
     C                   EVAL      SVTX19_16B=W16
