             PGM
             DCL        VAR(&LIB) TYPE(*CHAR) LEN(10) /* FILE +
                          LIBRARY */
             RTVDTAARA  DTAARA(FIRMDT (141 10)) RTNVAR(&LIB)

             RUNQRY     QRY(QPRORAP01) QRYFILE((&LIB/HEADF) +
                          (&LIB/FAKT)) OUTTYPE(*DISPLAY) +
                          OUTFORM(*SUMMARY) RCDSLT(*YES)
             ENDPGM
