     C     START         TAG
     C     WSGRUP        COMP      0                                      49
     C                   EXFMT     BILDE1
      **  AVSLUTT
     C     *IN03         IFEQ      *ON
     C                   MOVE      '1'           UIN01
     C                   GOTO      SLUTT
     C                   ENDIF                                                  <*IN03
      **
     C     *IN04         IFEQ      *ON
     C     WSCLOC        DIV       256           LINNBR
     C                   MVR                     POSNBR
     C     LINNBR        IFEQ      3
     C     LINNBR        OREQ      4
     C     LINNBR        IFEQ      3
     C                   MOVE      'SØKGRP  '    UTYPE             8
     C                   ELSE
     C                   MOVE      WSGRUP        UTYPE
     C                   END
     C                   CALL      'LL004R'
     C                   PARM                    UTYPE
     C                   PARM                    WSGRUP
     C                   PARM                    WSKJED
     C     WSGRUP        IFNE      0
     C                   MOVE      WSGRUP        YYKNR
     C     KUNKEY        CHAIN     CUNDL01                            30
     C  N30              MOVEL     KNAVN         NVNGRU
     C                   END
     C                   MOVE      WSKJED        YYKNR
     C     KUNKEY        CHAIN     CUNDL01                            30
     C  N30              MOVEL     KNAVN         NVNKJE
     C     *IN30         IFEQ      '0'
     C                   Z-ADD     7             LINNBR
     C                   Z-ADD     34            POSNBR
     C                   END
     C                   END
     C                   GOTO      START
     C                   END
