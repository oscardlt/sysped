        +'¬hepoen¬ : ¬'+%trim(%editc(HEPOEN:'Z'))+'¬, '
        +'¬hestl1¬ : ¬'+%trim(HESTL1)+'¬, '
        +'¬hestl2¬ : ¬'+%trim(HESTL2)+'¬, '
        +'¬hestl3¬ : ¬'+%trim(HESTL3)+'¬, '
        +'¬hestl4¬ : ¬'+%trim(HESTL4)+'¬, '
        +'¬hestn1¬ : ¬'+%trim(HESTN1)+'¬, '
        +'¬hestn2¬ : ¬'+%trim(HESTN2)+'¬, '
        +'¬hestn3¬ : ¬'+%trim(HESTN3)+'¬, '
        +'¬hestn4¬ : ¬'+%trim(HESTN4)+'¬, '
        +'¬hestn5¬ : ¬'+%trim(HESTN5)+'¬, '
        +'¬hestn6¬ : ¬'+%trim(HESTN6)+'¬, '
        +'¬hestn7¬ : ¬'+%trim(HESTN7)+'¬, '
        +'¬hestn8¬ : ¬'+%trim(HESTN8)+'¬, '
        +'¬hestn9¬ : ¬'+%trim(HESTN9)+'¬, '
        +'¬herfed¬ : ¬'+%trim(HERFED)+'¬, '
        +'¬helib¬ : ¬'+%trim(HELIB)+'¬, '
        +'¬hefile¬ : ¬'+%trim(HEFILE)+'¬, '
        +'¬hembr¬ : ¬'+%trim(HEMBR)+'¬, '
