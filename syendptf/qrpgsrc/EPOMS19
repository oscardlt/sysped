     C                   callp     updHtmlVar('WSDTIN':
     C                             %trim(%editw(DTIN(XN):'    .  .  ')))
     C                   SELECT
     C                   WHEN      D14(XN) = 'J'
     C                   callp     updHTMLvar('BGCOL':'bgcolor=red')
     C                   WHEN      D07(XN) = 'J'
     C                   callp     updHTMLvar('BGCOL':'bgcolor=yellow')
