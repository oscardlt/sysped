     C  N82              MOVE      KNAVN         WSNAFA
     C  N82              MOVEL     POSTNR        WSADFA
     C  N82              MOVE      ADR3          WSADFA
     C                   END
     C                   END
     C                   END
     C                   END
      * VED POTNRBASERT OPPDATAER HESTED
     C   42              EXSR      OPHEST
     C*
     C* OPPDATERER DISPONERINGSFILE
     C*
     C                   MOVE      'H'           DISTYP            1
     C                   EXSR      OPDDIS
     C*
     C* HVIS SATT PÅ TUR REBEREGNES TURTOTALER
     C*
     C     DITUAV        IFNE      *ZEROS
     C                   CALL      'SYTR36'
     C                   PARM                    DITUAV
     C                   PARM                    DITUR
     C                   END
     C*
     C                   MOVE      ' '           DISTYP
      * OPPDATER OPPDRAGSDATO PÅ ALLE FRISØKEVEIER
     C     HEADFK        SETLL     FRISOK
     C     HEADFK        READE     FRISOK                                 33
     C     *IN33         DOWEQ     '0'
     C                   MOVE      HEDTOP        FSDTOP
     C                   UPDATE    FRISOKR
     C     HEADFK        READE     FRISOK                                 33
     C  N33              ENDDO
     C* husk sist lagrede vekt/vil for å sjekke endring fra CS...
     c                   move      HEVKT         GMVKT2
     c                   move      HEM3          GMM32
N40   * Oppdatere advisering.. Dersom det er definert i FIRINL (kun på oppdrag med blank status)
N40  C     GMST          ifeq      ' '
N40  C     IFADVG        andne     *blanks
N40   * fjerne evt åpen
N40  c                   move      'N'           AdvFinns          1
N40  C     HEADFK        SETLL     FAKT
N40  C     HEADFK        READE     FAKT                                   33
N40  C     *IN33         DOWEQ     '0'
N40  C     FAVK          ifeq      IFADVG
N40  C     FAOPKO        ifle      'C'
N40  C                   DELETE    FAKTR
N40  c                   else
N40  c                   move      'J'           AdvFinns
N40  C                   UPDATE    FAKTR
N40  c                   endif
N40  C                   leave
N40  c                   endif
N40  C                   UPDATE    FAKTR
N40  C     HEADFK        READE     FAKT                                   33
N40  C                   enddo
N40   *
N40  c     TRFA22        ifeq      'X'
N40  c     AdvFinns      andeq     'N'
N40  C                   Move      IFADVB        XXADVB            3 0
N40  C     KUAVTA        Chain     CUTRAN                             33
N40  C     *in33         Ifeq      *OFF
N40  C     CTAVAD        Ifne      *zeros
N40  C                   Move      CTAVAD        XXADVB
N40  C                   end
N40  C     CTIKAD        Ifeq      'X'
N40  C                   Move      *ZEROS        XXADVB
N40  C                   end
N40  C                   end
N40  C     XXADVB        Ifne      *zeros
N40  C                   CLEAR                   FAKTR
N40  C                   MOVE      WSKDFF        FASK
N40  C                   MOVE      WSAVD         FAAVD
N40  C                   MOVE      WSOPD         FAOPD
N40  C                   MOVE      IFADVG        FAVK
N40  C                   MOVE      'NOK'         FAVAL
N40  C                   MOVE      'NOK'         FACU33
N40  C                   MOVE      'J'           FAKDM
N40  C                   MOVE      'M'           FAKDA
N40  C                   Z-ADD     XXADVB        FABELN
N40  C                   Z-ADD     XXADVB        FABELV
N40  C                   CALL      'SYGE06'
N40  C                   PARM                    FAAVD
N40  C                   PARM                    FAOPD
N40  C                   PARM                    FALI
N40  C                   WRITE     FAKTR
N40  c                   endif
N40  c                   endif
N40  c                   endif
N39
N39   * oppdater ETD/ETA dersom ATD/ATA ikke er satt   - Logg evt endring i T&T/CHD
N39  C                   CLEAR                   TRACKTR
N39  C     HEADFK        CHAIN     TRACKT                             33
N39  C   33              MOVE      HEAVD         TXAVD
N39  C   33              MOVE      HEOPD         TXOPD
N39  c     TXATDD        ifeq      *zeros
N39  C  N33              IF        TRSDFD<>TXETDD OR TRSDFK<>TXETDK
N39  C                   CALL      'SYTTCHD'
N39  C                   PARM                    TXAVD
N39  C                   PARM                    TXOPD
N39  C                   PARM      'CHD'         UACTI             3
N39  C                   PARM      'ETD'         UTYPTT           20
N39  C                   PARM      'ETD'         UTYPTTE          20
N39  C                   PARM      TXETDD        UGMDAT            8 0
N39  C                   PARM      TXETDK        UGMTID            4 0
N39  C                   PARM      TRSDFD        UNYDAT            8 0
N39  C                   PARM      TRSDFK        UNYTID            4 0
N39  C                   PARM                    UXTXT            70
N39  c                   endif
N39  C                   MOVE      TRSDFD        TXETDD
N39  C                   MOVE      TRSDFK        TXETDK
N39  c                   endif
N75  C   33              MOVE      TRSDFD        TXETDD
N75  C   33              MOVE      TRSDFK        TXETDK
N39  c     TXATAD        ifeq      *zeros
N39  C  N33              IF        TRSDTD<>TXETAD OR TRSDTK<>TXETAK
N39  C                   CALL      'SYTTCHD'
N39  C                   PARM                    TXAVD
N39  C                   PARM                    TXOPD
N39  C                   PARM      'CHD'         UACTI             3
N39  C                   PARM      'ETA'         UTYPTT           20
N39  C                   PARM      'ETA'         UTYPTTE          20
N39  C                   PARM      TXETAD        UGMDAT            8 0
N39  C                   PARM      TXETAK        UGMTID            4 0
N39  C                   PARM      TRSDTD        UNYDAT            8 0
N39  C                   PARM      TRSDTK        UNYTID            4 0
N39  C                   PARM                    UXTXT            70
N39  c                   endif
N39  C                   MOVE      TRSDTD        TXETAD
N39  C                   MOVE      TRSDTK        TXETAK
N39  c                   endif
N75  C   33              MOVE      TRSDTD        TXETAD
N75  C   33              MOVE      TRSDTK        TXETAK
N39  C  N33              UPDATE    TRACKTR
N39  C   33              WRITE     TRACKTR
N45
S53  C* DERSOM AVS/MOTTAKER HAR BEL/LE REGISTRERT - BRUK DENNE (OM MANU IKKE FINNES)
S53  C*                  CLEAR                   HEBRELER
S53  C*    HEADFK        CHAIN     HEBRELE                            33
S53  C*                  IF        HEHBRE=*ZEROS OR HEHBLK='K'
S53  C*                  MOVE      WSHBRE        HEHBRE
S53  C*                  MOVE      WSHLEN        HEHLEN
S53  C*                  MOVE      ' '           HEHBLK
S53  C*                  IF        HEHBRE<>*ZEROS
S53  C*                  MOVE      'K'           HEHBLK
S53  C*                  ENDIF
S53  C*                  ENDIF
S53  C*                  IF        HELBRE=*ZEROS OR HELBLK='K'
S53  C*                  MOVE      WSLBRE        HELBRE
S53  C*                  MOVE      WSLLEN        HELLEN
S53  C*                  MOVE      ' '           HELBLK
S53  C*                  IF        HELBRE<>*ZEROS
S53  C*                  MOVE      'K'           HELBLK
S53  C*                  ENDIF
S53  C*                  ENDIF
S53  C* N33              UPDATE    HEBRELER
S53  C*  33              MOVE      HEAVD         TRAVD
S53  C*  33              MOVE      HEOPD         TROPD
S53  C*  33              WRITE     HEBRELER
     C
N77   *
