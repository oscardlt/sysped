     FSVEH      IF   E           K DISK
      *//////////////
      *  Hodelogikk /
      *//////////////
     C                   EXSR      SRA
     C                   EXSR      SRB
     C                   MOVE      '1'           *INLR
      *////////////////////////////////////////////////////////////
      *  Initiering                                           SRA /
      *////////////////////////////////////////////////////////////
     C     SRA           BEGSR
      *_________________
      * Input parametrer
      *"""""""""""""""""
     C     *ENTRY        PLIST
     C                   PARM                    UVEHASYAV         4
     C                   PARM                    UVEHASYOP         7
     C                   PARM                    CALLTYP           1
     C                   MOVE      UVEHASYAV     UVEH_SYAV
     C                   MOVE      UVEHASYOP     UVEH_SYOP
      *___________________
      * Definiere Workfelt
      *"""""""""""""""""""
     C     *LIKE         DEFINE    SVEH_SYAV     UVEH_SYAV
     C     *LIKE         DEFINE    SVEH_SYOP     UVEH_SYOP
      *__________________
      * Definiere Nøkkler
      *""""""""""""""""""
     C     KEYA          KLIST
     C                   KFLD                    UVEH_SYAV
     C                   KFLD                    UVEH_SYOP
      *_____________
      * Init av felt
      *"""""""""""""
     C                   ENDSR
      *////////////////////////////////////////////////////////////
      *  Hent Tullid                                          SRB /
      *////////////////////////////////////////////////////////////
     C     SRB           BEGSR

     C     KEYA          CHAIN     SVEH                               51
      * Kall på utksriftsprogram
     C                   IF        *IN51=*OFF
N01  C                   CALL      'SVZ002R'
     C                   PARM                    SVEH_TUID
     C                   PARM                    CALLTYP           1
     C                   ENDIF
N01  C                   ENDSR
