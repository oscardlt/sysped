             PGM        PARM(&MBR &FIL &KAT)
             DCL        VAR(&MBR) TYPE(*CHAR) LEN(10)
             DCL        VAR(&FIL) TYPE(*CHAR) LEN(70)
             DCL        VAR(&KAT) TYPE(*CHAR) LEN(70)
             DCL        VAR(&TMPFIL) TYPE(*CHAR) LEN(7)
             DCL        VAR(&STMFIL) TYPE(*CHAR) LEN(50)
             DCL        VAR(&STMF) TYPE(*CHAR) LEN(70)
             DCL        VAR(&QNBR) TYPE(*CHAR) LEN(6)
             DCL        VAR(&FLR) TYPE(*CHAR) LEN(10) VALUE(QPGMR)

             RTVJOBA    NBR(&QNBR)
             CHGVAR     VAR(&TMPFIL) VALUE('W' *CAT &QNBR)
             CHGVAR     VAR(&STMF) VALUE(&TMPFIL *CAT '.XML')
             CHGVAR     VAR(&STMFIL) VALUE('/QDLS/QPGMR/' *CAT &STMF)
             CPYTOIMPF  FROMFILE(QTEMP/XMLTP1K) TOSTMF(&STMFIL) +
                          MBROPT(*REPLACE) STMFCODPAG(*PCASCII) +
                          RCDDLM(*CRLF) DTAFMT(*FIXED) FLDDLM(';') +
                          DECPNT(*COMMA)
/*           SNDDST     TYPE(*DOC) +
                          TOINTNET(('telefactor@dnbnorfinans.no')) +
                          DSTD('XML-faktura') MSG('XML-faktura fra +
                          Schianders') DOC(&STMF) FLR(&FLR) +
                          DOCD('XML-faktura fra Schianders') +
                          SUBJECT('XML-faktura fra Schianders')      */
             SNDDST     TYPE(*DOC) TOINTNET(('helpdesk@systema.no')) +
                          DSTD('XML-faktura') MSG('XML-faktura fra +
                          Schianders') DOC(&STMF) FLR(&FLR) +
                          DOCD('XML-faktura fra Schianders') +
                          SUBJECT('XML-faktura fra Schianders')
             DLTDLO     DLO(&STMF) FLR(&FLR)
             MONMSG     MSGID(CPF0000)
             DLTF       FILE(QTEMP/XMLTP1K)
             MONMSG     MSGID(CPF0000)

             ENDPGM
