/* KJØR GENERERING AV ORDINÆR SAMLESAD TIL GITTE TIDSPUNKT */
             PGM
             DCL        VAR(&QTIME) TYPE(*CHAR) LEN(6)

             CALL       PGM(SYSPED/INLIBL)

 START:      CALL       PGM(SYXML17T) PARM(' ')
             RTVSYSVAL  SYSVAL(QTIME) RTNVAR(&QTIME)
             IF         COND(&QTIME > '220000') THEN(GOTO +
                          CMDLBL(SLUTT))
             DLYJOB     DLY(3600)
             GOTO       CMDLBL(START)

 SLUTT:      ENDPGM
