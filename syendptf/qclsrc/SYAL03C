             PGM
             DCL        VAR(&FILIBF) TYPE(*CHAR) LEN(10)
             DCL        VAR(&STMF) TYPE(*CHAR) LEN(80)
/*___________*/
/* VARIABLER */
/*"""""""""""*/
             DCLF       FILE(FIRM) RCDFMT(*ALL)
             RCVF       DEV(*FILE) RCDFMT(*FILE)
             CHGVAR     VAR(&STMF) VALUE('/' *CAT &FILIBF *TCAT +
                                   'X/ALTINN/EDIRE1K/TEMP/*')
             WRKLNK     OBJ(&STMF)
             DMPCLPGM
             ENDPGM
