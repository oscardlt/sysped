             PGM        PARM(&PARM &FTXT)
             DCL        VAR(&PARM) TYPE(*CHAR) LEN(833)
             DCL        VAR(&FTXT) TYPE(*CHAR) LEN(900)
             DCL        VAR(&PAR1) TYPE(*CHAR) LEN(256)
             DCL        VAR(&QUSER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QJOB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QNBR) TYPE(*CHAR) LEN(6)
             DCL        VAR(&FAXNR) TYPE(*CHAR) LEN(21)
             DCL        VAR(&CVP) TYPE(*CHAR) LEN(1)
             DCLF       FILE(FAXSEN)
             RCVF       RCDFMT(*FILE)
       /* */
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             OVRPRTF    FILE(SYSJ27PF) PAGESIZE(48 198) CPI(15) +
                          FRONTMGN(0 0,5) OVRFLW(48) DRAWER(2) +
                          OUTQ(QEZJOBLOG)
       /* */
             CHGVAR     VAR(&PAR1) VALUE(%SST(&PARM 1 256))
             CALL       PGM(SYSJ27C) PARM(&PAR1)
       /* */
             RTVJOBA    JOB(&QJOB) USER(&QUSER) NBR(&QNBR)
             CHGVAR     VAR(&FAXNR) VALUE(%SST(&PARM 257 21))
             CALL       PGM(FAX017) PARM(&FAXNR)
             CHGVAR     VAR(&CVP) VALUE(%SST(&PARM 308 1))
       /* */
             IF         COND(&CVP = 'J') THEN(DO) /* MED COVERPAGE */
             DLTDTAARA  DTAARA(QTEMP/CVROVR)
             MONMSG     MSGID(CPF0000)
             CRTDTAARA  DTAARA(QTEMP/CVROVR) TYPE(*CHAR) LEN(384) +
                          VALUE(*BLANKS)
             CHGDTAARA  DTAARA(CVROVR (1 30)) VALUE(%SST(&PARM 543 30))
             CHGDTAARA  DTAARA(CVROVR (31 30)) VALUE(%SST(&PARM 583 +
                          30))
             CHGDTAARA  DTAARA(CVROVR (61 30)) VALUE(%SST(&PARM 623 +
                          30))
             CHGDTAARA  DTAARA(CVROVR (91 30)) VALUE(%SST(&PARM 663 +
                          30))
             CHGDTAARA  DTAARA(CVROVR (216 30)) VALUE(%SST(&PARM 703 +
                          30))
             CHGDTAARA  DTAARA(CVROVR (256 2)) VALUE(&FAXCPN)
             DLTDTAARA  DTAARA(QTEMP/CVRTXT)
             MONMSG     MSGID(CPF0000)
             CRTDTAARA  DTAARA(QTEMP/CVRTXT) TYPE(*CHAR) LEN(1875) +
                          VALUE(*BLANKS)
             CHGDTAARA  DTAARA(CVRTXT (1 900)) VALUE(&FTXT)
             SNDFSPL    SPF(SYSJ27PF &QJOB &QUSER &QNBR *LAST) +
                          DESC('Utskrift av MANIFEST') DIAL(*N +
                          &FAXNR) CVRID(&FAXCPN)
             ENDDO
             ELSE       CMD(DO)
             SNDFSPL    SPF(SYSJ27PF &QJOB &QUSER &QNBR *LAST) +
                          DESC('Utskrift av MANIFEST') DIAL(*N &FAXNR)
             ENDDO
       /* */
 SLUTT:      ENDPGM
