             PGM
             DCL        VAR(&IN01) TYPE(*LGL) LEN(1) VALUE('0')
             DCL        VAR(&DATO) TYPE(*CHAR) LEN(6) VALUE('000000')
             CALL       PGM(SLLAGE) PARM(&DATO &IN01)
             IF         COND(&IN01='1') THEN(DO)
             SNDPGMMSG  MSG('Jobben er avbrutt. Merking/sletting fra +
                          lagerliste er IKKE igangsatt')
             GOTO       CMDLBL(SLUTT)
             ENDDO
             SBMJOB     CMD(CALL PGM(SLLAGE2) PARM(&DATO)) +
                          JOB(SLLAGE) JOBQ(SYJOBQNMAX)
             SNDPGMMSG  MSG('Jobben er sendt til jobbkø SYJOBQNMAX')
 SLUTT:       ENDPGM
