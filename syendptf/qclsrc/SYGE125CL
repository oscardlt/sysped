             PGM        PARM(&PATH &START &ANTA)
             DCL        VAR(&ANTA) TYPE(*CHAR) LEN(10)
             DCL        VAR(&START) TYPE(*CHAR) LEN(10)
             DCL        VAR(&PATH) TYPE(*CHAR) LEN(50)
             DCL        VAR(&PATH2) TYPE(*CHAR) LEN(50)
             DCL        VAR(&QJOB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QUSER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&QNBR) TYPE(*CHAR) LEN(6)
             DCL        VAR(&QSPLF) TYPE(*CHAR) LEN(10) VALUE('QSYSPRT')
             RTVJOBA    JOB(&QJOB) USER(&QUSER) NBR(&QNBR)
             CHGVAR     VAR(&PATH2) VALUE(&PATH *TCAT '/' *TCAT &START +
                          *TCAT '*')
             DSPLNK     OBJ(&PATH2) OUTPUT(*PRINT) DSPOPT(*ALL)
/*  BYGG WORKFILE */
             CRTDUPOBJ  OBJ(SYGE125F) FROMLIB(*LIBL) OBJTYPE(*FILE) +
                          TOLIB(QTEMP) NEWOBJ(SYGE125F)
             MONMSG     MSGID(CPF0000)
             CLRPFM     FILE(QTEMP/SYGE125F)
             MONMSG     MSGID(CPF0000)
             CPYSPLF    FILE(&QSPLF) TOFILE(QTEMP/SYGE125F) +
                          JOB(&QNBR/&QUSER/&QJOB) SPLNBR(*LAST) +
                          MBROPT(*REPLACE) CTLCHAR(*NONE)
             DLTSPLF    FILE(&QSPLF) JOB(&QNBR/&QUSER/&QJOB) +
                          SPLNBR(*LAST)
             CALL       PGM(SYGE125B) PARM(&PATH &START &ANTA)
             RGZPFM     FILE(QTEMP/SYGE125F)

EXIT:        ENDPGM
