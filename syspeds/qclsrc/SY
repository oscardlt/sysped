/* KLARGJORT FOR TILBAKESTILLING TIL NY LØSNING 1 */
             PGM
             DCL        VAR(&CURLIB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&SPEDDTAARA) TYPE(*CHAR) LEN(2000) /* +
                          DENNE VARIABELEN SKAL BEVARE *LDA. */
             RTVDTAARA  DTAARA(*LDA *ALL) RTNVAR(&SPEDDTAARA) /* +
                          BEVARE *LDA. */
             CHGDTAARA  DTAARA(*LDA *ALL) +
                          VALUE('                              ') +
                          /* TØM LDA */
             RTVJOBA    CURLIB(&CURLIB)
             CALL       PGM(SYSPED/INHOVED)
             IF         COND(&CURLIB = 'L1DAT') THEN(DO)
             CHGLIBL    LIBL(IBMENDA IBMEND L1PGM L1STD QGPL QTEMP) +
                        CURLIB(L1DAT)
             ENDDO
 /* */
 /* LEGGE &SPEDDTAARA TILBAKE TIL *LDA  */
             CHGDTAARA  DTAARA(*LDA *ALL) VALUE(&SPEDDTAARA)
 /*  */
             ENDPGM
