/* *********************************************************************   */
/*       * FORENKLET PROSEDYRE                                            */
/* ********************************************************************    */
             PGM        PARM(&AVD &OPD &PRT)
             DCL        VAR(&AVD) TYPE(*DEC) LEN(4 0)
             DCL        VAR(&OPD) TYPE(*DEC) LEN(5 0)
             DCL        VAR(&PRT) TYPE(*CHAR) LEN(10)
             OVRMSGF    MSGF(QUSERMSG) TOMSGF(SYMSGF)
             OVRPRTF    FILE(FP0003P) PAGESIZE(70 80) OVRFLW(64) +
                          OUTQ(&PRT)
             CALL       PGM(FP0003) PARM(&AVD &OPD)

             ENDPGM
