     C*
      * HENTE/LEV-ADRESSER
     C     INNLAN        IFEQ      'N'
     C     INFULL        OREQ      'J'
     C     LINNBR        IFGE      10
     C     LINNBR        ANDLE     11
     C     POSNBR        ANDGE     1
     C     POSNBR        ANDLE     80
     C                   EXSR      SØKVA1
     C                   GOTO      SLSØK
     C                   END
     C*
     C     LINNBR        IFGE      13
     C     LINNBR        ANDLE     15
     C     POSNBR        ANDGE     1
     C     POSNBR        ANDLE     80
     C                   EXSR      SØKVA2
     C                   GOTO      SLSØK
     C                   END
      * INNL.MODUS
     C                   ELSE
     C     LINNBR        IFGE      7
     C     LINNBR        ANDLE     8
     C                   EXSR      SØKVA1
     C                   GOTO      SLSØK
     C                   END
     C*
