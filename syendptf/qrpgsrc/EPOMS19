N01  C                   callp     updHTMLvar('DELAY':DLY(XN))
N02   *
N02  C                   SELECT
N02  C                   WHEN      BSLINSO = 0
N02  C                   EVAL      SOKUND = BSKN
N02  C                   EVAL      SOINV  = WSKNK
N02  C                   EVAL      SOBES  = ELBES
N02  C                   EVAL      SOLIN  = ELLIN
N02  C                   EVAL      SOSTAT = ELSTA
N02  C                   EVAL      SOSKUT = 'J'
N02  C                   WRITE     EPOLSOR
N02  C                   callp     updHTMLvar('checked':'checked')
N02  C                   WHEN      WSBESSO = ELBES AND BSLINSO = ELLIN
N02  C     EPOLSOK1      CHAIN     EPOLSO                             33
N02  C                   IF        SOSKUT = 'J'
N02  C                   EVAL      SOSKUT = 'N'
