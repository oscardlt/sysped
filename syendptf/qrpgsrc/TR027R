     C                   EXSR      INIT
      *
     C     TRHK          CHAIN     TRUH                               33
     C                   IF        *IN33 = *OFF
     C     THAVD         CHAIN     TRUSTD                             33
     C     STD_THFMLL    COMP      'J'                                    27
     C                   MOVEL     THCATS        YKODE
     C     TRK01K        CHAIN     TRKODL01                           33
     C  N33              MOVEL     TKTXTN        XKTXTC
     C   33              EVAL      XKTXTC = THCATS
     C                   MOVEL     THTSB         YKODE
     C     TRK01K        CHAIN     TRKODL01                           33
     C  N33              MOVEL     TKTXTN        XKTXTB
     C   33              EVAL      XKTXTB = THTSB
     C                   MOVEL     THTSB         XXTSB
      *
     C                   MOVE      'N'           THST
     C                   MOVEL     THTRNR        XAN08             8
     C                   IF        XAN08 = 'NØDPROS.'
     C                   MOVE      THTRNR        XNPNR
     C                   ELSE
      * HENT NØDPROSEDYRENR
     C                   IF        STD_THNPNS = ' '
     C                   EVAL      STD_THNPNS = 'A'
     C                   END
     C     STD_THNPNS    CHAIN     SADTRTB                            33
     C                   IF        *IN33
     C                   EVAL      STTTYP = STD_THNPNS
     C                   Z-ADD     2             STTNR
     C                   WRITE     SADTRTR
     C                   Z-ADD     1             XNPNR
     C                   ELSE
     C                   EVAL      XNPNR = STTNR
     C                   ADD       1             STTNR
     C                   UPDATE    SADTRTR
     C                   END
     C                   EVAL      THTRNR = 'NØDPROS.   ' + STD_THNPNS
     C                   MOVE      XNPNR         THTRNR
     C                   END
     C                   UPDATE    TRUHR
      * HENT TOLLSTEDSNAVN
     C                   EXSR      HENTTS
     C                   EVAL      XHADA3 = %TRIM(THPNA) + ' ' + %TRIM(THPSA)
      *
     C                   MOVE      'SAD1'        XSIDE             4
     C     START         TAG
     C                   IF        XPDF = 'J'
     C                   MOVE      *BLANKS       UCMD
     C     XCMD11        CAT       XKOPNVN:0     UCMD
     C                   CAT       ')':0         UCMD
     C                   CAT       XCMD23:1      UCMD
     C                   CAT       XCMD31:0      UCMD
     C                   CAT       '1)':0        UCMD
     C                   CAT       XCMD41:0      UCMD
     C                   CAT       GRAAPP:0      UCMD
     C                   CAT       XKOPTY:0      UCMD
     C                   CAT       GRAAPP:0      UCMD
     C                   CAT       ')':0         UCMD
     C                   CAT       XCMD51:1      UCMD
s02  C*                  CAT       XSIDE:0       UCMD
s02  C*                  CAT       ')':0         UCMD
n02  c                   move      *blanks       FMOVL            20
n02  c                   eval      FMOVL='FRONTMGN(0.14 0.16)'                  PDF(LIKT SAD IMP)
n02  C                   CAT       FMOVL:1       UCMD
     C                   CALL      'QCMDEXC'                            34
     C                   PARM                    UCMD
     C                   PARM                    UCMDL
     C                   MOVE      *BLANKS       F11
s02  C                   EVAL      F11(1) = '.'
     C                   END
      *
     C                   OPEN      TR027PF
S05  C*    THGFT1        COMP      *BLANKS                            4949
N05  C     THGFT1        COMP      *BLANKS                                49
     C     1             DO        9             YN                3 0
     C                   IF        F11(YN) <> *BLANKS
     C                   EVAL      PFFM1 = F11(YN)
     C                   EVAL      PFFM2 = F12(YN)
S04  C*                  WRITE     TR027PF1
N04  C     TRHK          SETLL     TRUV
N04  C     TRHK          READE     TRUV                                   33
N04  C  N33TRHK          READE     TRUV                                   33
N04  c                   z-add     1             antlis
N04  c   33              z-add     0             antlis
N04  C                   WRITE     TR027PF1
     C                   IF        *IN33
     C                   WRITE     TR027PF2
     C                   ELSE
     C                   WRITE     TR027PF3
     C                   EXSR      SRLL
     C                   END
     C                   WRITE     TR027PF4
     C                   IF        XPDF = 'J'
     C                   WRITE     TR027PF6
     C                   ELSE
     C                   WRITE     TR027PF5
     C                   END
     C                   WRITE     TR027PF7
     C                   END
     C                   ENDDO
     C                   CLOSE     TR027PF
      *
     C                   IF        XPDF = 'J'
     C                   SELECT
     C                   WHEN      XSIDE = 'SAD1'
     C                   EVAL      XSIDE = 'SAD4'
     C                   GOTO      START
     C                   WHEN      XSIDE = 'SAD4'
     C                   EVAL      XSIDE = 'SAD5'
     C                   GOTO      START
     C                   ENDSL
     C                   END
N03   *
N03   * OPPDATER T&T
N03  C                   EVAL      TTAVD  = THAVD
N03  C                   EVAL      TTOPD  = THTDN
N03  C                   EVAL      TTACTI = 'TET'
N03  C                   MOVE      WDT           TTDATE
N03  C                   MOVE      WTM           TTTIME
N03  C                   EVAL      TTUSER = ZUSER
N03  C                   MOVE      WDT           TTDATL
