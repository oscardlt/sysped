      *EVENTUELT ENDRE FAKTURALINJER
      *
      * sjekker vaelinjer uansett ved gammel ordre
      *
s68  C* N33TRKDFF        IFNE      WSKDFF
     C  N33              EXSR      SRFALI
s68  C*                  ELSE
      *
      * BYTTET FAKTURAKUNDE
      *
      * SELGER
s68  C* N33HEKNSF        IFNE      0
s68  c*    WSKDFF        ANDEQ     'S'
s68  C*    HEKNSF        IFNE      WSKNSF
s68  C*    HEVALS        ORNE      WSVALS
s68  C*                  EXSR      SRFALI
s68  C*                  END
s68  c*                  ELSE
s68  C* KJØPER
s68  C*                  IF        WSKNKF = 0
s68  C*                  IF        WSKDFF = 'K'
s68  c*                  IF        HEKNKF <> WSKNSF OR HEVALK <> WSVALS
s68  C*                  EXSR      SRFALI
s68  C*                  END
s68  C*                  END
s68  c*                  ELSE
s68  C*    HEKNKF        IFNE      WSKNSF
s68  C*    HEKNKF        IFNE      WSKNKF
s68  C*    HEVALK        ORNE      WSVALS
s68  C*    HEVALK        ORNE      WSVALK
s68  C*                  EXSR      SRFALI
s68  c*                  END
s68  C*                  END
s68  C*                  END
s68  C*                  END
