/* LES FIRMAFIL */
/*""""""""""""""*/
             DCLF       FILE(FIRML1) RCDFMT(*ALL)
             RCVF       DEV(*FILE) RCDFMT(*FILE)
             ADDLIBLE   LIB(&L1STDN) POSITION(*LAST)
             IF         COND(&L1STDN = 'L1STD') THEN(ADDLIBLE +
                          LIB(L1PGM) POSITION(*LAST))
             IF         COND(&L1STDN = 'IBMDAT') THEN(ADDLIBLE +
                          LIB(IBMPGM) POSITION(*LAST))
             ADDLIBLE   LIB(IBMPGM) POSITION(*LAST)
             MONMSG     MSGID(CPF0000)
             ADDLIBLE   LIB(L1PGM) POSITION(*LAST)
             MONMSG     MSGID(CPF0000)
             CHGJOB     JOB(*) LOG(4 00 *SECLVL) LOGCLPGM(*YES)

             ADDLIBLE   LIB(MMAIL) POSITION(*LAST)
             MONMSG     MSGID(CPF0000)
             CHGLF      FILE(KUNDL01) LVLCHK(*NO)
             MONMSG     MSGID(CPF0000)
             CHGLF      FILE(KUNDL02) LVLCHK(*NO)
             MONMSG     MSGID(CPF0000)
             CHGLF      FILE(KUNDL03) LVLCHK(*NO)
             MONMSG     MSGID(CPF0000)
