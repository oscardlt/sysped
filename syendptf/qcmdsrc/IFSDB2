             CMD        PROMPT('KOPIERE FRA IFSFIL TIL DB2FIL')
             PARM       KWD(IFSFIL) TYPE(*CHAR) LEN(50) +
                          PROMPT('Tast fra IFS fil')
             PARM       KWD(SKILLE) TYPE(*CHAR) LEN(1) DFT(';') +
                          PROMPT('Tast Skilletegn')
             PARM       KWD(TILFIL) TYPE(*CHAR) LEN(10) DFT('*FILE') +
                          PROMPT('Til Filnavn')
             PARM       KWD(TILBIB) TYPE(*CHAR) LEN(10) DFT('*LIB') +
                          PROMPT('Til Bibliotek')
